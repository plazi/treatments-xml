<geoCoordinate id="4E6FBFAC0AFEE84A7CA47F88242AC1DE" degrees="25" direction="north" minutes="53" orientation="latitude" precision="1" seconds="32.64" value="25.8924">25°53'32.64&quot;N</geoCoordinate>
