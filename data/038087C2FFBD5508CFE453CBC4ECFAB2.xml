</taxonomicName>
, 
<taxonomicName id="4C294D57FFBD5508CD9C56BFC3D7FB5A" box="[751,915,1243,1268]" class="Insecta" family="Coenagrionidae" genus="Coenagrion" kingdom="Animalia" order="Odonata" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="pulchellum">
