<taxonomicName id="4C16B9A9FFF6FFDBFF3D7AA737C33682" authority="Cretzschmar, 1827" authorityName="Cretzschmar" authorityYear="1827" box="[218,762,1468,1492]" class="Aves" family="Sylviidae" genus="Sylvia" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="14" phylum="Chordata" rank="subSpecies" species="hortensis" subSpecies="crassirostris">
