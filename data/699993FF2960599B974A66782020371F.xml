<taxonomicName id="A10C5CC33B918F4B3C4B53BAA5B79436" authorityName="C.Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" lsidName="" order="Fagales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
