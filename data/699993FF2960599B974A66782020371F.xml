<taxonomicName id="4A26EC05A091D88AF361676179E377AB" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" lsidName="Corylus avellana" order="Fagales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="species" species="avellana">
