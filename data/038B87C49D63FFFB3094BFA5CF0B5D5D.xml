<collectingCountry id="F33576429D60FFFB3164BFA4CFA05CF1" box="[359,427,150,176]" name="Brazil" pageId="10" pageNumber="396">Brazil</collectingCountry>
(
<collectingRegion id="49E6F8309D60FFFB31B8BFA4CC135CF1" box="[443,536,150,176]" country="Brazil" name="Alagoas" pageId="10" pageNumber="396">Alagoas</collectingRegion>
