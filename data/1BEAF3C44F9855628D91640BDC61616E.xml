<taxonomicName id="BAA87FE53D48D63CC64B7329CAF424CE" authorityName="Foon &amp; Liew" authorityYear="2017" class="Gastropoda" family="Cyclophoridae" genus="Alycaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alycaeus alticola" order="Gastropoda" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="alticola">Alycaeus alticola</taxonomicName>
