<taxonomicName id="D9679317C27267C62CE6A522ED74A9DE" class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="635" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
