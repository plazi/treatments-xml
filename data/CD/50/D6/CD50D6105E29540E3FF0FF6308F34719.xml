<typeStatus box="[151,210,298,324]" pageId="1" pageNumber="576">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Tian &amp; Chen &amp; Ma, 2020" authorityName="Tian &amp; Chen &amp; Ma" authorityYear="2020" box="[313,537,297,323]" class="Insecta" family="Carabidae" genus="Miaotrechus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="576" phylum="Arthropoda" rank="species" species="mahua" status="n. sp.">
