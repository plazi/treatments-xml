: Neacrtic Region; 
<collectingCountry box="[1254,1322,727,753]" name="Brazil" pageId="30" pageNumber="31">Brazil</collectingCountry>
and 
<collectingCountry name="Paraguay" pageId="30" pageNumber="31">Paraguay</collectingCountry>
