<paragraph blockId="7.[151,1437,391,920]" pageId="7" pageNumber="240">
<emphasis bold="true" box="[151,451,391,416]" pageId="7" pageNumber="240">
Examined 
<typeStatus box="[281,332,391,416]" pageId="7" pageNumber="240">type</typeStatus>
material:
</emphasis>
<typeStatus box="[459,563,391,416]" pageId="7" pageNumber="240">Holotype</typeStatus>
