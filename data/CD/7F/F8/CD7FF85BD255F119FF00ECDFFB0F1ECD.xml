<subSubSection box="[198,1238,190,216]" pageId="2" pageNumber="55" type="etymology">
<paragraph blockId="2.[197,1238,151,216]" box="[198,1238,190,216]" pageId="2" pageNumber="55">
<emphasis bold="true" box="[198,339,190,216]" pageId="2" pageNumber="55">Etymology.</emphasis>
