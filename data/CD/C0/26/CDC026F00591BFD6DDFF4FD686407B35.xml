<taxonomicName baseAuthorityName="Schlotheim" baseAuthorityYear="1820" class="Gastropoda" family="Pleuroceridae" genus="Pleurocera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pleurocera strombiforme" order="Sorbeoconcha" pageId="281" pageNumber="282" phylum="Mollusca" rank="species" species="strombiforme">
<emphasis italics="true" pageId="281" pageNumber="282">Pleurocera strombiforme</emphasis>
</taxonomicName>
