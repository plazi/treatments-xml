<materialsCitation ID-GBIF-Occurrence="3047558489" box="[364,951,834,861]" country="Bolivia" latitude="-14.832111" location="Trinidad" longLatPrecision="1" longitude="-64.90267" pageId="22" pageNumber="23" specimenCount="1">
