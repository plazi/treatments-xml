</taxonomicName>
and 
<taxonomicName box="[1154,1268,729,752]" class="Magnoliopsida" family="Plumbaginaceae" genus="Limonium" kingdom="Plantae" order="Caryophyllales" pageId="6" pageNumber="42" phylum="Tracheophyta" rank="genus">
<emphasis box="[1154,1268,729,752]" italics="true" pageId="6" pageNumber="42">Limonium</emphasis>
