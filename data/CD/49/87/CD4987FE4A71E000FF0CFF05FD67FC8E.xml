<collectingRegion box="[376,434,621,647]" country="Maldives" name="Maale" pageId="29" pageNumber="30">Male</collectingRegion>
<quantity box="[441,528,621,648]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="29" pageNumber="30" unit="mm" value="6.5">6.5 mm</quantity>
