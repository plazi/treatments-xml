<subSubSection pageId="55" pageNumber="56" type="etymology">
<paragraph blockId="55.[151,1436,1104,1273]" pageId="55" pageNumber="56">
<emphasis bold="true" box="[199,334,1213,1238]" pageId="55" pageNumber="56">Etymology.</emphasis>
