<subSubSection pageId="36" pageNumber="237" type="etymology">
<paragraph blockId="36.[151,1437,1269,2015]" pageId="36" pageNumber="237">
<emphasis bold="true" box="[151,286,1269,1295]" pageId="36" pageNumber="237">Etymology.</emphasis>
