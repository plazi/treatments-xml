<emphasis bold="true" box="[681,772,1194,1219]" pageId="21" pageNumber="22">
<taxonomicNameLabel box="[681,772,1194,1219]" pageId="21" pageNumber="22" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName box="[787,967,1194,1219]" class="Insecta" family="Tineidae" genus="Edosa" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="curvidorsalis" status="sp. nov.">
<emphasis box="[787,967,1194,1219]" italics="true" pageId="21" pageNumber="22">E. curvidorsalis</emphasis>
