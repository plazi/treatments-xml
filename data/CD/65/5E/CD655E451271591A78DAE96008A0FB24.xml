<taxonomicNameLabel box="[975,1065,1194,1219]" pageId="21" pageNumber="22" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[1122,1288,1194,1219]" class="Insecta" family="Tineidae" genus="Edosa" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="duoprojecta" status="sp. nov.">
