<taxonomicName baseAuthorityName="Miyake &amp; Baba" baseAuthorityYear="1967" box="[899,1042,1303,1328]" class="Malacostraca" family="Galatheidae" genus="Leiogalathea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="235" phylum="Arthropoda" rank="species" species="imperialis">
