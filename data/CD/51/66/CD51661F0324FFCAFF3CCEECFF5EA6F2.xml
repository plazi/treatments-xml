<quantity box="[958,1124,764,789]" metricMagnitude="3" metricUnit="m" metricValue="1.075" metricValueMax="1.48" metricValueMin="0.67" pageId="29" pageNumber="230" unit="m" value="1075.0" valueMax="1480.0" valueMin="670.0">670 to 1480 m</quantity>
