<paragraph blockId="12.[151,1437,906,1759]" pageId="12" pageNumber="463">
<emphasis bold="true" box="[199,374,1554,1579]" pageId="12" pageNumber="463">
<typeStatus box="[199,258,1554,1579]" pageId="12" pageNumber="463">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[382,491,1554,1579]" pageId="12" pageNumber="463">Holotype</typeStatus>
: 
<emphasis box="[503,524,1554,1579]" italics="true" pageId="12" pageNumber="463">♂</emphasis>
, 
<collectingCountry box="[539,632,1555,1579]" name="China" pageId="12" pageNumber="463">CHINA</collectingCountry>
: Guangxi, Damingshan, Alt. 
<quantity box="[964,1109,1555,1579]" metricMagnitude="3" metricUnit="m" metricValue="1.05" metricValueMax="1.3" metricValueMin="0.8" pageId="12" pageNumber="463" unit="m" value="1050.0" valueMax="1300.0" valueMin="800.0">800–1300 m</quantity>
, 
