<materialsCitation ID-GBIF-Occurrence="3465844304" pageId="16" pageNumber="17" typeStatus="holotype">
<collectorName box="[1126,1167,1164,1188]" pageId="16" pageNumber="17">The</collectorName>
<typeStatus box="[1175,1222,1164,1188]" pageId="16" pageNumber="17">type</typeStatus>
strain is WF-MO7-1 
<superScript attach="left" box="[1457,1467,1163,1177]" fontSize="6" pageId="16" pageNumber="17">T</superScript>
(=DSM 110576 
<superScript attach="none" box="[998,1008,1193,1207]" fontSize="6" pageId="16" pageNumber="17">T</superScript>
= 
<accessionNumber box="[1022,1157,1195,1219]" httpUri="http://www.ncbi.nlm.nih.gov/protein/LMG31630" pageId="16" pageNumber="17">LMG 31630</accessionNumber>
<superScript attach="none" box="[1157,1167,1193,1207]" fontSize="6" pageId="16" pageNumber="17">T</superScript>
