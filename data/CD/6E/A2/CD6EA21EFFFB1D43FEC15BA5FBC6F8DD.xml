; length antennal segments I–V: 
<number box="[1278,1318,1457,1483]" pageId="34" pageNumber="35" value="0.4">0.4</number>
, 
<number box="[264,303,1497,1523]" pageId="34" pageNumber="35" value="1.4">1.4</number>
, 
<number box="[323,362,1497,1523]" pageId="34" pageNumber="35" value="0.6">0.6</number>
,?; length pronotum 
<number box="[634,673,1497,1523]" pageId="34" pageNumber="35" value="1.2">1.2</number>
, width 
<number box="[773,813,1497,1523]" pageId="34" pageNumber="35" value="2.7">2.7</number>
; length scutellum 
<number box="[1050,1089,1497,1523]" pageId="34" pageNumber="35" value="0.7">0.7</number>
