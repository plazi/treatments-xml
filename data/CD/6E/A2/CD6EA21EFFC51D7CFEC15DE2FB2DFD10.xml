; head length 
<number box="[264,303,444,470]" pageId="29" pageNumber="30" value="0.7">0.7</number>
, width 
<number box="[389,429,444,470]" pageId="29" pageNumber="30" value="0.7">0.7</number>
; length antennal segment I–IV: 
<number box="[799,838,444,470]" pageId="29" pageNumber="30" value="0.2">0.2</number>
, 
<number box="[850,889,444,470]" pageId="29" pageNumber="30" value="1.1">1.1</number>
, 
<number box="[901,940,444,470]" pageId="29" pageNumber="30" value="0.6">0.6</number>
, 
<number box="[952,992,444,470]" pageId="29" pageNumber="30" value="0.4">0.4</number>
; length pronotum 
<number box="[1204,1244,444,470]" pageId="29" pageNumber="30" value="0.7">0.7</number>
