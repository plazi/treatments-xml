<paragraph pageId="0" pageNumber="47">
<taxonomicName genus="O." lsidName="O. omanensis" pageId="0" pageNumber="47" rank="species" species="omanensis">
<emphasis italics="true" pageId="0" pageNumber="47">O. omanensis</emphasis>
