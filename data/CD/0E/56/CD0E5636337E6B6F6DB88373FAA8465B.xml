<taxonomicName authority="Schenkel, 1936" authorityName="Schenkel" authorityYear="1936" box="[283,620,1877,1900]" class="Arachnida" family="Dictynidae" genus="Sudesna" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="29" phylum="Arthropoda" rank="species" species="hedini">
<taxonomicName box="[283,438,1878,1900]" class="Arachnida" family="Dictynidae" genus="Sudesna" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="29" phylum="Arthropoda" rank="species" species="hedini">
