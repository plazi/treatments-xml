<taxonomicName class="Arachnida" family="Liphistiidae" genus="Liphistius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liphistius sumatranus" order="Araneae" pageId="5" pageNumber="126" phylum="Arthropoda" rank="species" species="sumatranus">Liphistius sumatranus</taxonomicName>
