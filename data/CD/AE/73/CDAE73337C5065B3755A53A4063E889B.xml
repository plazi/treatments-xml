<taxonomicName class="Arachnida" family="Liphistiidae" genus="Liphistius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liphistius desultor" order="Araneae" pageId="5" pageNumber="126" phylum="Arthropoda" rank="species" species="desultor">Liphistius desultor</taxonomicName>
<normalizedToken originalValue="Schiödte">Schioedte</normalizedToken>
