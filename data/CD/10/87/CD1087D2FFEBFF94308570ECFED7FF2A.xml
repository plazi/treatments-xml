<subSubSection pageId="1" pageNumber="442" type="description">
<paragraph blockId="1.[151,1437,757,2006]" pageId="1" pageNumber="442">
<emphasis bold="true" box="[199,535,865,890]" pageId="1" pageNumber="442">Description. Measurements.</emphasis>
