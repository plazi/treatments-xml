<subSubSection id="C33A6517FFBD2141D94EFC71FEFB66D1" pageId="48" pageNumber="2830" type="description">
<paragraph id="8B9F369CFFBD2141D94EFC71FD476194" blockId="48.[121,766,781,1343]" box="[121,744,997,1023]" pageId="48" pageNumber="2830">
Basonym 
<taxonomicName id="4C204D1FFFBD2141D9D5FC70FD386199" authority="Gu et al. 2013" authorityName="Gu" authorityYear="2013" box="[226,663,998,1022]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="48" pageNumber="2830" phylum="Firmicutes" rank="species" species="nenjiangensis">
