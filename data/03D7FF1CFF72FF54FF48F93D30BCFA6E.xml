<taxonomicName id="4C7E3589FF72FF54FC1FFF70354CFBC6" baseAuthorityName="Fabricius" baseAuthorityYear="1801" box="[919,1072,1054,1078]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="185" pageNumber="184" phylum="Arthropoda" rank="species" species="tridentata">
<emphasis id="B90A9218FF72FF54FC1FFF70354CFBC6" box="[919,1072,1054,1078]" italics="true" pageId="185" pageNumber="184">C. tridentata</emphasis>
</taxonomicName>
(
<collectingCountry id="F3690E9AFF72FF54FBB5FF7035AEFBC6" box="[1085,1234,1054,1078]" name="Guadeloupe" pageId="185" pageNumber="184">Guadeloupe</collectingCountry>
), 
<taxonomicName id="4C7E3589FF72FF54FB6BFF7034DCFBC6" box="[1251,1440,1054,1078]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="185" pageNumber="184" phylum="Arthropoda" rank="species" species="dominicensis">
<emphasis id="B90A9218FF72FF54FB6BFF7034DCFBC6" box="[1251,1440,1054,1078]" italics="true" pageId="185" pageNumber="184">C. dominicensis</emphasis>
</taxonomicName>
