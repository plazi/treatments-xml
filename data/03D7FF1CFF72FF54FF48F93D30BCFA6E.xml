<collectingCountry id="F3690E9AFF72FF54FF48FF703030FBC6" box="[192,332,1054,1078]" name="Martinique" pageId="185" pageNumber="184">Martinique</collectingCountry>
, 
<collectingCountry id="F3690E9AFF72FF54FEDEFF7030B6FBC6" box="[342,458,1054,1078]" name="Dominica" pageId="185" pageNumber="184">Dominica</collectingCountry>
and 
<collectingCountry id="F3690E9AFF72FF54FD8AFF7033EFFBC6" box="[514,659,1054,1078]" name="Guadeloupe" pageId="185" pageNumber="184">Guadeloupe</collectingCountry>
