<paragraph id="E2388677A926FFD11390FAB4DEBDFA89" blockId="20.[151,1436,1136,1375]" box="[151,734,1350,1375]" pageId="20" pageNumber="421">
XHAK 8 Westbeach ARMS 
<specimenCount id="F4814DFEA926FFD112BEFAB5DD84FA88" box="[441,487,1350,1375]" pageId="20" pageNumber="421" type="generic">
<quantity id="257F2B92A926FFD112BEFAB5DD80FA88" box="[441,483,1350,1375]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="20" pageNumber="421" unit="m" value="5.0">5 m</quantity>
