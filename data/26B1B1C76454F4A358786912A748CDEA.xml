<taxonomicName id="124BEB3F515FFCD91D15646DD7849DCA" class="Monogenea" family="Capsalidae" genus="Capsala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Capsala katsuwoni" order="Capsalidea" pageId="4" pageNumber="5" phylum="Platyhelminthes" rank="species" species="katsuwoni">Capsala katsuwoni</taxonomicName>
