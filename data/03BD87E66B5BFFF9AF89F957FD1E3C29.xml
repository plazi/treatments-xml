<quantity id="4CEC9B156B5BFFFFAEA5FA45FDCC3F05" box="[443,530,559,585]" metricMagnitude="-3" metricUnit="m" metricValue="9.9" pageId="58" pageNumber="59" unit="mm" value="9.9">9.9 mm</quantity>
