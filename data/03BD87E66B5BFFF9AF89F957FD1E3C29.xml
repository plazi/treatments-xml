<quantity id="4CEC9B156B5BFFFFADCCFA44FCF73F05" box="[722,809,558,585]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="58" pageNumber="59" unit="mm" value="5.9">5.9 mm</quantity>
