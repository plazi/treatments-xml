<paragraph id="8BF9A3554D259612FF61BEDEFEC3F8F3" blockId="22.[151,1437,151,1799]" pageId="22" pageNumber="23">
<taxonomicName id="4C46D8D64D259612FF61BEDEFE16F917" baseAuthorityName="Compere" baseAuthorityYear="1940" box="[199,443,1739,1763]" class="Insecta" family="Encyrtidae" genus="Aloencyrtus" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="niloticus">
