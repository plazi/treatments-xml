<taxonomicName id="4C2901BE042B1578FC764D91FB28FC26" authority="Boettger, 1901" authorityName="Boettger" authorityYear="1901" box="[853,1219,952,979]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="34" pageNumber="35" phylum="Mollusca" rank="species" species="eugeniae">
