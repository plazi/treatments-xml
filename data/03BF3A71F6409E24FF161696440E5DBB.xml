<taxonomicName id="4C16F0E4F64E9E24FB5212A547875DBB" authority="Cramer" authorityName="Cramer" class="Insecta" family="Pieridae" genus="Pareronia" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="63" phylum="Arthropoda" rank="species" species="valeria">
