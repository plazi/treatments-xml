<taxonomicName id="4C16F0E4F64E9E24FD5F12A4434C5DC0" authority="Krausse" authorityName="Krausse" box="[734,1077,151,176]" class="Insecta" family="Chalcididae" genus="Brachymeria" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="63" phylum="Arthropoda" rank="species" species="indica">
