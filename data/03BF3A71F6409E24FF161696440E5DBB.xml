<taxonomicName id="4C16F0E4F64E9E24FDE31280440A5DBB" authority="Drury" authorityName="Drury" box="[610,883,188,212]" class="Insecta" family="Pieridae" genus="Delias" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="63" phylum="Arthropoda" rank="species" species="eucharis">
