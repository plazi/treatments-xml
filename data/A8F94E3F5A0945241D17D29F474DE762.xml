<taxonomicName id="FC5FB681918475B6956571255111F989" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29082" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Formica fragilis Heer" lsidName-HNS="Formica fragilis Heer" order="Hymenoptera" pageNumber="136" phylum="Arthropoda" rank="species" species="fragilis">Formica fragilis</taxonomicName>
