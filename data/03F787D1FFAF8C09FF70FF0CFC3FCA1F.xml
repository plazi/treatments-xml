<typeStatus id="54E58865FFAF8C09FF20FCD6FEFCCD77" box="[199,258,834,859]" pageId="2" pageNumber="435">Type</typeStatus>
material
</emphasis>
. 
<typeStatus id="54E58865FFAF8C09FE9AFCD6FE1BCD77" box="[381,485,834,859]" pageId="2" pageNumber="435">Holotype</typeStatus>
♂, 
<collectingCountry id="F3497657FFAF8C09FDF6FCD7FD93CD77" box="[529,621,835,859]" name="China" pageId="2" pageNumber="435">CHINA</collectingCountry>
, Hainan, Ledong, Jianfengling, 
<date id="FFE01007FFAF8C09FC31FCD6FBA6CD77" box="[982,1112,834,859]" pageId="2" pageNumber="435" value="2005-11-15">
