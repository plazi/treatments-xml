<taxonomicName id="09ECE96CBEF5651BFDA4853ED91B3AFF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183835" class="Insecta" family="Formicidae" genus="Polyrhachis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polyrhachis (Myrmhopla) Forel" lsidName-HNS="Polyrhachis (Myrmhopla) Forel" order="Hymenoptera" pageNumber="146" phylum="Arthropoda" rank="subGenus" subGenus="Myrmhopla">P. (Myrmhopla)</taxonomicName>
