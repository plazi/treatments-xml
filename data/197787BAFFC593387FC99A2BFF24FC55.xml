<paragraph id="916136ACFFC593387FC99AC2FD0CFE6D" blockId="22.[151,1436,399,485]" box="[151,712,461,485]" pageId="22" pageNumber="23">
<taxonomicName id="56DE4D2FFFC593387FC99AC2FDCEFE6D" authority="Barker, 1960" authorityName="Barker" authorityYear="1960" box="[151,522,461,485]" class="Globothalamea" family="Uvigerinidae" genus="Euuvigerina" kingdom="Chromista" order="Rotaliida" pageId="22" pageNumber="23" phylum="Foraminifera" rank="species" species="peregrina">
