<taxonomicName id="63B14D28FFE8FFF27D528EACFD7AFC77" authority="THIELE, 1925" authorityName="THIELE" authorityYear="1925" box="[324,673,925,950]" class="Maxillopoda" family="Ctenosculidae" kingdom="Animalia" order="Dendrogastrida" pageId="34" pageNumber="823" phylum="Arthropoda" rank="family">CTENOSCULIDAE THIELE, 1925</taxonomicName>
