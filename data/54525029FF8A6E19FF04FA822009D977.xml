<taxonomicName id="1BFB9ABCFF8A6E12FE0BFAF7261CDD81" box="[408,570,1402,1427]" class="Malacostraca" family="Talitridae" genus="Capeorchestia" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="158" phylum="Arthropoda" rank="genus" status="gen. nov.">
