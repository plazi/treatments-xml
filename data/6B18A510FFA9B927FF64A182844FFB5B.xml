<caption id="B7CE448EFFAEB927FF64A71981D7FC62" httpUri="https://zenodo.org/record/288570/files/figure.png" pageId="5" pageNumber="82" targetBox="[226,1365,201,866]" targetPageId="5">
<paragraph id="E30E1406FFAEB927FF64A71981D7FC62" blockId="5.[151,1435,893,977]" pageId="5" pageNumber="82">
<emphasis id="D1C5C814FFAEB927FF64A7198791FC20" bold="true" box="[151,697,893,916]" pageId="5" pageNumber="82">
