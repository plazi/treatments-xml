<taxonomicName id="8B00EFEA11E9E90002DAE96CC60323DB" class="Magnoliopsida" family="Vitaceae" genus="Vitis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vitis vinifera" order="Vitales" pageId="17" pageNumber="80" phylum="Tracheophyta" rank="species" species="vinifera">Vitis vinifera</taxonomicName>
