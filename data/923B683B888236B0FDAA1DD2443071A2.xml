<taxonomicName id="E0A0CD17403C47CC3EDEF81CAFE05BC5" class="Insecta" family="Megachilidae" genus="Megachile" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megachile centuncularis" order="Hymenoptera" pageId="17" pageNumber="80" phylum="Arthropoda" rank="species" species="centuncularis">Megachile centuncularis</taxonomicName>
