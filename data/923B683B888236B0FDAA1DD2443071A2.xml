<taxonomicName id="C198670F59F1D3312F2017997BBFD838" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Centaurea" order="Asterales" pageId="17" pageNumber="80" phylum="Tracheophyta" rank="genus">Centaurea</taxonomicName>
