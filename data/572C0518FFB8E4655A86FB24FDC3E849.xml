<typeStatus id="003E0AACFFB8E46B5932FB69FCE1EB45" box="[773,868,1269,1289]" pageId="7" pageNumber="1308">Holotypus</typeStatus>
<taxonomicName id="1885CF8DFFB8E46B59B4FB69FBB8EB45" authority="Assing, 2008" authorityName="Assing" authorityYear="2008" box="[899,1085,1269,1289]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="1308" phylum="Arthropoda" rank="species" species="filabresica" status="sp.n.">
