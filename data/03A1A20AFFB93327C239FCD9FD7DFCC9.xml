<number id="3DC0013BFFB93326C119FA34FD81FA6C" box="[552,606,1433,1459]" pageId="12" pageNumber="13" value="1.41">1.41</number>
, Cu length 
<number id="3DC0013BFFB93326C1D5FA34FCCFFA6C" box="[740,784,1433,1459]" pageId="12" pageNumber="13" value="800.0">800</number>
