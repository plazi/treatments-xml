<emphasis id="B918EAEE8355FFF5FC5BFEEBFB52FDA6" bold="true" box="[985,1185,378,401]" pageId="8" pageNumber="814">5. Artengruppe</emphasis>
des 
<taxonomicName id="4C6C4D7F8355FFF5FB63FEEBF9E6FDA6" authorityName="L." box="[1249,1557,378,401]" class="Magnoliopsida" family="Caryophyllaceae" genus="Cerastium" kingdom="Plantae" order="Caryophyllales" pageId="8" pageNumber="814" phylum="Tracheophyta" rank="species" species="latifolium">
