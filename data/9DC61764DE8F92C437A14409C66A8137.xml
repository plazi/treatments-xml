<taxonomicName id="26A7963D92EF5E02CD6FA4332D6EE83D" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Curculionidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Blattodea" pageId="35" pageNumber="108" phylum="Arthropoda" rank="family">Curculionidae</taxonomicName>
