<taxonomicName id="E405CD6F927BA39CE04170D64CD0A21E" genus="Xyeloborus" lsidName="Xyeloborus" pageId="35" pageNumber="108" rank="genus">
<emphasis id="2EB8546DEB50DFE332AA135C64C51E00" italics="true" pageId="35" pageNumber="108">Xyeloborus</emphasis>
