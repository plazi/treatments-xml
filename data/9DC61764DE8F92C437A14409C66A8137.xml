<taxonomicName id="1BF43AA378F1586A0D538654EC353DEB" class="Dicotyledoneae" family="Juglandaceae" genus="Juglans" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Juglans regiae" order="Juglandales" pageId="35" pageNumber="108" phylum="Angiospermae" rank="species" species="regiae">
