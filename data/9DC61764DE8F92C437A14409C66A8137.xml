<taxonomicName id="7544CA898A6CAA29A1C5530380488622" class="Hexapoda" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Agrilus" order="Coleoptera" pageId="35" pageNumber="108" phylum="Arthropoda" rank="genus">
<emphasis id="A113FA5BDEC6FEB21A97EC07F1DEBAA8" italics="true" pageId="35" pageNumber="108">Agrilus</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="5C12480A38BAA92110DB262289FA3302" authorityName="Leach" authorityYear="1815" class="Insecta" family="Buprestidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="35" pageNumber="108" phylum="Arthropoda" rank="family">Buprestidae</taxonomicName>
