<taxonomicName id="A748CF4A44FBD16BA735A9D334786BF3" class="Arachnida" family="Gnaphosidae" genus="Hitobia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hitobia makotoi" order="Araneae" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="makotoi">Hitobia makotoi</taxonomicName>
