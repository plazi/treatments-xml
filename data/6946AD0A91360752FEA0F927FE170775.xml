<taxonomicName id="26EF679F91360752FE00F8A5FDAA071C" authority="Zhang, 2000" authorityName="Zhang" authorityYear="2000" box="[424,621,1794,1819]" class="Insecta" family="Aphididae" genus="Thelazacallis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="ranunculicola">
