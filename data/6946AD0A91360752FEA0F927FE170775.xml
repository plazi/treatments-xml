<taxonomicName id="26EF679F91360752FD78F88AFC9D0741" box="[720,858,1837,1862]" class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" kingdom="Plantae" order="Ranunculales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
