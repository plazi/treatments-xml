<taxonomicName id="26EF679F91360752FB3CF88AFB330741" box="[1172,1268,1837,1862]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
<emphasis id="D39BC00E91360752FB3CF88AFB330741" box="[1172,1268,1837,1862]" italics="true" pageId="21" pageNumber="22">Populus</emphasis>
