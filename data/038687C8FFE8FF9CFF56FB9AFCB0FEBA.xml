<quantity id="4CD79B3BFFE8FF9BFCA7FAD6FC5EFA8E" box="[870,961,1298,1322]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="1" pageNumber="584" unit="mm" value="3.9">3.9 mm</quantity>
