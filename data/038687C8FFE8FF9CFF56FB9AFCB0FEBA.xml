<quantity id="4CD79B3BFFE8FF9BFEEBFA9DFE1CFAD6" box="[298,387,1369,1394]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="1" pageNumber="584" unit="mm" value="1.6">1.6 mm</quantity>
