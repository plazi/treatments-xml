; fore femur = 
<quantity id="4CD79B3BFFE8FF9BFAB8FAD6FF59FAEA" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="1" pageNumber="584" unit="mm" value="2.4">2.4 mm</quantity>
; mid femur = 
<quantity id="4CD79B3BFFE8FF9BFEABFAF1FE5BFAEA" box="[362,452,1333,1358]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="1" pageNumber="584" unit="mm" value="3.5">3.5 mm</quantity>
; hind femur = 
<quantity id="4CD79B3BFFE8FF9BFDAFFAF2FD57FAEA" box="[622,712,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="1" pageNumber="584" unit="mm" value="3.4">3.4 mm</quantity>
; fore tibia = 
<quantity id="4CD79B3BFFE8FF9BFC9CFAF2FC28FAEA" box="[861,951,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="1" pageNumber="584" unit="mm" value="2.1">2.1 mm</quantity>
; mid tibia = 
<quantity id="4CD79B3BFFE8FF9BFB8BFAF2FB3BFAEA" box="[1098,1188,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="1" pageNumber="584" unit="mm" value="3.1">3.1 mm</quantity>
; hind tibia = 
<quantity id="4CD79B3BFFE8FF9BFAFCFAF2FA07FAEA" box="[1341,1432,1334,1358]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="1" pageNumber="584" unit="mm" value="2.9">2.9 mm</quantity>
