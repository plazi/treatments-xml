<subSubSection id="F1BB6520FFE0FFDFFF48FC4CFE346454" pageId="36" pageNumber="37" type="etymology">
<paragraph id="B91E36ABFFE0FFDFFF48FC4CFEB56402" blockId="36.[189,324,1009,1035]" box="[189,324,1009,1035]" pageId="36" pageNumber="37">
<heading id="E25681C7FFE0FFDFFF48FC4CFEB56402" bold="true" box="[189,324,1009,1035]" fontSize="11" level="3" pageId="36" pageNumber="37" reason="3">
