<paragraph id="CB6B8DA9E93EEECC5D267C845805714B" pageId="0" pageNumber="77">
<taxonomicName id="643DD660EC62BB93FF84E96B1C8DB558" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Homolobus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Homolobus stevestroudi" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="stevestroudi">
