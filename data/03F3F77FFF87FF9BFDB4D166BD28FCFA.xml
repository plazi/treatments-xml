<taxonomicName id="4C5A3DEAFF87FF9BFDB4D36FBD34FC97" authorityName="K. Andersen" authorityYear="1908" box="[565,639,797,821]" class="Mammalia" family="Pteropodidae" genus="Pteropus" kingdom="Animalia" order="Chiroptera" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="lylei">
