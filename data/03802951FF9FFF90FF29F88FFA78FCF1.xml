<paragraph id="8B969847FF9FFF91FF29F88FFD7EF8C6" blockId="4.[151,643,1838,1896]" box="[151,643,1838,1864]" pageId="4" pageNumber="5">
<heading id="D0DE2F2BFF9FFF91FF29F88FFD7EF8C6" bold="true" box="[151,643,1838,1864]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<emphasis id="B95D4455FF9FFF91FF29F88FFD7EF8C6" bold="true" box="[151,643,1838,1864]" pageId="4" pageNumber="5">
<taxonomicName id="4C29E3C4FF9FFF91FF29F88FFE1AF8C6" authority="Smith" authorityName="Smith" box="[151,487,1838,1864]" class="Insecta" family="Pergidae" genus="Lagideus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="magdalena" status="new species">
<emphasis id="B95D4455FF9FFF91FF29F88FFE6FF8C6" bold="true" box="[151,402,1838,1864]" italics="true" pageId="4" pageNumber="5">Lagideus magdalena</emphasis>
Smith
</taxonomicName>
, 
<taxonomicNameLabel id="A26EF92EFF9FFF91FE4DF88FFD7EF8C6" box="[499,643,1838,1864]" pageId="4" pageNumber="5" rank="species">new species</taxonomicNameLabel>
</emphasis>
