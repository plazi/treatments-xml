<treatment id="03D0EC22E712FF9EFF74D29CFCF3023E" ID-DOI="http://doi.org/10.5281/zenodo.6237220" ID-GBIF-Taxon="119370404" ID-Zenodo-Dep="6237220" LSID="urn:lsid:plazi:treatment:03D0EC22E712FF9EFF74D29CFCF3023E" httpUri="http://treatment.plazi.org/id/03D0EC22E712FF9EFF74D29CFCF3023E" lastPageId="5" lastPageNumber="6" pageId="4" pageNumber="5">
<subSubSection id="C3630EBFE712FF9FFF74D29CFD1501D3" box="[151,710,151,177]" pageId="4" pageNumber="5" type="nomenclature">
<paragraph id="8BC65D34E712FF9FFF74D29CFD1501D3" blockId="4.[151,710,151,177]" box="[151,710,151,177]" pageId="4" pageNumber="5">
<heading id="D08EEA58E712FF9FFF74D29CFD1501D3" bold="true" box="[151,710,151,177]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<emphasis id="B90D8126E712FF9FFF74D29CFD1501D3" bold="true" box="[151,710,151,177]" pageId="4" pageNumber="5">
Redescription of 
<taxonomicName id="4C7926B7E712FF9FFE8FD29CFD1501D3" authority="Kieffer, 1913" authorityName="Kieffer" authorityYear="1913" box="[364,710,151,177]" class="Insecta" family="Bethylidae" genus="Foenobethylus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
<emphasis id="B90D8126E712FF9FFE8FD29CFDCC01D3" bold="true" box="[364,543,151,177]" italics="true" pageId="4" pageNumber="5">Foenobethylus</emphasis>
<bibRefCitation id="EFE820C5E712FF9FFDC5D29CFD1501D3" author="Kieffer" box="[550,710,151,177]" pageId="4" pageNumber="14" refString="Kieffer, J. J. (1913) Serphides des Iles Philippines. Insecta, Revue illustree d'Entomologie, 3, 253 - 462." type="journal article" year="1913">
Kieffer, 
<number id="3DB14F13E712FF9FFD6FD29CFD1501D3" box="[652,710,151,177]" pageId="4" pageNumber="5" value="1913.0">1913</number>
</bibRefCitation>
</taxonomicName>
</emphasis>
