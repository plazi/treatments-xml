<quantity id="4C8E9BB3FFD8FFEFFBE7F9F8FB96F909" box="[1068,1143,1644,1670]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="46" unit="mm" value="10.0">10mm</quantity>
