<taxonomicName id="919477A3FFDDDC72E096F800FEAAF82C" authorityName="Borner" authorityYear="1903" box="[238,349,1970,1995]" class="Entognatha" family="Isotomidae" genus="Isotomurus" kingdom="Animalia" order="Collembola" pageId="17" pageNumber="404" phylum="Arthropoda" rank="genus">
<emphasis id="64E0D032FFDDDC72E096F800FEAAF82C" box="[238,349,1970,1995]" italics="true" pageId="17" pageNumber="404">Isotomurus</emphasis>
</taxonomicName>
recorded so far from 
<collectingCountry id="2E834CB0FFDDDC72E242F803FD72F82C" box="[570,645,1969,1995]" name="Algeria" pageId="17" pageNumber="404">Algeria</collectingCountry>
