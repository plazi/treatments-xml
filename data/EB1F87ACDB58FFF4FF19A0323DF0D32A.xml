<taxonomicName id="A4B64D39DB5BFFF5FE4AA51E3A6ED71B" authority="A. Warren, Carneiro" authorityName="A. Warren, Carneiro" box="[452,856,1806,1828]" class="Insecta" family="Hesperiidae" genus="Wahydra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="198" phylum="Arthropoda" rank="species" species="graslieae">
<emphasis id="51C2EAA8DB5BFFF5FE4AA51E3BB4D71C" box="[452,642,1806,1827]" italics="true" pageId="2" pageNumber="198">Wahydra graslieae</emphasis>
