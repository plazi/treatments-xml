<subSubSection id="2BAC6531DB5AFFF4FF49A0A43DF0D32A" pageId="3" pageNumber="199" type="etymology">
<paragraph id="630936BADB5AFFF4FF49A0A43DF0D32A" blockId="3.[151,1437,152,789]" pageId="3" pageNumber="199">
<emphasis id="51C2EAA8DB5AFFF4FF49A0A43878D2F2" bold="true" box="[199,334,692,717]" pageId="3" pageNumber="199">Etymology.</emphasis>
