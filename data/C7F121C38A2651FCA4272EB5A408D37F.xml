<taxonomicName id="CBFA5B7A6EC55D4B30B26D2AA663DA68" authorityName="Smith" authorityYear="1879" class="Insecta" family="Halictidae" genus="Sphecodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphecodes montanus" order="Hymenoptera" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="montanus">Sphecodes montanus</taxonomicName>
