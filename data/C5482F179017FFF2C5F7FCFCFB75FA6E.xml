[
<taxonomicName id="8AE1E5829017FFF2C5FCFCFCFCE7FC4E" authorityName="sensu McLay" authorityYear="1993" box="[718,808,876,901]" class="Malacostraca" family="Dromiidae" genus="Dromia" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="288" phylum="Arthropoda" rank="genus">
