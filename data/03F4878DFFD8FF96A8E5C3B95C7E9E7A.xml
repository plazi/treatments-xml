<emphasis id="B929EA89FFDAFF96A9DDC7C35D4B9DB3" box="[431,584,476,500]" italics="true" pageId="10" pageNumber="213">
<taxonomicName id="4C5D4D18FFDAFF96A9DDC7C35D299DB3" box="[431,554,476,500]" class="Insecta" family="Hydroptilidae" genus="Stactobiella" kingdom="Animalia" order="Trichoptera" pageId="10" pageNumber="213" phylum="Arthropoda" rank="species" species="parallelica">parallelica</taxonomicName>
