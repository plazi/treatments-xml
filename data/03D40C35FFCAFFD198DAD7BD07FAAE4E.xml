<quantity id="4C8510C6FFCAFFD199A3D4550031AE0E" box="[494,592,1806,1829]" metricMagnitude="-3" metricUnit="m" metricValue="1.98" pageId="9" pageNumber="10" unit="mm" value="1.98">1.98 mm</quantity>
) 
<emphasis id="B9096131FFCAFFD19A1AD455065EAE0E" box="[599,1087,1806,1829]" italics="true" pageId="9" pageNumber="10">
...................................................... 
<taxonomicName id="4C7DC6A0FFCAFFD19B89D455065EAE0E" authority="Valerio, Masner &amp; Austin" authorityName="Valerio, Masner &amp; Austin" box="[964,1087,1806,1829]" class="Insecta" family="Scelionidae" genus="Cyphacolus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="lucianae" status="n.sp.">C. lucianae</taxonomicName>
