SPECIES: 
<taxonomicName id="4C0518E38E1DFEA0FC3CFAB644593A4B" authority="HUBER, 2000" authorityName="HUBER" authorityYear="2000" box="[914,1103,1294,1317]" class="Liliopsida" family="Haemodoraceae" genus="Blancoa" kingdom="Plantae" order="Commelinales" pageId="329" pageNumber="330" phylum="Tracheophyta" rank="species" species="piacoa" status="new species">
