<number id="3DAF249CFF90FFF5FE03A344D2D4F883" box="[430,504,1796,1820]" pageId="11" pageNumber="12" value="38.0" valueMax="42.0" valueMin="34.0">34–42</number>
; V, 
<number id="3DAF249CFF90FFF5FD8AA344D15CF883" box="[551,624,1796,1820]" pageId="11" pageNumber="12" value="38.5" valueMax="39.0" valueMin="38.0">38–39</number>
; VI, 
<number id="3DAF249CFF90FFF5FD00A344D1D9F883" box="[685,757,1796,1820]" pageId="11" pageNumber="12" value="27.0" valueMax="28.0" valueMin="26.0">26–28</number>
; VII, 
<number id="3DAF249CFF90FFF5FC91A344D0A9F883" box="[828,901,1796,1820]" pageId="11" pageNumber="12" value="13.5" valueMax="15.0" valueMin="12.0">12–15</number>
