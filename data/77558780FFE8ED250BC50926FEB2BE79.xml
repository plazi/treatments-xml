) (
<collectionCode id="99EDAE53FFEFED2509CA0EDAF8D1BCD1" box="[664,720,223,247]" httpUri="http://grbio.org/cool/16z4-1tbx" name="Zoological Reference Collection, National University of Singapore" pageId="4" pageNumber="24">ZRC</collectionCode>
<number id="493424B1FFEFED2509850EDAF953BCD1" box="[727,850,223,247]" pageId="4" pageNumber="24" value="2011.0998">2011.0998</number>
), Sekotong, Lombok, 
<date id="8B421056FFEFED250F1B0EDAFECBBCD1" box="[1097,1226,223,247]" pageId="4" pageNumber="24" value="2005-07-30">
<number id="493424B1FFEFED250F1B0EDAFE8DBCD1" box="[1097,1164,223,247]" pageId="4" pageNumber="24" value="30.07">30.07</number>
. 
<number id="493424B1FFEFED250FDD0EDAFECBBCD1" box="[1167,1226,223,247]" pageId="4" pageNumber="24" value="2005.0">2005</number>
