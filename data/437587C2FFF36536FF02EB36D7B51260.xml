<collectionCode id="ADCDAE11FFF36536FE7AEC47D6731054" box="[495,601,1048,1074]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="55" pageNumber="56" type="Museum">NHMUK</collectionCode>
1849.12.3.59–62, 
<collectionCode id="ADCDAE11FFF36536FCB8EC47D7BD1054" box="[813,919,1048,1074]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="55" pageNumber="56" type="Museum">NHMUK</collectionCode>
