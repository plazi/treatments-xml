<subSubSection id="BEA06513FFED1B20FF071BF4FF4689E4" pageId="91" pageNumber="92" type="etymology">
<paragraph id="F6053698FFED1B20FF071BF4FF4689E4" blockId="91.[140,1108,976,1672]" pageId="91" pageNumber="92">
<emphasis id="C4CEEA8AFFED1B20FF071BF4FEA989B8" bold="true" box="[185,320,1187,1213]" pageId="91" pageNumber="92">
<emphasis id="C4CEEA8AFFED1B20FF071BF4FED289B8" box="[185,315,1187,1213]" inLineHeading="true" pageId="91" pageNumber="92" reason="1">Etymology</emphasis>
.
</emphasis>
