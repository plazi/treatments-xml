<taxonomicName id="4C0513BBFFFFFFE9FBF2C8A3FB7BF9C4" box="[1068,1221,1527,1551]" class="Magnoliopsida" family="Lauraceae" genus="Nectandra" kingdom="Plantae" order="Laurales" pageId="1" pageNumber="110" phylum="Tracheophyta" rank="variety" species="lanceolata" variety="grandifolia">
