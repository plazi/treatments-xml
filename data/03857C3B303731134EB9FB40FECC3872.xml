<heading id="D0DB7A41303731104EB9FB40FDD53F41" box="[140,545,1257,1284]" fontSize="11" level="2" pageId="1" pageNumber="256" reason="3">
<emphasis id="B958113F303731104EB9FB40FDD53F41" bold="true" box="[140,545,1257,1284]" pageId="1" pageNumber="256">
<taxonomicName id="4C2CB6AE303731104EB9FB40FE823F46" authorityName="Staudinger" authorityYear="1857" box="[140,374,1258,1284]" class="Insecta" family="Noctuidae" genus="Euxoa" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="256" phylum="Arthropoda" rank="species" species="westermanni">
<emphasis id="B958113F303731104EB9FB40FE823F46" bold="true" box="[140,374,1258,1284]" italics="true" pageId="1" pageNumber="256">Euxoa westermanni</emphasis>
</taxonomicName>
