<taxonomicName id="4C6B80F3A21EFFF0AB3CF8E1FF7AFD1B" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="110" phylum="Tracheophyta" rank="species" species="undetermined">
