<number id="943823111862FF98995C3B94FB8A1836" box="[1059,1093,488,514]" pageId="15" pageNumber="16" value="42.0">42</number>
'S
</geoCoordinate>
, 
<geoCoordinate id="47C457F11862FF9899173B94FB181836" box="[1128,1239,488,514]" direction="east" orientation="longitude" pageId="15" pageNumber="16" precision="925" value="132.15">
<number id="943823111862FF9899173B94FB591836" box="[1128,1174,488,514]" pageId="15" pageNumber="16" value="132.0">132</number>
°09'E
</geoCoordinate>
