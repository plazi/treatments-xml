</taxonomicName>
. 
<taxonomicName id="8DDA4D1C771EEC1D6D0CF88DFDAE5305" box="[312,588,1796,1821]" class="Insecta" family="Hoplopleuridae" genus="Hoplopleura" kingdom="Animalia" order="Phthiraptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="sciuricola">
