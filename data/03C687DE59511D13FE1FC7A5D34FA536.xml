<taxonomicName id="4C6F4D4B59531D15FF4AC37CD0D1A17B" authority="Camacho, 2007" authorityName="Camacho" authorityYear="2007" box="[233,472,1545,1564]" class="Malacostraca" family="Bathynellidae" genus="Vejdovskybathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="18" pageNumber="2833" phylum="Arthropoda" rank="species" species="caroloi" status="n. sp.">
