<emphasis id="DC92EA96FF86B3153FDB57C92A712A24" bold="true" box="[151,868,1266,1292]" pageId="13" pageNumber="52">
Genus 
<taxonomicName id="29E64D07FF86B3153FA257C92A712A24" authority="Bucquoy, Dautzenberg &amp; Dollfus, 1883" authorityName="Bucquoy, Dautzenberg &amp; Dollfus" authorityYear="1883" box="[238,868,1266,1292]" class="Gastropoda" family="Pyramidellidae" genus="Parthenina" kingdom="Animalia" pageId="13" pageNumber="52" phylum="Mollusca" rank="genus">
