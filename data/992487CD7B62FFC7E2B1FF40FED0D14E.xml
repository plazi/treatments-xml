<taxonomicName id="D68D4D587B62FFC7E2C8FF41FDE2D3B2" authority="Sieg, 1986" authorityName="Sieg" authorityYear="1986" box="[238,572,224,251]" class="Malacostraca" family="Akanthophoreidae" genus="Akanthophoreus" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
