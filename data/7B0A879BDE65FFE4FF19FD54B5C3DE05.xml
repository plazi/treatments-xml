<taxonomicName id="34A34D0EDE62FFE6FC53F94EB41DD987" authorityName="Mayr" authorityYear="1886" box="[989,1090,1708,1731]" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="dentata">
