<taxonomicName id="34A34D0EDE62FFE6FCADF96FB3DAD9E1" authorityName="Burks &amp; Heraty &amp; Dominguez &amp; Mottern" authorityYear="2018" box="[803,901,1679,1700]" class="Insecta" family="Eucharitidae" genus="Orasema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="evansi">
