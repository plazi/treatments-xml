<taxonomicName id="0C9F4D15FA4D1840BF4AFB31FAE2FB1D" authorityName="Gapud" authorityYear="2003" box="[1304,1404,1223,1247]" class="Insecta" family="Ochteridae" genus="Ochterus" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="48" phylum="Arthropoda" rank="species" species="zetteli">
