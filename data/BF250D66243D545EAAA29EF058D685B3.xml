<taxonomicName id="6500C8C974DBC10645FB924539EB91B3" authorityName="Y.Kobayasi" authorityYear="1941" class="Sordariomycetes" family="Ophiocordycipitaceae" genus="Polycephalomyces" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="Ascomycota" rank="order">Coleoptera</taxonomicName>
