<taxonomicName id="4C43ADDA3B4EFFAB3AF2C2C56118FC5A" box="[691,836,952,978]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="belizensis">
<emphasis id="B9370A4B3B4EFFAB3AF2C2C5609EFC5A" box="[691,706,952,977]" italics="true" pageId="14" pageNumber="15">P</emphasis>
. 
<emphasis id="B9370A4B3B4EFFAB3A93C2C56118FC5A" box="[722,836,952,977]" italics="true" pageId="14" pageNumber="15">belizensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C43ADDA3B4EFFAB3B15C2C561E3FC5A" box="[852,959,952,978]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ovatus">
<emphasis id="B9370A4B3B4EFFAB3B15C2C5613FFC5A" box="[852,867,952,977]" italics="true" pageId="14" pageNumber="15">P</emphasis>
. 
<emphasis id="B9370A4B3B4EFFAB3B32C2C561E3FC5A" box="[883,959,952,977]" italics="true" pageId="14" pageNumber="15">ovatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C43ADDA3B4EFFAB3BBDC2C566C8FC5A" box="[1020,1172,952,978]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="dominigos">
<emphasis id="B9370A4B3B4EFFAB3BBDC2C56656FC5A" box="[1020,1034,952,977]" italics="true" pageId="14" pageNumber="15">P</emphasis>
. 
<emphasis id="B9370A4B3B4EFFAB3C5BC2C566C8FC5A" box="[1050,1172,952,977]" italics="true" pageId="14" pageNumber="15">dominigos</emphasis>
</taxonomicName>
. 
<taxonomicName id="4C43ADDA3B4EFFAB3CE4C2C56281FC72" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="belizensis">
