<taxonomicName id="4C43ADDA3B4EFFAB3995C2156021FC0A" box="[468,637,872,897]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="parabelizensis">
