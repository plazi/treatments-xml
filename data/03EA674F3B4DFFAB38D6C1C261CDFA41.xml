<number id="3D8BC47E3B4EFFAB38A5C57562AFFBA9" box="[228,243,1032,1058]" pageId="14" pageNumber="15" value="3.0">3</number>
, z 
<number id="3D8BC47E3B4EFFAB394CC5756340FBA9" box="[269,284,1032,1058]" pageId="14" pageNumber="15" value="2.0">2</number>
