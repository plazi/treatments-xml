<taxonomicName id="2E8E4D0CFFF7FF96D8B8FC04F9B846BA" authorityName="Erwin" authorityYear="1973" box="[1453,1632,906,936]" class="Insecta" family="Carabidae" genus="Xystosomus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="21" phylum="Arthropoda" rank="species" species="spangleri">
<emphasis id="DBFAEA9DFFF7FF96D8B8FC04FA1E46BA" box="[1453,1478,906,936]" italics="true" pageId="24" pageNumber="21">X</emphasis>
