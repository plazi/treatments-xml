<emphasis id="DBFAEA9DFFF8FF99D8C1F5B1F8964F76" bold="true" box="[1492,1870,2623,2660]" pageId="23" pageNumber="20">
The 
<taxonomicName id="2E8E4D0CFFF8FF99DB3CF5B1F9014F71" authorityName="Bates Elytral" authorityYear="1871" box="[1577,1753,2623,2659]" class="Insecta" family="Carabidae" genus="Xystosomus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="20" phylum="Arthropoda" rank="species" species="elaphrinus">
<emphasis id="DBFAEA9DFFF8FF99DB3CF5B1F9014F71" bold="true" box="[1577,1753,2623,2659]" italics="true" pageId="23" pageNumber="20">elaphrinus</emphasis>
</taxonomicName>
