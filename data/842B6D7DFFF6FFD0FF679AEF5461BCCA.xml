<collectingCountry id="74959CFBFFF6FFD1FE4798BF552ABA99" box="[439,546,1925,1951]" name="Malaysia" pageId="10" pageNumber="35">Malaysia</collectingCountry>
(
<collectingCountry id="74959CFBFFF6FFD1FDC898BF55ABBA99" box="[568,675,1925,1951]" name="Malaysia" pageId="10" pageNumber="35">Sarawak</collectingCountry>
