<taxonomicName id="54DF68A6D75FFFB1A714A66A64729DE2" authority="McCoy 1847" authorityName="McCoy" authorityYear="1847" box="[324,857,1133,1158]" class="Trilobita" family="Brachymetopidae" genus="Brachymetopus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Proetida" pageId="11" pageNumber="12" phylum="Arthropoda" rank="subGenus" subGenus="Brachyemtopus">
