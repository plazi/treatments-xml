<taxonomicName id="A0E84D2D0E17FFC5FF7BBB10EFE1F786" authority="Raven, 2008" authorityName="Raven" authorityYear="2008" box="[198,412,2048,2069]" class="Arachnida" family="Barychelidae" genus="Monodontium" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="36" phylum="Arthropoda" rank="species" species="malkini" status="new species">
<emphasis id="559CEABC0E17FFC5FF7BBB10EFE1F786" box="[198,412,2048,2069]" italics="true" pageId="7" pageNumber="36">Monodontium malkini</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="4EAF57C70E17FFC5FE16BB10EC5EF786" box="[427,547,2048,2069]" pageId="7" pageNumber="36" rank="species">new species</taxonomicNameLabel>
