<taxonomicName id="4C4D9BC7FFADFFD4A77CFDBB54C9477E" authorityName="Curran, L. Tsacas" authorityYear="1933" box="[320,502,543,566]" class="Insecta" family="Curtonotidae" genus="Curtonotum" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="quinquevittatum">
<emphasis id="B9393C56FFADFFD4A77CFDBB54C9477E" box="[320,502,543,566]" italics="true" pageId="50" pageNumber="51">quinquevittatum</emphasis>
</taxonomicName>
