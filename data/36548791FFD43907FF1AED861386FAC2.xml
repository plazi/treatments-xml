(
<bibRefCitation id="DA6C4B76FFD43907FE41ED8712EEFBB3" author="Fabricius" box="[460,655,1736,1762]" pageId="5" pageNumber="15" refString="Fabricius, J. C. (1794) Entomilogia systematica emendata et auctea, secundum classes, ordines, genera, species adjecties synonymis, locis, observationibus, descriptionibus, vol 4, Christ. Gottl. Proft, Hafniae. 472 pp." type="book" year="1794">
Fabricius, 
<number id="083524A0FFD43907FDDEED8712EEFBB3" box="[595,655,1736,1762]" pageId="5" pageNumber="6" value="1794.0">1794</number>
