<taxonomicName id="13914D5BFFDCBB4A2FF32CE90B27FF3F" authority="(Schiecke, 1973)" baseAuthorityName="Schiecke" baseAuthorityYear="1973" box="[193,557,150,177]" class="Malacostraca" family="Liljeborgiidae" genus="Idunella" kingdom="Animalia" order="Amphipoda" pageId="12" pageNumber="385" phylum="Arthropoda" rank="species" species="nana">
