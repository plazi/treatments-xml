<taxonomicName id="A7A76A7F2E14CC8F753ED880421A1DD6" authorityName="LeConte" authorityYear="1848" class="Hexapoda" family="Carabidae" genus="Tachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="451" pageNumber="452" phylum="Arthropoda" rank="species" species="corruscus">
