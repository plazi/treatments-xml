<location id="8EAF60065E2DFFD91AC45453FE83FCF8" LSID="urn:lsid:plazi:treatment:03D987CB5E2DFFD91A7655EEFEB2FB5E:8EAF60065E2DFFD91AC45453FE83FCF8" box="[195,291,840,863]" country="Madagascar" municipality="Bosser" name="Ankazobe" pageId="21" pageNumber="176">Ankazobe</location>
