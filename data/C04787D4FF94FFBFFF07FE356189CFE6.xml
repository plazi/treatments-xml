<taxonomicName id="8FEE4D41FF94FFBFFF67FE35679FCAEA" authority="(Lucas, 1878)" baseAuthorityName="Lucas" baseAuthorityYear="1878" box="[247,732,402,429]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="trimaculatum">
