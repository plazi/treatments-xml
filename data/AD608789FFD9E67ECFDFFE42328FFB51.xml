<taxonomicName id="E2C94D1CFFD9E67ECA74FC7F3317FC09" authority="Linnaeus, 1759" authorityName="Linnaeus" authorityYear="1759" class="Insecta" family="Carabidae" genus="Carabus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="267" phylum="Arthropoda" rank="genus">
