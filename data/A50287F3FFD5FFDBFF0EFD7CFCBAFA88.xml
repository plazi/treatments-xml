<emphasis id="1FDFEAF7FFD5FFDBFF0EFD7CFF30FCB5" bold="true" box="[151,213,762,787]" pageId="56" pageNumber="57">*230.</emphasis>
<taxonomicName id="EAAB4D66FFD5FFDBFF44FD7CFDEEFCB5" authority="Plotz, 1886" authorityName="Plotz" authorityYear="1886" box="[221,523,762,787]" class="Insecta" family="Hesperiidae" genus="Gretna" kingdom="Animalia" order="Lepidoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="waga">
