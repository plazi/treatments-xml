<bibRefCitation id="EFBA4B32FFF2FFBF129CC6DD2CA03DC2" author="Stock" box="[759,926,1749,1775]" pageId="3" pageNumber="68" refString="Stock, J. H. (1958 b) Pycnogonida from the Mediterranean coast of Israel. Bulletin of the Research Council of Israel 7 B (3 - 4), 137 - 142." type="journal article" year="1958" yearSuffix="b">
Stock (
<number id="3DE324E4FFF2FFBF1321C6DD2CB93DC2" box="[842,903,1749,1775]" pageId="3" pageNumber="60" value="1958.0">1958</number>
b)
</bibRefCitation>
on the Mediterranean coast of 
<collectingCountry id="F33C7653FFF2FFBF157DC6DD2A653DC2" box="[1302,1371,1749,1775]" name="Israel" pageId="3" pageNumber="60">Israel</collectingCountry>
. 
<bibRefCitation id="EFBA4B32FFF2FFBF1503C6DD2E8E3C3A" author="Chimenz" pageId="3" pageNumber="68" refString="Chimenz Gusso, C. &amp; Lattanzi, L. (2003) Mediterranean Pycnogonida: faunistic, taxonomical and zoogeographical considerations. Biogeographia, 24, 251 - 262." type="journal article" year="2003">
Chimenz &amp; Lattanzi (
<number id="3DE324E4FFF2FFBF1101C6F52E963C3A" box="[362,424,1789,1815]" pageId="3" pageNumber="60" value="2003.0">2003</number>
)
</bibRefCitation>
