<taxonomicName id="4C1B4D55FFC4D15CFEFDB0C5805F9D88" authority="Tang &amp; Li, 2010" authorityName="Tang &amp; Li" authorityYear="2010" box="[360,628,1977,2000]" class="Arachnida" family="Thomisidae" genus="Borboropactus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="brevidens" status="sp. nov.">
<emphasis id="B96FEAC4FFC4D15CFEFDB0C5805F9D88" box="[360,628,1977,2000]" italics="true" pageId="9" pageNumber="10">Borboropactus brevidens</emphasis>
</taxonomicName>
<emphasis id="B96FEAC4FFC4D15CFDEFB0C480E49D88" bold="true" box="[634,719,1976,2000]" pageId="9" pageNumber="10">
