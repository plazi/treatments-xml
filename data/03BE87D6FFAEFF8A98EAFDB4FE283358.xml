<collectingRegion id="49D3F822FFAEFF8A9A19FD7DF95332AC" box="[612,786,655,681]" country="Mexico" name="Baja California" pageId="63" pageNumber="64">Baja California</collectingRegion>
(
<geoCoordinate id="EE235007FFAEFF8A9B5FFD7CF9C732AC" box="[802,902,654,681]" degrees="29" direction="north" minutes="35" orientation="latitude" pageId="63" pageNumber="64" precision="925" value="29.583334">29°35’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FFAEFF8A9BEEFD7CFE4932AC" box="[915,1032,654,681]" degrees="115" direction="west" minutes="48" orientation="longitude" pageId="63" pageNumber="64" precision="925" value="-115.8">115°48’W</geoCoordinate>
