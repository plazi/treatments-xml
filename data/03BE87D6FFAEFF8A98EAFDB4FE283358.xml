<quantity id="4CEF9B25FFAEFF8A9935FD08FBC53310" box="[328,388,762,789]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="63" pageNumber="64" unit="m" value="15.0">15 m</quantity>
(
<quantity id="4CEF9B25FFAEFF8A99EBFD09FB913313" box="[406,464,763,790]" metricMagnitude="1" metricUnit="m" metricValue="1.49352" pageId="63" pageNumber="64" unit="ft" value="49.0">49 ft</quantity>
) (Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, 
<collectingRegion id="49D3F822FFAEFF8A9975FCEDFB3A333F" box="[264,379,799,826]" country="United States of America" name="California" pageId="63" pageNumber="64">California</collectingRegion>
) and as deep as 
<quantity id="4CEF9B25FFAEFF8A9A4CFCEDF839333C" box="[561,632,799,825]" metricMagnitude="2" metricUnit="m" metricValue="1.77" pageId="63" pageNumber="64" unit="m" value="177.0">177 m</quantity>
(
<quantity id="4CEF9B25FFAEFF8A9AFBFCECF88A333F" box="[646,715,798,826]" metricMagnitude="2" metricUnit="m" metricValue="1.770888" pageId="63" pageNumber="64" unit="ft" value="581.0">581 ft</quantity>
) (Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, 
<collectingRegion id="49D3F822FFAEFF8A999AFCB1F81F335B" box="[487,606,835,862]" country="United States of America" name="California" pageId="63" pageNumber="64">California</collectingRegion>
