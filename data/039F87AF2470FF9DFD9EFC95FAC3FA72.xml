species: 
<taxonomicName id="4C364D3A2470FF9DFCC8FCC8FAA5FC71" authority="Lundblad, 1933" authorityName="Lundblad" authorityYear="1933" box="[895,1349,881,907]" class="Insecta" family="Micronectidae" genus="Micronectella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="acuta">
