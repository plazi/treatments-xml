<emphasis id="B97B8548FF93FFD9EFBA4361B152F89B" box="[931,1297,1835,1859]" italics="true" pageId="9" pageNumber="514">
<taxonomicName id="4C0F22D9FF93FFD9EFBA4361B079F89B" box="[931,1082,1836,1859]" class="Insecta" family="Arctiidae" genus="Eucereon" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="514" phylum="Arthropoda" rank="species" species="maternum">E. maternum</taxonomicName>
