<emphasis id="B952DF4CFF29FF2E89B9C3CE216A9B83" bold="true" box="[151,288,1501,1523]" pageId="152" pageNumber="153">FIGURE 87.</emphasis>
<taxonomicName id="4C2678DDFF29FF2E8803C3CE23F69B83" authority="(Ward &amp; Magath, 1917) Petter, 1974" authorityName="(Ward &amp; Magath, 1917) Petter" authorityYear="1974" box="[301,956,1501,1523]" class="Secernentea" family="Cucullanidae" genus="Truttaedacnitis" kingdom="Animalia" order="Ascaridida" pageId="152" pageNumber="153" phylum="Nematoda" rank="species" species="clitellarius">
<emphasis id="B952DF4CFF29FF2E8803C3CE22769B83" box="[301,572,1501,1523]" italics="true" pageId="152" pageNumber="153">Truttaedacnitis clitellarius</emphasis>
