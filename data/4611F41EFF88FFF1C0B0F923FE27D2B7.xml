<quantity id="0940E8EDFF89FFF1C228FEACFDD3D563" box="[527,620,264,290]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="3" pageNumber="48" unit="mm" value="3.6">3.6 mm</quantity>
