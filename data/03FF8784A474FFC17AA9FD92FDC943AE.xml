<taxonomicName id="4C564D11A474FFC17BDCFC4FFDC643C6" authorityName="Temminck" authorityYear="1841" box="[488,616,897,920]" class="Mammalia" family="Talpidae" genus="Urotrichus" kingdom="Animalia" order="Soricomorpha" pageId="21" pageNumber="22" phylum="Chordata" rank="genus">
