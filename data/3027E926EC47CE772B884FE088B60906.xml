<normalizedToken id="AE4F6DB10464A3EBA71A6DC055D788F7" originalValue="südlichen" pageId="null" pageNumber="307">suedlichen</normalizedToken>
Kalkalpen 
<normalizedToken id="FEB73EC199DECA4052B2E8ECCCA2024C" originalValue="östlich" pageId="null" pageNumber="307">oestlich</normalizedToken>
