<taxonomicName id="451743C5F4388753F56297675ECE3FB3" LSID="http://zoobank.org/C77056B3-F7E6-4DD1-B246-9F34291A0F82" authority="Haliday, 1835" authorityName="Haliday" authorityYear="1835" class="Insecta" family="Diapriidae" genus="Ismarus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ismarus madagascariensis" order="Hymenoptera" pageId="17" pageNumber="144" phylum="Arthropoda" rank="species" species="madagascariensis">Ismarus madagascariensis</taxonomicName>
