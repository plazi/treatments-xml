<taxonomicName id="FDE0848EFF8FB27DF55CFC89FE58A657" authorityName="Borojevic &amp; Boury-Esnault &amp; Vacelet" authorityYear="2000" box="[277,444,837,861]" class="Calcarea" family="Trichogypsiidae" kingdom="Animalia" order="Baerida" pageId="52" pageNumber="255" phylum="Porifera" rank="family">Trichogypsiidae</taxonomicName>
