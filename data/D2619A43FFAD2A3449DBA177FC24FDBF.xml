<taxonomicName id="9DC850D6FFAC2A374B7DA32DFC8BF8BA" authority="(Schrank)" baseAuthorityName="Schrank" baseAuthorityYear="1803" box="[561,886,1868,1892]" class="Insecta" family="Agromyzidae" genus="Phytomyza" kingdom="Animalia" order="Diptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="ranunculi">
