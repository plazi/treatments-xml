<taxonomicName id="9DC850D6FFAD2A364854A18FFE6AF9D6" box="[280,407,1518,1544]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="84" pageNumber="85" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
