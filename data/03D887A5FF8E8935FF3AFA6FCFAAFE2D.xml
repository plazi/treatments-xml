<taxonomicName id="4C714D30FF8E8936FC32F96ECA4EF903" authorityName="Laurenti" authorityYear="1768" box="[1006,1063,1719,1742]" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="97" pageNumber="98" phylum="Chordata" rank="genus">
