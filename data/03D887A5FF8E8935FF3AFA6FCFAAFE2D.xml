</taxonomicName>
<taxonomicNameLabel id="A23657DAFF8D8935FF0DFEC0CF28FEFD" box="[209,321,281,304]" pageId="98" pageNumber="99" rank="genus">new gen.</taxonomicNameLabel>
, 
<taxonomicName id="4C714D30FF8D8935FE8DFEC0CFBBFEFD" authority="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER, 2005" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" box="[337,466,281,304]" class="Amphibia" family="Hylidae" genus="Isthmohyla" kingdom="Animalia" order="Anura" pageId="98" pageNumber="99" phylum="Chordata" rank="genus" status="new gen.">
<emphasis id="B905EAA1FF8D8935FE8DFEC0CFBBFEFD" box="[337,466,281,304]" italics="true" pageId="98" pageNumber="99">Isthmohyla</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23657DAFF8D8935FE02FEC0CC27FEFD" box="[478,590,281,304]" pageId="98" pageNumber="99" rank="genus">new gen.</taxonomicNameLabel>
, 
<taxonomicName id="4C714D30FF8D8935FD82FEC0CF4EFE83" authority="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER, 2005" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" class="Amphibia" family="Hylidae" genus="Megastomatohyla" kingdom="Animalia" order="Anura" pageId="98" pageNumber="99" phylum="Chordata" rank="genus" status="new gen.">
<emphasis id="B905EAA1FF8D8935FD82FEC0CF4EFE83" italics="true" pageId="98" pageNumber="99">Megastomatohyla</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23657DAFF8D8935FEE5FEEFCFD9FE80" box="[313,432,310,333]" pageId="98" pageNumber="99" rank="genus">new gen.</taxonomicNameLabel>
