<caption id="DF7B6631FFFCFFDAFF960106FE70FC61" ID-DOI="http://doi.org/10.5281/zenodo.4657894" ID-Zenodo-Dep="4657894" httpUri="https://zenodo.org/record/4657894/files/figure.png" pageId="13" pageNumber="1700" startId="13.[125,185,848,867]" targetBox="[170,1128,133,813]" targetPageId="13">
<paragraph id="8BBB36B9FFFCFFDAFF960106FE70FC61" blockId="13.[125,1172,848,921]" pageId="13" pageNumber="1700">
