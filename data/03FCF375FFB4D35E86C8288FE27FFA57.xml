<materialsCitation id="3B3D483EFFB4D35F86722FE4E26BFB60" box="[379,1087,1185,1209]" collectionCode="USNM" pageId="2" pageNumber="1422" specimenCode="USNM 253299" specimenCount="1" specimenCount-juvenile="1">
