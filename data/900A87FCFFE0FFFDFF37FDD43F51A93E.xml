III 
<number id="AE6B24CDFFE0FFC3FD62FBAB38DDAC03" box="[706,752,1109,1135]" pageId="31" pageNumber="32" value="2.0" valueMax="3.0" valueMin="1.0">1‚Äì3</number>
</date>
; 
<date id="6C1D102AFFE0FFC3FD5EFBAB397FAC03" box="[766,850,1109,1135]" pageId="31" pageNumber="32">
IV 
