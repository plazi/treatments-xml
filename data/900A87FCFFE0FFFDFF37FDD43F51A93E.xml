VI 
<number id="AE6B24CDFFE0FFC3FC7EFBAB3E31AC03" box="[990,1052,1109,1135]" pageId="31" pageNumber="32" value="8.0" valueMax="10.0" valueMin="6.0">6‚Äì10</number>
</date>
; 
<date id="6C1D102AFFE0FFC3FB88FBAB3E88AC03" box="[1064,1189,1109,1135]" pageId="31" pageNumber="32" value="1912-07-15">
VII 
<number id="AE6B24CDFFE0FFC3FBF9FBAB3E88AC03" box="[1113,1189,1109,1135]" pageId="31" pageNumber="32" value="13.5" valueMax="15.0" valueMin="12.0">12‚Äì15</number>
</date>
; and VIII 0 or 
<number id="AE6B24CDFFE0FFC3FAF3FBAB3F4FAC03" box="[1363,1378,1109,1135]" pageId="31" pageNumber="32" value="1.0">1</number>
