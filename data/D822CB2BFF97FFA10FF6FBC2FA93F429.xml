<location id="55542CE6FF97FFA60DB3FAB7FCD3F378" LSID="urn:lsid:plazi:treatment:D822CB2BFF97FFA10FF6FBC2FA93F429:55542CE6FF97FFA60DB3FAB7FCD3F378" box="[722,839,1303,1329]" name="Stolleklint" pageId="3" pageNumber="589">Stolleklint</location>
