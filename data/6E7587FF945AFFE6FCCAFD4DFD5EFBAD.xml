<taxonomicName id="21DC4D6A945AFFE5FC1AF96DFA5DF90D" authority="(Pallas, 1770)" baseAuthorityName="Pallas" baseAuthorityYear="1770" box="[1006,1414,1754,1779]" class="Actinopterygii" family="Gobiidae" genus="Gobionellus" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="2" phylum="Chordata" rank="species" species="oceanicus">
