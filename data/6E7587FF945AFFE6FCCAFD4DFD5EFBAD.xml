<taxonomicName id="21DC4D6A945AFFE5FCA4F94DFBA3F8ED" authority="(Starks, 1913)" baseAuthorityName="Starks" baseAuthorityYear="1913" box="[848,1144,1786,1811]" class="Actinopterygii" family="Gobiidae" genus="Gobionellus" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="2" phylum="Chordata" rank="species" species="stomatus">
