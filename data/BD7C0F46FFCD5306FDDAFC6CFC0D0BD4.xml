<paragraph id="356ABE50FFCF5307FF24FE9CFEE30B4F" blockId="22.[189,342,267,293]" box="[189,342,267,293]" pageId="22" pageNumber="23">
<heading id="6E22093CFFCF5307FF24FE9CFEE30B4F" bold="true" box="[189,342,267,293]" fontSize="11" level="3" pageId="22" pageNumber="23" reason="3">
