<specimenCount id="9D42FD50621B8A45DB17E43D7D47A174" box="[309,380,1574,1596]" pageId="55" pageNumber="1200" type="generic">
<quantity id="4CBC9B3C621B8A45DB17E43D7D43A174" box="[309,376,1574,1596]" metricMagnitude="2" metricUnit="m" metricValue="8.5" pageId="55" pageNumber="1200" unit="m" value="850.0">850 m</quantity>
.
</specimenCount>
</paragraph>
<paragraph id="8BFB36D962148A4ADA81E2CF783AA7A6" blockId="56.[163,1025,211,233]" box="[163,1025,211,233]" pageId="56" pageNumber="1201">
