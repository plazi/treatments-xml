<taxonomicName id="2B8862CCFFDD4F31E1E50FDFB7B2FE11" authority="Bronn, 1825" authorityName="Bronn" authorityYear="1825" box="[195,447,466,490]" class="Stenolaemata" family="Stomatoporidae" genus="Stomatopora" kingdom="Animalia" order="Cyclostomatida" pageId="50" pageNumber="67" phylum="Bryozoa" rank="genus">
<emphasis id="DEFCC55DFFDD4F31E1E50FDFB735FE12" box="[195,312,466,489]" italics="true" pageId="50" pageNumber="67">Stomatopora</emphasis>
