<taxonomicName id="2B8862CCFFDD4F31E0EE0F46B479FE98" box="[456,628,331,355]" class="Globothalamea" family="Nummulitidae" genus="Nummulites" kingdom="Chromista" order="Rotaliida" pageId="50" pageNumber="67" phylum="Foraminifera" rank="species" species="rotula">
