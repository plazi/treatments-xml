<quantity id="C11C9B766B39FF93FAFAFE19FAE69483" box="[1324,1368,413,439]" metricMagnitude="-2" metricUnit="kg" metricValue="1.6" pageId="10" pageNumber="63" unit="g" value="16.0">16g</quantity>
