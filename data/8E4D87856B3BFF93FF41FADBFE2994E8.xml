<taxonomicName id="C1E44D106B39FF93FC77FE8CFB919415" box="[929,1071,264,289]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="10" pageNumber="63" phylum="Arthropoda" rank="species" species="bickorum">
