<taxonomicName id="C1E44D106B39FF93FD8DFEFCFD7A94A5" box="[603,708,376,401]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="10" pageNumber="63" phylum="Arthropoda" rank="species" species="pataxo">
