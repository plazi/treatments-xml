<taxonomicName id="4C5CADB94E32C152FDC5F342A0C6B919" authority="velatus (Michael, 1880)" authorityName="velatus (Michael" authorityYear="1880" baseAuthorityName="Michael" baseAuthorityYear="1880" box="[598,991,1829,1848]" class="Arachnida" family="Tectocepheidae" genus="Tectocepheus" kingdom="Animalia" order="Sarcoptiformes" pageId="21" pageNumber="61" phylum="Arthropoda" rank="subSpecies" species="velatus" subSpecies="velatus">
