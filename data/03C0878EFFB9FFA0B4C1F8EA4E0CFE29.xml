<taxonomicName id="4C694D1BFFBBFFA1B50AF9674AF8F88F" box="[348,507,1771,1796]" class="Insecta" family="Sciaridae" genus="Claustropyga" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="59" phylum="Arthropoda" rank="species" species="auriculata">
