<taxonomicName id="21B14D0C217DFFCDAEC2F823FAD207DF" authority="Jacquemart, 1961" authorityName="Jacquemart" authorityYear="1961" box="[910,1312,1929,1955]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="trispina">
