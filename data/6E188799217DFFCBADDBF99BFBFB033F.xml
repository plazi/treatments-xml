<taxonomicName id="21B14D0C217DFFCDAF06F81AFC9507B6" authority="Mosely, 1939" authorityName="Mosely" authorityYear="1939" box="[586,871,1968,1994]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="zoria">
