(
<bibRefCitation id="82204B7E217DFFCDAFF5F81AFCAC07B6" author="Mosely" box="[697,862,1968,1994]" pageId="3" pageNumber="9" refString="Mosely, M. E. (1939) Trichoptera. Ruwenzori Expedition 1394 - 5. British Museum (Natural History), 3 (1), 1 - 40." type="journal article" year="1939">
Mosely, 
<number id="507924A8217DFFCDAE6CF81AFCAC07B6" box="[800,862,1968,1994]" pageId="3" pageNumber="4" value="1939.0">1939</number>
</bibRefCitation>
)
