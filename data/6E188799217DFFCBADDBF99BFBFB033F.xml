<taxonomicName id="21B14D0C217DFFCDAC88F823FCBA07DF" authority="Jacquemart, 1966" authorityName="Jacquemart" authorityYear="1966" box="[452,840,1929,1955]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="falcifera">
<emphasis id="D4C5EA9D217DFFCDAC88F823FDAA07DE" box="[452,600,1929,1954]" italics="true" pageId="3" pageNumber="4">C. falcifera</emphasis>
