<geoCoordinate id="EE1549C9FF0855A1FC0D4224FBD7FC8C" box="[966,1105,837,862]" direction="west" orientation="longitude" pageId="183" pageNumber="184" precision="15" value="-16.709167">16°42′33″W</geoCoordinate>
; 
<elevation id="000CC83DFF0855A1FB974224FB59FC8C" box="[1116,1247,837,862]" metricMagnitude="2" metricUnit="m" metricValue="5.35" pageId="183" pageNumber="184" unit="m" value="535.0">
<quantity id="4CD982EBFF0855A1FB974224FB25FC8F" box="[1116,1187,837,862]" metricMagnitude="2" metricUnit="m" metricValue="5.35" pageId="183" pageNumber="184" unit="m" value="535.0">535 m</quantity>
a.s.l.
</elevation>
), 
<date id="FF9F09CEFF0855A1FB3D4224FAE1FC8C" box="[1270,1383,837,862]" pageId="183" pageNumber="184" value="1986-08">
<collectingDate id="EFDBF026FF0855A1FB3D4224FAE1FC8C" box="[1270,1383,837,862]" pageId="183" pageNumber="184" value="1986-08">VIII.1986</collectingDate>
</date>
, leg. 
