<taxonomicName id="A29F392D8D2C6828CDD37F13F38DE3CE" authority="Mjöberg, 1925" authorityName="Mjöberg" authorityYear="1925" class="Insecta" family="Lycidae" genus="Duliticola" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Duliticola korinchiana" order="Coleoptera" pageId="17" pageNumber="46" phylum="Arthropoda" rank="species" species="korinchiana">Duliticola korinchiana</taxonomicName>
