<taxonomicName id="35B14D74A272FFABFBE76C5A3AF85D6C" box="[1122,1235,262,286]" class="Cyanophyceae" family="Nostocaceae" genus="Nostoc" kingdom="Bacteria" order="Nostocales" pageId="30" pageNumber="211" phylum="Cyanobacteria" rank="species" species="undetermined">
