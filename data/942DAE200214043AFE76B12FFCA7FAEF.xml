<taxonomicName id="DB8464B50214043AFE3AB12FFD57FC03" authority="Kissinger" authorityName="Kissinger" authorityYear="2005" box="[458,675,893,914]" class="Insecta" family="Brentidae" genus="Hecyrapion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="75" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="2EF0C3240214043AFE3AB12FFDCAFC03" bold="true" box="[458,574,893,914]" italics="true" pageId="4" pageNumber="75">Hecyrapion</emphasis>
Kissinger
</taxonomicName>
, 
<taxonomicNameLabel id="35C37E5F0214043AFD5FB12FFCE2FC03" box="[687,790,893,914]" pageId="4" pageNumber="75" rank="genus">new genus</taxonomicNameLabel>
</heading>
</paragraph>
