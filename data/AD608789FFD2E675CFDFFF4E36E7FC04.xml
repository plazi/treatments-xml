<paragraph id="2576369FFFD2E675CFDFFF4E3185FF5D" blockId="13.[151,982,152,178]" box="[151,982,152,178]" pageId="13" pageNumber="274">
<heading id="7E3E81F3FFD2E675CFDFFF4E3185FF5D" bold="true" box="[151,982,152,178]" fontSize="11" level="1" pageId="13" pageNumber="274" reason="1">
<emphasis id="17BDEA8DFFD2E675CFDFFF4E3185FF5D" bold="true" box="[151,982,152,178]" pageId="13" pageNumber="274">
<taxonomicName id="E2C94D1CFFD2E675CFDFFF4E3177FF5D" authority="Obenberger, 1928" authorityName="Obenberger" authorityYear="1928" box="[151,804,152,178]" class="Insecta" family="Buprestidae" genus="Anthaxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="274" phylum="Arthropoda" rank="species" species="sulcicollis" subGenus="Capanthaxia">
<emphasis id="17BDEA8DFFD2E675CFDFFF4E335BFF5D" bold="true" box="[151,264,152,178]" italics="true" pageId="13" pageNumber="274">Anthaxia</emphasis>
(
<emphasis id="17BDEA8DFFD2E675CE50FF4E33E7FF5D" bold="true" box="[280,436,152,178]" italics="true" pageId="13" pageNumber="274">Capanthaxia</emphasis>
