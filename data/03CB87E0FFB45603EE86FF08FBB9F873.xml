<taxonomicName id="4C624D75FFB55603EF62FBC0FDB5FBC0" box="[371,522,1119,1144]" class="Cestoda" family="Anoplocephalidae" genus="Microcephaloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="9" pageNumber="10" phylum="Platyhelminthes" rank="species" species="variabilis">
