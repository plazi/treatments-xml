, 
<taxonomicName id="A4D54D49FFDFFFDBFC7B7CE69C72F0A6" box="[990,1125,152,175]" class="Insecta" family="Trogossitidae" genus="Grynocharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="genus">
