and 
<collectingRegion id="C235F856FFCA8A2CFAE7EE0DF9A8C8B0" box="[1323,1581,4524,4563]" country="South Africa" name="Eastern Cape" pageId="5" pageNumber="284">Eastern Cape</collectingRegion>
