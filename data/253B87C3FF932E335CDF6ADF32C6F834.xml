<paragraph id="AD2D36D5FF932E335C8E6B0932C9F8A5" blockId="7.[151,1436,1734,2018]" pageId="7" pageNumber="40">
Shrubs 
<quantity id="6A6A9B30FF932E335D686B093154F8FC" box="[288,363,1808,1834]" metricMagnitude="0" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="7" pageNumber="40" unit="m" value="2.0" valueMax="3.0" valueMin="1.0">1–3 m</quantity>
