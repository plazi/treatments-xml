<taxonomicName id="A64DA6BC529CD739E51ADAE91863A0BE" authorityName="Walsingham" authorityYear="1895" class="Insecta" family="Vitaceae" genus="Coptodisca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="0" pageNumber="105" phylum="Arthropoda" rank="family">Vitaceae</taxonomicName>
