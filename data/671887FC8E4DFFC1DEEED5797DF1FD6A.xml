<location id="EA6E60318E4DFFC1DDDAD6197B0AFD8B" LSID="urn:lsid:plazi:treatment:671887FC8E4DFFC1DEEED5797DF1FD6A:EA6E60318E4DFFC1DDDAD6197B0AFD8B" box="[931,1036,515,539]" country="Peru" county="Carbonell" municipality="There" name="Specimen" pageId="39" pageNumber="384">Specimen</location>
