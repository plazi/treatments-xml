<taxonomicName id="4C4E4D75FFABFFCFB774EC38FD7FFC0C" box="[359,642,556,580]" class="Insecta" family="Tephritidae" genus="Anastrepha" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="oiapoquensis">
<emphasis id="B93AEAE4FFABFFCFB774EC38FD7FFC0C" box="[359,642,556,580]" italics="true" pageId="4" pageNumber="65">Anastrepha oiapoquensis</emphasis>
