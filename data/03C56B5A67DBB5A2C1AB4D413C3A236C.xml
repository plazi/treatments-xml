<taxonomicName id="4C6CA1CF67DAB5A2C46549713938236C" class="Magnoliopsida" family="Asteraceae" genus="Picris" kingdom="Plantae" order="Asterales" pageId="399" pageNumber="400" phylum="Tracheophyta" rank="species" species="hieracioides">
