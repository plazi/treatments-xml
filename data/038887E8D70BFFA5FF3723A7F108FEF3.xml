, 
<collectingCountry id="F336766ED70BFFA2FB1221ACF63AF806" box="[1202,1291,1951,1977]" name="Bolivia" pageId="3" pageNumber="60">Bolivia</collectingCountry>
, 
<collectingCountry id="F336766ED70BFFA2FAB921ACF652F806" box="[1305,1379,1951,1977]" name="Brazil" pageId="3" pageNumber="60">Brazil</collectingCountry>
, and 
<collectingCountry id="F336766ED70BFFA2FF3721F5F3CFF85F" box="[151,254,1990,2016]" name="Uruguay" pageId="3" pageNumber="60">Uruguay</collectingCountry>
(
<bibRefCitation id="EFB04B0FD70BFFA2FEAC21F5F2EBF85F" author="Maldonado" box="[268,474,1990,2016]" pageId="3" pageNumber="62" refString="Maldonado Capriles, J. (1990) Systematic catalogue of the Reduviidae of the world (Insecta: Heteroptera). Caribbean Journal of Science, Special edition, 1 - 694." type="book chapter" year="1990">
