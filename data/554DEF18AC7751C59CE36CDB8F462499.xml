<taxonomicName id="F9F69DA5FC8025A31B678DF2268B8C0B" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Pinales" pageId="0" pageNumber="48785" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
