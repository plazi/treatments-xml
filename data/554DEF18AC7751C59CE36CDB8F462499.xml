<figureCitation id="302995678FE59F396613CD217ADE274F" captionStartId="F5442593" pageId="0" pageNumber="48785">4</figureCitation>
<figureCitation id="D89900AB584D9AB089C084C35CEE4546" captionStartId="F5442600" pageId="0" pageNumber="48785">c</figureCitation>
