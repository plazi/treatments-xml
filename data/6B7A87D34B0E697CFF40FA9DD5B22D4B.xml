<taxonomicName id="24D34D464B0E697CFAE2F893D6F62D4B" class="Chilopoda" family="Scolopendridae" genus="Otostigmus" kingdom="Animalia" order="Scolopendromorpha" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="sulcatus">
