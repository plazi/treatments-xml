<taxonomicName id="439367CAFF68F60AFDBCDD8BFDCEF8A1" baseAuthorityName="Schneider" baseAuthorityYear="1799" box="[534,633,1770,1796]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="fuscus">
