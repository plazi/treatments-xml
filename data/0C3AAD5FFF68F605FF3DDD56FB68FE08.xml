<taxonomicName id="439367CAFF67F605FC1ADBF7FB00FF14" authorityName="Freitas" authorityYear="1959" box="[944,1207,151,177]" class="Chromadorea" family="Cosmocercidae" genus="Schrankiana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="8" pageNumber="9" phylum="Nematoda" rank="species" species="formosula">
