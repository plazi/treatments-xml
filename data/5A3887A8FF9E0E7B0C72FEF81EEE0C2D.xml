<taxonomicName id="15914D3DFF9E0E7B0B18FD4D1FA50A98" authorityName="Kozur &amp; Mostler" authorityYear="1979" box="[1033,1275,739,763]" class="Polycystinea" family="Hagiastridae" genus="Tetraporobrachia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Spumellaria" pageId="15" pageNumber="390" phylum="Retaria" rank="species" species="haeckeli">
