<collectingCountry id="AA86762EFF9E0E7B0A79FD531EF30B76" box="[1384,1453,765,789]" name="Turkey" pageId="15" pageNumber="390">Turkey</collectingCountry>
, Huğlu Tuffite (Forel 
<emphasis id="E0E5EAACFF9E0E7B0CE8FCB61F770B53" box="[1017,1065,792,816]" italics="true" pageId="15" pageNumber="390">et al.</emphasis>
2019a); 
<taxonomicName id="15914D3DFF9E0E7B0B93FCB61FBC0B53" authority="Torres" authorityName="Torres" box="[1154,1250,792,816]" class="Liliopsida" family="Poaceae" genus="Nicoraella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="15" pageNumber="390" phylum="Tracheophyta" rank="genus">
<emphasis id="E0E5EAACFF9E0E7B0B93FCB61FBC0B53" box="[1154,1250,792,816]" italics="true" pageId="15" pageNumber="390">Nicoraella</emphasis>
</taxonomicName>
