<taxonomicName id="D9B64BC1FFA7FFDFD26E20C6FCAF248D" box="[602,818,1933,1955]" pageId="7" pageNumber="580" rank="species" species="cohnhafti" status="sp. nov." subGenus="Miroculis">
(
<emphasis id="2CC2EC50FFA7FFDFD25720C6FD59248D" box="[611,708,1933,1955]" italics="true" pageId="7" pageNumber="580">Miroculis</emphasis>
) 
<taxonomicName id="D9B64BC1FFA7FFDFD2E620C6FCAF248D" box="[722,818,1933,1955]" pageId="7" pageNumber="580" rank="species" species="cohnhafti" status="sp. nov.">
<emphasis id="2CC2EC50FFA7FFDFD2E620C6FCAF248D" box="[722,818,1933,1955]" italics="true" pageId="7" pageNumber="580">cohnhafti</emphasis>
