<paragraph id="D3D65C4E6BE0CF379021A8201D8F5473" pageId="255" pageNumber="256">
<taxonomicName id="46F9FD7176F796B90668032B7A58C287" class="Insecta" family="Scarabaeidae" genus="Phanaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phanaeus noctis" order="Coleoptera" pageId="255" pageNumber="256" phylum="Arthropoda" rank="species" species="noctis">Phanaeus noctis</taxonomicName>
