<taxonomicName id="08E02B5E5A9E2F397A7BB63839AAFA46" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26816" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Camponotus inflatus Lubbock" lsidName-HNS="Camponotus inflatus Lubbock" order="Hymenoptera" pageNumber="51" phylum="Arthropoda" rank="species" species="inflatus">C. inflatus</taxonomicName>
