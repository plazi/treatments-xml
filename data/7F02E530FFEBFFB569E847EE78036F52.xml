<taxonomicName id="30AB2FA5FFEBFFB5688744877E556F7E" box="[504,624,762,788]" class="Aves" family="Accipitridae" genus="Buteo" kingdom="Animalia" order="Accipitriformes" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="lagopus">
