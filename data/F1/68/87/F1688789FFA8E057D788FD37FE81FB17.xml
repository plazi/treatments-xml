<emphasis box="[237,508,1464,1489]" italics="true" pageId="9" pageNumber="10">Trachythyone cynthiae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[516,612,1463,1489]" pageId="9" pageNumber="10" rank="species">
<emphasis bold="true" box="[516,612,1463,1489]" pageId="9" pageNumber="10">sp. nov.</emphasis>
</taxonomicNameLabel>
(below); 
<taxonomicName box="[730,1052,1464,1489]" class="Holothuroidea" family="Cucumariidae" genus="Trachythyone" kingdom="Animalia" order="Dendrochirotida" pageId="9" pageNumber="10" phylum="Echinodermata" rank="species" species="mackenzieae" status="sp. nov.">
<emphasis box="[730,1052,1464,1489]" italics="true" pageId="9" pageNumber="10">Trachythyone mackenzieae</emphasis>
