<taxonomicName authority="Ludwig, 1874" authorityName="Ludwig" authorityYear="1874" box="[584,1011,1502,1528]" class="Holothuroidea" family="Cucumariidae" genus="Trachythyone" kingdom="Animalia" order="Dendrochirotida" pageId="9" pageNumber="10" phylum="Echinodermata" rank="species" species="parva">
