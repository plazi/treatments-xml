<taxonomicName authority="Peyer, 1944" authorityName="Peyer" authorityYear="1944" box="[529,873,728,755]" class="Asteroidea" family="Paleobenthopectinidae" genus="Plesiastropecten" higherTaxonomySource="GBIF" kingdom="Animalia" order="Paxillosida" pageId="29" pageNumber="178" phylum="Echinodermata" rank="genus">
