<paragraph pageId="15" pageNumber="26">Remarks.</paragraph>
<paragraph pageId="15" pageNumber="26">
<taxonomicName baseAuthorityName="Rossmaessler" baseAuthorityYear="1835" class="Gastropoda" family="Lymnaeidae" genus="Radix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Basommatophora" pageId="15" pageNumber="26" phylum="Mollusca" rank="species" species="labiata">
