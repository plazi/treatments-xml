<taxonomicName class="Aves" family="Sulidae" genus="Morus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Morus australis" order="Suliformes" pageId="0" pageNumber="117" phylum="Chordata" rank="species" species="australis">
