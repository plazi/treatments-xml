<paragraph blockId="4.[264,1323,1107,1333]" pageId="4" pageNumber="43">
<typeStatus box="[312,415,1107,1133]" pageId="4" pageNumber="43">Paratype</typeStatus>
female: unless otherwise described, as in 
<taxonomicName box="[903,997,1108,1133]" class="Insecta" family="Halictidae" genus="Agapostemonoides" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="43" phylum="Arthropoda" rank="species" species="hurdi">
<emphasis box="[903,997,1108,1133]" italics="true" pageId="4" pageNumber="43">A. hurdi</emphasis>
</taxonomicName>
