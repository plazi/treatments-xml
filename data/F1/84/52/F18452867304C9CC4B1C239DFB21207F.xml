<paragraph pageId="37" pageNumber="122">
<taxonomicName authority="Bernhauer, 1909" authorityName="Bernhauer" authorityYear="1909" class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta (Pseudota) klagesi" order="Coleoptera" pageId="37" pageNumber="122" phylum="Arthropoda" rank="species" species="klagesi" subGenus="Pseudota">Atheta (Pseudota) klagesi Bernhauer, 1909</taxonomicName>
