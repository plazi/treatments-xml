<taxonomicName authorityName="Chilean" baseAuthorityName="Bonaparte" baseAuthorityYear="1857" box="[943,1188,1904,1930]" class="Aves" family="Stercorariidae" genus="Stercorarius" kingdom="Animalia" order="Charadriiformes" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="chilensis">
