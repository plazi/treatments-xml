<taxonomicName authority="Distant, 1879" authorityName="Distant" authorityYear="1879" box="[333,675,1326,1348]" class="Insecta" family="Coreidae" genus="Mygdonia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="448" phylum="Arthropoda" rank="species" species="elongata">
