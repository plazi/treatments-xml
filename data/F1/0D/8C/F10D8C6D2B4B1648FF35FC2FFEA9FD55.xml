<heading box="[151,585,1005,1027]" fontSize="8" level="2" pageId="1" pageNumber="544" reason="2">
<emphasis bold="true" box="[151,585,1005,1027]" pageId="1" pageNumber="544">
<taxonomicName authority="Scheuchl &amp; Gusenleitner &amp; A, 2007" authorityName="Scheuchl &amp; Gusenleitner &amp; A" authorityYear="2007" box="[151,503,1005,1027]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="544" phylum="Arthropoda" rank="species" species="arabica" status="nov.sp." subGenus="Poecilandrena">
<emphasis bold="true" box="[151,242,1005,1027]" italics="true" pageId="1" pageNumber="544">Andrena</emphasis>
(
<emphasis bold="true" box="[257,409,1005,1027]" italics="true" pageId="1" pageNumber="544">Poecilandrena</emphasis>
) 
<emphasis bold="true" box="[424,503,1005,1027]" italics="true" pageId="1" pageNumber="544">arabica</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[509,585,1005,1027]" pageId="1" pageNumber="544" rank="species">nov.sp.</taxonomicNameLabel>
