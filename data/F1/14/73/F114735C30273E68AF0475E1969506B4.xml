<taxonomicName family="Platysteiridae" lsidName="" pageId="1" pageNumber="72" rank="family">Platysteiridae</taxonomicName>
, 
<taxonomicName family="Pycnonotidae" lsidName="" pageId="1" pageNumber="72" rank="family">Pycnonotidae</taxonomicName>
