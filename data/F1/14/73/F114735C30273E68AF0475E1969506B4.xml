<taxonomicName family="Monarchidae" lsidName="" pageId="1" pageNumber="72" rank="family">Monarchidae</taxonomicName>
<taxonomicName family="Nectariniidae" lsidName="" pageId="1" pageNumber="72" rank="family">Nectariniidae</taxonomicName>
