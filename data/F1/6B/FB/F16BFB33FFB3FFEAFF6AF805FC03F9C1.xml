of 
<collectingRegion box="[448,537,1631,1656]" country="Mexico" name="Mexico" pageId="17" pageNumber="18">
<collectingCountry box="[448,537,1631,1656]" name="Mexico" pageId="17" pageNumber="18">Mexico</collectingCountry>
