<taxonomicName authorityName="Enderlein" authorityYear="1936" class="Insecta" family="Phytomyzinae" genus="Aulagromyza" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aulagromyza" order="Diptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Aulagromyza</taxonomicName>
