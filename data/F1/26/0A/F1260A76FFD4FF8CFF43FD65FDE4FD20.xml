</paragraph>
<paragraph blockId="109.[151,1437,1657,1862]" pageId="109" pageNumber="110">
<taxonomicName authorityName="Philippi" authorityYear="1836" box="[151,248,1657,1682]" class="Scaphopoda" family="Dentaliidae" genus="Dentalium" kingdom="Animalia" order="Dentaliida" pageId="109" pageNumber="110" phylum="Mollusca" rank="species" species="pusillum">
