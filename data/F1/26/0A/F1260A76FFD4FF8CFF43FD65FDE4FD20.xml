</taxonomicName>
(
<taxonomicName box="[269,387,1657,1682]" class="Scaphopoda" family="Dentaliidae" genus="Dentalium" kingdom="Animalia" order="Dentaliida" pageId="109" pageNumber="110" phylum="Mollusca" rank="genus">
<emphasis box="[269,387,1657,1682]" italics="true" pageId="109" pageNumber="110">Dentalium</emphasis>
</taxonomicName>
(
