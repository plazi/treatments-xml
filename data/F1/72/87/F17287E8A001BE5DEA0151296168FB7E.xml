<number box="[721,765,1586,1612]" pageId="2" pageNumber="3" value="100.0">100</number>
(
<number box="[781,871,1586,1612]" pageId="2" pageNumber="3" value="102.5" valueMax="108.0" valueMin="97.0">97–108</number>
) long, 
<number box="[954,983,1586,1612]" pageId="2" pageNumber="3" value="46.0">46</number>
(
<number box="[999,1075,1586,1612]" pageId="2" pageNumber="3" value="43.0" valueMax="52.0" valueMin="34.0">34–52</number>
) wide at widest level anterior to JV 
<number box="[428,484,1625,1651]" pageId="2" pageNumber="3" value="2.65">2, 65</number>
(
<number box="[501,577,1625,1651]" pageId="2" pageNumber="3" value="65.5" valueMax="68.0" valueMin="63.0">63–68</number>
