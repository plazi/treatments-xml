<taxonomicName id="4C208B963863FFCDFC32FEEB2E3B0749" authority="(Eigenmann, 1915)" baseAuthorityName="Eigenmann" baseAuthorityYear="1915" box="[902,1377,304,329]" class="Actinopterygii" family="Characidae" genus="Serrapinnus" kingdom="Animalia" order="Characiformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="heterodon">
