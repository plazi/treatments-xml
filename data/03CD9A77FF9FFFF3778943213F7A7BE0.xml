locality.
</emphasis>
<collectingCountry id="F3736BF1FF9FFFF376C943813BC57DA0" box="[384,463,963,987]" name="Brazil" pageId="7" pageNumber="6">Brazil</collectingCountry>
, 
<collectingRegion id="49A0E583FF9FFFF37693438138887DA0" box="[474,642,963,987]" country="Brazil" name="Minas Gerais" pageId="7" pageNumber="6">Minas Gerais</collectingRegion>
