<taxonomicName id="9FC3D76C2B1AB9A69CF4E179D21ABAC9" class="Polychaeta" family="Glyceridae" genus="Glycera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glycera tridactyla" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="tridactyla">Glycera tridactyla</taxonomicName>
