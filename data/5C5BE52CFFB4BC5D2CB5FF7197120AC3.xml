<quantity id="130AF9DFFFB4BC5E2EECFEEA90210914" box="[718,789,258,283]" metricMagnitude="4" metricUnit="m" metricValue="2.0" pageId="37" pageNumber="488" unit="km" value="20.0">20 km</quantity>
