<taxonomicName id="4C46D2AA2C03FFB1FEDD65FBFE6D055C" authorityName="Villeneuve" authorityYear="1936" box="[368,507,1267,1293]" class="Insecta" family="Muscidae" genus="Atherigona" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="reversura">
