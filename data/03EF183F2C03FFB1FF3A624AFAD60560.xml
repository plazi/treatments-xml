<emphasis id="B932753B2C03FFB1FC5065A2FB5B0494" box="[1021,1229,1194,1220]" italics="true" pageId="26" pageNumber="27">Eleusine coracana</emphasis>
Gaertn
</taxonomicName>
, 
<taxonomicName id="4C46D2AA2C03FFB1FA8165A2FED704B8" authority="(Retz.)" baseAuthorityName="Retz." class="Liliopsida" family="Poaceae" genus="Eriochloa" kingdom="Plantae" order="Poales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="species" species="procera">
<emphasis id="B932753B2C03FFB1FA8165A2FF7904B8" italics="true" pageId="26" pageNumber="27">Eriochloa procera</emphasis>
(Retz.)
</taxonomicName>
, 
<taxonomicName id="4C46D2AA2C03FFB1FEE265C6FDED04B9" authority="(Rottler)" baseAuthorityName="Rottler" box="[335,635,1230,1257]" class="Liliopsida" family="Poaceae" genus="Sehima" kingdom="Plantae" order="Poales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="species" species="nervosum">
<emphasis id="B932753B2C03FFB1FEE265C6FD8204B8" box="[335,532,1230,1256]" italics="true" pageId="26" pageNumber="27">Sehima nervosum</emphasis>
(Rottler)
</taxonomicName>
, 
<emphasis id="B932753B2C03FFB1FD2A65C6FCD404B8" box="[647,834,1230,1256]" italics="true" pageId="26" pageNumber="27">
Sorghum 
<taxonomicName id="4C46D2AA2C03FFB1FD5C65C6FCD404B8" authority="Macquart, 1835" authorityName="Macquart" authorityYear="1835" box="[753,834,1230,1256]" class="Insecta" family="Muscidae" genus="Spilogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="bicolor">bicolor</taxonomicName>
