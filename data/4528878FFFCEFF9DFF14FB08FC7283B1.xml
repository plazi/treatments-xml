<paragraph id="CD3E3699FFCEFF9DFF3CFA92FE4B83E1" blockId="4.[191,1440,1353,1441]" pageId="4" pageNumber="3">
<taxonomicName id="0A814D1AFFCEFF9DFF3CFA92FE258321" box="[232,501,1353,1377]" class="Insecta" family="Cerambycidae" genus="Lamacoscylus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="albatus">
