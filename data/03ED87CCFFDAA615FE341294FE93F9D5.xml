<taxonomicName id="4C444D59FFDAA61AFE341294FD36F9D0" authority="Burckhardt &amp; Basset, 2000" authorityName="Burckhardt &amp; Basset" authorityYear="2000" box="[413,643,1513,1537]" class="Insecta" family="Psyllidae" genus="Calophya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="90" phylum="Arthropoda" rank="species" species="hermicitae" status="sp. n.">
