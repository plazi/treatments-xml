<taxonomicName id="73C34D0B1D47FF8B160237DB3767F95F" authority="Bochkov and Wauthy, 2009" authorityName="Bochkov and Wauthy" authorityYear="2009" box="[293,793,1688,1710]" class="Arachnida" family="Psoroptidae" genus="Listropsoralges" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="faini">
