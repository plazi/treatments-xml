<taxonomicName id="B2BD245801EAF025CF910B7A3EF67CD7" class="Gastropoda" family="Hydrobiidae" genus="Pyrgula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyrgula (Turricaspia) rudis" order="Littorinimorpha" pageId="60" pageNumber="91" phylum="Mollusca" rank="species" species="rudis" subGenus="Turricaspia">Pyrgula (Turricaspia) rudis</taxonomicName>
