<normalizedToken id="6801E17656E20CBF5BA02157064DB5FE" originalValue="Schlüssel" pageId="null" pageNumber="507">Schluessel</normalizedToken>
zur Gattung 
<taxonomicName id="2533AB8F4A6D7358AE7A95111055419D" class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="507" phylum="Tracheophyta" rank="genus">Senecio</taxonomicName>
