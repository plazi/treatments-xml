<taxonomicName id="E9B78C8AFFD4FF8BEDFFFB81297DFB17" authorityName="Matschie" authorityYear="1916" box="[973,1110,1135,1158]" class="Mammalia" family="Didelphidae" genus="Marmosops" kingdom="Animalia" order="Didelphimorphia" pageId="30" pageNumber="31" phylum="Chordata" rank="genus">
