<taxonomicName id="4C174D10F258FF9CFC0EFA5007DA7867" authority="Drew &amp; Hancock" authorityName="Drew &amp; Hancock" authorityYear="1994" box="[967,1430,1847,1870]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="gombokensis">
