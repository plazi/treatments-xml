<taxonomicName id="C9EF1EBB08CE3EF8298BCFC5D29E6497" class="Magnoliopsida" family="Asteraceae" genus="Eupatorium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eupatorium perfoliatum" order="Asterales" pageId="7" pageNumber="328" phylum="Tracheophyta" rank="species" species="perfoliatum">Eupatorium perfoliatum</taxonomicName>
