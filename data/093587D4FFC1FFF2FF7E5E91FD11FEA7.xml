<taxonomicName id="469C4D41FFC4FFF3FEC5593EFD9DF94A" authorityName="Xue, Guo &amp; Hong" authorityYear="2013" box="[300,558,1651,1673]" class="Arachnida" family="Eriophyidae" genus="Calepitrimerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="flexuosus" status="sp. nov.">
<emphasis id="B3E8EAD0FFC4FFF3FEC5593EFD9DF94A" box="[300,558,1651,1673]" italics="true" pageId="36" pageNumber="37">Calepitrimerus flexuosus</emphasis>
</taxonomicName>
