<taxonomicName id="4C1D4D00FFFDFF95FEB45FA3FDFE04E7" authority="Bílý, 2007" authorityName="Bílý" authorityYear="2007" box="[316,618,2011,2036]" class="Insecta" family="Buprestidae" genus="Endelomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="43" phylum="Arthropoda" rank="species" species="biakensis" status="sp. nov.">
