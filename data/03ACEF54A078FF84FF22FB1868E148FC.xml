<taxonomicName id="4C0525C1A079FF8BFED5F8EF6E7A4EC1" box="[352,513,1914,1936]" class="Insecta" family="Cicadellidae" genus="Regalana" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="232" phylum="Arthropoda" rank="species" species="ericki" status="sp. nov.">
<emphasis id="B9718250A079FF8BFED5F8EF6E7A4EC1" box="[352,513,1914,1936]" italics="true" pageId="7" pageNumber="232">Regalana ericki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2423F2BA079FF8BFDBDF8EE6E234EC1" box="[520,600,1915,1936]" pageId="7" pageNumber="232" rank="species">
