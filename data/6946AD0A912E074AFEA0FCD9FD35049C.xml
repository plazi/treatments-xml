<taxonomicName id="26EF679F912E074AFC86FCD9FAD6039F" authority="Clarke 1903" authorityName="Clarke" authorityYear="1903" box="[814,1297,894,920]" class="Insecta" family="Aphididae" genus="Macrosiphum" kingdom="Animalia" order="Aphidomorpha" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="valerianae">
<emphasis id="D39BC00E912E074AFC86FCD9FB99039F" bold="true" box="[814,1118,894,920]" italics="true" pageId="13" pageNumber="14">Macrosiphum valerianae</emphasis>
(Clarke 
<number id="57270E3B912E074AFB64FCD9FACE039F" box="[1228,1289,894,920]" pageId="13" pageNumber="14" value="1903.0">1903</number>
)
</taxonomicName>
</emphasis>
