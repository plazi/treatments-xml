<taxonomicName id="9F8EECC1FFAC586FFC272CF9FC2D43FE" authorityName="Thorell" authorityYear="1878" box="[912,1014,1172,1196]" class="Arachnida" family="Psechridae" genus="Psechrus" kingdom="Animalia" order="Araneae" pageId="44" pageNumber="45" phylum="Arthropoda" rank="genus">
