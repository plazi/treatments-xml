<quantity id="4CE99B44A440FFDCEC52FF30FA279B89" box="[1350,1436,223,248]" metricMagnitude="3" metricUnit="m" metricValue="1.157" pageId="46" pageNumber="47" unit="m" value="1157.0">1157 m</quantity>
