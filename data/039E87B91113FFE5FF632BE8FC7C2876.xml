. 
<collectingCountry id="F320763F1115FFE5FE932EDFFE752932" box="[359,426,1512,1538]" name="China" pageId="5" pageNumber="213">China</collectingCountry>
(
<collectingRegion id="49F3F84D1115FFE5FE4D2EDFFD9D2933" box="[441,578,1512,1539]" country="China" name="Guangdong" pageId="5" pageNumber="213">Guangdong</collectingRegion>
