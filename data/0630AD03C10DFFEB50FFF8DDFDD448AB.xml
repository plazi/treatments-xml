; head length ahead of eyes, 
<number id="38510E32C103FFEB5489FBB3FAA6481E" box="[1249,1301,1066,1090]" pageId="8" pageNumber="569" value="0.37">0.37</number>
; interocular distance, 
<number id="38510E32C103FFEB5169FBD6FE85483B" box="[257,310,1103,1127]" pageId="8" pageNumber="569" value="0.87">0.87</number>
; length of antennal segments: I, 
<number id="38510E32C103FFEB52C9FBD6FD65483B" box="[673,726,1103,1127]" pageId="8" pageNumber="569" value="0.27">0.27</number>
; II, 
<number id="38510E32C103FFEB536AFBD7FC85483B" box="[770,822,1102,1127]" pageId="8" pageNumber="569" value="0.63">0.63</number>
