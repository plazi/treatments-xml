<taxonomicName id="4C55DAA36D3EDD6BFF9D50E2E0FEFF6C" authority=": Simon, 1893: 294" authorityName="Simon" authorityPageNumber="294" authorityYear="1893" box="[120,396,225,249]" class="Arachnida" family="Oonopidae" genus="Salsula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus">
