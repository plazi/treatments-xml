<taxonomicName id="44BD5CD7A5772D4140E2F800FD47F82C" authority="Hansen" authorityName="Hansen" authorityYear="1976" box="[425,694,1951,1975]" class="Insecta" family="Chironomidae" genus="Diamesa" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="441" phylum="Arthropoda" rank="species" species="leoniella">
