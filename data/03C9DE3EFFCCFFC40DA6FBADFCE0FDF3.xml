<typeStatus id="54DBD18AFFCCFFC50831F8AEFA83F89A" box="[1280,1376,1853,1879]" pageId="46" pageNumber="197">paratype</typeStatus>
of 
<taxonomicName id="4C6014ABFFCCFFC508B9F8AEFF34F8B7" class="Insecta" family="Mymaridae" genus="Polynema" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="197" phylum="Arthropoda" rank="species" species="frater">
<emphasis id="B914B33AFFCCFFC508B9F8AEFF34F8B7" italics="true" pageId="46" pageNumber="197">P. frater</emphasis>
