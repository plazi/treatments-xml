<taxonomicName id="4C6014ABFFCCFFC50F94F8F2FCF7F8B7" box="[677,788,1888,1914]" class="Insecta" family="Mymaridae" genus="Polynema" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="197" phylum="Arthropoda" rank="species" species="draperi">
<emphasis id="B914B33AFFCCFFC50F94F8F2FCF7F8B7" box="[677,788,1888,1914]" italics="true" pageId="46" pageNumber="197">P. draperi</emphasis>
</taxonomicName>
