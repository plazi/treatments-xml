<taxonomicName id="4C6014ABFFCCFFC50E1EF8AFFB8FF89A" authority="(Girault)" baseAuthorityName="Girault" box="[815,1132,1852,1879]" class="Insecta" family="Mymaridae" genus="Palaeoneura" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="197" phylum="Arthropoda" rank="species" species="zolai">
<emphasis id="B914B33AFFCCFFC50E1EF8AFFC1DF89B" box="[815,1022,1852,1878]" italics="true" pageId="46" pageNumber="197">Palaeoneura zolai</emphasis>
(Girault)
</taxonomicName>
