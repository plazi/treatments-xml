<typeStatus id="54E03AC77E28FFA2FAD7C57F26A1F889" box="[1383,1436,1821,1844]" pageId="6" pageNumber="64">Type</typeStatus>
locality: Valencia, 
<collectingCountry id="F34CC4F57E28FFA2FE3FC55F22F2F8E9" box="[399,463,1853,1876]" name="Spain" pageId="6" pageNumber="64">Spain</collectingCountry>
