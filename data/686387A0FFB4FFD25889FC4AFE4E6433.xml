<taxonomicName id="27CA4D35FFB4FFD25D38FBB9FDB3650B" authority="rupicapra (Linnaeus, 1758)" authorityName="rupicapra (Linnaeus" authorityYear="1758" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Mammalia" family="Bovidae" genus="Rupicapra" kingdom="Animalia" order="Artiodactyla" pageId="121" pageNumber="122" phylum="Chordata" rank="species" species="rupicapra">
