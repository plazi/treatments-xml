<taxonomicName id="D9AD4D160E27FF85FE99AC0FFE42F8DF" baseAuthorityName="Koenike" baseAuthorityYear="1908" box="[272,447,1823,1842]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="28" pageNumber="169" phylum="Arthropoda" rank="species" species="ovalis">
