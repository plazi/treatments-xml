<collectorName id="243C4CD6AC3C3B5BFDB6DC165EE1FBC6" box="[615,674,1107,1127]" pageId="112" pageNumber="505">du Zié</collectorName>
, piedmont, 
<date id="FD770FC0AC3C3B5BFCC2DC165F3EFBC6" box="[787,893,1107,1127]" pageId="112" pageNumber="505" value="1991-06-12">
<collectingDate id="ED33F628AC3C3B5BFCC2DC165F3EFBC6" box="[787,893,1107,1127]" pageId="112" pageNumber="505" value="1991-06-12">12.VI.1991</collectingDate>
