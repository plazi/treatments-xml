<materialsCitation id="39A1235DAC3C3B5BFE72DC1658F6FBC6" box="[419,1205,1107,1127]" collectingDate="1991-06-12" collectorName="du Zie" location="Nimba" pageId="112" pageNumber="505" specimenCount="1" stateProvince="Nimba">
