<taxonomicName id="B22FC69E01BA6E5FDF95576D1D8EEFAC" LSID="http://zoobank.org/84BA2890-9AB1-4FAC-B6DB-B7595BFEC808" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus monteverde" order="Hymenoptera" pageId="120" pageNumber="121" phylum="Arthropoda" rank="species" species="monteverde">Heterospilus monteverde Marsh</taxonomicName>
