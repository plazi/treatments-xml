<geoCoordinate id="EE235007FFD3FFF79A0DFE29F89531F0" box="[624,724,475,501]" degrees="32" direction="north" minutes="21" orientation="latitude" pageId="66" pageNumber="67" precision="925" value="32.35">32°21’N</geoCoordinate>
