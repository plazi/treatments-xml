<taxonomicName id="491FDCDDF5354ABAAAAC1B9BC2056531" class="Insecta" family="Corydiidae" genus="Arenivaga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arenivaga galeana" order="Blattodea" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="galeana">Arenivaga galeana</taxonomicName>
