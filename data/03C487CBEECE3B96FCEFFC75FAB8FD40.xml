<td id="768C5DE3EEF2C5CAFC6EF9AAFC15F9BF" box="[939,976,1636,1662]" gridcol="1" gridrow="1" pageId="419" pageNumber="420">0-0</td>
<td id="768C5DE3EEF2C5CAFC35F9AAFBD0F9BF" box="[1008,1045,1636,1662]" gridcol="2" gridrow="1" pageId="419" pageNumber="420">1-I</td>
