<th id="768C5DE3EEF2C5CAFCEFF969FC61F903" box="[810,932,1703,1730]" gridcol="0" gridrow="3" pageId="419" pageNumber="420">Leg 4</th>
<td id="768C5DE3EEF2C5CAFC6EF969FC15F903" box="[939,976,1703,1730]" gridcol="1" gridrow="3" pageId="419" pageNumber="420">0-0</td>
