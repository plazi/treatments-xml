<emphasis id="31958A53FFABF854FCCEFC0CF9FA3FEA" box="[809,1235,924,946]" italics="true" pageId="14" pageNumber="975">
<typeStatus id="DC5AE8E3FFABF854FCCEFC0CFE773FE9" box="[809,862,925,946]" pageId="14" pageNumber="975">Type</typeStatus>
species: 
<taxonomicName id="C4E12DC2FFABF854FC22FC0DF9FA3FEA" authorityName="Criscione &amp; Hallan &amp; Puillandre &amp; Fedosov" authorityYear="2021" box="[965,1235,924,945]" class="Gastropoda" family="Raphitomidae" genus="Nodothauma" kingdom="Animalia" order="Neogastropoda" pageId="14" pageNumber="975" phylum="Mollusca" rank="species" species="magnifica">Nodothauma magnifica</taxonomicName>
