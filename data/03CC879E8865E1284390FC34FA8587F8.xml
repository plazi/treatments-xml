<paragraph id="8BDA36888865E12D4390FBDBFACD826B" blockId="1.[151,1395,1069,1091]" box="[151,1395,1069,1091]" pageId="1" pageNumber="383">
<taxonomicName id="4C654D0B8865E12D4390FBDBFE4E826B" authority="Hutton, 1901: 75" authorityName="Hutton" authorityPageNumber="75" authorityYear="1901" box="[151,496,1069,1091]" class="Insecta" family="Muscidae" genus="Exsul" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="383" phylum="Arthropoda" rank="species" species="singularis">
<emphasis id="B911EA9A8865E12D4390FBDBFE84826B" box="[151,314,1069,1091]" italics="true" pageId="1" pageNumber="383">Exsul singularis</emphasis>
Hutton, 
<number id="3DAD24AF8865E12D4293FBD8FE74826B" box="[404,458,1070,1091]" pageId="1" pageNumber="383" value="1901.0">1901</number>
: 
<number id="3DAD24AF8865E12D42D3FBDBFE4E826B" box="[468,496,1069,1091]" pageId="1" pageNumber="383" value="75.0">75</number>
</taxonomicName>
