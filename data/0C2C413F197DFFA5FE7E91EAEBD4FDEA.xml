<collectingRegion id="46413ECB197DFFA5FD7F905AEA80FED3" box="[641,702,362,381]" country="Czech Republic" name="Praha" pageId="29" pageNumber="326">Prague</collectingRegion>
, 
<collectingCountry id="FC92B0B9197DFFA5FD389059EB73FED2" box="[710,845,361,380]" name="Czech Republic" pageId="29" pageNumber="326">Czech Republic</collectingCountry>
[p] // 
<taxonomicName id="43858BAA197DFFA5FC819059EC7FFED2" authorityName="Balthasar" authorityYear="1968" box="[895,1089,361,380]" class="Insecta" family="Scarabaeidae" genus="Tiniocellus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="326" phylum="Arthropoda" rank="species" species="asmarensis">Tiniocellus asmarensis</taxonomicName>
/ 
<bibRefCitation id="E0148DD8197DFFA5FF6590B4E91EFE39" author="BALTHASAR V." box="[155,288,388,407]" pageId="29" pageNumber="326" pagination="953 - 958" refId="ref23455" refString="BALTHASAR V. 1968: Neue Scarabaeiden-Arten. Beitrage zur Entomologie 18: 953 - 958." type="journal article" year="1968">Balthasar, 1968</bibRefCitation>
