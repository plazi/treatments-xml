<paragraph id="8BCD911CFB78FFC6FF6522CE2D464BB6" blockId="1.[151,1395,617,670]" box="[151,1395,648,670]" pageId="1" pageNumber="470">
<taxonomicName id="4C72EA9FFB78FFC6FF6522CE299C4BB6" authority="Meyrick, 1889" authorityName="Meyrick" authorityYear="1889" box="[151,425,648,670]" class="Insecta" family="Erebidae" genus="Hectogama" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="470" phylum="Arthropoda" rank="genus">
