<paragraph id="8B8436F5FFF9FFC5FF11F92FFBF5F88A" blockId="6.[151,1436,1729,2006]" pageId="6" pageNumber="189">
<emphasis id="B94FEAE7FFF9FFC5FF11F92FFE73F91D" bold="true" box="[199,410,1765,1790]" pageId="6" pageNumber="189">Characterization.</emphasis>
Shell patelliform; L 
<quantity id="4CC39B10FFF9FFC5FD5AF92FFD1CF91E" box="[652,757,1765,1790]" metricMagnitude="-3" metricUnit="m" metricValue="9.45" pageId="6" pageNumber="189" unit="mm" value="9.45">9.45 mm</quantity>
; H 
<quantity id="4CC39B10FFF9FFC5FCC9F92CFC61F91E" box="[799,904,1766,1790]" metricMagnitude="-3" metricUnit="m" metricValue="3.77" pageId="6" pageNumber="189" unit="mm" value="3.77">3.77 mm</quantity>
; 
<geoCoordinate id="EE0F5032FFF9FFC5FC40F92CFC03F91D" box="[918,1002,1765,1790]" direction="west" orientation="longitude" pageId="6" pageNumber="189" precision="555" value="-6.36">
W 
<quantity id="4CC39B10FFF9FFC5FC6FF92FFC03F91D" box="[953,1002,1765,1790]" metricMagnitude="-3" metricUnit="m" metricValue="6.36" pageId="6" pageNumber="189" unit="mm" value="6.36">6.36</quantity>
</geoCoordinate>
mm; apex at 
<number id="3DF324D2FFF9FFC5FB5CF92FFB52F91D" box="[1162,1211,1765,1790]" pageId="6" pageNumber="189" value="0.85">0.85</number>
