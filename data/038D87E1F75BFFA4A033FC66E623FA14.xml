<taxonomicName id="4C244D74F75BFFA4A1A5FAE0E679FAD4" authority="Waagen &amp; Wentzel, 1886" authorityName="Waagen &amp; Wentzel" authorityYear="1886" box="[180,606,1363,1390]" class="Stenolaemata" family="Fistuliporidae" genus="Dybowskiella" kingdom="Animalia" order="Cystoporida" pageId="11" pageNumber="162" phylum="Bryozoa" rank="genus">
