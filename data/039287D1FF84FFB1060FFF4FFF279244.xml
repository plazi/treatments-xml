<taxonomicName id="4C3B4D44FF9AFFB105B3FB4EFE6091C7" authorityName="Nitare &amp; Ainsworth &amp; Larsson &amp; Parfitt &amp; Suz &amp; Svantesson &amp; Larsson" authorityYear="2021" box="[153,392,1163,1185]" class="Agaricomycetes" family="Bankeraceae" genus="Hydnellum" kingdom="Fungi" order="Thelephorales" pageId="17" pageNumber="250" phylum="Basidiomycota" rank="species" species="scabrosellum">
