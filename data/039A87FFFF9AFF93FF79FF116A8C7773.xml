<taxonomicName id="4C334D6AFF9BFF93FF23FDAE6E497773" box="[205,351,552,578]" class="Insecta" family="Scarabaeidae" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="tribe" tribe="Didactyliini">Didactyliini</taxonomicName>
] 
<emphasis id="B947EAFBFF9BFF93FE88FDAE6C797770" box="[358,879,552,577]" italics="true" pageId="9" pageNumber="10">.......................................................................</emphasis>
