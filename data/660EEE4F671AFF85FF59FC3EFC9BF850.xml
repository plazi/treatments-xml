<collectionCode id="88B6C79C671AFF87FF09F9A8FEA6FA10" box="[199,271,1593,1618]" pageId="8" pageNumber="501">JACP</collectionCode>
and 
<collectionCode id="88B6C79C671AFF87FE98F9A8FE06FA10" box="[342,431,1593,1618]" pageId="8" pageNumber="501">AAVAS</collectionCode>
records.
