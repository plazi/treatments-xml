<taxonomicName id="8AAE4D32595A7A1FFDBD64C2FD58FA1A" authorityName="Solier" authorityYear="1848" box="[557,726,1494,1520]" class="Insecta" family="Tenebrionidae" genus="Blaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="emondi">
