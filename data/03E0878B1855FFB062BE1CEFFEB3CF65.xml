<taxonomicName id="4C494D1E1855FFB0633A1CC3FE0ACF2D" authorityName="de Beauchamp" authorityYear="1927" box="[275,399,763,789]" class="Rhabditophora" family="Karkinorhynchidae" genus="Cheliplana" kingdom="Animalia" order="Rhabdocoela" pageId="34" pageNumber="487" phylum="Platyhelminthes" rank="genus">
