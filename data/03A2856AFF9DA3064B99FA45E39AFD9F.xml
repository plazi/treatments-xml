<quantity id="4CF39999FF9CA30648C9FE81E416FECC" box="[994,1051,313,335]" metricMagnitude="4" metricUnit="m" metricValue="1.2" pageId="17" pageNumber="1274" unit="km" value="12.0">12 km</quantity>
NE 
