<quantity id="4CF39999FF9CA3064AC6FEE8E243FEE6" box="[493,590,336,358]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="7.0" metricValueMin="6.0" pageId="17" pageNumber="1274" unit="m" value="650.0" valueMax="700.0" valueMin="600.0">600-700 m</quantity>
