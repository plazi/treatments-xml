<geoCoordinate id="EE3F52BBFF9CA3064A6BFEE8E187FEE5" box="[320,394,336,358]" degrees="22" direction="north" minutes="46" orientation="latitude" pageId="17" pageNumber="1274" precision="925" value="22.766666">22°46'N</geoCoordinate>
