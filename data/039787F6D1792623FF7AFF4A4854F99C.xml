</taxonomicName>
<emphasis id="B94AEAF2D1792623FC25FA2D4FE2FA50" box="[968,973,1413,1439]" italics="true" pageId="38" pageNumber="239">,</emphasis>
<taxonomicName id="4C3E4D63D1792623FC39FA2E48CAFA50" authority="Mendivil et al." authorityName="Mendivil" box="[980,1253,1413,1439]" class="Insecta" family="Ptiloneuridae" genus="Loneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="38" pageNumber="239" phylum="Arthropoda" rank="species" species="deibyi">
