<taxonomicName id="4C3E4D63D1792623FDD7FA2E4FE7FA50" authority="Garcia Aldrete et al." authorityName="Garcia Aldrete" box="[570,968,1413,1439]" class="Insecta" family="Ptiloneuridae" genus="Loneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="38" pageNumber="239" phylum="Arthropoda" rank="species" species="colombiana">
