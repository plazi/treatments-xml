<taxonomicName id="4C2C89E1FFC3CB48FEE0A74CF74F215F" authority="Fieber, 1851" authorityName="Fieber" authorityYear="1851" box="[258,631,1058,1083]" class="Insecta" family="Notonectidae" genus="Notonecta" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="species" species="fabricii">
<emphasis id="B9582E70FFC3CB48FEE0A74CF4ED215E" box="[258,469,1059,1083]" italics="true" pageId="4" pageNumber="306">Notonecta fabricii</emphasis>
Fieber, 1851
</taxonomicName>
(family 
<taxonomicName id="4C2C89E1FFC3CB48FD04A74DF6B8215F" authorityName="Latreille" authorityYear="1802" box="[742,896,1058,1082]" class="Insecta" family="Notonectidae" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="family">Notonectidae</taxonomicName>
), syn. 
<taxonomicName id="4C2C89E1FFC3CB48FC37A74CF19B215E" authorityName="Linnaeus" authorityYear="1758" box="[981,1187,1059,1083]" class="Insecta" family="Notonectidae" genus="Notonecta" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="species" species="glauca">
<emphasis id="B9582E70FFC3CB48FC37A74CF19B215E" box="[981,1187,1059,1083]" italics="true" pageId="4" pageNumber="306">Notonecta glauca</emphasis>
</taxonomicName>
L., 1758 
<emphasis id="B9582E70FFC3CB48FAC6A74CF0A3215E" box="[1316,1435,1059,1083]" italics="true" pageId="4" pageNumber="306">sensu lato</emphasis>
(
<bibRefCitation id="EFBD8F93FFC3CB48FF7CA729F47E213B" author="Razzauti, A." box="[158,326,1094,1118]" pageId="4" pageNumber="306" pagination="196 - 224" refId="ref19335" refString="Razzauti, A. (1917) Contributi alla conoscenza faunistica delle isole toscane. I. Isola di Capraia. Atti della Societa Toscana di Scienze Naturali, Pisa, Memorie, 31, 196 - 224." type="journal article" year="1917">Razzauti 1917</bibRefCitation>
; see discussion) and 
<taxonomicName id="4C2C89E1FFC3CB48FDD9A729F643213B" authority="Fieber, 1851" authorityName="Fieber" authorityYear="1851" box="[571,891,1094,1118]" class="Insecta" family="Corixidae" genus="Corisa" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="species" species="fabricii">
<emphasis id="B9582E70FFC3CB48FDD9A729F7DC213B" box="[571,740,1094,1118]" italics="true" pageId="4" pageNumber="306">Corisa fabricii</emphasis>
Fieber, 1851
</taxonomicName>
(family 
<taxonomicName id="4C2C89E1FFC3CB48FC03A729F16C213B" box="[993,1108,1094,1118]" class="Insecta" family="Corixidae" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="family">Corixidae</taxonomicName>
), syn. 
<emphasis id="B9582E70FFC3CB48FB42A729F0A4213B" box="[1184,1436,1094,1118]" italics="true" pageId="4" pageNumber="306">
<taxonomicName id="4C2C89E1FFC3CB48FB42A729F1D3213B" authorityName="Fabricius" authorityYear="1775" box="[1184,1259,1094,1118]" class="Insecta" family="Corixidae" genus="Sigara" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="306" phylum="Arthropoda" rank="genus">Sigara</taxonomicName>
