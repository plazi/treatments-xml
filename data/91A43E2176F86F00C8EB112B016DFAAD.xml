<taxonomicName id="278E972CAE8E862CCE7F1D796700BE90" class="Arachnida" family="Anapidae" genus="Anapisona" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="gertschi">Anapisona gertschi</taxonomicName>
