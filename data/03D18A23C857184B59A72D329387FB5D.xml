<materialsCitation id="3B103168C857184B59B12E539508FD7C" collectionCode="III, IV, UF" pageId="21" pageNumber="76" specimenCount="2" typeStatus="holotype">
<typeStatus id="54C38597C857184B59B12E53967CFD9C" box="[211,310,513,537]" pageId="21" pageNumber="76">Holotype</typeStatus>
