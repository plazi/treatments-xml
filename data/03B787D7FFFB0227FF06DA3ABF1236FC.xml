<taxonomicName id="4C1E4D42FFFB0227FB99DE26B82C36B4" authority="Gorochov, 2008" authorityName="Gorochov" authorityYear="2008" box="[1032,1370,1913,1938]" class="Insecta" family="Tettigoniidae" genus="Leptoteratura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="586" phylum="Arthropoda" rank="species" species="uniformis">
