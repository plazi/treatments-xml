<taxonomicName id="BDEC87BFB5C74D0C53FF61B967339B2A" authorityName="Ferussac" authorityYear="1823" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis bouei" order="Pteropoda" pageId="194" pageNumber="195" phylum="Mollusca" rank="species" species="bouei">
<emphasis id="6662A0BE746DB6F9A486F0271C5CC778" italics="true" pageId="194" pageNumber="195">Melanopsis bouei</emphasis>
</taxonomicName>
<normalizedToken id="45C34FF9F5409D8ABC5F7D5185AF3174" originalValue="Férussac">Ferussac</normalizedToken>
