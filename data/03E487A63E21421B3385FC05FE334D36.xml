, 
<taxonomicName id="4C4D4D333E21421532BEF84DFDBC4D9F" box="[428,569,1935,1960]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="63" phylum="Chordata" rank="species" species="gracilis">
