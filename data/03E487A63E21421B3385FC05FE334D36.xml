. chamaeleon
</taxonomicName>
, 
<taxonomicName id="4C4D4D333E21421537FDF8AAFAEB4DB6" box="[1263,1390,1896,1921]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="63" phylum="Chordata" rank="species" species="necasi">
<collectingCountry id="F35A76203E21421537FDF8AAFA904DB6" box="[1263,1301,1896,1921]" name="Switzerland" pageId="6" pageNumber="63">Ch</collectingCountry>
. necasi
</taxonomicName>
, 
<taxonomicName id="4C4D4D333E214215366BF8AAFE914D9F" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="63" phylum="Chordata" rank="species" species="zeylanicus">
<collectingCountry id="F35A76203E214215366BF8AAFA1E4DB6" box="[1401,1435,1896,1921]" name="Switzerland" pageId="6" pageNumber="63">Ch</collectingCountry>
zeylanicus
</taxonomicName>
, 
<taxonomicName id="4C4D4D333E214215320DF84DFE244D9F" box="[287,417,1935,1960]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="63" phylum="Chordata" rank="species" species="dilepis">
<collectingCountry id="F35A76203E214215320DF84DFEC04D9F" box="[287,325,1935,1960]" name="Switzerland" pageId="6" pageNumber="63">Ch</collectingCountry>
