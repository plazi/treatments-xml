</taxonomicName>
, 
<taxonomicName id="3EFFFCBCFFEA6A59EA2AF95319D1BDA3" authority="Lowry &amp; Myers, 2013" authorityName="Lowry &amp; Myers" authorityYear="2013" class="Malacostraca" family="Talitridae" genus="Floresorchestia" kingdom="Animalia" order="Amphipoda" pageId="28" pageNumber="179" phylum="Arthropoda" rank="species" species="pohnpei">
