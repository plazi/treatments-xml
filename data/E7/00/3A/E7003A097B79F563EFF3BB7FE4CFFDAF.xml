<subSubSection pageId="4" pageNumber="5" type="type species">
<paragraph pageId="4" pageNumber="5">Type species.</paragraph>
<paragraph pageId="4" pageNumber="5">
<taxonomicName class="Anthozoa" family="Lithotelestidae" genus="Nanipora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nanipora kamurai" order="Helioporacea" pageId="4" pageNumber="5" phylum="Cnidaria" rank="species" species="kamurai">Nanipora kamurai</taxonomicName>
