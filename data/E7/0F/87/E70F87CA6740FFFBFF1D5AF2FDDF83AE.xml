<taxonomicName box="[816,975,1066,1091]" class="Arachnida" family="Gonyleptidae" genus="Temucus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="569" phylum="Arthropoda" rank="species" species="palpiconus">
