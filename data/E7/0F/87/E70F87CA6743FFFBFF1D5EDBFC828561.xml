<taxonomicName authority="Roewer, 1943" authorityName="Roewer" authorityYear="1943" box="[844,1176,476,501]" class="Arachnida" family="Gonyleptidae" genus="Temucus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="569" phylum="Arthropoda" rank="species" species="palpiconus" status="comb. nov.">
