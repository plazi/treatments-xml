<emphasis italics="true" pageId="0" pageNumber="57">C. waltonii</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Apiaceae" genus="C." kingdom="Plantae" lsidName="C. peucedanifolia" order="Apiales" pageId="0" pageNumber="57" phylum="Tracheophyta" rank="species" species="peucedanifolia">
<emphasis italics="true" pageId="0" pageNumber="57">C. peucedanifolia</emphasis>
