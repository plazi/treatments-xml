<paragraph blockId="11.[151,1436,1424,2025]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[199,309,1784,1809]" pageId="11" pageNumber="12">Variation</emphasis>
. Table 
<number box="[398,412,1784,1808]" pageId="11" pageNumber="12" value="6.0">6</number>
