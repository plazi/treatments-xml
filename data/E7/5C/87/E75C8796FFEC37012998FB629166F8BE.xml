<heading bold="true" box="[151,576,1162,1188]" fontSize="11" level="1" pageId="36" pageNumber="37" reason="1">
<taxonomicName authority="Dunn, 1982" authorityName="Dunn" authorityYear="1982" box="[151,576,1162,1188]" class="Anthozoa" family="Hormathiidae" genus="Paraphelliactis" kingdom="Animalia" order="Actiniaria" pageId="36" pageNumber="37" phylum="Cnidaria" rank="species" species="pabista">
<emphasis bold="true" box="[151,576,1162,1188]" pageId="36" pageNumber="37">
<emphasis bold="true" box="[151,427,1162,1188]" italics="true" pageId="36" pageNumber="37">Paraphelliactis pabista</emphasis>
<bibRefCitation author="Dunn, D. F." box="[434,576,1162,1188]" pageId="36" pageNumber="37" pagination="51 - 56" refId="ref23975" refString="Dunn, D. F. (1982) Paraphelliactis pabista, a new species of hormathiid sea anemone from abyssal northeastern Pacific waters (Coelenterata: Actiniaria). Syesis, 15, 51 - 56." type="journal article" year="1982">Dunn, 1982</bibRefCitation>
</emphasis>
</taxonomicName>
