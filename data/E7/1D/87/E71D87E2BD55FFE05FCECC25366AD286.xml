<number box="[298,349,861,887]" pageId="3" pageNumber="50" value="1.9">1.90</number>
/ total 
<number box="[429,500,861,887]" pageId="3" pageNumber="50" value="17.7">17.70</number>
; [leg II missing]; III: 6.00/ 
<number box="[831,883,861,887]" pageId="3" pageNumber="50" value="2.08">2.08</number>
/ 
<number box="[900,952,861,887]" pageId="3" pageNumber="50" value="5.15">5.15</number>
/ 
<number box="[969,1021,861,887]" pageId="3" pageNumber="50" value="4.6">4.60</number>
/ 
