<taxonomicName authority="Breddin, 1914" authorityName="Breddin" authorityYear="1914" box="[170,545,1320,1342]" class="Insecta" family="Pentatomidae" genus="Acledra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="species" species="hians" status="n. syn." subGenus="Acledra">
