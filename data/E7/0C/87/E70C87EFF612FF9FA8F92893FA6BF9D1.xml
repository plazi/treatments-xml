<taxonomicName authorityName="Kottelat &amp; Tan" authorityYear="2008" box="[1313,1435,685,707]" class="Actinopterygii" family="Cyprinidae" genus="Lobocheilos" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="7" pageNumber="550" phylum="Chordata" rank="species" species="terminalis">
