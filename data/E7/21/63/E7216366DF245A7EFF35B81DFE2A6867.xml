<emphasis box="[1326,1436,1623,1647]" italics="true" pageId="38" pageNumber="205">R. molanoi</emphasis>
</taxonomicName>
<emphasis bold="true" box="[151,232,1659,1683]" pageId="38" pageNumber="205">
<taxonomicNameLabel box="[151,232,1659,1683]" pageId="38" pageNumber="205" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(paratype); B. 
<taxonomicName authorityName="Padilla-Gil" authorityYear="2015" box="[386,470,1659,1682]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="205" phylum="Arthropoda" rank="species" species="penta">
<emphasis box="[386,470,1659,1682]" italics="true" pageId="38" pageNumber="205">R. penta</emphasis>
</taxonomicName>
(paratype); C. 
<taxonomicName authorityName="Padilla-Gil" authorityYear="2011" box="[623,733,1659,1682]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="205" phylum="Arthropoda" rank="species" species="rosensis">
<emphasis box="[623,733,1659,1682]" italics="true" pageId="38" pageNumber="205">R. rosensis</emphasis>
</taxonomicName>
(paratype); D. 
<taxonomicName authorityName="Padilla-Gil" authorityYear="2015" box="[888,1024,1659,1683]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="205" phylum="Arthropoda" rank="species" species="santanderi">
<emphasis box="[888,1024,1659,1683]" italics="true" pageId="38" pageNumber="205">R. santanderi</emphasis>
