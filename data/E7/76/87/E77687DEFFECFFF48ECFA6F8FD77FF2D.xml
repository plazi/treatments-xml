<emphasis box="[1299,1355,1851,1875]" italics="true" pageId="14" pageNumber="255">et al.</emphasis>
(
<number box="[1369,1428,1851,1875]" pageId="14" pageNumber="255" value="2004.0">2004</number>
)
</bibRefCitation>
(
<tableCitation box="[158,246,1886,1911]" captionStart="TABLE 4" captionStartId="15.[151,239,260,283]" captionTargetBox="[159,1422,312,1139]" captionTargetPageId="15" captionText="TABLE 4. Measurements of Johnstoniana parva larvae." httpUri="http://table.plazi.org/id/3BA06640FFEDFFF48ECFA10EFD5FFEE2" pageId="14" pageNumber="255" tableUuid="3BA06640FFEDFFF48ECFA10EFD5FFEE2">
Table 
<number box="[229,246,1887,1911]" pageId="14" pageNumber="255" value="4.0">4</number>
</tableCitation>
