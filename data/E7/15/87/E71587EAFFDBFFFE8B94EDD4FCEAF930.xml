, 
<taxonomicName authority="Bate, 1856" authorityName="Bate" authorityYear="1856" box="[151,817,1654,1679]" class="Malacostraca" family="Ischyroceridae" genus="Siphonoecetes" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="74" phylum="Arthropoda" rank="species" species="kroyeranus" subGenus="Centraloecetes">
