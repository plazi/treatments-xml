<heading bold="true" box="[151,471,295,322]" fontSize="11" level="1" pageId="4" pageNumber="205" reason="1">
<emphasis bold="true" box="[151,471,295,322]" pageId="4" pageNumber="205">
<taxonomicName authority="García Aldrete, 2017" authorityName="García Aldrete" authorityYear="2017" box="[151,398,295,321]" class="Insecta" family="Lachesillidae" genus="Lachesilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="4" pageNumber="205" phylum="Arthropoda" rank="species" species="cameroni" status="n. sp.">
<emphasis bold="true" box="[151,398,295,321]" italics="true" pageId="4" pageNumber="205">Lachesilla cameroni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[405,471,296,322]" pageId="4" pageNumber="205" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
</heading>
