<taxonomicName box="[151,245,1807,1830]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="11" pageNumber="212" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,245,1807,1830]" italics="true" pageId="11" pageNumber="212">Quercus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[299,444,1806,1830]" class="Magnoliopsida" family="Hamamelidaceae" genus="Liquidambar" higherTaxonomySource="GBIF" kingdom="Plantae" order="Saxifragales" pageId="11" pageNumber="212" phylum="Tracheophyta" rank="genus">
<emphasis box="[299,444,1806,1830]" italics="true" pageId="11" pageNumber="212">Liquidambar</emphasis>
</taxonomicName>
. A. N. García Aldrete &amp; B. García González. 2 paratypes male. MEXICO. Chiapas. Km 21, Altamirano-San Cristóbal de las Casas, 
<quantity box="[645,734,1843,1867]" metricMagnitude="3" metricUnit="m" metricValue="1.938" pageId="11" pageNumber="212" unit="m" value="1938.0">1938 m</quantity>
