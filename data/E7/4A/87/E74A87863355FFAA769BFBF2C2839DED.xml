<taxonomicName authorityName="Wan Nur Asiah &amp; Che Salmah" authorityYear="2009" box="[924,1172,1851,1875]" class="Insecta" family="Perlidae" genus="Etrocorema" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="184" phylum="Arthropoda" rank="species" species="belumensis">
