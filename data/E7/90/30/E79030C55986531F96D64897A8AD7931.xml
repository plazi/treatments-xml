<taxonomicName baseAuthorityName="Malloch" baseAuthorityYear="1939" class="Insecta" family="Tephritidae" genus="Bactrocera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bactrocera simulata" order="Diptera" pageId="0" pageNumber="49" phylum="Arthropoda" rank="species" species="simulata">
<emphasis italics="true" pageId="0" pageNumber="49">Bactrocera simulata</emphasis>
</taxonomicName>
(Malloch) 
<emphasis bold="true" pageId="0" pageNumber="49">A</emphasis>
head 
<emphasis bold="true" pageId="0" pageNumber="49">B</emphasis>
head and scutum 
<emphasis bold="true" pageId="0" pageNumber="49">C</emphasis>
abdomen 
