<collectingCountry box="[729,820,1736,1759]" name="Taiwan" pageId="8" pageNumber="28">Taiwan</collectingCountry>
. Triangle, 
<taxonomicName box="[951,1231,1736,1759]" class="Diplopoda" family="Paradoxosomatidae" genus="Anoplodesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="28" phylum="Arthropoda" rank="species" species="spiniger" status="sp. nov.">
<emphasis box="[951,1231,1736,1759]" italics="true" pageId="8" pageNumber="28">Anoplodesmus spiniger</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1240,1337,1736,1760]" pageId="8" pageNumber="28">
<taxonomicNameLabel box="[1240,1337,1736,1760]" pageId="8" pageNumber="28" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
Square, 
<taxonomicName box="[151,418,1768,1791]" class="Diplopoda" family="Paradoxosomatidae" genus="Anoplodesmus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="28" phylum="Arthropoda" rank="species" species="aspinosus" status="sp. nov.">
<emphasis box="[151,418,1768,1791]" italics="true" pageId="8" pageNumber="28">Anoplodesmus aspinosus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[425,510,1768,1792]" pageId="8" pageNumber="28">
<taxonomicNameLabel box="[425,510,1768,1792]" pageId="8" pageNumber="28" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</paragraph>
