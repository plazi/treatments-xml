<quantity box="[410,491,830,854]" metricMagnitude="-2" metricUnit="m" metricValue="3.4" pageId="3" pageNumber="242" unit="mm" value="34.0">34 mm</quantity>
and maximum width 
<quantity box="[744,827,830,854]" metricMagnitude="-2" metricUnit="m" metricValue="2.3" pageId="3" pageNumber="242" unit="mm" value="23.0">23 mm</quantity>
