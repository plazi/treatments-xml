<taxonomicName box="[676,943,1912,1937]" class="Insecta" family="Chrysomelidae" genus="Taiwanaenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="160" phylum="Arthropoda" rank="species" species="collaris">
