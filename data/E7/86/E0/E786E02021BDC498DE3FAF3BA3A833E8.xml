, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33971" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole rogeri Emery" lsidName-HNS="Pheidole rogeri Emery" order="Hymenoptera" pageNumber="694" phylum="Arthropoda" rank="species" species="rogeri">rogeri</taxonomicName>
