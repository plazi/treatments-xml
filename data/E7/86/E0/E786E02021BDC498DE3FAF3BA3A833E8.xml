<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182038" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole grantae Forel" lsidName-HNS="Pheidole grantae Forel" order="Hymenoptera" pageNumber="694" phylum="Arthropoda" rank="species" species="grantae">grantae</taxonomicName>
