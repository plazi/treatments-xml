<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182095" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole zoster Wilson" lsidName-HNS="Pheidole zoster Wilson" order="Hymenoptera" pageNumber="694" phylum="Arthropoda" rank="species" species="zoster">zoster</taxonomicName>
