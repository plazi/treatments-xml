<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34030" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole stulta Forel" lsidName-HNS="Pheidole stulta Forel" order="Hymenoptera" pageNumber="694" phylum="Arthropoda" rank="species" species="stulta">stulta</taxonomicName>
