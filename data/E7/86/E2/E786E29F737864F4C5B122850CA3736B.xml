<taxonomicName baseAuthorityName="Sowerby" baseAuthorityYear="1833" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="59" pageNumber="60" phylum="Mollusca" rank="species" species="bivittatus">
