<title pageId="null" pageNumber="428">
<subSubSection pageId="null" pageNumber="428" type="nomenclature">
<paragraph pageId="null" pageNumber="428">
<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Carduus" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="428" phylum="Tracheophyta" rank="species" species="acanthoides">
