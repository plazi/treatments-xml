<subSubSection box="[237,628,1468,1493]" pageId="1" pageNumber="98" type="nomenclature">
<paragraph blockId="1.[237,628,1468,1493]" box="[237,628,1468,1493]" pageId="1" pageNumber="98">
<heading bold="true" box="[237,628,1468,1493]" centered="true" fontSize="10" level="2" pageId="1" pageNumber="98" reason="2">
<emphasis bold="true" box="[237,628,1468,1493]" pageId="1" pageNumber="98">
<taxonomicName baseAuthorityName="F.Smith" baseAuthorityYear="1858" box="[237,466,1468,1493]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="98" phylum="Arthropoda" rank="species" species="pachycerus">
<emphasis bold="true" box="[237,466,1468,1493]" italics="true" pageId="1" pageNumber="98">Aenictus pachycerus</emphasis>
</taxonomicName>
