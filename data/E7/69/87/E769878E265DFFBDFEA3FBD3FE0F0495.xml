<subSubSection pageId="3" pageNumber="100" type="distribution">
<paragraph blockId="3.[114,652,445,470]" box="[114,652,445,470]" pageId="3" pageNumber="100">
<emphasis bold="true" box="[114,254,445,470]" pageId="3" pageNumber="100">
<emphasis bold="true" box="[114,247,445,470]" italics="true" pageId="3" pageNumber="100">Distribution</emphasis>
.
