<collectingCountry box="[1300,1386,1675,1697]" name="Vietnam" pageId="1" pageNumber="98">Vietnam</collectingCountry>
, 
<collectingCountry box="[1399,1448,1676,1697]" name="Laos" pageId="1" pageNumber="98">Laos</collectingCountry>
, and 
<collectingCountry box="[892,979,1704,1726]" name="Thailand" pageId="1" pageNumber="98">Thailand</collectingCountry>
).................................................. 
