<quantity box="[1365,1451,1617,1639]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="1" pageNumber="98" unit="mm" value="0.15">0.15 mm</quantity>
(
<collectingCountry box="[857,968,1646,1668]" name="Philippines" pageId="1" pageNumber="98">Philippines</collectingCountry>
)................................................................ 
<taxonomicName authorityName="Chapman" authorityYear="1963" box="[1363,1450,1646,1667]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="98" phylum="Arthropoda" rank="species" species="reyesi">
<emphasis bold="true" box="[1363,1450,1646,1667]" italics="true" pageId="1" pageNumber="98">A. reyesi</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="1" pageNumber="98">
<paragraph blockId="1.[813,1452,1243,2100]" pageId="1" pageNumber="98">
<keyLead pageId="1" pageNumber="98">
