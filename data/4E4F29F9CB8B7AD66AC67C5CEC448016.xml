<normalizedToken id="23F3F6337E297189D382379E925A9FAA" originalValue="A–E">A-E</normalizedToken>
<taxonomicName id="803063C1061EF92566AA1917F16D482F" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica quadriforceps" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="quadriforceps">Tetraserica quadriforceps</taxonomicName>
