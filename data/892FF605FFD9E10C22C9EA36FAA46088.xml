<taxonomicName id="C6863C90FFD9E10C25B3ECF4FAAD6088" box="[1217,1349,1236,1262]" class="Insecta" family="Eupelmidae" genus="Anastatus" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="463" phylum="Arthropoda" rank="species" species="apantelesi">
<emphasis id="33F29B01FFD9E10C25B3ECF4FAAD6088" box="[1217,1349,1236,1262]" italics="true" pageId="6" pageNumber="463">A. apantelesi</emphasis>
</taxonomicName>
.
