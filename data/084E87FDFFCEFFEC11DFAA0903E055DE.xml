<quantity id="471F9B0EFFCEFFEF15BFAF04066A5519" box="[1271,1374,1868,1895]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="1" pageNumber="232" unit="mm" value="2.2">2.20 mm</quantity>
