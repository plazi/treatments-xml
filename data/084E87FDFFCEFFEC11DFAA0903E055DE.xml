<quantity id="471F9B0EFFCEFFEF151AAF04078E5519" box="[1106,1210,1868,1895]" metricMagnitude="-4" metricUnit="m" metricValue="9.2" pageId="1" pageNumber="232" unit="mm" value="0.92">0.92 mm</quantity>
