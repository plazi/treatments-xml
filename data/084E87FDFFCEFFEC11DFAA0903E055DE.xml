<quantity id="471F9B0EFFCEFFEF124CAF0400585519" box="[772,876,1868,1895]" metricMagnitude="-4" metricUnit="m" metricValue="5.3" pageId="1" pageNumber="232" unit="mm" value="0.53">0.53 mm</quantity>
; PL: 
<quantity id="471F9B0EFFCEFFEF12E0AF04073A5519" box="[936,1038,1868,1895]" metricMagnitude="-4" metricUnit="m" metricValue="8.299999999999999" pageId="1" pageNumber="232" unit="mm" value="0.83">0.83 mm</quantity>
