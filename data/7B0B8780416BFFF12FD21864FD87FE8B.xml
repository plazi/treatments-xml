. 
<taxonomicName id="34A24D154169FFF02E5F1C4CFE13F9C1" box="[282,443,1600,1622]" class="Arachnida" family="Oonopidae" genus="Xestaspis" kingdom="Animalia" order="Araneae" pageId="6" phylum="Arthropoda" rank="species" species="kandy">
