<taxonomicName id="4C6308DD9B516064FDE2FAEB5545F41E" baseAuthorityName="Curran" baseAuthorityYear="1924" box="[564,857,1356,1383]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="61" phylum="Arthropoda" rank="species" species="fasciatus">
