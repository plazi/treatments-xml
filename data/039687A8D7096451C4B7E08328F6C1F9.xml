<quantity id="4CC79B5BD7096451C623E1512814C23C" box="[515,576,259,281]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="17" pageNumber="492" unit="mm" value="8.0">8 mm</quantity>
