<number id="3DEE24A8471DFFEA05EDFF19CC95FF36" box="[1184,1221,188,213]" pageId="21" pageNumber="374" value="3.5">3.5</number>
, 
<number id="3DEE24A8471DFFEA059CFF19CCA4FF36" box="[1233,1268,188,213]" pageId="21" pageNumber="374" value="8.5">8.5</number>
and 
<number id="3DEE24A8471DFFEA0466FF19CD1EFF36" box="[1323,1358,188,213]" pageId="21" pageNumber="374" value="5.6">5.6</number>
