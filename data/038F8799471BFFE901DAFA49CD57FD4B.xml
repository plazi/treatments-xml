<number id="3DEE24A8471EFFE900C5FF7AC9F1FF1B" box="[392,417,223,248]" pageId="22" pageNumber="375" value="18.0">
<date id="FF98104F471EFFE900C5FF7AC9F1FF1B" box="[392,417,223,248]" pageId="22" pageNumber="375" value="1918-01">18</date>
</number>
</figureCitation>
