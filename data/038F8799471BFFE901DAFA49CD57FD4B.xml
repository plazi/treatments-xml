<number id="3DEE24A8471DFFEA00BEFF19CA44FF36" box="[499,532,188,213]" pageId="21" pageNumber="374" value="44.0">44</number>
: 
<number id="3DEE24A8471DFFEA035AFF19CA67FF36" box="[535,567,188,213]" pageId="21" pageNumber="374" value="60.0">60</number>
: 
<number id="3DEE24A8471DFFEA0376FF19CA0AFF36" box="[571,602,188,213]" pageId="21" pageNumber="374" value="27.0">27</number>
: 
<number id="3DEE24A8471DFFEA0313FF19CA2DFF36" box="[606,637,188,213]" pageId="21" pageNumber="374" value="68.0">68</number>
