<paragraph id="8BF536838A5FFF8EFF3DFE579639B227" blockId="2.[148,977,420,574]" pageId="2" pageNumber="440">
<materialsCitation id="3B223CDE8A5FFF8EFF3DFE579635B227" collectingDate="1905-09-05" collectionCode="ZMHB" collectorName="F. Ohaus. Ohaus &amp; His &amp; Specimens" country="Ecuador" county="The" elevation="3000" location="Villonaco" municipality="This" pageId="2" pageNumber="440" specimenCount="1" stateProvince="Loja">
<collectingCounty id="62944E0F8A5FFF8EFF3DFE579583B1A3" box="[172,209,421,442]" pageId="2" pageNumber="440">The</collectingCounty>
