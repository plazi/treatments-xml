<taxonomicName id="9DF04D48FFF1FFAEFCBAED972FD68E60" authority="(Feist-Castel, 1971)" authorityName="Soulie-Marsche" authorityYear="1989" baseAuthorityName="Feist-Castel" baseAuthorityYear="1971" class="Charophyceae" family="Characeae" genus="Lychnothamnus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Charales" pageId="20" pageNumber="405" phylum="Charophyta" rank="species" species="grambastii">
