<emphasis id="6884EAD9FFF1FFAEFD59EE782DDE8E24" bold="true" box="[648,682,1589,1607]" pageId="20" pageNumber="405">H -K</emphasis>
, 
<taxonomicName id="9DF04D48FFF1FFAEFD64EE782B348E24" authority="(Grambast &amp; Paul, 1965)" baseAuthorityName="Grambast &amp; Paul" baseAuthorityYear="1965" box="[693,1088,1589,1607]" class="Charophyceae" family="Characeae" genus="Lychnothamnus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Charales" pageId="20" pageNumber="405" phylum="Charophyta" rank="species" species="major">
