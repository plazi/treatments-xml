<collectionCode id="A2F5AE512B5AFF9F3BB92F08CE838765" box="[427,487,528,554]" country="Israel" httpUri="http://grbio.org/cool/ze71-842p" name="Tel-Aviv University" pageId="33" pageNumber="34">TAU</collectionCode>
, 
<collectionCode id="A2F5AE512B5AFF9F3BE02F08CD2C8765" box="[498,584,528,554]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="33" pageNumber="34">ZMUC</collectionCode>
)
</materialsCitation>
