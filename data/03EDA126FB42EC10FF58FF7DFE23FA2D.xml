<taxonomicName id="4C446BB3FB43EC10FB2FFAADFADFFA99" box="[1248,1362,1351,1376]" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="56" phylum="Arthropoda" rank="species" species="minuta">
<emphasis id="B930CC22FB43EC10FB2FFAADFADFFA99" box="[1248,1362,1351,1376]" italics="true" pageId="11" pageNumber="56">B. minuta</emphasis>
</taxonomicName>
