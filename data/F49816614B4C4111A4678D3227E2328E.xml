<taxonomicName id="2CC6A3C1C8129F1DCFAA7B99F250121B" class="Insecta" family="Cicadellidae" genus="Thaioneura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thaioneura nigrilinea" order="Hemiptera" pageId="1" pageNumber="8" phylum="Arthropoda" rank="species" species="nigrilinea">Thaioneura nigrilinea</taxonomicName>
