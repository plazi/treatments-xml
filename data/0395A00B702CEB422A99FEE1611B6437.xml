<taxonomicName id="4C3C6A9E702CEB422B8DFE1267C866D6" box="[387,665,462,488]" class="Magnoliopsida" family="Apiaceae" genus="Cryptotaenia" kingdom="Plantae" order="Apiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="canadensis">
