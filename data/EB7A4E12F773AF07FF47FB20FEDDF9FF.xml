<taxonomicName id="A4D38487F770AF07FE9AFC2BFD36FBFA" authority="(SHENEFELT, 1970)" authorityName="Papp" authorityYear="2013" baseAuthorityName="SHENEFELT" baseAuthorityYear="1970" box="[352,688,1004,1028]" class="Insecta" family="Braconidae" genus="Bassus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="299" phylum="Arthropoda" rank="species" species="bruesi" status="comb. n.">
