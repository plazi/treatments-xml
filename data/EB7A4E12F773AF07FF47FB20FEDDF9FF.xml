</paragraph>
<paragraph id="636CFF04F770AF07FF47FEACFE24FD5F" blockId="2.[189,794,329,673]" pageId="2" pageNumber="299">
<taxonomicName id="A4D38487F770AF07FF47FEACFEEBFE7F" authorityName="Papp" authorityYear="2013" box="[189,365,361,385]" class="Insecta" family="Braconidae" genus="Bassus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="299" phylum="Arthropoda" rank="species" species="szepligetii">
