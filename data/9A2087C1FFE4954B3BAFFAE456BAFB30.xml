<collectingCountry id="6A9E7647FFE0954B3A5FFBC45790FBC0" box="[359,454,1053,1079]" name="Ecuador" pageId="52" pageNumber="53">Ecuador</collectingCountry>
, 
<collectingRegion id="D04DF835FFE0954B3AEAFBC454EAFBC0" box="[466,700,1053,1079]" country="Ecuador" name="Sucumbios" pageId="52" pageNumber="53">Sucumbios Province</collectingRegion>
