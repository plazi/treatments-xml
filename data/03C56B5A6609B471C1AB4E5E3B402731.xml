<taxonomicName id="4C6CA1CF6609B471C5784D543CED276C" box="[1092,1271,1572,1597]" class="Magnoliopsida" family="Nymphaeaceae" genus="Nymphaea" kingdom="Plantae" order="Nymphaeales" pageId="92" pageNumber="93" phylum="Tracheophyta" rank="species" species="alba">
<emphasis id="B918065E6609B471C5784D543CED276C" box="[1092,1271,1572,1597]" italics="true" pageId="92" pageNumber="93">Nymphaea alba</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6CA1CF6609B471C43A4D543D86276C" box="[1286,1436,1572,1597]" class="Magnoliopsida" family="Nymphaeaceae" genus="Nuphar" kingdom="Plantae" order="Nymphaeales" pageId="92" pageNumber="93" phylum="Tracheophyta" rank="species" species="lutea">
<emphasis id="B918065E6609B471C43A4D543D86276C" box="[1286,1436,1572,1597]" italics="true" pageId="92" pageNumber="93">Nuphar lutea</emphasis>
</taxonomicName>
(
<taxonomicName id="4C6CA1CF6609B471C1A24D3739572731" authorityName="R.A.Salisbury" authorityYear="1805" box="[158,333,1607,1632]" class="Magnoliopsida" family="Nymphaeaceae" kingdom="Plantae" order="Nymphaeales" pageId="92" pageNumber="93" phylum="Tracheophyta" rank="family">Nymphaeaceae</taxonomicName>
