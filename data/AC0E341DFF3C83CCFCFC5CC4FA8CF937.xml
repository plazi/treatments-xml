<collectionCode id="42B61DCEFF3C83CCFB995E73FBFBF9D0" box="[1117,1131,1555,1579]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="129" pageNumber="129" type="Herbarium">P</collectionCode>
