<taxonomicName id="E3A7FE88FF3C83CCFB2B5E74FA53F9D7" box="[1263,1475,1556,1580]" class="Magnoliopsida" family="Brassicaceae" genus="Diceratium" kingdom="Plantae" order="Brassicales" pageId="129" pageNumber="129" phylum="Tracheophyta" rank="species" species="floccosum">
