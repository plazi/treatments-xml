<collectorName id="1014533B1057FFC1F26AFD39B48CFD60" box="[598,699,679,705]" pageId="11" pageNumber="26">D. Rider</collectorName>
(
<collectionCode id="DBF0AE281057FFC1F2F6FD39B51FFD60" box="[714,808,679,705]" pageId="11" pageNumber="26">CWOB</collectionCode>
