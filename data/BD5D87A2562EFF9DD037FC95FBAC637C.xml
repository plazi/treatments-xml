<taxonomicName id="F2F44D37562FFF9AD1FFF980FE866306" authorityName="J. A. Allen" authorityYear="1904" box="[196,305,1633,1657]" class="Mammalia" family="Phyllostomidae" genus="Lonchophylla" kingdom="Animalia" order="Chiroptera" pageId="51" pageNumber="51" phylum="Chordata" rank="species" species="thomasi">
