<taxonomicName id="4C514D01BD70FFDDA8C48C05FA90FBB7" authority="Parker, 1940" authorityName="Parker" authorityYear="1940" box="[965,1340,1024,1048]" class="Amphibia" family="Limnodynastidae" genus="Philoria" kingdom="Animalia" order="Anura" pageId="8" pageNumber="65" phylum="Chordata" rank="species" species="loveridgei">
