<quantity id="F51E3896E562FFF7FAF3F93505C8117C" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.5" metricValueMin="1.5" pageId="12" pageNumber="109" unit="mm" value="2.0" valueMax="2.5" valueMin="1.5">1.5–2.5 mm</quantity>
