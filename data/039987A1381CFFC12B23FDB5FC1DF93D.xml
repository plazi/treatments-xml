<quantity id="4CC89B52381DFFC12B73FD7DFD39FD35" box="[570,677,659,685]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="15" pageNumber="16" unit="mm" value="0.9">0.90 mm</quantity>
