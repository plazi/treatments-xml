<taxonomicName id="4C784D0EBF1CFFBA26E93962662FFDF4" authorityName="Pierce" authorityYear="1909" box="[917,1082,536,559]" class="Insecta" family="Stylopidae" genus="Stylops" kingdom="Animalia" order="Strepsiptera" pageId="21" pageNumber="326" phylum="Arthropoda" rank="species" species="multiplicatae">
