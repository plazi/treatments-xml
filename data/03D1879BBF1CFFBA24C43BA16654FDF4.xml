<taxonomicName id="4C784D0EBF1CFFBA27E83A8260F5FDD4" box="[660,736,504,527]" class="Insecta" family="Stylopidae" genus="Stylops" kingdom="Animalia" order="Strepsiptera" pageId="21" pageNumber="326" phylum="Arthropoda" rank="genus">
