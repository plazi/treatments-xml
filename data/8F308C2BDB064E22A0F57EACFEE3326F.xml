<subSubSection id="4F836EB6DB064E21A0F57EE3FC363036" box="[151,998,766,789]" pageId="1" pageNumber="52" type="materials_examined">
<paragraph id="07263D3DDB064E21A0F57EE3FC363036" blockId="1.[151,998,766,789]" box="[151,998,766,789]" pageId="1" pageNumber="52">
<typeStatus id="D822839FDB064E21A0F57EE3FF1C3036" box="[151,204,766,789]" pageId="1" pageNumber="52">Type</typeStatus>
species: 
<taxonomicName id="C09946BEDB064E21A14F7EE3FD003036" authority="Bocak, 1999: 167" authorityName="Bocak" authorityPageNumber="167" authorityYear="1999" box="[301,720,766,789]" class="Insecta" family="Lycidae" genus="Wakarumbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="52" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis id="35EDE12FDB064E21A14F7EE3FDDC3036" box="[301,524,766,789]" italics="true" pageId="1" pageNumber="52">Wakarumbia gracilis</emphasis>
<bibRefCitation id="630840CCDB064E21A2767EE3FD003036" author="Bocak, L." box="[532,720,766,789]" pageId="1" pageNumber="52" pagination="166 - 170" refId="ref6620" refString="Bocak, L. (1999) New taxa from the subtribe Hemiconderinina. Entomologische Blatter, 95, 166 - 170." type="journal article" year="1999">Bocak, 1999: 167</bibRefCitation>
</taxonomicName>
