<number id="C3444667FFDFFFD5AA22F8DC0BE4F88B" box="[620,679,1839,1863]" pageId="9" pageNumber="10" value="1847.0">1847</number>
</bibRefCitation>
; 
<bibRefCitation id="111D29B1FFDFFFD5AAFAF8DC0BB3F88B" author="Zeller" box="[692,752,1839,1863]" pageId="9" pageNumber="10" refString="Zeller, P. C. (1854) Lokalitaten an der Ostkuste Siciliens, in lepidopterologischer Hinsicht dargestellt. Bulletin de la SocietÂ´imperiale des Naturalistes de Moscou, 27, 3 - 52." type="journal article" year="1854">
<number id="C3444667FFDFFFD5AAFAF8DC0BB3F88B" box="[692,752,1839,1863]" pageId="9" pageNumber="10" value="1854.0">1854</number>
</bibRefCitation>
). 
<bibRefCitation id="111D29B1FFDFFFD5AB4BF8DC0A84F88B" author="Razowski" box="[773,967,1839,1863]" pageId="9" pageNumber="10" refString="Razowski, J. (1970) Cochylidae. In: Amsel, H. G., Gregor, F., Reisser, H., (Eds.), Microlepidoptera Palaearctica, 3, Vienna, pp. 1 - 528, 161 pls." type="journal article" year="1970">
