<date id="01327280FFDFFFD5AD65F8FF0991F88B" pageId="9" pageNumber="10">
<number id="C3444667FFDFFFD5AD65F8FF0C04F8E8" box="[1323,1351,1804,1828]" pageId="9" pageNumber="10" value="23.0">23</number>
