<taxonomicName id="4C434D27FFA4FFF1FEB6FA81FE0D9F66" box="[263,422,1303,1326]" class="Entognatha" family="Isotomidae" genus="Folsomides" kingdom="Animalia" order="Collembola" pageId="26" pageNumber="327" phylum="Arthropoda" rank="species" species="americanus">
