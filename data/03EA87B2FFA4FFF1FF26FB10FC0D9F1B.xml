<taxonomicName id="4C434D27FFA4FFF1FD6DFA81FCF19F67" box="[732,858,1302,1327]" class="Entognatha" family="Isotomidae" genus="Folsomides" kingdom="Animalia" order="Collembola" pageId="26" pageNumber="327" phylum="Arthropoda" rank="species" species="parvulus">
