<emphasis id="B942EAB1FFFA4716FD7FFED3FC7FFE89" box="[752,949,267,290]" italics="true" pageId="62" pageNumber="161">
<taxonomicName id="4C364D20FFFA4716FD7FFED3FC78FE89" box="[752,946,267,290]" class="Magnoliopsida" family="Goodeniaceae" genus="Scaevola" kingdom="Plantae" order="Asterales" pageId="62" pageNumber="161" phylum="Tracheophyta" rank="species" species="frutescens">Scaevola frutescens</taxonomicName>
