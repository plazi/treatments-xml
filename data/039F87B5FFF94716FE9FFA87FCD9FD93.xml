<emphasis id="B942EAB1FFFA4716FBADFED3FB36FE89" box="[1058,1276,267,290]" italics="true" pageId="62" pageNumber="161">
<taxonomicName id="4C364D20FFFA4716FBADFED3FB32FE89" box="[1058,1272,267,290]" class="Magnoliopsida" family="Euphorbiaceae" genus="Excoecaria" kingdom="Plantae" order="Malpighiales" pageId="62" pageNumber="161" phylum="Tracheophyta" rank="species" species="agallocha">Excoecaria agallocha</taxonomicName>
