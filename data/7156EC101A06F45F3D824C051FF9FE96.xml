<taxonomicName id="3EFF26851A07F45F3EAF4FE8181BFF06" box="[954,1117,226,252]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="undetermined">
