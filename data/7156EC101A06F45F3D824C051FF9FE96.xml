<taxonomicName id="3EFF26851A07F45F39644E27193EFEBC" box="[1137,1400,301,326]" class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Caryophyllales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="aviculare">
