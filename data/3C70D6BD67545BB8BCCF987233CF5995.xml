<taxonomicName id="3D696FB93A0BA08BE0380054457841CC" class="Sordariomycetes" family="Chaetosphaeriaceae" genus="Catenularia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Catenularia angulospora" order="Chaetosphaeriales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="angulospora">
