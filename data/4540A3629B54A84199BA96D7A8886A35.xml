<taxonomicName id="598339ED5C1E8FB6A815F3BD99AD81F0" authority="" authorityName="" class="Gastropoda" family="Helicinidae" genus="Helicoidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Helicoidea serica" order="Cycloneritida" pageId="14" pageNumber="15" phylum="Mollusca" rank="species" species="serica">serica</taxonomicName>
