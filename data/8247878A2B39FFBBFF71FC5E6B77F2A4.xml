<taxonomicName id="CDEE4D1F2B39FFBBFECBFC1168BCF32F" authority="Schultze, 1851" authorityName="Schultze" authorityYear="1851" box="[301,703,1005,1028]" family="Typhloplanidae" genus="Mesostomum" higherTaxonomySource="CoL" kingdom="Animalia" order="Rhabdocoela" pageId="8" pageNumber="9" phylum="Platyhelminthes" rank="species" species="obtusum">
