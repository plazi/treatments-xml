<subSubSection id="49F36527FF4CFFA48B57AADCACA05A7B" pageId="1" pageNumber="1145" type="etymology">
<paragraph id="015636ACFF4CFFA48B57AADCACC65A1B" blockId="1.[813,1418,236,355]" box="[813,936,236,259]" pageId="1" pageNumber="1145">
<emphasis id="339DEABEFF4CFFA48B57AADCACC65A1B" bold="true" box="[813,936,236,259]" pageId="1" pageNumber="1145">Etymology</emphasis>
</paragraph>
<paragraph id="015636ACFF4CFFA48B28AB3CACA05A7B" blockId="1.[813,1418,236,355]" pageId="1" pageNumber="1145">
