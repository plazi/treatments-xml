<taxonomicName id="4C49CBEAFFEDFF9EFD6DFD8435AFFDEC" box="[743,1014,583,608]" class="Polychaeta" family="Syllidae" genus="Sphaerosyllis" kingdom="Animalia" order="Phyllodocida" pageId="2" pageNumber="540" phylum="Annelida" rank="species" species="hystrix">
