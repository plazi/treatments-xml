<emphasis id="B956AB793A5AFFE2FF0AA6E3FE8BF8AA" bold="true" box="[199,317,1773,1798]" pageId="9" pageNumber="258">Remarks:</emphasis>
<taxonomicName id="4C220CE83A5AFFE2FE89A6E0FD9FF8A9" authorityName="Claparede" authorityYear="1870" box="[324,553,1773,1797]" class="Polychaeta" family="Capitellidae" genus="Notomastus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Capitellida" pageId="9" pageNumber="258" phylum="Annelida" rank="species" species="lineatus">
