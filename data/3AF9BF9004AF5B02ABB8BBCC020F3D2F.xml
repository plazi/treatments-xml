<taxonomicName id="09D5A53DF05F3FBD8B71C89B220EE3D9" LSID="3AF9BF90-04AF-5B02-ABB8-BBCC020F3D2F" authority="Tasanathai, Kuephadungphan &amp; Luangsa-ard" authorityName="Tasanathai, Kuephadungphan &amp; Luangsa-ard" authorityYear="2020" class="Sordariomycetes" family="Cordycipitaceae" genus="Gibellula" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Gibellula cebrennini" order="Hypocreales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="cebrennini" status="sp. nov.">Gibellula cebrennini Tasanathai, Kuephadungphan &amp; Luangsa-ard</taxonomicName>
