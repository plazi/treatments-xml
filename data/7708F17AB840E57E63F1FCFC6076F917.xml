<date id="8B1F66ACB840E57E60A5FB3A6710FBE3" box="[963,1148,1186,1212]" pageId="98" pageNumber="99">19 June–30 July</date>
(
<collectionCode id="99B0D8A9B840E57E67EAFB3A67D3FBE3" box="[1164,1215,1186,1212]" pageId="98" pageNumber="99">RJL</collectionCode>
