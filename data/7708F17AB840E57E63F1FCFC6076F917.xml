); 
<date id="8B1F66ACB840E57E666FFB3A66F0FBE3" box="[1289,1436,1186,1212]" pageId="98" pageNumber="99" value="1974-07-17">17 July 1974</date>
(
<collectionCode id="99B0D8A9B840E57E63F9FB5E63A9FBBF" box="[159,197,1222,1248]" country="USA" lsid="urn:lsid:biocol.org:col:15659" name="San Diego Natural History Museum" pageId="98" pageNumber="99" type="Herbarium">SD</collectionCode>
