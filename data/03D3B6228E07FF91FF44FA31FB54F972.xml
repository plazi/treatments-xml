<taxonomicName id="4C7A7CB78E07FF91FCAEF9FFFC54F990" authority="Walker, 1846" authorityName="Walker" authorityYear="1846" box="[893,973,1624,1646]" class="Insecta" family="Mymaridae" genus="Lymaenon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="litoralis">
