<taxonomicName id="D7F64D08DF127253C079FCCCFD84744F" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,612,837,860]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="rego">
