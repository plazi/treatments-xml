<taxonomicName id="BFFB5A2435E15C4D473C014D3B627D47" class="Dicotyledoneae" family="Asteraceae" genus="Anthemis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Anthemis" order="Campanulales" pageId="5" pageNumber="44" phylum="Angiospermae" rank="genus">
