</taxonomicName>
and 
<taxonomicName id="4C2736CA7C53D457FDBFF891BBBDF87C" box="[595,717,1916,1939]" class="Insecta" family="Platygastridae" genus="Trichacis" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="procera">
