<taxonomicName id="4C6765B6A47DFF9AFD4AEE14F157F9C1" authorityName="Haeckel" authorityYear="1870" box="[747,893,1591,1617]" class="Calcarea" family="Amphoriscidae" genus="Amphoriscus" kingdom="Animalia" order="Leucosolenida" pageId="25" pageNumber="64" phylum="Porifera" rank="genus">
