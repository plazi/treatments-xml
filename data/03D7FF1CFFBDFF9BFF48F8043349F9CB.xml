<taxonomicName id="4C7E3589FFBDFF9BFF55FF0E333BFB88" authority="(Blanchard, 1851)" authorityName="" baseAuthorityName="Blanchard" baseAuthorityYear="1851" box="[221,583,1120,1144]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="118" pageNumber="117" phylum="Arthropoda" rank="species" species="leprieuri">
