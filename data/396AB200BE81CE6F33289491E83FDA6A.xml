<paragraph id="AB2F9BE8E54DD6EFC357F0D664150CE8" pageId="3" pageNumber="68">
<taxonomicName id="54755E71AF7C8322B33A653AFC7C81C3" LSID="http://zoobank.org/816821D0-8B10-4BEE-889D-CA45A80ABFDD" authority="Mayr, 1861" authorityName="Mayr" authorityYear="1861" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Temnothorax pilagens" order="Hymenoptera" pageId="3" pageNumber="68" phylum="Arthropoda" rank="species" species="pilagens">Temnothorax pilagens</taxonomicName>
<taxonomicNameLabel id="95C3030BAFA6B79801AB65A724641C4D" pageId="3" pageNumber="68">sp. n.</taxonomicNameLabel>
</paragraph>
