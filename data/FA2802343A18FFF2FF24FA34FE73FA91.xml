and 
<taxonomicName id="B581C8A13A1FFFF3FDB0FF37FD70FF21" box="[515,674,224,248]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="529" phylum="Arthropoda" rank="species" species="bigorrensis">
