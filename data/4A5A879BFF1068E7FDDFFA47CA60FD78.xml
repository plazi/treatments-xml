<taxonomicName id="05F34D0EFF1E68E7FB96FD5CCA04FD44" authorityName="K. H. Barnard" authorityYear="1930" box="[1121,1299,691,717]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="puncticulata">
