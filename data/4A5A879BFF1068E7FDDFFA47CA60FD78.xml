<taxonomicName id="05F34D0EFF1E68E7FC80FD5CCCD9FD44" authority="Verheye, 2017" authorityName="d’Acoz &amp; Verheye" authorityYear="2017" box="[887,974,692,717]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="teres" status="sp. nov.">
