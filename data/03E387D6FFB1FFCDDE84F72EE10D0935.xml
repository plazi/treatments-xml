<taxonomicName id="4C4A4D43FFB1FFCDD877F506E1090935" authority="(Swainson, 1820)" baseAuthorityName="Swainson" baseAuthorityYear="1820" class="Gastropoda" family="Bulimulidae" genus="Auris" kingdom="Animalia" order="Stylommatophora" pageId="29" pageNumber="86" phylum="Mollusca" rank="species" species="melastoma">
