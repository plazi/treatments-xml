<number id="3DBAFA04FFC7FFE0FEBCFEBFFE3F6BE3" box="[375,410,368,392]" pageId="2" pageNumber="235" value="0.9">0.9</number>
; 
<collectionCode id="ED6370E6FFC7FFE0FE64FEBFFE796BEC" box="[431,476,368,391]" pageId="2" pageNumber="235">LIII</collectionCode>
<number id="3DBAFA04FFC7FFE0FE28FEA0FDA36BE3" box="[483,518,367,392]" pageId="2" pageNumber="235" value="8.5">8.5</number>
