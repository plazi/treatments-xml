<taxonomicName id="22A36CD83B4EFFDAFF16FA1EFEAF0EDD" box="[151,288,1481,1504]" class="Actinopterygii" family="Acropomatidae" genus="Kaperangus" kingdom="Animalia" order="Perciformes" pageId="14" pageNumber="57" phylum="Chordata" rank="genus" status="n. gen.">
