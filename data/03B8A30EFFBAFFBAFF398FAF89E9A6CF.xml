<paragraph id="8BAE1218FFBAFFBAFF74886D8B0AA72E" blockId="1.[174,1233,997,1376]" box="[227,781,1062,1089]" pageId="1" pageNumber="12">
<materialsCitation id="3B791845FFBAFFBAFF74886D8B0EA72E" box="[227,777,1062,1089]" location="Nizhyn District" pageId="1" pageNumber="12" specimenCount="1" stateProvince="Chernihiv">
