<taxonomicName id="0C4B3613F277A7CDE99F0A8681687052" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lesteva" order="Coleoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="genus">Lesteva</taxonomicName>
