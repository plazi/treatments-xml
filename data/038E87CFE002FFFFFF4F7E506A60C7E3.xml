<paragraph id="8B9836D9E002FFFEFF4F7E506D57C16E" blockId="2.[151,548,1626,1653]" box="[151,548,1626,1653]" pageId="2" pageNumber="200">
<heading id="D0D081B5E002FFFEFF4F7E506D57C16E" bold="true" box="[151,548,1626,1653]" fontSize="11" level="1" pageId="2" pageNumber="200" reason="1">
<emphasis id="B953EACBE002FFFEFF4F7E506D57C16E" bold="true" box="[151,548,1626,1653]" pageId="2" pageNumber="200">
Genus 
<taxonomicName id="4C274D5AE002FFFEFF367E516D57C16E" authority="Hancock, 1913" authorityName="Hancock" authorityYear="1913" box="[238,548,1626,1653]" class="Insecta" family="Tetrigidae" genus="Tegotettix" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="200" phylum="Arthropoda" rank="genus">
<emphasis id="B953EACBE002FFFEFF367E516E17C16F" bold="true" box="[238,356,1627,1653]" italics="true" pageId="2" pageNumber="200">Tegotettix</emphasis>
<bibRefCitation id="EFB64B28E002FFFEFEB47E516D57C16E" author="Hancock, J. L." box="[364,548,1626,1653]" pageId="2" pageNumber="200" pagination="39 - 54" refId="ref6596" refString="Hancock, J. L. (1913) Studies of Tetriginae (Acrydinae) from Sarawak Museum, Borneo. The Sarawak Museum Journal, 1 (3), 39 - 54." type="journal article" year="1913">Hancock, 1913</bibRefCitation>
</taxonomicName>
</emphasis>
