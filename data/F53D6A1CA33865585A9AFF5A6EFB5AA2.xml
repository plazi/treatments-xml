<taxonomicName id="BA94A089A338655858C9FD4E6FCC5BDE" box="[708,962,715,741]" class="Magnoliopsida" family="Fabaceae" genus="Dolichos" kingdom="Plantae" order="Fabales" pageId="9" pageNumber="504" phylum="Tracheophyta" rank="species" species="angustifolius">
