(
<taxonomicName id="BA94A089A338655859DFFD4E68305BDD" box="[978,1086,715,742]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="9" pageNumber="504" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
