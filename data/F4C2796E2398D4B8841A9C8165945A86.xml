<taxonomicName id="68D43543319E1AC271892473E882E7D7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27179" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="C. Sicheli" lsidName-HNS="C. Sicheli" order="Hymenoptera" pageNumber="88" phylum="Arthropoda" rank="species" species="sicheli">C. Sicheli</taxonomicName>
