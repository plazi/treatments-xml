<taxonomicName id="4C1FDBCDFFD6FFC9FE30A383FDDBB98E" box="[483,621,1491,1517]" class="Insecta" family="Coenagrionidae" genus="Minagrion" kingdom="Animalia" order="Odonata" pageId="8" pageNumber="184" phylum="Arthropoda" rank="species" species="caldense">
