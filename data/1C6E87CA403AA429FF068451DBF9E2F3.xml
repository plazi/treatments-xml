<taxonomicName id="53C74D5F403AA429FDCD84E5DC49E2F1" box="[604,838,728,755]" class="Pinopsida" family="Pinaceae" genus="Abies" kingdom="Plantae" order="Pinales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="nordmanniana">
<emphasis id="A6B3EACE403AA429FDCD84E5DC49E2F1" bold="true" box="[604,838,728,755]" italics="true" pageId="33" pageNumber="34">Abies nordmanniana</emphasis>
