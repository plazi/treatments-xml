<taxonomicName id="53C74D5F403AA429FBB48489DA92E2CF" authority="Hadzibeili" authorityName="Hadzibeili" box="[1061,1437,691,719]" class="Insecta" family="Margarodidae" genus="Marchalina" kingdom="Animalia" order="Hemiptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="caucasica">
