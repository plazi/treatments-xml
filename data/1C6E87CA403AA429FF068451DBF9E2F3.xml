<taxonomicName id="53C74D5F403AA429FBAC84E5DBE1E2F1" box="[1085,1262,728,755]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="orientalis">
