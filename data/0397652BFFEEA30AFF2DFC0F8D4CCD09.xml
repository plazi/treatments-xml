<emphasis id="B94A082FFFEEA309FF7FF8E88F03CF5D" bold="true" box="[197,363,1818,1842]" pageId="1" pageNumber="96">
<typeStatus id="54856A9FFFEEA309FF7FF8E88E95CF5D" box="[197,253,1818,1842]" pageId="1" pageNumber="96">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54856A9FFFEEA309FECBF8E88FBCCF5E" box="[369,468,1818,1841]" pageId="1" pageNumber="96">Holotype</typeStatus>
<number id="3DF6C61AFFEEA309FE6EF8E88F80CF5D" box="[468,488,1818,1842]" pageId="1" pageNumber="96" value="3.0">3</number>
, 
<collectingCountry id="F32994ADFFEEA309FE4FF8E88C24CF5E" box="[501,588,1818,1841]" name="China" pageId="1" pageNumber="96">CHINA</collectingCountry>
, Yunnan, Mengla, Menglun, 
<date id="FF80F2FDFFEEA309FC38F8E88A7DCF5E" box="[898,1045,1818,1841]" pageId="1" pageNumber="96" value="2007-07-22">
<number id="3DF6C61AFFEEA309FC38F8E88DC8CF5E" box="[898,928,1818,1841]" pageId="1" pageNumber="96" value="22.0">22</number>
. VII. 
<number id="3DF6C61AFFEEA309FC67F8E88A7DCF5E" box="[989,1045,1818,1841]" pageId="1" pageNumber="96" value="2007.0">2007</number>
</date>
