<taxonomicName id="4C24E3F7FF9E1B145D2CFEEFFC37FE64" authorityName="Strecker" authorityYear="1873" box="[832,1021,378,404]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="42" phylum="Arthropoda" rank="species" species="faustina">
