<taxonomicName id="5CBECCE51366EE68FE62FA5AFDE9F9D5" authorityName="MOORE &amp; BRUSCA" authorityYear="2003" box="[405,591,1521,1543]" class="Malacostraca" family="Cirolanidae" genus="Colopisthus" kingdom="Animalia" order="Isopoda" pageId="16" pageNumber="1344" phylum="Arthropoda" rank="species" species="canna">
