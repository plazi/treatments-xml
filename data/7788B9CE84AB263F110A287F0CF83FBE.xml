<paragraph id="F3A64DFC660791A19D35F60A431B13E9" pageId="30" pageNumber="123">
<taxonomicName id="490EDF8DEBB8C5818474F49780B4E41E" LSID="http://zoobank.org/EE7D1A31-9D2F-4B74-A6A6-C95012768953" authority="Kang" authorityName="Förster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lytopylus luisgaritai" order="Hymenoptera" pageId="30" pageNumber="123" phylum="Arthropoda" rank="species" species="luisgaritai">Lytopylus luisgaritai Kang</taxonomicName>
