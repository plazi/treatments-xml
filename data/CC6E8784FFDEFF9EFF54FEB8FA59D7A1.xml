<quantity id="833F9B77FFDEFF9CFC2BFB53FAA0D076" box="[631,718,1271,1297]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="36" pageNumber="37" unit="mm" value="11.0">11 mm</quantity>
