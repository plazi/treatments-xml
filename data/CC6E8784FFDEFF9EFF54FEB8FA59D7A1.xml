<quantity id="833F9B77FFDEFF9CFCFAF883FB56D226" box="[678,824,1831,1857]" metricMagnitude="1" metricUnit="m" metricValue="7.8" metricValueMax="10.3" metricValueMin="5.3" pageId="36" pageNumber="37" unit="m" value="78.0" valueMax="103.0" valueMin="53.0">53 to 103 m</quantity>
(Penna­Neme 
