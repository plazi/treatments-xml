<collectingCountry id="F3540194FF938C3E582E7B06FA345166" box="[1310,1436,525,551]" name="Argentina" pageId="31" pageNumber="32">Argentina</collectingCountry>
(
<collectingRegion id="49878FE6FF938C3E5DAF7B39FEB9510D" box="[159,273,562,588]" country="Argentina" name="Tucuman" pageId="31" pageNumber="32">Tucumán</collectingRegion>
, 
<collectingRegion id="49878FE6FF938C3E5C2D7B39FEF6510D" box="[285,350,562,588]" country="Argentina" name="Jujuy" pageId="31" pageNumber="32">Jujuy</collectingRegion>
) (
<figureCitation id="13785D81FF938C3E5C467B39FE75510D" box="[374,477,562,588]" captionStart="FIGURE 148" captionStartId="57.[151,258,1979,2003]" captionTargetBox="[316,1281,306,1948]" captionTargetId="figure-64@57.[306,1281,306,1958]" captionTargetPageId="57" captionText="FIGURE 148. Distribution of P. discoideus, P. floresi, P. rubromaculatus and P. stenocephalus in southern South America." pageId="31" pageNumber="32">Fig. 148</figureCitation>
