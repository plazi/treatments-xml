<taxonomicName id="4C144D736BE2FF46AE7BFF1FFC8D3AC1" authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[357,851,1909,1933]" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="pseudofuscipes" status="sp. n.">
<emphasis id="B960EAE26BE2FF46AE7BFF1FFDB73AC1" box="[357,617,1909,1933]" italics="true" pageId="131" pageNumber="132">Maladera pseudofuscipes</emphasis>
Ahrens, Fabrizi &amp; Liu
</taxonomicName>
, 
<taxonomicNameLabel id="A25357996BE2FF46AC43FF1FFC4A3AC0" box="[861,916,1909,1932]" pageId="131" pageNumber="132" rank="species">sp. n.</taxonomicNameLabel>
(holotype); 
<emphasis id="B960EAE26BE2FF46AB0FFF1FFB9F3AC1" bold="true" box="[1041,1089,1909,1933]" pageId="131" pageNumber="132">E–I:</emphasis>
<taxonomicName id="4C144D736BE2FF46AB56FF1CFA473AC1" authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[1096,1433,1909,1933]" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="snizeki" status="sp. n.">
<emphasis id="B960EAE26BE2FF46AB56FF1CFB713AC1" box="[1096,1199,1909,1933]" italics="true" pageId="131" pageNumber="132">M. snizeki</emphasis>
Ahrens, Fabrizi &amp; Liu
</taxonomicName>
, 
<taxonomicNameLabel id="A25357996BE2FF46AF89FFF3FF103AFC" box="[151,206,1945,1968]" pageId="131" pageNumber="132" rank="species">sp. n.</taxonomicNameLabel>
(holotype); 
<emphasis id="B960EAE26BE2FF46AE55FFF3FE5B3AFD" bold="true" box="[331,389,1945,1969]" pageId="131" pageNumber="132">J–M:</emphasis>
<taxonomicName id="4C144D736BE2FF46AE95FFF0FD373AFD" authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[395,745,1945,1969]" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="fangana" status="sp. n.">
<emphasis id="B960EAE26BE2FF46AE95FFF0FDDE3AFD" box="[395,512,1945,1969]" italics="true" pageId="131" pageNumber="132">M. fangana</emphasis>
Ahrens, Fabrizi &amp; Liu
</taxonomicName>
, 
<taxonomicNameLabel id="A25357996BE2FF46ADEDFFF3FCF43AFC" box="[755,810,1945,1968]" pageId="131" pageNumber="132" rank="species">sp. n.</taxonomicNameLabel>
