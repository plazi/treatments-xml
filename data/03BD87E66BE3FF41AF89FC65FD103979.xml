<quantity id="4CEC9B156BE3FF47ADC8FD66FCF3386B" box="[726,813,1292,1319]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="130" pageNumber="131" unit="mm" value="3.5">3.5 mm</quantity>
