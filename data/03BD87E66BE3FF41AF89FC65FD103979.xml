<quantity id="4CEC9B156BE3FF47AEA2FD66FDCD386B" box="[444,531,1292,1319]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="130" pageNumber="131" unit="mm" value="4.9">4.9 mm</quantity>
