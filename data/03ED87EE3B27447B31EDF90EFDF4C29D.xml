<taxonomicName id="4C444D7B3B21447B31EDFC34FE62C58B" authority="Kieffer, 1904 a" authorityName="Kieffer" authorityYear="1904" box="[151,431,923,947]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
