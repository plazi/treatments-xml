<taxonomicName id="4C444D7B3B21447B305CFC15FD71C5EA" authority="Buysson, 1897" authorityName="Buysson" authorityYear="1897" box="[294,700,954,978]" class="Insecta" family="Bethylidae" genus="Goniozus" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="transvaalensis">
