<taxonomicName id="4C34B2B23C72CD4892502F25FE58451E" authority="Fibiger" authorityName="Fibiger" box="[311,453,1323,1348]" class="Insecta" family="Erebidae" genus="Flax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="genus" status="new genus">
