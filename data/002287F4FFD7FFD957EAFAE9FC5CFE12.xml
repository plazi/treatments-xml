<taxonomicName id="4F8B4D61FFD0FFD85550F83CFD56F858" authorityName="Zhixin &amp; Tao &amp; Weiliang &amp; Li &amp; Libin" authorityYear="2021" box="[557,715,1960,1983]" class="Insecta" family="Gryllidae" genus="Pluviam" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="449" phylum="Arthropoda" rank="species" species="guangxiensis">
