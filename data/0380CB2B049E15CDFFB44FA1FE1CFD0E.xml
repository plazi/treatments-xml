<subSubSection id="C33329B6049E15CDFFB44C95FE1CFD0E" pageId="151" pageNumber="152" type="discussion">
<paragraph id="8B967A3D049E15CDFFB44C95FE1CFD0E" blockId="151.[151,1436,392,762]" pageId="151" pageNumber="152">
<emphasis id="B95DA62F049E15CDFFB44C95FEE0FD22" bold="true" box="[151,267,700,726]" pageId="151" pageNumber="152">Remarks.</emphasis>
The genus 
<taxonomicName id="4C2901BE049E15CDFEB24C95FE12FD22" authorityName="Deshayes" authorityYear="1830" box="[401,505,700,726]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="151" pageNumber="152" phylum="Mollusca" rank="genus">
