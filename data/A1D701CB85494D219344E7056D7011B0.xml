<taxonomicName id="6B0A8FDE521EA363F561286B68E9527E" class="Actinopterygii" family="Cyprinidae" genus="Engraulicypris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Engraulicypris howesi" order="Cypriniformes" pageId="15" pageNumber="136" phylum="Chordata" rank="species" species="howesi">Engraulicypris howesi</taxonomicName>
.
</paragraph>
</caption>
