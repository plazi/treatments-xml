<taxonomicName id="4C644D203006C047FCC3F873D78BF631" authorityName="Skuse" authorityYear="1889" box="[887,957,2037,2058]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="242" phylum="Arthropoda" rank="species" species="nubifer">nubifer</taxonomicName>
