<taxonomicName id="4C514D1B6F458422FEAFFD8E709EFE51" authorityName="Dunn" authorityYear="1949" box="[287,480,551,574]" class="Amphibia" family="Microhylidae" genus="Chiasmocleis" kingdom="Animalia" order="Anura" pageId="55" pageNumber="55" phylum="Chordata" rank="species" species="shudikarensis">
