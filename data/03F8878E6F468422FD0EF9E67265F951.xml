<specimenCode id="DBF79EE36F468421FC1FF92B7525FACC" box="[943,1115,1677,1699]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="52" pageNumber="52">AMNH 43674</specimenCode>
