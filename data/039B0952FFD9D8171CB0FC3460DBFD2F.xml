<taxonomicName id="4C32C3C7FFD9D8181ECDFB546114FB4F" authorityName="Carpenter" authorityYear="1931" box="[701,842,1218,1242]" class="Insecta" family="Panorpidae" genus="Panorpa" kingdom="Animalia" order="Mecoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="anomala">
