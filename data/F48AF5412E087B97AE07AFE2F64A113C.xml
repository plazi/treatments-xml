<taxonomicName id="08C067144A39D4D06E65C319A6F42FB3" class="Insecta" family="Formicidae" genus="Leptogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptogenys distinguenda" order="Hymenoptera" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="distinguenda">Leptogenys distinguenda</taxonomicName>
and 
<taxonomicName id="5EF0F67D4C63FC92589C3EB684448BEA" class="Insecta" family="Formicidae" genus="Leptogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptogenys mutabilis" order="Hymenoptera" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="mutabilis">Leptogenys mutabilis</taxonomicName>
