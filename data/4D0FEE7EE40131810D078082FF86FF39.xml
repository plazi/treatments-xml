<taxonomicName id="544E5A1556D8ED706E6BEFF98E182950" LSID="4D0FEE7E-E401-3181-0D07-8082FF86FF39" authority="L." authorityName="L." class="Magnoliopsida" family="Ericaceae" genus="Andromeda" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Andromeda polifolia" order="Ericales" pageId="22" pageNumber="45" phylum="Tracheophyta" rank="species" species="polifolia">Andromeda polifolia L.</taxonomicName>
