<taxonomicName id="65B54D20FEA54C5EFDD2DB8DFD4B7E10" authorityName="Baba" authorityYear="2018" box="[547,644,1278,1302]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="307" pageNumber="308" phylum="Arthropoda" rank="species" species="mesodme">
