<taxonomicName id="5DEB94F4B1D41E17EB9EAA88744A01EA" LSID="03B714DB-4EBC-389C-4B7A-F2D6174646A1" authority="L." authorityName="L." class="Magnoliopsida" family="Euphorbiaceae" genus="Jatropha" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Jatropha curcas" order="Malpighiales" pageId="68" pageNumber="69" phylum="Tracheophyta" rank="species" species="curcas">Jatropha curcas L.</taxonomicName>
