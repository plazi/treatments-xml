<taxonomicName id="60BD57BEFFA4FFF1FC4AF8B71B68F88A" box="[973,1075,1798,1820]" class="Dothideomycetes" family="Euantennariaceae" genus="Antennula" higherTaxonomySource="GBIF" kingdom="Fungi" order="Capnodiales" pageId="45" pageNumber="51" phylum="Ascomycota" rank="genus">antennula</taxonomicName>
