<emphasis id="FCF749783A75DA7AFF7F6FFFFED6F813" bold="true" box="[199,315,1929,1955]" pageId="2" pageNumber="577">Remarks.</emphasis>
<taxonomicName id="0983EEE93A75DA7AFEFB6FFFFD24F813" authority="pulchellus" authorityName="pulchellus Arrow" authorityYear="1910" box="[323,713,1929,1955]" class="Insecta" family="Cetoniidae" genus="Cymophorus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="577" phylum="Arthropoda" rank="subSpecies" species="pulchellus" subSpecies="pulchellus">
