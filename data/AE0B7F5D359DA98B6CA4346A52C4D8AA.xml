<taxonomicName id="AE4721C2E6D9CFC6E571E55D0B7FD35B" authority="Neumayr, 1880" authorityName="Neumayr" authorityYear="1880" class="Gastropoda" family="Melanopsidae" genus="Melanoptychia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Melanoptychia" order="Sorbeoconcha" pageId="183" pageNumber="184" phylum="Mollusca" rank="genus">
