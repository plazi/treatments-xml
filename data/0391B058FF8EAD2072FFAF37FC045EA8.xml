<collectingCountry id="F32F41DEFF8FAD207067AAF0FD595EA8" box="[554,647,402,424]" name="Malaysia" pageId="35" pageNumber="2060">Malaysia</collectingCountry>
and 
<collectingCountry id="F32F41DEFF8FAD2070F5AAF1FCC35EA8" box="[696,797,403,424]" name="Myanmar" pageId="35" pageNumber="2060">Myanmar</collectingCountry>
, new for 
<collectingCountry id="F32F41DEFF8FAD207136AAF0FC0B5EA8" box="[891,981,402,424]" name="Vietnam" pageId="35" pageNumber="2060">Vietnam</collectingCountry>
