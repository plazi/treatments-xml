<taxonomicName id="4C7A4D1FB78EFF3B67F57FAEDFCDFD2E" authority="Kugelann, 1794" authorityName="Kugelann" authorityYear="1794" box="[673,957,667,693]" class="Insecta" family="Staphylinidae" genus="Bryaxis" kingdom="Animalia" order="Coleoptera" pageId="223" pageNumber="224" phylum="Arthropoda" rank="genus">
