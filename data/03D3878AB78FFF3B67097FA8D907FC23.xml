........................................................... 
<taxonomicName id="4C7A4D1FB78FFF3A61DB7AFCD907F873" authority="Santam." authorityName="Santam." box="[1167,1399,1998,2025]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Cryptandromyces" kingdom="Fungi" order="Laboulbeniales" pageId="222" pageNumber="223" phylum="Ascomycota" rank="species" species="euplecti">
