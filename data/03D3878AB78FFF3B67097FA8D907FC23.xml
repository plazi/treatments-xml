<taxonomicName id="4C7A4D1FB78EFF3B66117C67D8FBFEF4" authority="Reitter, 1891" authorityName="Reitter" authorityYear="1891" box="[837,1163,341,367]" class="Insecta" family="Staphylinidae" genus="Plectophloeus" kingdom="Animalia" order="Coleoptera" pageId="223" pageNumber="224" phylum="Arthropoda" rank="genus">
