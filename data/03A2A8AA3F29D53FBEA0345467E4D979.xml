<subSubSection id="E3F70676D3882EDFAC83EAF2098DB250" pageId="null" pageNumber="448" type="nomenclature">
<paragraph id="5A21222759762ECB36E71C046EDB98EE" pageId="null" pageNumber="448">
<taxonomicName id="4B11B0314DAFB4581C6ACE04A00C453F" authority="Willd." class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="448" phylum="Tracheophyta" rank="species" species="nervosa">
