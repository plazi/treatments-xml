<taxonomicName id="E9A10F81FFAFFF82CBA5FAE9FB8A3E2B" authorityName="Latreille" authorityYear="1802" box="[984,1085,1364,1386]" class="Insecta" family="Evaniidae" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="975" phylum="Arthropoda" rank="family">Evaniidae</taxonomicName>
