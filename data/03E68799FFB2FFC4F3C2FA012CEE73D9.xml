<emphasis id="B93BEA9DFFB2FFC4F392F87B2F4973A0" bold="true" box="[199,321,1926,1951]" inLineHeading="true" pageId="81" pageNumber="82" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFFB2FFC4F21CF87B2FD573A1" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,477,1926,1950]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="fera">
