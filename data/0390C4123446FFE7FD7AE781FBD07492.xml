</taxonomicName>
, 
<taxonomicName id="4C390E873447FFE7FE5DE01FFD047472" authority="(JEZEK &amp; TKOC 2012)" baseAuthorityName="JEZEK &amp; TKOC" baseAuthorityYear="2012" box="[372,740,605,628]" class="Insecta" family="Psychodidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">
