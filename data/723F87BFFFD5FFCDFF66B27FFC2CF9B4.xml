REMARKS. — Compared to 
<taxonomicName id="3D964D2AFFD5FFCDFD94B4A1FC5BF9DC" authority="Scarabino, 1995" authorityName="Scarabino" authorityYear="1995" box="[512,909,1514,1538]" class="Scaphopoda" family="Pulsellidae" genus="Pulsellum" kingdom="Animalia" order="Gadilida" pageId="34" pageNumber="249" phylum="Mollusca" rank="species" species="thomassini">
