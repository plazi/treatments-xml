<taxonomicName id="4C1DA6B8FFAECF0F53ECF07FD00FF81E" authorityName="Silvestri" authorityYear="1897" box="[724,872,1918,1943]" class="Diplopoda" family="Spirostreptidae" genus="Nanostreptus" kingdom="Animalia" order="Spirostreptida" pageId="34" pageNumber="185" phylum="Arthropoda" rank="genus">
