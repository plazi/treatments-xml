<taxonomicName id="4C1DA6B8FFAECF0F55B4F05BD647F8FA" authorityName="Silvestri" authorityYear="1897" box="[1164,1312,1882,1907]" class="Diplopoda" family="Spirostreptidae" genus="Nanostreptus" kingdom="Animalia" order="Spirostreptida" pageId="34" pageNumber="185" phylum="Arthropoda" rank="genus">
