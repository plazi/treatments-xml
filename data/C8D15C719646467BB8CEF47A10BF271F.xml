<taxonomicName id="CD714CAD99D5E5AB1AB28F668FFF9FAA" genus="F." lsidName="F. imparis" pageId="14" pageNumber="15" rank="species" species="imparis">F. imparis</taxonomicName>
sp. n., 
<taxonomicName id="86BA39E2525643C9D0D607C42B82D746" genus="F." lsidName="F. macrochaetosa" pageId="14" pageNumber="15" rank="species" species="macrochaetosa">F. macrochaetosa</taxonomicName>
