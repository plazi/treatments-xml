<subSubSection id="95A6655C7F70FFC828FF3595E585D491" pageId="8" pageNumber="553" type="distribution">
<paragraph id="DD0336D77F70FFC828FF3595E585D491" blockId="8.[151,1436,1470,1639]" pageId="8" pageNumber="553">
<emphasis id="EFC8EAC57F70FFC828FF3595E529D4BD" bold="true" box="[151,297,1470,1495]" pageId="8" pageNumber="553">Distribution</emphasis>
. 
<collectingCountry id="A5AB76477F70FFC8295F3594E57BD4BC" box="[311,379,1471,1494]" name="Japan" pageId="8" pageNumber="553">Japan</collectingCountry>
