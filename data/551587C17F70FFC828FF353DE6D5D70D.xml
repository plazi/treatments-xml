<collectingCountry id="A5AB76477F70FFC82C5F3594E07ED4BC" box="[1079,1150,1471,1494]" name="South Korea" pageId="8" pageNumber="553">Korea</collectingCountry>
, 
<collectingCountry id="A5AB76477F70FFC82CE13595E0D4D4BD" box="[1161,1236,1470,1495]" name="Russia" pageId="8" pageNumber="553">Russia</collectingCountry>
