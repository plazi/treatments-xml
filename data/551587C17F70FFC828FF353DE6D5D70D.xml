<collectingCountry id="A5AB76477F70FFC82D313595E19CD4BD" box="[1369,1436,1470,1495]" name="China" pageId="8" pageNumber="553">China</collectingCountry>
(
<collectingRegion id="1F78F8357F70FFC828F735C9E4FDD491" box="[159,253,1506,1531]" country="China" name="Shaanxi" pageId="8" pageNumber="553">Shaanxi</collectingRegion>
