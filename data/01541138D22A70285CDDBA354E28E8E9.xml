<taxonomicName id="4EFDDBADD22D702B5D47B8004EA7EC1A" authorityName="Packard" authorityYear="1874" box="[269,400,1374,1400]" class="Branchiopoda" family="Limnadiidae" genus="Eulimnadia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="4" pageNumber="378" phylum="Arthropoda" rank="genus">
