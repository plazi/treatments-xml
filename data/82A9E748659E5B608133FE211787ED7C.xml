<taxonomicName id="75A5B3C87F1B993FCC29A727B3FD2320" class="Insecta" family="Philopteridae" genus="Vernonia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Phthiraptera" pageId="5" pageNumber="26" phylum="Arthropoda" rank="species" species="dummeri">
