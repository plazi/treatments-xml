<geoCoordinate id="BDA2459DB317A719189735429A525BE8" degrees="12" direction="north" minutes="22" orientation="latitude" precision="925" value="12.366667">12°22'N</geoCoordinate>
, 
<geoCoordinate id="1C8DF35FBA7CD60AB5C3293799776573" degrees="12" direction="west" minutes="19" orientation="longitude" precision="925" value="-12.316667">12°19'W</geoCoordinate>
, 200 m, 22 Sep 1994, Sambou et Madsen, Goudiaby, 
<normalizedToken id="3FBCEA0AF64A968D60F4471AADE6CE5C" originalValue="Traoré">Traore</normalizedToken>
