<taxonomicName id="4C1993D5FFAEFFA1FF50FEA3FE33D6C5" authority="Johannsen, 1905: 135" authorityName="Johannsen" authorityPageNumber="135" authorityYear="1905" box="[151,511,366,388]" class="Insecta" family="Chironomidae" genus="Ablabesmyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="250" phylum="Arthropoda" rank="genus">
