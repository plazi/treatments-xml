<paragraph id="8BAE3688F3303815045AFE9AC005C600" blockId="32.[279,628,305,338]" box="[279,628,305,338]" pageId="32" pageNumber="233">
<taxonomicName id="4C114D0BF3303815045AFE9AC014C602" authority="(Pallas, 1766)" baseAuthorityName="Pallas" baseAuthorityYear="1766" box="[279,613,310,338]" class="Mammalia" family="Orycteropodidae" genus="Orycteropus" kingdom="Animalia" order="Tubulidentata" pageId="32" pageNumber="233" phylum="Chordata" rank="species" species="afer">
