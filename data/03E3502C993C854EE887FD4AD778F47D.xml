<taxonomicName id="4C4A9AB9993C854EE8DFFD4BD7F7F2A1" authority="Loew, 1859" authorityName="Loew" authorityYear="1859" box="[207,678,692,719]" class="Insecta" family="Dolichopodidae" genus="Teuchophorus" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="monacanthus">
