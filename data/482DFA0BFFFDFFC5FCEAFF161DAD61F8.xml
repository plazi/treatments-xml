<emphasis id="F2F0970FFFFDFFC5FBC7FED61ADD6498" box="[1033,1248,363,389]" italics="true" pageId="33" pageNumber="342">
<taxonomicName id="0784309EFFFDFFC5FBC7FED61AE16498" baseAuthorityName="Gruvel" baseAuthorityYear="1906" box="[1033,1244,363,389]" class="Maxillopoda" family="Scalpellidae" genus="Weltnerium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pedunculata" pageId="33" pageNumber="342" phylum="Arthropoda" rank="species" species="bouvieri">Weltnerium bouvieri</taxonomicName>
