<taxonomicName id="4C644D75FF98C61772A9FAF4FDA1FAC9" authority="☿ Forel Djebel Ozmor" authorityName="☿ Forel Djebel Ozmor" authorityYear="1890" box="[222,541,1282,1308]" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="248" phylum="Arthropoda" rank="species" species="alii">
