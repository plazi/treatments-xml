<taxonomicName id="C2AA9BB8AF2E57242BB4F80FFE63F819" authority="Ascuntar-Osnas, 2018" authorityName="Ascuntar-Osnas" authorityYear="2018" box="[151,433,1934,1960]" class="Insecta" family="Cerambycidae" genus="Oideterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="189" phylum="Arthropoda" rank="species" species="farallonensis" status="sp. nov.">
