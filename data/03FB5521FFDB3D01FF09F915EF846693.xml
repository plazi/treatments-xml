(
<figureCitation id="1369F8B2FFDB3D03FF01F931EA0E6710" box="[159,287,1791,1816]" captionStart="FIGURES 14 – 15" captionStartId="10.[151,264,1952,1975]" captionTargetBox="[202,1389,210,1922]" captionTargetId="figure@10.[179,1408,193,1932]" captionTargetPageId="10" captionText="FIGURES 14 – 15. Megaselia montseniensis García-Romera sp. nov. male. 14, left face of hypopygium. 15, right face of hypopygium. Scale bars = 0.1 mm." httpUri="https://zenodo.org/record/229675/files/figure.png" pageId="9" pageNumber="253">
Figs 
<number id="3D9AF610FFDB3D03FF49F931EA0E6710" box="[215,287,1791,1816]" pageId="9" pageNumber="253" value="14.5" valueMax="15.0" valueMin="14.0">14–15</number>
</figureCitation>
)
</paragraph>
