<taxonomicName id="05D1676754DFFC1650EE8B492CA06D1B" class="Magnoliopsida" family="Polygonaceae" genus="Coccoloba" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Coccoloba manzinellensis" order="Caryophyllales" pageId="2" pageNumber="57" phylum="Tracheophyta" rank="species" species="manzinellensis">Coccoloba manzinellensis</taxonomicName>
