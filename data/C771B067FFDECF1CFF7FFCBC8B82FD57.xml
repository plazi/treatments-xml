<subSubSection id="07C252FAFFDFCF1CFF2FFF218BAFFF2B" box="[199,946,188,215]" pageId="23" pageNumber="24" type="etymology">
<paragraph id="4F670171FFDFCF1CFF2FFF218BAFFF2B" blockId="23.[151,1437,152,683]" box="[199,946,188,215]" pageId="23" pageNumber="24">
