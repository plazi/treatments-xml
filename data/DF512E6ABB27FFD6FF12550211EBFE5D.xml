<collectingCountry id="2FEFDFECBB24FFD6FCB2537016D8FE85" box="[823,937,272,298]" name="Paraguay" pageId="2" pageNumber="37">Paraguay</collectingCountry>
, Sa. 
<collectingCountry id="2FEFDFECBB24FFD6FC6553701136FE85" box="[992,1095,272,298]" name="Trinidad and Tobago" pageId="2" pageNumber="37">Trinidad</collectingCountry>
, 
<date id="2346B9BCBB24FFD6FBD7537011A7FE85" box="[1106,1238,272,298]" pageId="2" pageNumber="37" value="1915-07-10">
<number id="E1308D5BBB24FFD6FBD75370111EFE85" box="[1106,1135,272,298]" pageId="2" pageNumber="37" value="10.0">10</number>
.vii. 
<number id="E1308D5BBB24FFD6FB1D537011A7FE85" box="[1176,1238,272,298]" pageId="2" pageNumber="37" value="1915.0">1915</number>
</date>
/ 
<number id="E1308D5BBB24FFD6FB6253701063FE85" box="[1255,1298,272,298]" pageId="2" pageNumber="37" value="183.0">183</number>
/ 
<taxonomicName id="90F8E4FFBB24FFD6FAA753701406FEFD" class="Insecta" family="Lauxaniidae" genus="Rhabdolauxania" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="37" phylum="Arthropoda" rank="species" species="immaculata">Rhabdolauxania immaculata</taxonomicName>
