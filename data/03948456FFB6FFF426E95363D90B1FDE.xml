<taxonomicName id="4C3D4EC3FFB6FFF427945160DFFC1F27" box="[490,721,1833,1859]" class="Insecta" family="Gyropidae" genus="Gyropus" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="mesoamericanus">
