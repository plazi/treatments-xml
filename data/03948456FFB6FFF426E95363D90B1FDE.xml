<number id="3DF52767FFB6FFF427B25130DF261FF6" box="[460,523,1913,1939]" pageId="9" pageNumber="10" value="1924.0">1924</number>
</bibRefCitation>
: 
<number id="3DF52767FFB6FFF424665130DF141FF6" box="[536,569,1913,1939]" pageId="9" pageNumber="10" value="22.0">22</number>
</treatmentCitation>
; 
<treatmentCitation id="0A9C1351FFB6FFF424395130DE0B1FF6" author="Werneck" box="[583,806,1913,1939]" page="55" pageId="9" pageNumber="10" year="1948">
<bibRefCitation id="EFAC48B1FFB6FFF424395130DFDA1FF6" author="Werneck" box="[583,759,1913,1939]" pageId="9" pageNumber="24" refString="Werneck, F. L. (1948) Os Malofagos de Mamferos. Parte I: Amblycera e Ischnocera (Philopteridae e parte de Trichodectidae). Revista Brasileira de Biologia Special, Rio de Janeiro. 243 pp." type="book" year="1948">
Werneck 
<number id="3DF52767FFB6FFF424C65130DFDA1FF6" box="[696,759,1913,1939]" pageId="9" pageNumber="10" value="1948.0">1948</number>
</bibRefCitation>
: 
<number id="3DF52767FFB6FFF4257B5130DE0B1FF6" box="[773,806,1913,1939]" pageId="9" pageNumber="10" value="55.0">55</number>
</treatmentCitation>
