<taxonomicName id="4C3D4EC3FFB6FFF422745160D8B41F26" authority="Mendez 1969: 499" authorityName="Mendez" authorityPageNumber="499" authorityYear="1969" box="[1034,1433,1833,1859]" class="Insecta" family="Gyropidae" genus="Gyropus" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="emersoni">
