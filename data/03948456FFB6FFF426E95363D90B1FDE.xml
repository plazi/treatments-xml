<collectionCode id="ED2CAD85FFB6FFF425E6539FDEDC1D95" LSID="urn:lsid:biocol.org:col:34871" box="[920,1009,1494,1520]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="9" pageNumber="10">USNM</collectionCode>
