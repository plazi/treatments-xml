<taxonomicName id="4C3D4EC3FFB6FFF423465130D8B11FF7" box="[1336,1436,1913,1938]" class="Insecta" family="Gyropidae" genus="Gyropus" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
