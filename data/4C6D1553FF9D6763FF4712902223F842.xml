<typeStatus id="1B7F1AE7FF9D6760FF1915C4215AF82A" box="[201,260,1987,2011]" pageId="1" pageNumber="20">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="03C4DFC6FF9D6760FEBB15C423FEF82B" authority="Linzmeier and Konstantinov" authorityName="Linzmeier and Konstantinov" box="[363,928,1986,2011]" class="Insecta" family="Chrysomelidae" genus="Andersonaltica" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="20" phylum="Arthropoda" rank="species" species="pecki" status="new species">
