<emphasis id="7FBE650FFFC93A65FF216B76F33AFE7F" bold="true" box="[151,789,367,393]" pageId="27" pageNumber="28">
<number id="FB02AB3AFFC93A65FF216B76F099FE7F" box="[151,182,367,393]" pageId="27" pageNumber="28" value="20.0">20</number>
. 
<taxonomicName id="8ACAC29EFFC93A65FF756B76F33AFE7F" authority="Yu &amp; Montgomery, 2007" authorityName="Yu &amp; Montgomery" authorityYear="2007" box="[195,789,367,393]" class="Insecta" family="Derodontidae" genus="Laricobius" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="taiwanensis">
<emphasis id="7FBE650FFFC93A65FF756B76F1F6FE7F" bold="true" box="[195,473,367,393]" italics="true" pageId="27" pageNumber="28">Laricobius taiwanensis</emphasis>
<bibRefCitation id="295BC4ECFFC93A65FE576B76F33AFE7F" author="Yu" box="[481,789,367,393]" pageId="27" pageNumber="28" refString="Yu, G. &amp; Montgomery, M. E. (2007) A new species of Laricobius (Coleoptera: Derodontidae) from Taiwan. Formosan Entomology, 27, 341 - 347." type="journal article" year="2007">
Yu &amp; Montgomery, 
<number id="FB02AB3AFFC93A65FD6D6B76F33AFE7F" box="[731,789,367,393]" pageId="27" pageNumber="28" value="2007.0">2007</number>
