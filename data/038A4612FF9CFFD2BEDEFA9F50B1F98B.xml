<taxonomicName id="4C238C87FF9CFFD1BEDEFA9F506FFAF2" authority="(Savigny, 1816)" authorityName="Savigny" authorityYear="1816" baseAuthorityName="Savigny" baseAuthorityYear="1816" box="[151,529,1340,1366]" class="Ascidiacea" family="Styelidae" genus="Styela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="20" pageNumber="311" phylum="Chordata" rank="species" species="canopus">
