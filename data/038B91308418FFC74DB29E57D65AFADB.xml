<quantity id="4CDA8DC38418FFC748129EC6D19EFD1A" box="[1335,1437,726,751]" metricMagnitude="-4" metricUnit="m" metricValue="6.4" pageId="4" pageNumber="194" unit="mm" value="0.64">0.64 mm</quantity>
