<quantity id="335E9B0A165DFFD4FC7AB2BE5ABAD5FD" box="[913,1011,1172,1197]" metricMagnitude="-2" metricUnit="m" metricValue="8.25" metricValueMax="10.5" metricValueMin="6.0" pageId="22" pageNumber="173" unit="cm" value="8.25" valueMax="10.5" valueMin="6.0">6-10.5 cm</quantity>
