<taxonomicName id="33A64D6C165DFFD4FD05B2495A75D52F" authorityName="Wight" authorityYear="1848" box="[750,828,1122,1145]" class="Magnoliopsida" family="Ehretiaceae" genus="Ehretia" kingdom="Plantae" order="Boraginales" pageId="22" pageNumber="173" phylum="Tracheophyta" rank="species" species="cuneata">cuneata</taxonomicName>
