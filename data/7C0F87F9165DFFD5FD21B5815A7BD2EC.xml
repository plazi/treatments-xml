<taxonomicName id="33A64D6C165DFFD4FC83B23D5AEBD57B" box="[872,930,1046,1069]" class="Monothalamea" family="Baculellidae" genus="Arbor" kingdom="Chromista" order="Astrorhizida" pageId="22" pageNumber="173" phylum="Foraminifera" rank="species" species="usque">usque</taxonomicName>
