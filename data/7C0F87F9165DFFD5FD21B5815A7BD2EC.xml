<quantity id="335E9B0A165DFFD4FBC3B23E5DDFD57B" box="[1064,1174,1045,1069]" metricMagnitude="-2" metricUnit="m" metricValue="7.75" metricValueMax="10.5" metricValueMin="5.0" pageId="22" pageNumber="173" unit="cm" value="7.75" valueMax="10.5" valueMin="5.0">5-10.5 cm</quantity>
