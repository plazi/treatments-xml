<quantity id="335E9B0A165DFFD4FB66B2CB5D96D5A1" box="[1165,1247,1248,1272]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="22" pageNumber="173" unit="mm" value="3.5" valueMax="4.0" valueMin="3.0">3-4 mm</quantity>
