<taxonomicName id="33A64D6C165DFFD4FBB9B2495DD9D52C" box="[1106,1168,1122,1146]" class="Magnoliopsida" family="Ehretiaceae" genus="Bourreria" kingdom="Plantae" order="Boraginales" pageId="22" pageNumber="173" phylum="Tracheophyta" rank="species" species="glabra">glabra</taxonomicName>
