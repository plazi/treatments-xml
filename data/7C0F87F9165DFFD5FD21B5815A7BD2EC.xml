<quantity id="335E9B0A165DFFD4FD21B2515A7CD5C4" box="[714,821,1146,1170]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" metricValueMax="4.3" metricValueMin="1.7" pageId="22" pageNumber="173" unit="cm" value="3.0" valueMax="4.3" valueMin="1.7">1.7-4.3 cm</quantity>
longo. Inflorescentiae terminale, 
<taxonomicName id="33A64D6C165DFFD4FB9EB2575D95D5C2" box="[1141,1244,1148,1172]" class="Magnoliopsida" family="Cordiaceae" genus="Cordia" kingdom="Plantae" order="Boraginales" pageId="22" pageNumber="173" phylum="Tracheophyta" rank="species" species="paniculata">paniculata</taxonomicName>
