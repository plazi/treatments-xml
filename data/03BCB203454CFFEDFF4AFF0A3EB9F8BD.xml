<emphasis id="B961DF07454EFFEAFF4AFBF53A2DFBE9" bold="true" box="[151,274,1056,1078]" pageId="3" pageNumber="392">FIGURE 5.</emphasis>
<taxonomicName id="4C157896454EFFEAFEC4FBF53AC8FBE9" box="[281,503,1056,1077]" class="Insecta" family="Miridae" genus="Ectagela" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="392" phylum="Arthropoda" rank="species" species="kermanensis" status="sp. nov.">
<emphasis id="B961DF07454EFFEAFEC4FBF53AC8FBE9" box="[281,503,1056,1077]" italics="true" pageId="3" pageNumber="392">Ectagela kermanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A252627C454EFFEAFE23FBF53970FBE9" box="[510,591,1056,1077]" pageId="3" pageNumber="392" rank="species">
