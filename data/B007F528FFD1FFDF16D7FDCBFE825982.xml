<emphasis id="0ADA982CFFD2FFDF14A4FF1AFC4759AE" box="[740,917,187,213]" italics="true" pageId="10" pageNumber="11">
<taxonomicName id="FFAE3FBDFFD2FFDF14A4FF1AFC5959AE" authorityName="Reuss" authorityYear="1833" box="[740,907,187,213]" class="Amphibia" family="Bufonidae" genus="Bufo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="regularis">Bufo regularis</taxonomicName>
