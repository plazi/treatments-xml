<subSubSection id="C31C65379D71922FFF6116BCFD35F837" pageId="25" pageNumber="70" type="diagnosis">
<paragraph id="8BB936BC9D71922FFF6116BCFD35F837" blockId="25.[151,1437,1723,2039]" pageId="25" pageNumber="70">
<emphasis id="B972EAAE9D71922FFF6116BCFE9FF86F" bold="true" box="[199,314,1940,1966]" pageId="25" pageNumber="70">Diagnosis</emphasis>
