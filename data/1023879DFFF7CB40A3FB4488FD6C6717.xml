</emphasis>
</taxonomicNameLabel>
<number id="2E4224ACFFF6CB43A1E044E9FD0166C0" box="[652,717,1420,1442]" pageId="41" pageNumber="442" value="95.5" valueMax="96.0" valueMin="95.0">
95− 
<specimenCount id="8E8CFD02FFF6CB43A1DF44E9FD0166C0" box="[691,717,1420,1442]" pageId="41" pageNumber="442" type="male">96</specimenCount>
</number>
male, left palp (
<number id="2E4224ACFFF6CB43A01F44E9FC4166C0" box="[883,909,1420,1442]" pageId="41" pageNumber="442" value="95.0">95</number>
ventral, 
<number id="2E4224ACFFF6CB43A08A44E9FBCC66C0" box="[998,1024,1420,1442]" pageId="41" pageNumber="442" value="96.0">96</number>
retrolateral); 
<number id="2E4224ACFFF6CB43A7E744E9FB0766C0" box="[1163,1227,1420,1442]" pageId="41" pageNumber="442" value="97.5" valueMax="98.0" valueMin="97.0">
97− 
<specimenCount id="8E8CFD02FFF6CB43A7DE44E9FB0766C0" box="[1202,1227,1420,1442]" pageId="41" pageNumber="442" type="female">98</specimenCount>
</number>
female (
<number id="2E4224ACFFF6CB43A64944E9FAF366C0" box="[1317,1343,1420,1442]" pageId="41" pageNumber="442" value="97.0">97</number>
epigyne, ventral view, 
<number id="2E4224ACFFF6CB43A27344CFFEF466A2" box="[287,312,1450,1472]" pageId="41" pageNumber="442" value="98.0">98</number>
vulva, dorsal view). C = conductor; CD = copulatory duct; CO = copulatory opening; dRTA = dorsal branch of RTA; E = embolus; FD = fertilization duct; GP = glandular projection; LL = lateral lobes; MS = median septum; SP = spermathecae; vRTA 
<number id="2E4224ACFFF6CB43A2074490FEBE6563" box="[363,370,1525,1537]" pageId="41" pageNumber="442" value="1.0">1</number>
