<number id="2E4224ACFFF6CB43A26244E9FE9D66C3" box="[270,337,1420,1441]" pageId="41" pageNumber="442" value="96.5" valueMax="98.0" valueMin="95.0">95−98</number>
.
</emphasis>
<taxonomicName id="5F8A4D08FFF6CB43A23044E9FDE266C0" box="[348,558,1420,1442]" class="Arachnida" family="Sparassidae" genus="Curicaberis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="urquizai" status="sp. nov.">
<emphasis id="AAFEEA99FFF6CB43A23044E9FDE266C0" box="[348,558,1420,1442]" italics="true" pageId="41" pageNumber="442">Curicaberis urquizai</emphasis>
</taxonomicName>
