<collectingCountry id="F4A3766F9729FF82FCB65D81FC16FD57" box="[861,981,741,765]" name="Cameroon" pageId="62" pageNumber="63">Cameroon</collectingCountry>
; 
<collectingCountry id="F4A3766F9729FF82FC085D81FBF2FD57" box="[995,1073,741,765]" name="Gabon" pageId="62" pageNumber="63">Gabon</collectingCountry>
; 
<collectingCountry id="F4A3766F9729FF82FBAB5D81FAD3FD57" box="[1088,1296,741,765]" name="Equatorial Guinea" pageId="62" pageNumber="63">Equatorial Guinea</collectingCountry>
