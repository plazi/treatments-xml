<taxonomicName id="7E2C0B722D55E8D95005A3BB3A96570F" authorityName="Carpenter" authorityYear="1938" class="Insecta" family="Panorpidae" genus="Panorpa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Panorpa difficilis" order="Mecoptera" pageId="0" pageNumber="309" phylum="Arthropoda" rank="species" species="difficilis">Panorpa difficilis</taxonomicName>
