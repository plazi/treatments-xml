<taxonomicName id="4C314D2ADE487A3DFDAC0C7B393EFABE" authorityName="Mugrabi &amp; Azevedo" authorityYear="2016" box="[588,626,1380,1403]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="104" pageNumber="555" phylum="Arthropoda" rank="species" species="tisa">
<emphasis id="B945EABBDE487A3DFDAC0C7B393EFABE" box="[588,626,1380,1403]" italics="true" pageId="104" pageNumber="555">tisa</emphasis>
