<taxonomicName id="3BE948112A93630052357F00A3C726A9" class="Insecta" family="Carabidae" genus="Galerita" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Galerita" order="Coleoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="genus">Galerita</taxonomicName>
