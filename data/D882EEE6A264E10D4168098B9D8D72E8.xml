<taxonomicName id="F3B5DD53BF73C798D9A81667EB216D24" class="Insecta" family="Carabidae" genus="Galerita" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Galerita" order="Coleoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="genus">Galerita</taxonomicName>
