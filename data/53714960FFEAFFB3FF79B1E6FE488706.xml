<taxonomicName id="1CD883F5FFEBFFB3FDCAB61CFD408725" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[548,755,260,284]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="badiana">
