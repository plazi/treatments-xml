<quantity id="7F27F0F8FFA1FFAD59EDFCA4FAB60C44" box="[1233,1333,818,844]" metricMagnitude="3" metricUnit="m" metricValue="4.9799999999999995" pageId="32" pageNumber="33" unit="m" value="4980.0">4,980 m</quantity>
, and not reaching the abyss in 
<collectingCountry id="C0C81D8DFFA1FFAD5CA8FCCCFDB70C7C" box="[404,564,858,884]" name="Argentina" pageId="32" pageNumber="33">the Argentine</collectingCountry>
Basin, 
<quantity id="7F27F0F8FFA1FFAD5FB1FCCCFCBD0C7C" box="[653,830,858,884]" metricMagnitude="3" metricUnit="m" metricValue="3.173" metricValueMax="3.906" metricValueMin="2.44" pageId="32" pageNumber="33" unit="m" value="3173.0" valueMax="3906.0" valueMin="2440.0">
<geoCoordinate id="DDEB3BDAFFA1FFAD5FB1FCCCFD4C0C7C" box="[653,719,858,884]" degrees="2,440" direction="north" orientation="latitude" pageId="32" pageNumber="33" precision="55" value="2.44">2,440</geoCoordinate>
