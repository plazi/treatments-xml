<taxonomicName id="4C114D28FFE9947D69D28989FC5CFE69" authority="Risbec, 1953" authorityName="Risbec" authorityYear="1953" box="[683,983,422,448]" class="Insecta" family="Platygastridae" genus="Mandraka" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="pauliani">
