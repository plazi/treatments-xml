<taxonomicName id="4C114D28FFE9947D6A5D89E3FE55FE4F" box="[292,478,461,486]" class="Insecta" family="Platygastridae" genus="Leptacis" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="risbeci">
