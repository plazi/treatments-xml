<taxonomicName id="4C7357FAFF83FFE492EBD0A0FE12FF7B" authority="Schaffer, 1763" authorityName="Schaffer" authorityYear="1763" box="[151,467,151,178]" class="Insecta" family="Ascalaphidae" genus="Libelloides" kingdom="Animalia" order="Neuroptera" pageId="17" pageNumber="304" phylum="Arthropoda" rank="genus">
