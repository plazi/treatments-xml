<taxonomicName id="4C4BC7CF9D4BFFBBF8E75A62FD61FC3C" box="[1003,1140,874,899]" class="Insecta" family="Formicidae" genus="Nylanderia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="543" phylum="Arthropoda" rank="species" species="terricola">
<emphasis id="B93F605E9D4BFFBBF8E75A62FD61FC3C" box="[1003,1140,874,899]" italics="true" pageId="11" pageNumber="543">N. terricola</emphasis>
</taxonomicName>
