<date id="164510702F63AA5DFBD5F8E54EDBF8F6" box="[1078,1228,1824,1848]" pageId="2" pageNumber="351" value="1938-01-23">
<number id="D43324972F63AA5DFBD5F8E54E45F8F6" box="[1078,1106,1824,1848]" pageId="2" pageNumber="351" value="23.0">23</number>
Jan. 
<number id="D43324972F63AA5DFB71F8E54EDBF8F6" box="[1170,1228,1824,1848]" pageId="2" pageNumber="351" value="1938.0">1938</number>
</date>
, M.A. Carriker Jr (
<collectionCode id="04EAAE752F63AA5DFF7DF8814AEAF892" box="[158,253,1860,1884]" httpUri="http://grbio.org/cool/6yx0-3nm7" name="Museu de Zoologia da Universidade de Sao Paulo" pageId="2" pageNumber="351">MZUSP</collectionCode>
# 
<number id="D43324972F63AA5DFEF7F8814B56F893" box="[276,321,1860,1885]" pageId="2" pageNumber="351" value="677.0">677</number>
