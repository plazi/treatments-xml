<collectingCountry id="CB967601D55AFFE2219C03313A31207C" box="[510,616,1873,1898]" name="Australia" pageId="30" pageNumber="287">Australia</collectingCountry>
, but 
<bibRefCitation id="D7104B60D55AFFE222FF03313BA7207C" author="Jaschhof" box="[669,1022,1873,1898]" pageId="30" pageNumber="287" refString="Jaschhof, M. &amp; Jaschhof, C. (2010) The genus Manota Williston (Diptera: Mycetophilidae) in New Zealand. Zootaxa, 2387, 28 - 38." type="journal article" year="2010">
Jaschhof &amp; Jaschhof (
<number id="054924B6D55AFFE223F303323B96207C" box="[913,975,1874,1898]" pageId="30" pageNumber="287" value="2010.0">2010</number>
: 
<number id="054924B6D55AFFE223BA03323BAC207C" box="[984,1013,1874,1898]" pageId="30" pageNumber="287" value="37.0">37</number>
)
</bibRefCitation>
