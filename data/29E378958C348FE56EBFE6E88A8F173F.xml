<paragraph id="69004D7969E68325DC7CAD5C5F646096" pageId="34" pageNumber="65">
<taxonomicName id="1198044E856DC01D2518D47A7AD6EB8A" authority="Dejean, 1833" authorityName="Dejean" authorityYear="1833" class="Insecta" family="Staphylinidae" genus="Astenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Astenus" order="Coleoptera" pageId="34" pageNumber="65" phylum="Arthropoda" rank="genus">Astenus Dejean, 1833</taxonomicName>
