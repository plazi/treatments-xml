<subSubSection id="88DA6516144D506A3799E90A3924FB23" pageId="4" pageNumber="293" type="discussion">
<paragraph id="C07F369D144D506A3799E90A3924FB23" blockId="4.[136,1452,158,1192]" pageId="4" pageNumber="293">
<emphasis id="F2B4EA8F144D506A3799E90A3968FBB7" bold="true" box="[189,321,1058,1084]" pageId="4" pageNumber="293">
<emphasis id="F2B4EA8F144D506A3799E90A3911FBB7" bold="true" box="[189,312,1058,1084]" italics="true" pageId="4" pageNumber="293">Comments</emphasis>
:
</emphasis>
— 
<taxonomicName id="07C04D1E144D506A367AE90A3A7CFBB7" authorityName="Bakhit &amp; Abdel-Aziz" authorityYear="2021" box="[350,597,1058,1084]" class="Sordariomycetes" family="Nectriaceae" genus="Chaetopsina" kingdom="Fungi" order="Hypocreales" pageId="4" pageNumber="293" phylum="Ascomycota" rank="species" species="aquatica">
