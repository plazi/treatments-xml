<subSubSection id="96CD7686AC19D3419EC1FF53FC5AFF52" pageId="6" pageNumber="206" type="diagnosis">
<paragraph id="DE68250DAC19D3419EC1FF53FC5AFF52" blockId="6.[151,1437,152,431]" pageId="6" pageNumber="206">
<emphasis id="ECA3F91FAC19D3419EC1FF53FEB7FF37" bold="true" box="[151,274,152,179]" pageId="6" pageNumber="206">Diagnosis.</emphasis>
