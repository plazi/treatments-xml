<quantity id="AB5E9B62FFCCFF9EFB23FDF8FB40CB8D" box="[1153,1197,621,645]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="3" pageNumber="504" unit="m" value="3.0">3 m</quantity>
