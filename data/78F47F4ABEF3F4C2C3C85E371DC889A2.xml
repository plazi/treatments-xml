<subSubSection id="29F0FA2120BDCEBA5A8EC80500C9AD71" lastPageId="24" lastPageNumber="73" pageId="23" pageNumber="72" type="material examined">
<paragraph id="3133230C746F57976FE95B658DA33B79" pageId="23" pageNumber="72">Material examined.</paragraph>
