<collectingCountry id="76DF763A5F26FFF4FCD9FF45FCB730A6" box="[774,845,186,212]" name="Brazil" pageId="6" pageNumber="541">Brazil</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FC85FF45FC3130A6" box="[858,971,186,212]" name="Colombia" pageId="6" pageNumber="541">Colombia</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FC07FF45FBAC30A6" box="[984,1110,186,212]" name="Costa Rica" pageId="6" pageNumber="541">Costa Rica</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FBBCFF45FB5B30A6" box="[1123,1185,186,212]" name="Cuba" pageId="6" pageNumber="541">Cuba</collectingCountry>
, French Guiana, 
<collectingCountry id="76DF763A5F26FFF4FABBFF45FF1F308A" name="Guatemala" pageId="6" pageNumber="541">Guatemala</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FF2FFF21FEB0308A" box="[240,330,222,248]" name="Guyana" pageId="6" pageNumber="541">Guyana</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FE89FF21FE49308A" box="[342,435,222,248]" name="Jamaica" pageId="6" pageNumber="541">Jamaica</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FE60FF21FDED308A" box="[447,535,222,248]" name="Mexico" pageId="6" pageNumber="541">Mexico</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FDFCFF21FD6B308A" box="[547,657,222,248]" name="Suriname" pageId="6" pageNumber="541">Suriname</collectingCountry>
, 
<collectingCountry id="76DF763A5F26FFF4FD43FF21FC73308A" box="[668,905,222,248]" name="Trinidad and Tobago" pageId="6" pageNumber="541">Trinidad and Tobago</collectingCountry>
