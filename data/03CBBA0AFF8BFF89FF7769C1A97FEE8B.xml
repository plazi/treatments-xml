<taxonomicName id="4C62709FFF85FF8CFE726D44AA08E950" baseAuthorityName="Gertsch et Soleglad" baseAuthorityYear="1972" box="[453,528,1809,1830]" class="Arachnida" family="Vaejovidae" genus="Pseudouroctonus" kingdom="Animalia" order="Scorpiones" pageId="57" pageNumber="54" phylum="Arthropoda" rank="species" species="reddelli">
