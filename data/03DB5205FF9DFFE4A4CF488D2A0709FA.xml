<emphasis id="B9063F01FF9DFFE4A49F4AC32E980FA9" bold="true" box="[199,432,729,754]" pageId="8" pageNumber="32">Material examined.</emphasis>
<typeStatus id="54C95DB1FF9DFFE4A5EF4AC32D370FAA" box="[439,543,729,753]" pageId="8" pageNumber="32">Holotype</typeStatus>
<number id="3DBAF134FF9DFFE4A67F4AC32D140FAA" box="[551,572,729,753]" pageId="8" pageNumber="32" value="3.0">3</number>
, 
<collectingCountry id="F365A383FF9DFFE4A6124AC02D870FA9" box="[586,687,730,754]" name="Brazil" pageId="8" pageNumber="32">BRAZIL</collectingCountry>
, Amazonas, Pitinga, R. dos Paturis, 0 
<date id="FFCCC5D3FF9DFFE4A0014AC32BDE0FAA" box="[1113,1270,729,753]" pageId="8" pageNumber="32" value="1998-06-02" valueMax="1998-06-04" valueMin="1998-06-02">
