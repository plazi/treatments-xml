<paragraph id="647B124FF5393F584B99EC16F8CBC52D" blockId="53.[442,1841,371,1093]" pageId="53" pageNumber="64">
<taxonomicName id="A3C469CCF5393F584B99EC16FD0DC2C1" authorityName="Usinger" authorityYear="1946" box="[505,745,894,937]" class="Insecta" family="Miridae" genus="Lygus" kingdom="Animalia" order="Hemiptera" pageId="53" pageNumber="64" phylum="Arthropoda" rank="species" species="guamensis">
<emphasis id="56B0CE5DF5393F584B99EC16FD0DC2C1" box="[505,745,894,937]" italics="true" pageId="53" pageNumber="64">L. guamensis</emphasis>
</taxonomicName>
