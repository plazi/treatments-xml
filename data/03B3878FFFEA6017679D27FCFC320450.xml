<number id="3DD224BEFFEC60176654249BFEB9068E" box="[350,363,152,175]" pageId="4" pageNumber="193" value="8.0">8</number>
with anterior margin straight, lateral margins convergent distally, posteromedian excavation Vshaped; sternite 
<number id="3DD224BEFFEC6017664924B9FE8C06F0" box="[323,350,186,209]" pageId="4" pageNumber="193" value="10.0">10</number>
with 
<number id="3DD224BEFFEC6017669324B9FE7406F0" box="[409,422,186,209]" pageId="4" pageNumber="193" value="4.0">4</number>
