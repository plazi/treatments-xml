<number id="3DD224BEFFEC6017630224B9FBF606F0" box="[1032,1060,186,209]" pageId="4" pageNumber="193" value="22.0">22</number>
</figureCitation>
) with wide, short necks, measuring 
<number id="3DD224BEFFEC6017679D24DEFEC806D5" box="[151,282,221,244]" pageId="4" pageNumber="193" value="11.600000000000001" valueMax="11.8" valueMin="11.4">11.40–11.80</number>
(
<number id="3DD224BEFFEC6017662224DEFEB506D5" box="[296,359,221,244]" pageId="4" pageNumber="193" value="11.6">11.60</number>
, n = 
<number id="3DD224BEFFEC6017669124DEFE7B06D5" box="[411,425,221,244]" pageId="4" pageNumber="193" value="2.0">2</number>
) by 
<number id="3DD224BEFFEC601766D324DEFDD506D5" box="[473,519,221,244]" pageId="4" pageNumber="193" value="6.3">6.30</number>
–7.00 (
<number id="3DD224BEFFEC6017655824DEFD5606D5" box="[594,644,221,244]" pageId="4" pageNumber="193" value="6.6">6.60</number>
