<number id="3DD224BEFFEA6011623B238BFA4901BE" box="[1329,1435,1928,1951]" pageId="2" pageNumber="191" value="0.54" valueMax="0.56" valueMin="0.52">0.52–0.56</number>
(
<number id="3DD224BEFFEA6011679423A9FF0301E0" box="[158,209,1962,1985]" pageId="2" pageNumber="191" value="0.54">0.54</number>
, n= 
<number id="3DD224BEFFEA601167F423A9FEDE01E0" box="[254,268,1962,1985]" pageId="2" pageNumber="191" value="2.0">2</number>
) mm, costal ratio 
<number id="3DD224BEFFEA601166C423A9FDEB01E0" box="[462,569,1962,1985]" pageId="2" pageNumber="191" value="0.78" valueMax="0.79" valueMin="0.77">0.77–0.79</number>
(
<number id="3DD224BEFFEA6011654D23A9FDAB01E0" box="[583,633,1962,1985]" pageId="2" pageNumber="191" value="0.78">0.78</number>
