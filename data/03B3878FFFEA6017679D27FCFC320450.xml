<number id="3DD224BEFFEA601165A723A9FD6F01E0" box="[685,701,1962,1985]" pageId="2" pageNumber="191" value="2.0">2</number>
); second radial cell 
<number id="3DD224BEFFEA6011649823A9FC2E01E0" box="[914,1020,1962,1985]" pageId="2" pageNumber="191" value="2.27" valueMax="2.44" valueMin="2.1">2.10–2.44</number>
(
<number id="3DD224BEFFEA6011630023A9FBEE01E0" box="[1034,1084,1962,1985]" pageId="2" pageNumber="191" value="2.27">2.27</number>
