<number id="3DD224BEFFEC6017642424DEFC6006D5" box="[814,946,221,244]" pageId="4" pageNumber="193" value="10.5" valueMax="10.7" valueMin="10.3">10.30–10.70</number>
(
<number id="3DD224BEFFEC601764CB24DEFBD306D5" box="[961,1025,221,244]" pageId="4" pageNumber="193" value="10.5">10.50</number>
, n = 
<number id="3DD224BEFFEC6017633F24DEFB9106D5" box="[1077,1091,221,244]" pageId="4" pageNumber="193" value="2.0">2</number>
) by 
<number id="3DD224BEFFEC6017637924DEFB0F06D5" box="[1139,1245,221,244]" pageId="4" pageNumber="193" value="5.7" valueMax="5.9" valueMin="5.5">5.50–5.90</number>
(
<number id="3DD224BEFFEC601763E624DEFACC06D5" box="[1260,1310,221,244]" pageId="4" pageNumber="193" value="5.7">5.70</number>
, n = 
<number id="3DD224BEFFEC6017625924DEFAB306D5" box="[1363,1377,221,244]" pageId="4" pageNumber="193" value="2.0">2</number>
