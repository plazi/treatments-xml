<taxonomicName id="E1C81386FFD4FFB3FCE4486CFC11FC88" box="[841,924,830,855]" class="Magnoliopsida" family="Asparagaceae" genus="Ruscus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="genus">
