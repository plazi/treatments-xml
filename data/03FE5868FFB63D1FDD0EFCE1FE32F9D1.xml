<materialsCitation id="3B3FE323FFB63D18DF94FB6EFCFCFAA0" ID-GBIF-Occurrence="3700114361" collectionCode="MB" collectorName="Ramann" country="Germany" county="Heegermuhle" location="Heegermuhle" municipality="Ordovician" pageId="67" pageNumber="68" specimenCode="MB.C.11672.2" specimenCount="1" stateProvince="Brandenburg" typeStatus="lectotype">
