<quantity id="4E00D9CDFFA7FFA29EA5F805426688DC" box="[1006,1096,1950,1975]" metricMagnitude="3" metricUnit="m" metricValue="2.254" pageId="20" pageNumber="539" unit="m" value="2254.0">2254 m</quantity>
