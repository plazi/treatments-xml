, 
<taxonomicName id="1ACE429A4155DD3F120980811EE1F51F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181845" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole boliviana Wilson" lsidName-HNS="Pheidole boliviana Wilson" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="boliviana">boliviana</taxonomicName>
, 
<taxonomicName id="CA0658DC199D794C26B777240240869B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181861" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole crinita Wilson" lsidName-HNS="Pheidole crinita Wilson" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="crinita">crinita</taxonomicName>
, 
<taxonomicName id="A6C0197C306A860146B15496CF94AC96" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181899" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole melastomae Wilson" lsidName-HNS="Pheidole melastomae Wilson" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="melastomae">melastomae</taxonomicName>
, 
<taxonomicName id="9D60E7357FD58E56B1F50D0A02450F9B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181905" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mixteca Wilson" lsidName-HNS="Pheidole mixteca Wilson" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="mixteca">mixteca</taxonomicName>
