<taxonomicName id="CC6E1B2F2CD2BF8309C1128E2DD8EBE0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181940" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole scitula Wilson" lsidName-HNS="Pheidole scitula Wilson" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="scitula">scitula</taxonomicName>
, 
<taxonomicName id="CCDFBED93BD38D593E266AFB29F480CF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34052" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole terresi Wheeler &amp; Mann" lsidName-HNS="Pheidole terresi Wheeler &amp; Mann" order="Hymenoptera" pageNumber="369" phylum="Arthropoda" rank="species" species="terresi">terresi</taxonomicName>
