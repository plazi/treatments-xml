<taxonomicName id="4CCF4D4EFFCE0D29FE923E8EFDD0FE17" box="[389,524,440,464]" class="Insecta" family="Pteromalidae" genus="Apsilocera" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="462" phylum="Arthropoda" rank="species" species="maculata">
