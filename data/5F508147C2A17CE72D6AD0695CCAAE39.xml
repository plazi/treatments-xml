<taxonomicName id="8F122479C2B25988450C5245A70B0D90" family="Scarabaeidae" genus="C." kingdom="Animalia" lsidName="C. contracta" order="Coleoptera" pageId="54" pageNumber="155" rank="species" species="contracta">
