<taxonomicName id="7EBCB1ECDA8227AC90227F5C8F475E37" family="Scarabaeidae" genus="C." kingdom="Animalia" lsidName="C. contraria" order="Coleoptera" pageId="54" pageNumber="155" rank="species" species="contraria">
