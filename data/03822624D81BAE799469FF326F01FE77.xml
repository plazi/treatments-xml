), 
<location id="8EF4C1E9D81BAE7E9251FEF868B2FE0E" LSID="urn:lsid:plazi:treatment:03822624D81BAE799469FF326F01FE77:8EF4C1E9D81BAE7E9251FEF868B2FE0E" box="[1034,1159,383,407]" municipality="Vallone Catrica" name="Pleistocene" pageId="3" pageNumber="1047" stateProvince="Calabria">Pleistocene</location>
