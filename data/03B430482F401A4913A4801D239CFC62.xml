<collectingCountry id="F30AC1CE2F431A48122E86A321CFFF3F" box="[285,411,151,176]" name="Costa Rica" pageId="2" pageNumber="63">
<collectingRegion id="49D94FBC2F431A48122E86A32109FF3F" box="[285,349,152,176]" country="New Zealand" name="West Coast" pageId="2" pageNumber="63">Costa</collectingRegion>
Rica
</collectingCountry>
, 
<quantity id="4CE52CBB2F431A48129B86A32188FF3F" box="[424,476,151,176]" metricMagnitude="4" metricUnit="m" metricValue="1.2874752" pageId="2" pageNumber="63" unit="mi" value="8.0">8 mi</quantity>
S.E. Punta Chayote [Punta Coyote], 
<collectingRegion id="49D94FBC2F431A4810B586A32450FF20" box="[902,1028,152,175]" country="Costa Rica" name="Puntarenas" pageId="2" pageNumber="63">Puntarenas</collectingRegion>
, I. 
<quantity id="4CE52CBB2F431A48172D86A324F1FF20" box="[1054,1189,152,176]" metricMagnitude="5" metricUnit="m" metricValue="3.37108" pageId="2" pageNumber="63" unit="m" value="337108.0">337, 108 m</quantity>
, 
<collectionCode id="ED0C199B2F431A48178086A32543FF3F" box="[1203,1303,152,176]" pageId="2" pageNumber="63">MZUCR</collectionCode>
1665-01 (1 ovigerous female, 
<quantity id="4CE52CBB2F431A4812558687219FFF5B" box="[358,459,188,213]" metricMagnitude="-2" metricUnit="m" metricValue="3.95" pageId="2" pageNumber="63" unit="mm" value="39.5">39.5 mm</quantity>
