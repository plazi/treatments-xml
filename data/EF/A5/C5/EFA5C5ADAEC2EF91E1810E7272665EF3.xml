<paragraph pageId="null" pageNumber="554">
<taxonomicName authority="L." class="Magnoliopsida" family="Fabaceae" genus="Coronilla" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="554" phylum="Tracheophyta" rank="species" species="minima">
