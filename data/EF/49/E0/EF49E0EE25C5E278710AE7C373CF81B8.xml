<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1823" class="Hexapoda" family="Carabidae" genus="Olisthopus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="730" pageNumber="731" phylum="Arthropoda" rank="species" species="parmatus">
