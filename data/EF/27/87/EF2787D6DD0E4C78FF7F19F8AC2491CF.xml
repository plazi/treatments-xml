: Central and Southern Europe (except 
<collectingCountry box="[800,858,525,551]" name="Italy" pageId="13" pageNumber="36">Italy</collectingCountry>
); 
<collectingCountry box="[879,969,525,551]" name="Tunisia" pageId="13" pageNumber="36">Tunisia</collectingCountry>
, 
<collectingCountry box="[980,1048,525,551]" name="Israel" pageId="13" pageNumber="36">Israel</collectingCountry>
, 
<collectingCountry box="[1060,1141,525,551]" name="Jordan" pageId="13" pageNumber="36">Jordan</collectingCountry>
, 
<collectingCountry box="[1153,1259,525,551]" name="Lebanon" pageId="13" pageNumber="36">Lebanon</collectingCountry>
, 
<collectingCountry box="[1270,1335,525,551]" name="Syria" pageId="13" pageNumber="36">Syria</collectingCountry>
, 
<collectingCountry box="[1347,1432,525,551]" name="Turkey" pageId="13" pageNumber="36">Turkey</collectingCountry>
, 
<collectingCountry box="[151,201,562,588]" name="Iraq" pageId="13" pageNumber="36">Iraq</collectingCountry>
, 
<collectingCountry box="[212,293,562,588]" name="Russia" pageId="13" pageNumber="36">Russia</collectingCountry>
