<collectingCountry box="[1153,1249,799,825]" name="Bulgaria" pageId="22" pageNumber="458">Bulgaria</collectingCountry>
(
<quantity box="[1263,1362,798,825]" metricMagnitude="2" metricUnit="m" metricValue="1.755" metricValueMax="3.5" metricValueMin="0.01" pageId="22" pageNumber="458" unit="m" value="175.5" valueMax="350.0" valueMin="1.0">1–350 m</quantity>
