<taxonomicName authorityName="R.Y.Roshevitz" authorityYear="1934" class="Liliopsida" family="Poaceae" genus="Eremopoa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eremopoa" order="Poales" pageId="8" pageNumber="77" phylum="Tracheophyta" rank="genus">Eremopoa</taxonomicName>
