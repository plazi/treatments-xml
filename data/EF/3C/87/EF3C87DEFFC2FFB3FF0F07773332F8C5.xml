<emphasis box="[825,838,1579,1600]" italics="true" pageId="2" pageNumber="191">P</emphasis>
. 
<emphasis box="[850,956,1578,1600]" italics="true" pageId="2" pageNumber="191">brevicollis</emphasis>
</taxonomicName>
, 
<quantity box="[969,1050,1578,1600]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="2" pageNumber="191" unit="mm" value="2.2">2.2 mm</quantity>
; 
<number box="[1059,1073,1578,1600]" pageId="2" pageNumber="191" value="3.0">3</number>
, 
<taxonomicName box="[1085,1236,1579,1600]" class="Insecta" family="Staphylinidae" genus="Phymatura" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="191" phylum="Arthropoda" rank="species" species="gonggaensis">
