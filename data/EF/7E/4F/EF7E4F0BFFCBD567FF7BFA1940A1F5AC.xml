<subScript attach="right" box="[1387,1395,725,739]" fontSize="6" pageId="1" pageNumber="126">2</subScript>
– 
<superScript attach="none" box="[1410,1442,716,741]" fontSize="6" pageId="1" pageNumber="126">111</superScript>
/ 
<subScript attach="none" box="[1449,1457,725,739]" fontSize="6" pageId="1" pageNumber="126">2</subScript>
. Anal fin rays 
<superScript attach="none" box="[972,1007,748,773]" fontSize="6" pageId="1" pageNumber="126">161</superScript>
/ 
<subScript attach="right" box="[1014,1022,757,771]" fontSize="6" pageId="1" pageNumber="126">2</subScript>
