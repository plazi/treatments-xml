(
<taxonomicName family="Moraceae" lsidName="" pageId="0" pageNumber="4961" rank="family">Moraceae</taxonomicName>
), 
<taxonomicName class="Magnoliopsida" family="Lauraceae" genus="Litsea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Litsea" order="Laurales" pageId="0" pageNumber="4961" phylum="Tracheophyta" rank="genus">Litsea</taxonomicName>
