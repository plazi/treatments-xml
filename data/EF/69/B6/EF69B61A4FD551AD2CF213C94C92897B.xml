<taxonomicName class="Magnoliopsida" family="Myristicaceae" genus="Knema" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Knema" order="Magnoliales" pageId="0" pageNumber="4961" phylum="Tracheophyta" rank="genus">Knema</taxonomicName>
