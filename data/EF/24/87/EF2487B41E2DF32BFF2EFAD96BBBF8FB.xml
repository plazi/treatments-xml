<emphasis bold="true" box="[886,965,1918,1940]" pageId="2" pageNumber="121">sp. nov.</emphasis>
</taxonomicNameLabel>
(1–3) and 
<taxonomicName authority="Haddad, Lyle, Bosselaers &amp; Ramirez, 2009" authorityName="Haddad, Lyle, Bosselaers &amp; Ramirez" authorityYear="2009" class="Arachnida" family="Gallieniellidae" genus="Austrachelas" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="121" phylum="Arthropoda" rank="species" species="bergi">
