<paragraph pageId="1" pageNumber="564">
<table box="[151,1399,770,1612]" gridcols="4" gridrows="12" pageId="1" pageNumber="564">
<tr box="[151,1399,770,834]" gridrow="0" pageId="1" pageNumber="564" rowspan-0="1">
