and 
<taxonomicName authority="Chakrabarti &amp; Medda, 1993" authorityName="Chakrabarti &amp; Medda" authorityYear="1993" box="[936,1346,719,743]" class="Insecta" family="Aphididae" genus="Dysaphis" kingdom="Animalia" order="Aphidomorpha" pageId="1" pageNumber="564" phylum="Arthropoda" rank="species" species="indica">
