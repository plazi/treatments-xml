<number box="[581,594,656,680]" pageId="35" pageNumber="266" value="3.0">3</number>
is most similar to 
<taxonomicName authority="Stokes, 1887" authorityName="Stokes" authorityYear="1887" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="35" pageNumber="266" phylum="Gastrotricha" rank="species" species="acanthophorus" subGenus="Hystricochaetonotus">
<emphasis box="[796,1351,655,680]" italics="true" pageId="35" pageNumber="266">Chaetonotus (Hystricochaetonotus) acanthophorus</emphasis>
Stokes, 
<number box="[151,208,692,716]" pageId="35" pageNumber="266" value="1887.0">1887</number>
</taxonomicName>
, 
<emphasis box="[229,746,692,717]" italics="true" pageId="35" pageNumber="266">
Chaetontus (
<taxonomicName box="[377,603,692,717]" class="Not" family="Chaetonotidae" genus="Hystricochaetonotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chaetonotida" pageId="35" pageNumber="266" phylum="Gastrotricha" rank="genus">Hystricochaetonotus</taxonomicName>
) balsamoae
</emphasis>
Kisielewski, 
<number box="[914,968,692,716]" pageId="35" pageNumber="266" value="1997.0">1997</number>
(b), 
<taxonomicName authority="Greuter, 1917" authorityName="Greuter" authorityYear="1917" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="35" pageNumber="266" phylum="Gastrotricha" rank="species" species="quintospinosus" subGenus="Hystricochaetonotus">
<emphasis italics="true" pageId="35" pageNumber="266">Chaetonotus (Hystricochaetonotus) quintospinosus</emphasis>
Greuter, 
<number box="[414,471,728,752]" pageId="35" pageNumber="266" value="1917.0">1917</number>
</taxonomicName>
, and 
<taxonomicName authority="Stokes, 1887" authorityName="Stokes" authorityYear="1887" box="[527,1181,727,752]" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="35" pageNumber="266" phylum="Gastrotricha" rank="species" species="spinulosus" subGenus="Hystricochaetonotus">
