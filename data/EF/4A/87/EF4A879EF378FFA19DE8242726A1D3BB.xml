<emphasis bold="true" box="[151,342,1219,1242]" pageId="3" pageNumber="99">FIGURES 10–12.</emphasis>
<taxonomicName box="[351,591,1220,1242]" class="Insecta" family="Tetrigidae" genus="Cleostratoides" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="99" phylum="Arthropoda" rank="species" species="exoticus" status="sp. nov.">
<emphasis box="[351,591,1220,1242]" italics="true" pageId="3" pageNumber="99">Cleostratoides exoticus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[601,684,1220,1241]" pageId="3" pageNumber="99" rank="species">
