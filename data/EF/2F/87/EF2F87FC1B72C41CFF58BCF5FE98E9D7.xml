<taxonomicName authority="Jeffreys, 1877" authorityName="Jeffreys" authorityYear="1877" box="[755,1072,1871,1896]" class="Gastropoda" family="Fissurellidae" genus="Puncturella" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepetellida" pageId="20" pageNumber="457" phylum="Mollusca" rank="species" species="profundi">
