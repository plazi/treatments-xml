<taxonomicName authorityName="J.F.Brandt" authorityYear="1848" box="[302,447,688,709]" class="Malacostraca" family="Lithodidae" genus="Paralithodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="9" phylum="Arthropoda" rank="genus">
