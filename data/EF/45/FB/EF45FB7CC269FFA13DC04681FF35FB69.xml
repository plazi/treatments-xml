<taxonomicName authority="Regan" authorityName="Regan" box="[293,589,845,867]" class="Actinopterygii" family="Himantolophidae" genus="Himantolophus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lophiiformes" pageId="12" pageNumber="279" phylum="Chordata" rank="species" species="danae">
