<paragraph blockId="5.[132,777,1191,1427]" pageId="5" pageNumber="472">
<materialsCitation ID-GBIF-Occurrence="3003594302" collectionCode="LOCALITY" county="Kinaros" elevation="296" location="Leros. The" municipality="Amorgos" pageId="5" pageNumber="472" specimenCount="296" stateProvince="Voreio Aigaio" typeStatus="holotype">
