<quantity box="[457,513,1224,1244]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="23" pageNumber="924" unit="m" value="100.0">100 m</quantity>
, 
<date box="[522,616,1224,1244]" pageId="23" pageNumber="924" value="2006-05-24">24.V.2006</date>
