<number box="[823,900,272,298]" pageId="5" pageNumber="6" value="52.5" valueMax="55.0" valueMin="50.0">50–55</number>
; VI, 
<number box="[961,1038,272,298]" pageId="5" pageNumber="6" value="47.0" valueMax="50.0" valueMin="44.0">44–50</number>
; VII, 
<number box="[1110,1186,272,298]" pageId="5" pageNumber="6" value="21.5" valueMax="23.0" valueMin="20.0">20–23</number>
