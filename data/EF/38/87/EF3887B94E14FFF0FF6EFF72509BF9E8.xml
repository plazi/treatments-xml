<quantity box="[571,616,866,892]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="3" pageNumber="4" unit="in" value="4.0">4 in</quantity>
each aster, rarely 3, 11– 
<number box="[898,926,866,892]" pageId="3" pageNumber="4" value="15.0">15</number>
marginal between asters, 
<number box="[1229,1287,866,892]" pageId="3" pageNumber="4" value="11.5" valueMax="14.0" valueMin="9.0">9–14</number>
anterior; III, 
<number box="[151,228,906,932]" pageId="3" pageNumber="4" value="18.5" valueMax="21.0" valueMin="16.0">16–21</number>
; IV, 
<number box="[282,359,906,932]" pageId="3" pageNumber="4" value="25.0" valueMax="28.0" valueMin="22.0">22–28</number>
