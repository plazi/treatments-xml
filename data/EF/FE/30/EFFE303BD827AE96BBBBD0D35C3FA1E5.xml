<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:132408" class="Insecta" family="Formicidae" genus="Atta" kingdom="Animalia" lsidName="Atta (Acromyrmex) emilii var. ajax Forel" lsidName-HNS="Atta (Acromyrmex) emilii var. ajax Forel" order="Hymenoptera" pageNumber="58" phylum="Arthropoda" rank="variety" species="emilii" species.completionEvidence="missing" species.completionRound="1" subGenus="Acromyrmex" variety="ajax">Atta (Acromyrmex) Emilii Forel, var. Ajax</taxonomicName>
<taxonomicNameLabel rank="variety">n. var.</taxonomicNameLabel>
