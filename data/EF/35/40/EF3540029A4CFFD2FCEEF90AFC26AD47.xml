<taxonomicName authorityName=", D. Dupont" box="[202,302,1636,1654]" class="Insecta" family="Pentatomidae" genus="Pentatoma" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="414" phylum="Arthropoda" rank="genus">
<emphasis box="[202,302,1636,1654]" italics="true" pageId="1" pageNumber="414">Pentatoma</emphasis>
</taxonomicName>
) Guérin, 
<emphasis box="[397,489,1635,1654]" italics="true" pageId="1" pageNumber="414">
Chlor. 
<taxonomicName authorityName="Spinola" authorityYear="1837" box="[459,489,1636,1654]" class="Insecta" family="Pentatomidae" genus="Chlorocoris" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="414" phylum="Arthropoda" rank="species" species="tau">tau</taxonomicName>
