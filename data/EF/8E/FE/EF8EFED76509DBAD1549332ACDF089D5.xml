<taxonomicName authority="Loeve" authorityName="Loeve" class="Magnoliopsida" family="Polygonaceae" genus="Acetosa" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="723" phylum="Tracheophyta" rank="subSpecies" species="pratensis" subSpecies="pratensis">
