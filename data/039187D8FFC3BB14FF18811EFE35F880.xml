(
<figureCitation id="13032A4BFFC3BB14FF108132FE96FA6D" box="[159,267,1400,1426]" captionStart="FIGURE 1" captionStartId="3.[151,250,1474,1498]" captionTargetBox="[158,1432,565,1446]" captionTargetId="figure-172@3.[151,1437,561,1451]" captionTargetPageId="3" captionText="FIGURE 1. Distribution of Zarcosia species across South Asia. New taxa are indicated in boldface. For Z. lemairei n. sp., the broader geographical range is highlighted with a red area with a dotted line." figureDoi="http://doi.org/10.5281/zenodo.4403787" httpUri="https://zenodo.org/record/4403787/files/figure.png" pageId="5" pageNumber="156">Figures 1</figureCitation>
; 
