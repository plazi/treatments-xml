<taxonomicName id="4C3CDAC2FFE4CA44FF3D13D9FCEFFC39" authority=": Takaoka &amp; Davies, 1995: 148" authorityName=": Takaoka &amp; Davies" authorityPageNumber="148" authorityYear="1995" box="[151,891,978,1003]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="fuscopilosum" subGenus="Simulium">
