<collectionCode id="D9993082FFEF752FFB3BF998FB5DE34C" box="[1217,1237,1602,1626]" country="USA" lsid="urn:lsid:biocol.org:col:15406" name="Harvard University - Arnold Arboretum" pageId="31" pageNumber="118" type="Herbarium">A</collectionCode>
