<collectionCode id="D9993082FFEF752FFB45F9FBFB5DE32F" box="[1215,1237,1569,1593]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="31" pageNumber="118" type="Herbarium">G</collectionCode>
