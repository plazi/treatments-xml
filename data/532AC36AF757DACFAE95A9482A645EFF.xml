<paragraph id="6630B620320307A45B02C6B3CCAF1A0F" pageId="4" pageNumber="81">
<taxonomicName id="0B0ECC9B7061390E306D83C1842E0170" authority="Sharp, 1908" authorityName="Sharp" authorityYear="1908" class="Insecta" family="Lucanidae" genus="Apterocyclus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apterocyclus munroi" order="Coleoptera" pageId="4" pageNumber="81" phylum="Arthropoda" rank="species" species="munroi">Apterocyclus munroi Sharp, 1908</taxonomicName>
