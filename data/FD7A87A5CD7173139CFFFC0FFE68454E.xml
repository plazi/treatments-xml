<taxonomicName id="B2D34D30CD7173139857F8F6FAB0452A" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[1087,1350,1806,1833]" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="568" phylum="Arthropoda" rank="genus">
<emphasis id="47A7EAA1CD7173139857F8F6FB67452A" box="[1087,1169,1807,1833]" italics="true" pageId="1" pageNumber="568">Mutilla</emphasis>
<bibRefCitation id="11424B42CD71731398F1F8F6FAB0452A" author="Linnaeus, C." box="[1177,1350,1806,1833]" pageId="1" pageNumber="568" pagination="5" refId="ref8011" refString="Linnaeus, C. (1758) Systema Naturae per Regna Tria Naturae, secundum Classes, Ordines, Genera, Species, cum Characteribus, Differentiis, Synonymis, Locis. Tomus I. Editio Decima Reformata. Laurentii Salvii, Holmiae [Stockholm], [4] + 823 + [5] pp. https: // doi. org / 10.5962 / bhl. title. 542" type="journal article" year="1758">Linnaeus, 1758</bibRefCitation>
</taxonomicName>
(
<bibRefCitation id="11424B42CD717313993CF8F6FF24454E" author="Andre, E." pageId="1" pageNumber="568" pagination="261 - 277" refId="ref6937" refString="Andre, E. (1896) Notes pour servir a la connaissance de mutilles palearctiques et description de quelques especes nouvelles. Deuxieme partie. Memoires de la Societe Zoologique de France, 9, 261 - 277." type="journal article" year="1896">André 1896</bibRefCitation>
, 
<bibRefCitation id="11424B42CD7173139CB5F8CAFE64454E" author="Peringuey, L." box="[221,402,1843,1869]" pageId="1" pageNumber="568" pagination="33 - 94" refId="ref8368" refString="Peringuey, L. (1898) Description of some new or little known South African Mutillidae in the collection of the South African Museum. Annals of the South African Museum, 1 (1), 33 - 94. https: // doi. org / 10.5962 / bhl. part. 13904" type="journal article" year="1898">Péringuey 1898</bibRefCitation>
