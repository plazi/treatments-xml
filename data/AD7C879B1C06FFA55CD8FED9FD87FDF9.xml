<taxonomicName id="E2D54D0E1C06FFA55DCCFE48FDC3FDD5" box="[387,617,510,534]" class="Insecta" family="Eumenidae" genus="Polistes" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="49" phylum="Arthropoda" rank="species" species="mandarinus">
