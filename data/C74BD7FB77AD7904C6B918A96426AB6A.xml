<taxonomicName id="935EA429CD49ECA8B63213F774A2F7B3" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus solitarius" order="Hymenoptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="solitarius">Euplectrus solitarius</taxonomicName>
