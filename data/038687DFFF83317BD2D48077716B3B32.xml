<taxonomicName id="4C2F4D4AFF813175D02F826A76A03AF1" authorityName="Khaustov &amp; Whitaker" authorityYear="2019" box="[784,906,1901,1925]" class="Arachnida" family="Neopygmephoridae" genus="Crossdania" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="314" phylum="Arthropoda" rank="genus">
