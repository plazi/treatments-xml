<taxonomicName id="4C2F4D4AFF813175D07E824C76913A17" authorityName="Khaustov &amp; Whitaker" authorityYear="2019" box="[833,955,1867,1891]" class="Arachnida" family="Neopygmephoridae" genus="Crossdania" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="314" phylum="Arthropoda" rank="genus">
