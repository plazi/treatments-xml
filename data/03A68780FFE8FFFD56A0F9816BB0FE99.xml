<number id="3DC724B1FFEFFFFC5517FF636A22FF28" box="[800,828,188,212]" pageId="28" pageNumber="429" value="20.0">20</number>
'N
</geoCoordinate>
<geoCoordinate id="EE3B5051FFEFFFFC556DFF636AA6FF29" box="[858,952,188,213]" direction="west" orientation="longitude" pageId="28" pageNumber="429" precision="925" value="-106.0">
<number id="3DC724B1FFEFFFFC556DFF636A9DFF29" box="[858,899,188,213]" pageId="28" pageNumber="429" value="106.0">106</number>
°0'W
