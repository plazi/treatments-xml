, 
<date id="FFB11056FFEFFFFC55F5FF636D29FF29" box="[962,1079,188,213]" pageId="28" pageNumber="429" value="1955-06-17">
<number id="3DC724B1FFEFFFFC55F5FF636AC3FF29" box="[962,989,188,213]" pageId="28" pageNumber="429" value="17.0">17</number>
.vi. 
<number id="3DC724B1FFEFFFFC55CBFF636D29FF29" box="[1020,1079,188,213]" pageId="28" pageNumber="429" value="1955.0">1955</number>
