<taxonomicName id="4C125EBF0D6483769E59FBB7FC7AFBB5" authority="Stauffer, Bowers, Kellogg &amp; McKaye, 1997" authorityName="Stauffer, Bowers, Kellogg &amp; McKaye" authorityYear="1997" box="[801,945,1075,1099]" class="Teleostei" family="Cichlidae" genus="Metriaclima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="callainos">
