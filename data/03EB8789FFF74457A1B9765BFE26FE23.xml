<taxonomicName id="4C424D1CFFF74468A116722AFE7CF93A" authorityName="d' Orbigny" authorityYear="1851" box="[282,494,1645,1668]" class="Gymnolaemata" family="Onychocellidae" genus="Latereschara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="31" pageNumber="1687" phylum="Bryozoa" rank="species" species="achates">
