<taxonomicName id="C7D3D1FCE4F10E0C0DBF44758B4D905C" LSID="http://zoobank.org/1DDED0AC-8C88-4D62-BB55-261003E92F35" authority="Brown" authorityName="Brown" authorityYear="2014" class="Insecta" family="Tortricidae" genus="Circanota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Circanota simplex" order="Lepidoptera" pageId="4" pageNumber="129" phylum="Arthropoda" rank="species" species="simplex">Circanota simplex Brown</taxonomicName>
