<collectingRegion id="49E6F81F5211A175FBEAFCBF70B76977" box="[1070,1180,808,832]" country="Iran" name="Hamadan" pageId="2" pageNumber="876">Hamadan</collectingRegion>
, 
<collectingCountry id="F335766D5211A175FB63FCBE70FE6977" box="[1191,1237,809,832]" name="Iran" pageId="2" pageNumber="876">Iran</collectingCountry>
