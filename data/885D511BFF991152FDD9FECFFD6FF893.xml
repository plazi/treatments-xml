<quantity id="C70C4DE8FF991153FDD3FB8EFD63FB88" box="[598,691,1098,1124]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="8" pageNumber="9" unit="mm" value="1.8">1.8 mm</quantity>
