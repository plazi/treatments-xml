<taxonomicName id="F8AE6DCC50CA3095894DD9F163DE6C85" class="Insecta" family="Carabidae" genus="Anillinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anillinus" order="Coleoptera" pageId="3" pageNumber="74" phylum="Arthropoda" rank="genus">Anillinus</taxonomicName>
