<paragraph id="8BAA36CAAF3FFFB3FF68FB16FE23486C" blockId="2.[151,773,1243,1304]" box="[151,463,1278,1304]" pageId="2" pageNumber="3">
<figureCitation id="132E2A4FAF3FFFB3FF68FB16FECE486C" box="[151,290,1278,1304]" captionStart="FIGURES 1 – 9. 1 – 3" captionStartId="3.[151,269,1762,1786]" captionTargetBox="[185,1381,185,1729]" captionTargetId="figure@3.[151,1436,176,1756]" captionTargetPageId="3" captionText="FIGURES 1 – 9. 1 – 3. Labia. 1. Canthonella jarmilea; 2. Canthonella quesquaya; 3. Canthonella sikesi. 4 – 6. Heads 4. Canthonella jarmilea; 5. Canthonella quesquaya; 6: Canthonella sikesi. 7 – 9. Left elytra 7, Canthonella jarmilea; 8. Canthonella quesquaya; 9. Canthonella sikesi." httpUri="https://zenodo.org/record/180819/files/figure.png" pageId="2" pageNumber="3">
Figs. 
<number id="3DDD24EDAF3FFFB3FF27FB16FEE9486C" box="[216,261,1278,1304]" pageId="2" pageNumber="3" value="2.5">2, 5</number>
, 
<number id="3DDD24EDAF3FFFB3FEEDFB16FECE486C" box="[274,290,1278,1304]" pageId="2" pageNumber="3" value="8.11">8</number>
</figureCitation>
, 11, 
<number id="3DDD24EDAF3FFFB3FEA5FB16FE4A486C" box="[346,422,1278,1304]" pageId="2" pageNumber="3" value="14.17">14, 17</number>
, 
<number id="3DDD24EDAF3FFFB3FE4DFB16FE23486C" box="[434,463,1278,1304]" pageId="2" pageNumber="3" value="20.0">20</number>
</paragraph>
