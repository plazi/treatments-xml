<taxonomicName id="4C104D5DFFFAFF99FED7738FFD8C54D7" authority="Mercet" authorityName="Mercet" box="[346,629,1767,1789]" class="Insecta" family="Encyrtidae" genus="Aphycus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="secundus">
