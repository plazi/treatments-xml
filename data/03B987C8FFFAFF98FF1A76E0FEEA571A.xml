<caption id="DF6F6656FFFAFF99FF1A738EFAF354D7" box="[151,1290,1766,1789]" httpUri="https://zenodo.org/record/285936/files/figure.png" pageId="8" pageNumber="9" targetBox="[169,1414,1133,1728]" targetPageId="8">
<paragraph id="8BAF36DEFFFAFF99FF1A738EFAF354D7" blockId="8.[151,1290,1766,1789]" box="[151,1290,1766,1789]" pageId="8" pageNumber="9">
