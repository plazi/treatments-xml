<number id="3DD824F9FFFBFF98FB467600FB1E51AA" box="[1227,1255,872,896]" pageId="9" pageNumber="10" value="19.0">19</number>
(
<number id="3DD824F9FFFBFF98FB7B760FFAEF51AA" box="[1270,1302,871,896]" pageId="9" pageNumber="10" value="25.0">25</number>
