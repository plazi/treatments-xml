Democratic 
<collectingCountry id="4DCD765FFFC65105FE61F8A1FE4BF873" box="[497,576,1916,1940]" name="Democratic Republic of the Congo" pageId="8" pageNumber="9">
<collectingRegion id="F71EF82DFFC65105FE61F8A1FE4BF873" box="[497,576,1916,1940]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="8" pageNumber="9">Congo</collectingRegion>
</collectingCountry>
: 
<collectingRegion id="F71EF82DFFC65105FDDAF8A1FF2BF872" box="[586,800,1916,1941]" country="Democratic Republic of the Congo" name="Orientale" pageId="8" pageNumber="9">Orientale Province</collectingRegion>
