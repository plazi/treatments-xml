<taxonomicName id="4C52B7B5DD6BFF8BFB10F8E84F27035F" box="[1207,1315,1828,1853]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="sepasi">
