<taxonomicName id="4C3B4D34955BFFBCCBB95AD3FA792097" authority="(KZN)" baseAuthorityName="KZN" box="[1241,1408,222,248]" class="Mollusca (awaiting allocation)" family="Mollusca (awaiting allocation)" genus="Durban" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca (awaiting allocation)" pageId="50" pageNumber="51" phylum="Mollusca" rank="genus">Durban (KZN)</taxonomicName>
