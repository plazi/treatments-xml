<geoCoordinate id="38EC504DD659FFE7FDD0FEA7FD23FEC5" box="[550,725,295,321]" degrees="86" direction="east" minutes="37" orientation="longitude" pageId="28" pageNumber="429" precision="1" seconds="0.12" value="86.6167">86° 37’ 0.12” E</geoCoordinate>
