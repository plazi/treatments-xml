<taxonomicName id="4C663190FFBF255EE9E2FC6A1E205FCE" box="[658,831,908,933]" class="Insecta" family="Mantidae" genus="Mantis" kingdom="Animalia" order="Mantodea" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="costalis">
