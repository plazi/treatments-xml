<taxonomicName id="1E2B8DECA9F6406745957BC264C1DE9C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25976" class="Insecta" family="Formicidae" genus="Azteca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Azteca brevis Forel" lsidName-HNS="Azteca brevis Forel" order="Hymenoptera" pageNumber="121" phylum="Arthropoda" rank="species" species="brevis">Azteca brevis</taxonomicName>
