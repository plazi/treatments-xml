<taxonomicName id="27CA4D35FF54FF325A96FD02FCD463D0" box="[648,871,655,681]" class="Insecta" family="Muscidae" genus="Pyrellia" kingdom="Animalia" order="Diptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="ochrifacies">
