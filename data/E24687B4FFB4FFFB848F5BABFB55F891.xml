<taxonomicName id="ADEF4D21FFB4FFFB84BB5846FEECF892" box="[163,278,1800,1825]" class="Diplopoda" family="Parajulidae" genus="Parajulus" kingdom="Animalia" order="Julida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
