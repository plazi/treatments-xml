<taxonomicName id="ADEF4D21FFB4FFFB85325846FE7FF892" box="[298,389,1800,1825]" class="Diplopoda" family="Parajulidae" genus="Paraiulus" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="viganus">
