<taxonomicName id="589D4D3BFF89075BFC6275D1FD06FCB7" authority=", Ensenadan" authorityName="Ensenadan" class="Mammalia" family="Canidae" genus="Chrysocyon" kingdom="Animalia" order="Carnivora" pageId="82" pageNumber="83" phylum="Chordata" rank="species" species="brachyurus">
