<collectingRegion id="5D59F85AFF89075BFC00745BFBBEFCDE" box="[983,1105,849,871]" country="United States of America" name="California" pageId="82" pageNumber="83">California</collectingRegion>
, 
<collectingRegion id="5D59F85AFF89075BFBB1745BFB28FCDE" box="[1126,1223,849,871]" country="United States of America" name="Arizona" pageId="82" pageNumber="83">Arizona</collectingRegion>
