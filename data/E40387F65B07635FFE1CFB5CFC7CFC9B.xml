<heading id="375D818C5B07635DFE1CFB5CFCE3FB99" bold="true" box="[414,863,1236,1261]" centered="true" fontSize="8" level="2" pageId="4" pageNumber="79" reason="3">
<taxonomicName id="ABAA4D635B07635DFE1CFB5CFCE3FB99" authority="(Schilling, 1829)" baseAuthorityName="Schilling" baseAuthorityYear="1829" box="[414,863,1236,1261]" class="Insecta" family="Rhopalidae" genus="Brachycarenus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="79" phylum="Arthropoda" rank="species" species="tigrinus">
<emphasis id="5EDEEAF25B07635DFE1CFB5CFCE3FB99" bold="true" box="[414,863,1236,1261]" pageId="4" pageNumber="79">
<emphasis id="5EDEEAF25B07635DFE1CFB5CFD1EFB98" bold="true" box="[414,674,1236,1261]" italics="true" pageId="4" pageNumber="79">Brachycarenus tigrinus</emphasis>
