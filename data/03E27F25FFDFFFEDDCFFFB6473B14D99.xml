<taxonomicName id="4C4BB5B0FFDFFFEEDEE2FA9173474A72" baseAuthorityName="Allman" baseAuthorityYear="1872" box="[650,853,1338,1361]" class="Hydrozoa" family="Hydractiniidae" genus="Stylactaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="33" pageNumber="34" phylum="Cnidaria" rank="species" species="inermis">
