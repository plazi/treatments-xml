<paragraph id="23B428E594458FA5240778FBBCD3BA85" pageNumber="70">
<taxonomicName id="D1539F1C6081C5263521DF61742EC7FB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139584" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica acervorum Fabricius" order="Hymenoptera" pageNumber="70" phylum="Arthropoda" rank="species" species="acervorum">Formica acervorum Fabricius</taxonomicName>
