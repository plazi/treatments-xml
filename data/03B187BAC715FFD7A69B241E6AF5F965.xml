<taxonomicName id="4C184D2FC718FFD7A79E247F6D07FAED" box="[402,622,1374,1397]" class="Gastropoda" family="Helicidae" genus="Hemicycla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="8" pageNumber="9" phylum="Mollusca" rank="species" species="laurijona" status="sp. nov.">
