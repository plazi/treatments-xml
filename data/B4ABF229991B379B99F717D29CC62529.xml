<taxonomicName id="53858935331F353CE801DD46914818F3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27942" class="Insecta" family="Formicidae" genus="Cerapachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cerapachys mayri Forel" order="Hymenoptera" pageNumber="244" phylum="Arthropoda" rank="subGenus" species="mayri" subGenus="MAYRI">CERAPACHYS MAYRI</taxonomicName>
