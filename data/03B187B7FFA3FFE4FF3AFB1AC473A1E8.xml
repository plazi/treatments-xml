<taxonomicName id="4C184D22FFA0FFE4FDCBFEDEC487A104" authorityName="Levesque-Beaudin &amp; Mlynarek" authorityYear="2020" box="[614,721,333,359]" class="Insecta" family="Milichiidae" genus="Paramyia" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="lustrum">
