<taxonomicName id="4C762EC6FFE1FFFD78CFF889A5D6872A" authority="Steffan" authorityName="Steffan" box="[980,1185,1804,1829]" class="Insecta" family="Sciaridae" genus="Bradysia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="69" phylum="Arthropoda" rank="species" species="bishopi">
