</emphasis>
: 
<taxonomicName id="62AC4D37546CFFFAFECCB920FD43FEC2" authority="Gerstaecker" authorityName="Gerstaecker" authorityYear="1885" box="[371,748,258,282]" class="Insecta" family="Myrmeleontidae" genus="Myrmeleon" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="40" phylum="Arthropoda" rank="species" species="illustris">
