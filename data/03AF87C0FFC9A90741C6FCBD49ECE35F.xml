<number id="3DCE24F1FFC9A90844DEFFCE49EBE56C" box="[1423,1435,1788,1809]" pageId="7" pageNumber="8" value="4.0">4</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30F22E73FFC9A90841C6FE2B49ECE5BC" pageId="7" pageNumber="8">
<paragraph id="8BB936D6FFC9A90841C6FE2B49ECE514" blockId="7.[151,1437,1494,2014]" pageId="7" pageNumber="8">
<keyLead id="30F795E3FFC9A90841C6FE2B49ECE514" pageId="7" pageNumber="8">
<number id="3DCE24F1FFC9A90841C6FE2B4CD6E553" box="[151,166,1817,1838]" pageId="7" pageNumber="8" value="4.0">4</number>
