<subSubSection id="B0CE3A8BFF93FFB715BAF72002E9FA81" pageId="29" pageNumber="30" type="nomenclature">
<paragraph id="F86B6900FF93FFB715BAF7200119FAE4" blockId="29.[151,542,1311,1372]" box="[151,542,1311,1337]" pageId="29" pageNumber="30">
<heading id="A323DE6CFF93FFB715BAF7200119FAE4" bold="true" box="[151,542,1311,1337]" fontSize="11" level="1" pageId="29" pageNumber="30" reason="1">
<emphasis id="CAA0B512FF93FFB715BAF7200119FAE4" bold="true" box="[151,542,1311,1337]" pageId="29" pageNumber="30">
<taxonomicName id="3FD41283FF93FFB715BAF72002D2FAE4" authority="Troubridge" authorityName="Troubridge" authorityYear="2008" box="[151,469,1311,1337]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="mut" status="sp. n.">
<emphasis id="CAA0B512FF93FFB715BAF7200238FAE4" bold="true" box="[151,319,1311,1337]" italics="true" pageId="29" pageNumber="30">Sympistis mut</emphasis>
Troubridge
</taxonomicName>
