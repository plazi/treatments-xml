<taxonomicName id="E925661F44C920955E3BF10265FB96D7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:151635" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole variabilis redunca Crawley" order="Hymenoptera" pageId="3" pageNumber="235" phylum="Arthropoda" rank="variety" species="variabilis" variety="redunca">Pheidole variabilis, Mayr., var. redunca</taxonomicName>
