<taxonomicName id="4C0A4D0BFFF30C61972A1F9AFE7EF82B" box="[337,464,1968,1990]" class="Insecta" family="Argidae" genus="Antargidium" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="genus">
