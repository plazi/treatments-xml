<emphasis id="B97EEA9AFFF30C61931B1F9AFAD0F82A" bold="true" box="[1376,1406,1968,1991]" pageId="3" pageNumber="50">(8)</emphasis>
<taxonomicName id="4C0A4D0BFFF30C6193FD1F9BFF5AF808" class="Insecta" family="Argidae" genus="Antargidium" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="allucente">
<emphasis id="B97EEA9AFFF30C6193FD1F9BFF5AF808" italics="true" pageId="3" pageNumber="50">A. allucente</emphasis>
</taxonomicName>
