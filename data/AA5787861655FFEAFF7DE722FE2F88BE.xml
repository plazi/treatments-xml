<quantity id="E5069B751655FFEAFD44E7A8FCE2880E" box="[686,774,1790,1814]" metricMagnitude="-2" metricUnit="m" metricValue="1.4" pageId="5" pageNumber="246" unit="mm" value="14.0">14 mm</quantity>
