<paragraph id="CC44694EB8676035D494385DE50E9E10" pageId="15" pageNumber="142">
<taxonomicName id="C33673919D662DA489C51B0F273C4FFB" class="Insecta" family="Scarabaeidae" genus="Parapucaya" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parapucaya amazonica" order="Coleoptera" pageId="15" pageNumber="142" phylum="Arthropoda" rank="species" species="amazonica">
