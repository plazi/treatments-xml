<taxonomicName id="4C392BD28F60A77AFE6BF1FD3E9BFE6B" authority="nigrocincta" authorityName="nigrocincta Lacordaire" authorityYear="1848" box="[451,733,392,415]" class="Insecta" family="Chrysomelidae" genus="Clytra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="8" phylum="Arthropoda" rank="subSpecies" species="nigrocincta" subSpecies="nigrocincta">
<emphasis id="B94D8C438F60A77AFE6BF1FD3E9BFE6B" box="[451,733,392,415]" italics="true" pageId="15" pageNumber="8">C. nigrocincta nigrocincta</emphasis>
