<number id="304824F8791A0C073EA4FF0CFC5BFF30" box="[965,1020,190,216]" pageId="42" pageNumber="43" value="1.66">1.66</number>
, 
<number id="304824F8791A0C073968FF0CFBE7FF30" box="[1033,1088,190,216]" pageId="42" pageNumber="43" value="1.66">1.66</number>
