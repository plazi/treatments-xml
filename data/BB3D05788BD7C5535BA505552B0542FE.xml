<taxonomicName id="3965C1BA5E8FB34538CEACA73F1AA36D" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina perstriata" order="Stylommatophora" pageId="70" pageNumber="71" phylum="Mollusca" rank="species" species="perstriata">
<pageBreakToken id="BFCAA368B11CE1FFA5549846E4583854" pageId="70" pageNumber="71" start="start">Montenegrina</pageBreakToken>
