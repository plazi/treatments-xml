<taxonomicName id="4C0518E38F91FF2CFE13FE6042383E81" authorityName="HUBER" authorityYear="2000" box="[445,558,472,495]" class="Arachnida" family="Pholcidae" genus="Mesabolivar" kingdom="Animalia" order="Araneae" pageId="197" pageNumber="198" phylum="Arthropoda" rank="species" species="huambisa">
