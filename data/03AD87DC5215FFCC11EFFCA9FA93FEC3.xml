<taxonomicName id="4C044D495217FFCC1036FEBFFE6CFEC3" box="[334,493,332,356]" class="Clitellata" family="Glossoscolecidae" genus="Andiorrhinus" kingdom="Animalia" order="Haplotaxida" pageId="7" pageNumber="110" phylum="Annelida" rank="species" species="yukuna" subGenus="Turedrilus">
