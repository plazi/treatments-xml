<taxonomicName id="4C044D495217FFCC1288FEDBFB14FEE7" box="[1008,1173,295,320]" class="Clitellata" family="Glossoscolecidae" genus="Andiorrhinus" kingdom="Animalia" order="Haplotaxida" pageId="7" pageNumber="110" phylum="Annelida" rank="species" species="yukuna" subGenus="Turedrilus">
