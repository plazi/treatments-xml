<paragraph id="8BAD36A8FFC7AE23FED1923FFD07FEDB" blockId="17.[264,642,283,345]" box="[264,642,283,310]" pageId="17" pageNumber="18">
<emphasis id="B966EABAFFC7AE23FED1923FFD07FEDB" bold="true" box="[264,642,283,310]" pageId="17" pageNumber="18">
<taxonomicName id="4C124D2BFFC7AE23FED1923FFE66FED8" authority="Holzenthal &amp; Blahnik, 2006" authorityName="Holzenthal &amp; Blahnik" authorityYear="2006" box="[264,483,284,310]" class="Insecta" family="Glossosomatidae" genus="Protoptila" kingdom="Animalia" order="Trichoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="cristula" status="new species">
