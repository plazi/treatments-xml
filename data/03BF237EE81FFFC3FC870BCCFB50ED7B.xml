<date id="FFA8B4A8E813FFC5FF9908CEFBFCEB60" box="[905,1071,1791,1817]" pageId="11" pageNumber="21" value="2004-08-17">
<number id="3DDE804FE813FFC5FF9908CEFC70EB60" box="[905,931,1791,1817]" pageId="11" pageNumber="21" value="17.0">17</number>
