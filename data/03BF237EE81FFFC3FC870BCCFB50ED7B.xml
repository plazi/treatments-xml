<quantity id="4CEE3F8DE813FFC5FF3108CEFCAAEB60" box="[801,889,1791,1817]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="11" pageNumber="21" unit="m" value="1500.0">1500m</quantity>
