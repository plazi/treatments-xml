<number id="1003A240686A0B39FB2AFB8253E557D3" box="[1188,1240,1135,1159]" pageId="11" pageNumber="50" value="7.78">7.78</number>
, ta 
<number id="1003A240686A0B39FB79FB9353C257D8" box="[1271,1279,1150,1164]" pageId="11" pageNumber="50" value="1.0">1</number>
