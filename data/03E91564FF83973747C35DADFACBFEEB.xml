<collectingCountry id="F357E4E2FF86973746335C71FE3DFEE8" box="[359,462,331,357]" name="Australia" pageId="12" pageNumber="515">Australia</collectingCountry>
(south-eastern 
<collectingRegion id="49846A90FF869737452F5C71FCF1FEE8" box="[635,770,331,357]" country="Australia" name="Queensland" pageId="12" pageNumber="515">Queensland</collectingRegion>
, 
<collectingRegion id="49846A90FF86973744595C71FC2BFEE8" box="[781,984,331,357]" country="Australia" name="New South Wales" pageId="12" pageNumber="515">New South Wales</collectingRegion>
, 
<collectingRegion id="49846A90FF86973744B65C71FBCCFEE8" box="[994,1087,331,357]" country="Australia" name="Victoria" pageId="12" pageNumber="515">Victoria</collectingRegion>
, and 
<collectingRegion id="49846A90FF869737432D5C71FADFFEEB" box="[1145,1324,331,358]" country="Australia" name="South Australia" pageId="12" pageNumber="515">South Australia</collectingRegion>
