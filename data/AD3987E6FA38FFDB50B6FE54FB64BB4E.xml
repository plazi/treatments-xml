<taxonomicName id="E2904D73FA38FFDB529AFC2CFC2ABDD2" box="[699,903,974,1000]" class="Bivalvia" family="Unionidae" genus="Unio" kingdom="Animalia" order="Unionoida" pageId="10" pageNumber="11" phylum="Mollusca" rank="species" species="depauperata">
