<taxonomicName id="3F6AFD30677296A9BFC84785E5A3FAD6" authorityName="Simon" authorityYear="1896" class="Arachnida" family="Trachelidae" genus="Trachelas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachelas vulcani" order="Araneae" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="vulcani">
