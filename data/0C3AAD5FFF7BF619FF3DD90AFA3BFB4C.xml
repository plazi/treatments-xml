2014) and 
<taxonomicName id="439367CAFF7BF619FD93DFAFFC22FB4C" authority="Heyer" authorityName="Heyer" authorityYear="2005" box="[569,917,1231,1257]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="20" pageNumber="21" phylum="Chordata" rank="species" species="paraensis">
