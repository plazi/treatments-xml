<number id="3D9F24E3FF95FFD2FC01FE6913B70EFD" box="[945,1005,425,449]" pageId="40" pageNumber="41" value="2010.0">2010</number>
: 
<number id="3D9F24E3FF95FFD2FC4DFE6914410EFD" box="[1021,1051,425,449]" pageId="40" pageNumber="41" value="14.0">14</number>
, figs 
<number id="3D9F24E3FF95FFD2FBD1FE6914FD0EFE" box="[1121,1191,425,450]" pageId="40" pageNumber="41" value="49.5" valueMax="50.0" valueMin="49.0">49–50</number>
