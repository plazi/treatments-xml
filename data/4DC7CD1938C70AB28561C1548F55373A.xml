<taxonomicName id="606B7E96CC06286B01701A7855C12607" class="Agaricomycetes" family="Omphalotaceae" genus="Gymnopus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Gymnopus ceraceicola" order="Agaricales" pageId="6" pageNumber="37" phylum="Basidiomycota" rank="species" species="ceraceicola">Gymnopus ceraceicola</taxonomicName>
