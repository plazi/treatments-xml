<number id="3DA024CBFFA8B34EFE4AD1575CD7FD5E" box="[441,500,618,644]" pageId="10" pageNumber="445" value="1815.0">1815</number>
</taxonomicName>
(type-species: 
<emphasis id="B91CEAFEFFA8B34EFD5FD1575E4BFD5E" bold="true" box="[684,872,618,644]" italics="true" pageId="10" pageNumber="445">
Gryllus 
<taxonomicName id="4C684D6FFFA8B34EFCFED1575E4BFD5E" authority="Linnaeus" authorityName="Linnaeus" authorityYear="1838" box="[781,872,618,644]" class="Insecta" family="Pamphagidae" genus="Batrachotettix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="445" phylum="Arthropoda" rank="species" species="elephas">elephas</taxonomicName>
