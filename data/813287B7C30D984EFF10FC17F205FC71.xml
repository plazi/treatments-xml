<subSubSection id="4181652AC30E984EFF18FF28F3A0FDB6" pageId="6" pageNumber="217" type="materials_examined">
<paragraph id="092436A1C30E984EFF18FF28F310FEA8" blockId="6.[189,1179,175,264]" pageId="6" pageNumber="217">
<materialsCitation id="B9F33CFCC30E984EFF18FF28F314FEA8" collectingDate="2005-02-07" collectingDateMax="2005-02-12" collectingDateMin="2005-02-07" collectionCode="HANEL" country="Saint Helena, Ascension and Tristan da Cunha" latitude="-37.15167" location="Edinburgh village" longLatPrecision="19" longitude="-12.573889" pageId="6" pageNumber="217" specimenCount="2" specimenCount-female="1" specimenCount-male="1">
<collectingCountry id="718C7631C30E984EFF18FF28F0B0FF6B" box="[189,446,175,199]" name="Saint Helena, Ascension and Tristan da Cunha" pageId="6" pageNumber="217">TRISTAN DA CUNHA</collectingCountry>
, west of 
