<taxonomicName id="199C12E6F11CC73129A0BC623DB33F2E" authorityName="Blakemore" authorityYear="2013" class="Clitellata" family="Lumbricidae" genus="Eisenia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Crassiclitellata" pageId="5" pageNumber="6" phylum="Annelida" rank="subSpecies" species="nordenskioldi" subSpecies="onon">
