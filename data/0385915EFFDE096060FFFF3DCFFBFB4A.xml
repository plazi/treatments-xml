<subSubSection id="C33673C3FFDE0960608CFEE6CF2AFED1" pageId="73" pageNumber="66" type="type_taxon">
<paragraph id="8B932048FFDE0960608CFEE6CF2AFED1" blockId="73.[113,1089,257,356]" pageId="73" pageNumber="66">
<typeStatus id="54979EEAFFDE0960608CFEE6CC23FEEA" box="[475,521,257,276]" pageId="73" pageNumber="66">Type</typeStatus>
species: 
<taxonomicName id="4C2C5BCBFFDE09606331FEE6CDC1FEEA" authority="Reichardt, 1924" authorityName="Reichardt" authorityYear="1924" box="[614,1003,257,276]" class="Insecta" family="Histeridae" genus="Ammostyphrus" kingdom="Animalia" order="Coleoptera" pageId="73" pageNumber="66" phylum="Arthropoda" rank="species" species="cerberus">
<emphasis id="B958FC5AFFDE09606331FEE6CD62FEEA" box="[614,840,257,276]" italics="true" pageId="73" pageNumber="66">Ammostyphrus cerberus</emphasis>
<bibRefCitation id="EFBD5DB9FFDE09606205FEE6CDC1FEEA" author="REICHARDT A." box="[850,1003,257,276]" pageId="73" pageNumber="66" pagination="162 - 166" refId="ref120236" refString="REICHARDT A. 1924: Zwei neue Histeriden aus Turkestan. Entomologische Blatter 20: 162 - 166." type="journal article" year="1924">Reichardt, 1924</bibRefCitation>
