, 
<taxonomicName id="4C29A5ACC972FFC2FBCFF9D8D216F905" authorityName="Bocourt" authorityYear="1868" class="Actinopterygii" family="Characidae" genus="Tetragonopterus" kingdom="Animalia" order="Characiformes" pageId="37" pageNumber="1367" phylum="Chordata" rank="species" species="belizianus">A. belizianus</taxonomicName>
