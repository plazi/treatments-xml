<emphasis id="B9023A23FFFFFFA1B26FFD24A14EFD49" bold="true" box="[264,1039,644,670]" pageId="15" pageNumber="16">
Subgenus 
<taxonomicName id="4C769DB2FFFFFFA1B2EFFD24A7F9FD49" authority="Sayce 1902" authorityName="Sayce" authorityYear="1902" box="[392,696,644,670]" class="Branchiopoda" family="Thamnocephalidae" genus="Branchinella" kingdom="Animalia" order="Anostraca" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
<emphasis id="B9023A23FFFFFFA1B2EFFD24A766FD49" bold="true" box="[392,551,644,670]" italics="true" pageId="15" pageNumber="16">Branchinella</emphasis>
Sayce 
<number id="3DBEF416FFFFFFA1B11AFD24A7F9FD49" box="[637,696,644,670]" pageId="15" pageNumber="16" value="1902.0">1902</number>
</taxonomicName>
