<taxonomicName id="F7F68F167D457CA54EA022E69A589C76" class="Insecta" family="Ichneumonidae" genus="Tryphon" kingdom="Animalia" lsidName="Tryphon duplicatus" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="duplicatus">Tryphon duplicatus</taxonomicName>
