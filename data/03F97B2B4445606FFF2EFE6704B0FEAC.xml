, on 
<date id="FFEEECFD44456060FC0FFDD60276FDE7" box="[950,1115,575,599]" pageId="6" pageNumber="257" value="1985-04-12">
<number id="3D98D81A44456060FC0FFDD605FFFDE7" box="[950,978,575,599]" pageId="6" pageNumber="257" value="12.0">12</number>
April 
<number id="3D98D81A44456060FB9AFDD60276FDE7" box="[1059,1115,575,599]" pageId="6" pageNumber="257" value="1985.0">1985</number>
