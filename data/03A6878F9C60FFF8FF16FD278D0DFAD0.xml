<typeStatus id="54B4883B9C60FFF8FCF2FCB38DEDFCC0" box="[883,936,835,856]" pageId="6" pageNumber="107">Type</typeStatus>
locality: 
<collectingCountry id="F31876099C60FFF8FB95FCB28A11FCC0" box="[1044,1108,834,856]" name="Brazil" pageId="6" pageNumber="107">Brazil</collectingCountry>
, State of Amazonas, Barcelos Municipality, Serra do Aracá, base, Igarapé da Anta; 
<typeStatus id="54B4883B9C60FFF8FD60FC928D58FCEF" box="[737,797,866,887]" pageId="6" pageNumber="107">Types</typeStatus>
