<taxonomicName id="3ECB9B5C6E9F39723B3FF55CF0DD8B82" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:135421" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica ruficeps Smith" order="Hymenoptera" pageNumber="54" phylum="Arthropoda" rank="species" species="ruficeps">Formica ruficeps</taxonomicName>
