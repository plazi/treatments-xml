species: 
<taxonomicName id="4C0D4D7CFFBDFFF7E0B82BE77F6C4CA7" authority="Brain" authorityName="Brain" box="[737,1042,1229,1251]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="burnerae">
