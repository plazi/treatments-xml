<taxonomicName id="4C3CBFDCFFC313247F2AFF43FBB2A6B1" box="[873,1064,186,213]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="berberidifolia">
