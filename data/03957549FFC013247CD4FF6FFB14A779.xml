<taxonomicName id="4C3CBFDCFFC31324788CFF43FF46A69D" authority="(Burks 1979)" baseAuthorityName="Burks" baseAuthorityYear="1979" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="dumosa">
