<collectingCountry id="F34A76659D67FFD0FE64FAB23301FAA5" box="[462,570,1372,1396]" name="Colombia" pageId="27" pageNumber="678">Colombia</collectingCountry>
, 
<collectingCountry id="F34A76659D67FFD0FDEEFAB23386FAA5" box="[580,701,1372,1396]" name="Costa Rica" pageId="27" pageNumber="678">Costa Rica</collectingCountry>
, 
<collectingCountry id="F34A76659D67FFD0FD6DFAB331F6FA44" name="Ecuador" pageId="27" pageNumber="678">Ecuador</collectingCountry>
, and 
<collectingCountry id="F34A76659D67FFD0FEACFA903064FA44" box="[262,351,1406,1429]" name="Panama" pageId="27" pageNumber="678">Panama</collectingCountry>
. This species has only been reported in the submontane forest of Cerro Pirre, between 800 and 
<quantity id="4CA59B109D67FFD0FF00FA2E31C7FA09" box="[170,252,1472,1496]" metricMagnitude="3" metricUnit="m" metricValue="1.05" pageId="27" pageNumber="678" unit="m" value="1050.0">1050 m</quantity>
