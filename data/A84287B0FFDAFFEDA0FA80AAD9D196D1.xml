<taxonomicName id="E7EB4D25FFDAFFEDA55182D0D8F89185" authority="Enderlein, 1928" authorityName="Enderlein" authorityYear="1928" box="[1340,1436,972,995]" class="Insecta" family="Sarcophagidae" genus="Discachaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="gigas">
