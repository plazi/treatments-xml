<taxonomicName id="808B4D1EFFECFFFA588277952AEEFB14" authority="(DENIS &amp; SCHIFFERMULLER 1775)" baseAuthorityName="DENIS &amp; SCHIFFERMULLER" baseAuthorityYear="1775" box="[178,763,1199,1221]" class="Polypodiopsida" family="Cyatheaceae" genus="Alsophila" kingdom="Plantae" order="Cyatheales" pageId="11" pageNumber="1248" phylum="Tracheophyta" rank="species" species="aescularia">
