<taxonomicName id="B8880741FFC2006BA00BEABDD357E788" genus="A." lsidName="A. mellifera" pageId="0" pageNumber="8050" rank="subSpecies" species="mellifera" subSpecies="mellifera">A. mellifera mellifera</taxonomicName>
