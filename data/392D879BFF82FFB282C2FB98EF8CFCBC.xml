<taxonomicName id="76844D0EFF82FFB582C2FB98EC17FB02" authority="Mosely, 1939" authorityName="Mosely" authorityYear="1939" box="[151,596,1143,1169]" class="Insecta" family="Hydroptilidae" genus="Acostatrichia" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="56" phylum="Arthropoda" rank="species" species="simulans">
<emphasis id="83F0EA9FFF82FFB582C2FB98EC17FB02" bold="true" box="[151,596,1143,1169]" pageId="2" pageNumber="56">
<emphasis id="83F0EA9FFF82FFB582C2FB98EFEFFB02" bold="true" box="[151,428,1143,1169]" italics="true" pageId="2" pageNumber="56">Acostatrichia simulans</emphasis>
<bibRefCitation id="D5154B7CFF82FFB583E6FB98EC17FB02" author="Mosely" box="[435,596,1143,1169]" pageId="2" pageNumber="60" refString="Mosely, M. E. (1939) The Brazilian Hydroptilidae (Trichoptera). Novitates Zoologicae, 41, 217 - 239." type="journal article" year="1939">
Mosely, 
<number id="074C24AAFF82FFB5804FFB98EC17FB02" box="[538,596,1143,1169]" pageId="2" pageNumber="56" value="1939.0">1939</number>
</bibRefCitation>
