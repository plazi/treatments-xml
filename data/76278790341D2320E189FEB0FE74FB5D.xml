<collectingCountry id="86997616341D2320E26FFD77FD23FD54" box="[622,678,675,696]" name="Brazil" pageId="2" pageNumber="194">Brasil</collectingCountry>
(
<taxonomicName id="398E4D05341D2320E2B7FD77FC19FD54" authority="Nonfried, 1894" authorityName="Nonfried" authorityYear="1894" box="[694,924,675,696]" class="Insecta" family="Buprestidae" genus="Ectinogonia" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="species" species="cariosa">
