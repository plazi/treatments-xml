<emphasis id="A4A4EABA5C7169147206FACCFE3E69A6" bold="true" box="[272,386,1401,1422]" pageId="11" pageNumber="-5">Diagnosis.</emphasis>
Oval; 
<quantity id="51289B4D5C71691472D8FACCFDEE69A6" box="[462,594,1401,1422]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" metricValueMax="4.2" metricValueMin="3.4" pageId="11" pageNumber="-5" unit="mm" value="3.8" valueMax="4.2" valueMin="3.4">3.4–4.2 mm</quantity>
