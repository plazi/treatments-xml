<taxonomicName id="C3A1B7860706FFCFFF20FC07FE9BFC77" authority="Walker, 1850" authorityName="Walker" authorityYear="1850" box="[243,375,933,958]" class="Insecta" family="Cicadidae" genus="Fidicina" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="54" phylum="Arthropoda" rank="species" species="viridifemur">viridifemur</taxonomicName>
