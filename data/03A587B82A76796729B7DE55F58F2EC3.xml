</collectingRegion>
, 
<collectingCountry id="F31B763E2A76796729B7DD0BF6E52E76" box="[151,241,1615,1639]" name="Guyana" pageId="41" pageNumber="42">Guyana</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672822DD0AF7662E76" box="[258,370,1614,1639]" name="Honduras" pageId="41" pageNumber="42">Honduras</collectingCountry>
, 
<collectingRegion id="49C8F84C2A76796728A2DD0AF7CE2E76" box="[386,474,1614,1639]" country="Mexico" name="Mexico" pageId="41" pageNumber="42">
<collectingCountry id="F31B763E2A76796728A2DD0AF7CE2E76" box="[386,474,1614,1639]" name="Mexico" pageId="41" pageNumber="42">Mexico</collectingCountry>
</collectingRegion>
, 
<collectingRegion id="49C8F84C2A76796728CCDD0BF45C2E77" box="[492,584,1615,1638]" country="Panama" name="Panama" pageId="41" pageNumber="42">
