<collectingCountry id="F31B763E2A7679672BDADD6EF55B2E52" box="[762,847,1578,1603]" name="Bolivia" pageId="41" pageNumber="42">Bolivia</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672A7BDD6EF5B62E52" box="[859,930,1578,1603]" name="Brazil" pageId="41" pageNumber="42">Brazil</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672A8DDD6EF20A2E52" box="[941,1054,1578,1603]" name="Colombia" pageId="41" pageNumber="42">Colombia</collectingCountry>
, 
<collectingCountry id="F31B763E2A7679672D09DD6FF2B22E52" box="[1065,1190,1578,1603]" name="Costa Rica" pageId="41" pageNumber="42">
<collectingRegion id="49C8F84C2A7679672D09DD6FF27D2E52" box="[1065,1129,1579,1603]" country="New Zealand" name="West Coast" pageId="41" pageNumber="42">Costa</collectingRegion>
Rica
</collectingCountry>
, 
