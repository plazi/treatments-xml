<taxonomicName id="4C6812CFCF4ED71277F2FD13FCF59444" box="[695,884,754,780]" class="Liliopsida" family="Orchidaceae" genus="Aerangis" kingdom="Plantae" order="Asparagales" pageId="5" pageNumber="50" phylum="Tracheophyta" rank="species" species="punctata">Aerangis punctata</taxonomicName>
