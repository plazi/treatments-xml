<quantity id="4CEF9B25FF43FF679D2EF888FFDD3790" box="[1363,1436,1914,1941]" metricMagnitude="2" metricUnit="m" metricValue="5.96" pageId="210" pageNumber="211" unit="m" value="596.0">596 m</quantity>
