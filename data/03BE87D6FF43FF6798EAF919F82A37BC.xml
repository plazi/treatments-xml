<quantity id="4CEF9B25FF43FF6798B3F86CFB6937BF" box="[206,296,1950,1978]" metricMagnitude="2" metricUnit="m" metricValue="5.95884" pageId="210" pageNumber="211" unit="ft" value="1955.0">1,955 ft</quantity>
