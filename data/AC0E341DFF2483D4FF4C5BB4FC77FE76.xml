<taxonomicName id="E3A7FE88FF2483D4FF795E13FDA7F977" authority="Scop., Fl. Carniol." authorityName="Scop., Fl. Carniol." box="[189,567,1651,1676]" class="Magnoliopsida" family="Brassicaceae" genus="Cakile" kingdom="Plantae" order="Brassicales" pageId="153" pageNumber="153" phylum="Tracheophyta" rank="species" species="maritima">
