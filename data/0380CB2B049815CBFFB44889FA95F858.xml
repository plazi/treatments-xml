<taxonomicName id="4C2901BE049815CBFB0C4944FB17F873" authorityName="Pilsbry" authorityYear="1895" box="[1071,1276,1901,1927]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="145" pageNumber="146" phylum="Mollusca" rank="species" species="purpurata">
