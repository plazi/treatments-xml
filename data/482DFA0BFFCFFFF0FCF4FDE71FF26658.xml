<td id="3D652023FFCF001BFE3FFAB41C276000" box="[497,538,1289,1309]" gridcol="5" gridrow="2" pageId="19" pageNumber="328">27/17</td>
<td id="3D652023FFCF001BFDFDFAB41C616000" box="[563,604,1289,1309]" gridcol="6" gridrow="2" pageId="19" pageNumber="328">35/32</td>
