, 
<location id="673B84B9B956FF99100D5E8234D5FB6F" LSID="urn:lsid:plazi:treatment:EA4D6374B956FF9712D258953682FE59:673B84B9B956FF99100D5E8234D5FB6F" box="[584,700,1189,1215]" country="Seychelles" county="Mt Pot" municipality="Jardin Marron" name="Silhouette" pageId="22" pageNumber="223">Silhouette</location>
, 
<collectingMunicipality id="823F4818B956FF99108F5E823505FB10" box="[714,876,1189,1216]" pageId="22" pageNumber="223">Jardin Marron</collectingMunicipality>
vers 
