<taxonomicName id="4C4BB5B0FFE6FFD7DFD8F8AF74674838" authorityName="Kroyer" authorityYear="1845" box="[944,1141,1796,1819]" class="Actinopterygii" family="Ceratiidae" genus="Ceratias" kingdom="Animalia" order="Lophiiformes" pageId="24" pageNumber="25" phylum="Chordata" rank="species" species="holboelli">
<emphasis id="B93F1221FFE6FFD7DFD8F8AF74674838" box="[944,1141,1796,1819]" italics="true" pageId="24" pageNumber="25">Ceratias holboelli</emphasis>
