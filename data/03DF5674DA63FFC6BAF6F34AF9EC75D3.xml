new 
<emphasis id="B9023B70DA63FFC6B8E3F0FCFCBA74A4" box="[718,838,728,766]" italics="true" pageId="2" pageNumber="6">species</emphasis>
on page 325 
<emphasis id="B9023B70DA63FFC6BE1EF0FCFA9E74A4" box="[1075,1378,728,766]" italics="true" pageId="2" pageNumber="6">under the heading</emphasis>
&quot; 
<taxonomicName id="4C769CE1DA63FFC6BF50F0FCF93074A4" authority="Stegomyia" authorityName="Stegomyia" box="[1405,1740,728,766]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="6" phylum="Arthropoda" rank="genus">
