<taxonomicName id="4C414D390C51FF8189E4FD19FA4F1855" authority="(Fox)" authorityName="Fox" baseAuthorityName="FOx" baseAuthorityYear="1897" box="[1273,1430,767,789]" class="Insecta" family="Pompilidae" genus="Ageniella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="139" phylum="Arthropoda" rank="species" species="varipes">
