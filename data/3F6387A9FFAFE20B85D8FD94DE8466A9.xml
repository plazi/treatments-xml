<taxonomicName id="70CA4D3CFFAFE20B84E7FD5CDF796781" box="[424,631,655,680]" class="Hydrozoa" family="Oceaniidae" genus="Rhizogeton" kingdom="Animalia" order="Anthoathecata" pageId="66" pageNumber="67" phylum="Cnidaria" rank="species" species="nudum">
