<taxonomicName id="70CA4D3CFFAFE20B8698FC97D9A96675" box="[983,1191,836,861]" class="Hydrozoa" family="Tubulariidae" genus="Ectopleura" kingdom="Animalia" order="Anthoathecata" pageId="66" pageNumber="67" phylum="Cnidaria" rank="species" species="crocea">
