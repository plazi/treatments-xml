<taxonomicName id="6183B088FF5EE0C1FD8B63DEFCA9FCD2" box="[598,864,764,789]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="142" pageNumber="143" phylum="Arthropoda" rank="species" species="ancillae">
