<keyStep id="D4182E7F2807FFD2FF50FF2F6DEAFEAD" pageId="7" pageNumber="43">
<paragraph id="6F5336DA2807FFD2FF50FF2F6DE4FF0D" blockId="7.[151,1436,152,623]" pageId="7" pageNumber="43">
<keyLead id="D41D95EF2807FFD2FF50FF2F6DE4FF0D" pageId="7" pageNumber="43">
<number id="D92424FD2807FFD2FF50FF2F68D8FF4D" box="[151,167,152,175]" pageId="7" pageNumber="43" value="6.0">6</number>
