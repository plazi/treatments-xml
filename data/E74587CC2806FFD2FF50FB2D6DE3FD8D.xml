<keyLead id="D41D95EF2807FFD2FF50FEEF6DE3FE4D" pageId="7" pageNumber="43">
<number id="D92424FD2807FFD2FF50FEEF68D8FE8D" box="[151,167,344,367]" pageId="7" pageNumber="43" value="7.0">7</number>
