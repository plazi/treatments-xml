<taxonomicName id="F9ACD589120242FB1AA8D19D621F7B05" class="Arachnida" family="Liphistiidae" genus="Songthela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Songthela yunnanensis" order="Araneae" pageId="14" pageNumber="135" phylum="Arthropoda" rank="species" species="yunnanensis">Songthela yunnanensis</taxonomicName>
