<quantity id="3859ED89B847E57960F6FB896082FB73" box="[912,1006,1041,1068]" metricMagnitude="0" metricUnit="m" metricValue="2.195" pageId="101" pageNumber="102" unit="m" value="2.195">2,195 m</quantity>
