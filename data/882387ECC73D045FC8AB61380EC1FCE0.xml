<taxonomicName id="C78A4D79C73B045FCBC667A709BDFD33" box="[1018,1242,712,737]" class="Malacostraca" family="Mysidae" genus="Amblyops" kingdom="Animalia" order="Mysida" pageId="5" pageNumber="48" phylum="Arthropoda" rank="species" species="longisquamosus">
