<emphasis id="ECE8EAD0BE2B634AFE8E237764F4A14B" box="[259,443,1577,1600]" italics="true" pageId="3" pageNumber="1350">
<taxonomicName id="199C4D41BE2B634AFE8E2377643EA14B" box="[259,369,1577,1600]" class="Insecta" family="Masaridae" genus="Quartinia" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="1350" phylum="Arthropoda" rank="genus">Quartinia</taxonomicName>
