<emphasis id="86B7EA9A1D1AFFD617B0369E3567F805" bold="true" box="[151,281,2013,2036]" pageId="64" pageNumber="65">FIGURE 54</emphasis>
. 
<taxonomicName id="73C34D0B1D1AFFD61602369D3655F805" box="[293,555,2014,2036]" class="Arachnida" family="Listropsoralgidae" genus="Didelphialges" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="metachirus" status="gen. n., sp. n.">
<emphasis id="86B7EA9A1D1AFFD61602369D3655F805" box="[293,555,2014,2036]" italics="true" pageId="64" pageNumber="65">Didelphialges metachirus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="9D8457E11D1AFFD61516369D36C3F802" box="[561,701,2014,2036]" pageId="64" pageNumber="65" rank="species">
<emphasis id="86B7EA9A1D1AFFD61516369D3606F802" bold="true" box="[561,632,2014,2035]" pageId="64" pageNumber="65">gen. n.</emphasis>
, 
<emphasis id="86B7EA9A1D1AFFD615A3369D36C3F802" bold="true" box="[644,701,2014,2035]" pageId="64" pageNumber="65">sp. n.</emphasis>
