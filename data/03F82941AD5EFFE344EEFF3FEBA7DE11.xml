<taxonomicName id="4C51E3D4AD58FFE345CBFF1BEBD1DFD9" box="[434,652,188,213]" class="Arachnida" family="Mideopsidae" genus="Djeboa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="bimaculata">
