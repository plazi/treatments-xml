<taxonomicName id="4C63EFE7FFCAFF99869910CBC693B1A5" authority="Saussure, 1862" authorityName="Saussure" authorityYear="1862" box="[910,1436,1698,1723]" class="Insecta" family="Blaberidae" genus="Phoraspis" kingdom="Animalia" order="Blattodea" pageId="20" pageNumber="321" phylum="Arthropoda" rank="species" species="porcellana" subGenus="Thorax">
