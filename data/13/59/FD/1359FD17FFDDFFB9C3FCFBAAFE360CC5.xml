<taxonomicName authority="Cancello, Oliveira &amp; Rocha" authorityName="Cancello, Oliveira &amp; Rocha" box="[151,689,1040,1067]" class="Insecta" family="Termitidae" genus="Muelleritermes" kingdom="Animalia" order="Isoptera" pageId="1" pageNumber="259" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[151,335,1040,1066]" italics="true" pageId="1" pageNumber="259">Muelleritermes</emphasis>
Cancello, Oliveira &amp; Rocha
</taxonomicName>
, 
<taxonomicNameLabel box="[702,831,1041,1067]" pageId="1" pageNumber="259" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="1.[151,580,1112,1134]" box="[151,580,1112,1134]" pageId="1" pageNumber="259">
