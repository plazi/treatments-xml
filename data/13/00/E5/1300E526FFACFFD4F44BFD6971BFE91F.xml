<paragraph blockId="1.[264,1323,798,1905]" pageId="1" pageNumber="56">
<emphasis bold="true" box="[264,384,798,824]" pageId="1" pageNumber="56">Diagnosis</emphasis>
. Dorsal shield 
<number box="[588,632,799,825]" pageId="1" pageNumber="56" value="387.0">387</number>
