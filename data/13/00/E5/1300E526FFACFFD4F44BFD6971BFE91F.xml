</emphasis>
(
<number box="[659,690,1599,1625]" pageId="1" pageNumber="56" value="13.0">13</number>
), 
<emphasis box="[714,754,1599,1624]" italics="true" pageId="1" pageNumber="56">
Jv 
<number box="[740,754,1599,1624]" pageId="1" pageNumber="56" value="4.0">4</number>
</emphasis>
(
<number box="[773,804,1599,1625]" pageId="1" pageNumber="56" value="28.0">28</number>
