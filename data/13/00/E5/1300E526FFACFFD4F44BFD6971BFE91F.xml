<emphasis box="[685,719,1399,1424]" italics="true" pageId="1" pageNumber="56">
st 
<number box="[705,719,1399,1424]" pageId="1" pageNumber="56" value="3.0">3</number>
</emphasis>
(
<number box="[736,766,1399,1425]" pageId="1" pageNumber="56" value="21.0">21</number>
