, NE and central 
<collectingCountry box="[234,304,947,973]" name="China" pageId="43" pageNumber="44">China</collectingCountry>
, 
<collectingCountry box="[315,386,948,974]" name="South Korea" pageId="43" pageNumber="44">Korea</collectingCountry>
