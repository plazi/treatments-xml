<emphasis box="[967,1023,1535,1561]" italics="true" pageId="55" pageNumber="56">Salix</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1036,1100,1536,1561]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="55" pageNumber="56" phylum="Tracheophyta" rank="genus">
<emphasis box="[1036,1100,1536,1561]" italics="true" pageId="55" pageNumber="56">Picea</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1112,1176,1536,1561]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="55" pageNumber="56" phylum="Tracheophyta" rank="genus">
