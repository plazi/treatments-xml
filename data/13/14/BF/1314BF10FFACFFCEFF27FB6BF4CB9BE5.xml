<taxonomicName box="[345,407,1636,1662]" class="Magnoliopsida" family="Grossulariaceae" genus="Ribes" kingdom="Plantae" order="Saxifragales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="genus">
<emphasis box="[345,407,1636,1662]" italics="true" pageId="44" pageNumber="45">Ribes</emphasis>
</taxonomicName>
, 
<taxonomicName authority="(Beljaev 1993)" baseAuthorityName="Beljaev" baseAuthorityYear="1993" box="[421,707,1636,1662]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="genus">
