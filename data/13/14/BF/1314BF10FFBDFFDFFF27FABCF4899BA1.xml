<emphasis box="[787,899,1840,1866]" italics="true" pageId="61" pageNumber="62">Medicago</emphasis>
</taxonomicName>
, 
<taxonomicName box="[911,1015,1840,1866]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="genus">
<emphasis box="[911,1015,1840,1866]" italics="true" pageId="61" pageNumber="62">Trifolium</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1027,1158,1840,1866]" class="Magnoliopsida" family="Fabaceae" genus="Onobrychis" kingdom="Plantae" order="Fabales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="genus">
