<collectingCountry box="[1225,1313,1343,1368]" name="Mexico" pageId="5" pageNumber="424">Mexico</collectingCountry>
, 
<collectingCountry box="[1327,1432,1343,1368]" name="Bahamas" pageId="5" pageNumber="424">Bahamas</collectingCountry>
