species
</emphasis>
. 
<taxonomicName authority="Tsurusaki, Tchemeris &amp; Logunov 2000" authorityName="Tsurusaki, Tchemeris &amp; Logunov" authorityYear="2000" box="[313,1086,222,247]" class="Arachnida" family="Phalangiidae" genus="Acanthomegabunus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="1" pageNumber="568" phylum="Arthropoda" rank="species" species="sibiricus">
