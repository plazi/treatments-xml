. 
<taxonomicName box="[330,649,1056,1080]" class="Arachnida" family="Phalangiidae" genus="Acanthomegabunus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="569" phylum="Arthropoda" rank="species" species="altaicus" status="sp. n.">
