<subSubSection box="[151,1002,224,251]" pageId="8" pageNumber="9" type="materials_examined">
<paragraph blockId="8.[151,1438,224,1102]" box="[151,1002,224,251]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[151,315,224,250]" pageId="8" pageNumber="9">
<typeStatus box="[151,212,224,250]" pageId="8" pageNumber="9">Type</typeStatus>
species:
</emphasis>
<taxonomicName box="[323,615,225,250]" class="Insecta" family="Coenagrionidae" genus="Denticulobasis" kingdom="Animalia" order="Odonata" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="garrisoni" status="sp. nov.">
<emphasis box="[323,615,225,250]" italics="true" pageId="8" pageNumber="9">Denticulobasis garrisoni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[622,715,224,251]" pageId="8" pageNumber="9" rank="species">
<emphasis bold="true" box="[622,708,224,250]" pageId="8" pageNumber="9">sp. nov</emphasis>
