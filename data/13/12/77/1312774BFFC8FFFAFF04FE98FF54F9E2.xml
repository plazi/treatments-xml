<subSubSection pageId="17" pageNumber="18" type="etymology">
<paragraph blockId="17.[151,1437,495,1604]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[151,297,495,521]" pageId="17" pageNumber="18">Etymology:</emphasis>
