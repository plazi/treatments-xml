<taxonomicName class="Dicotyledoneae" family="Rubiaceae" genus="Coffea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Coffea" order="Gentianales" pageId="0" pageNumber="79" phylum="Angiospermae" rank="genus">
