<taxonomicName class="Dicotyledoneae" family="Fabaceae" genus="Bauhinia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Bauhinia variegata" order="Rosales" pageId="0" pageNumber="79" phylum="Angiospermae" rank="species" species="variegata">
