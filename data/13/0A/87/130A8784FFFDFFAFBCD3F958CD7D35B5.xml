<subSubSection box="[151,473,1755,1781]" pageId="1" pageNumber="34" type="nomenclature">
<paragraph blockId="1.[151,473,1755,1781]" box="[151,473,1755,1781]" pageId="1" pageNumber="34">
<heading bold="true" box="[151,473,1755,1781]" fontSize="11" level="1" pageId="1" pageNumber="34" reason="1">
<emphasis bold="true" box="[151,473,1755,1781]" pageId="1" pageNumber="34">
<taxonomicName box="[151,379,1755,1781]" class="Malacostraca" family="Aphanodactylidae" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="34" phylum="Arthropoda" rank="family">Aphanodactylidae</taxonomicName>
