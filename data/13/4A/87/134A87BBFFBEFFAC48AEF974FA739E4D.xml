<emphasis bold="true" box="[179,513,1668,1692]" pageId="1" pageNumber="60">KEY TO THE SPECIES OF</emphasis>
<taxonomicName authority="FROM" authorityName="FROM" class="Insecta" family="Pteromalidae" genus="Homoporus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="genus">
<emphasis box="[522,687,1668,1691]" italics="true" pageId="1" pageNumber="60">HOMOPORUS</emphasis>
<emphasis bold="true" box="[343,425,1700,1724]" pageId="1" pageNumber="60">FROM</emphasis>
</taxonomicName>
<emphasis bold="true" box="[433,522,1700,1724]" pageId="1" pageNumber="60">
<collectingCountry box="[433,522,1700,1724]" name="China" pageId="1" pageNumber="60">CHINA</collectingCountry>
</emphasis>
</heading>
</paragraph>
