<paragraph blockId="52.[151,1437,1700,1979]" pageId="52" pageNumber="53">
<emphasis bold="true" box="[199,275,1845,1871]" pageId="52" pageNumber="53">Notes:</emphasis>
<materialsCitation ID-GBIF-Occurrence="2831262328" collectingDate="1968-11-23" county="Wallowa Co." location="Joseph" municipality="This" pageId="52" pageNumber="53" specimenCount="1" stateProvince="Oregon">
