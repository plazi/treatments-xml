<location LSID="urn:lsid:plazi:treatment:131D87EFFF9DFFAEFFDC5938FD59FF2D:9E6B6022FF9DFFACFB725834FB77F8D1" box="[1081,1160,1845,1871]" county="Wallowa Co." municipality="This" name="Joseph" pageId="52" pageNumber="53" stateProvince="Oregon">Joseph</location>
, along 
<location LSID="urn:lsid:plazi:treatment:131D87EFFF9DFFAEFFDC5938FD59FF2D:9E6B6022FF9DFFACFB915834FA67F8D1" box="[1242,1432,1845,1871]" county="Wallowa Co." municipality="This" name="Hurricane Creek" pageId="52" pageNumber="53" stateProvince="Oregon">Hurricane Creek</location>
, 
<collectingCounty box="[151,300,1881,1907]" pageId="52" pageNumber="53">Wallowa Co.</collectingCounty>
, 
<collectingRegion box="[312,399,1881,1907]" country="United States of America" name="Oregon" pageId="52" pageNumber="53">Oregon</collectingRegion>
, from specimens collected 
<date box="[707,927,1881,1907]" pageId="52" pageNumber="53" value="1968-11-23">
<collectingDate box="[707,927,1881,1907]" pageId="52" pageNumber="53" value="1968-11-23">23 November 1968</collectingDate>
</date>
