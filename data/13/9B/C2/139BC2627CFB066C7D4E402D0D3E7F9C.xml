<paragraph pageId="null" pageNumber="206">
<taxonomicName authority="L." class="Liliopsida" family="Potamogetonaceae" genus="Zannichellia" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="206" phylum="Tracheophyta" rank="species" species="palustris">
