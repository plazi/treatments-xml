<subSubSection pageId="6" pageNumber="109" type="diagnosis">
<paragraph blockId="6.[839,1467,1943,1996]" pageId="6" pageNumber="109">
<emphasis bold="true" box="[839,949,1943,1967]" pageId="6" pageNumber="109">Diagnosis</emphasis>
