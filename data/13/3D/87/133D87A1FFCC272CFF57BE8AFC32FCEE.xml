<taxonomicName authority="Kozur, 1985" authorityName="? Kozur" authorityYear="1985" box="[465,775,1905,1932]" class="Ostracoda" family="Cavellinidae" genus="Sulcella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="23" pageNumber="306" phylum="Arthropoda" rank="species" species="mesopermiana">
