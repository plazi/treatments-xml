<number box="[1075,1129,836,860]" pageId="10" pageNumber="63" value="0.96">0.96</number>
); IV 
<number box="[1186,1235,836,860]" pageId="10" pageNumber="63" value="9.57">9.57</number>
(
<number box="[1251,1303,836,860]" pageId="10" pageNumber="63" value="2.51">2.51</number>
