<number box="[520,572,188,212]" pageId="12" pageNumber="65" value="1.89">1.89</number>
, 
<number box="[584,636,188,212]" pageId="12" pageNumber="65" value="2.08">2.08</number>
, 
<number box="[646,698,188,212]" pageId="12" pageNumber="65" value="1.64">1.64</number>
, 
<number box="[709,762,188,212]" pageId="12" pageNumber="65" value="1.1">1.10</number>
); IV 
<number box="[818,881,188,212]" pageId="12" pageNumber="65" value="10.68">10.68</number>
(
