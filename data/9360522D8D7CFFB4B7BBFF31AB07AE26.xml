<collectingRegion id="D90D2DD98D7CFFB4B313FEECAAF0AA7E" box="[1087,1192,325,350]" country="Argentina" name="Misiones" pageId="5" pageNumber="426">Misiones</collectingRegion>
</materialsCitation>
