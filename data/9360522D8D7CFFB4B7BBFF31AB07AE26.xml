<materialsCitation id="ABA1E9668D7CFFB4B39FFEECAFF0AA86" ID-GBIF-Occurrence="1563123559" collectionCode="USNM" country="Argentina" location="Misiones" pageId="5" pageNumber="426" specimenCount="1" stateProvince="Misiones" typeStatus="holotype">
