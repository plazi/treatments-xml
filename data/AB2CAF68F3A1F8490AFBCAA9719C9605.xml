<taxonomicName id="744783644AAFCC9D1DD4A05FC2EE9827" baseAuthorityName="Freitag" baseAuthorityYear="1969" class="Insecta" family="Carabidae" genus="Cyclotrachelus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="559" pageNumber="560" phylum="Arthropoda" rank="species" species="sodalis">
