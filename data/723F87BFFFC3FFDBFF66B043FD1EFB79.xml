REMARKS. — Compared with 
<taxonomicName id="3D964D2AFFC3FFDBFD8AB54CFC00FBC3" authority="Sharp &amp; Pilsbry" authorityName="Sharp &amp; Pilsbry" box="[542,982,1029,1053]" class="Scaphopoda" genus="Compressidens" kingdom="Animalia" order="Gadilida" pageId="52" pageNumber="267" phylum="Mollusca" rank="species" species="platyceras">
