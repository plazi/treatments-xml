<taxonomicName id="3D964D2AFFC3FFDBFBB3B562FB19FB9E" authorityName="Scarabino" authorityYear="2008" box="[1063,1231,1064,1088]" class="Scaphopoda" genus="Compressidens" kingdom="Animalia" order="Gadilida" pageId="52" pageNumber="267" phylum="Mollusca" rank="species" species="hienghenensis">
