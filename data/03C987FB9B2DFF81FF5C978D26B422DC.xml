<heading id="D09781819B2DFF9CFF5C978D22D522A2" bold="true" box="[151,498,404,430]" fontSize="11" level="1" pageId="15" pageNumber="266" reason="1">
<emphasis id="B914EAFF9B2DFF9CFF5C978D22D522A2" bold="true" box="[151,498,404,430]" pageId="15" pageNumber="266">
<taxonomicName id="4C604D6E9B2DFF9CFF5C978D228F22A2" box="[151,424,404,430]" class="Insecta" family="Ptiloneuridae" genus="Triplocania" kingdom="Animalia" order="Psocodea" pageId="15" pageNumber="266" phylum="Arthropoda" rank="species" species="manauara" status="n. sp.">
<emphasis id="B914EAFF9B2DFF9CFF5C978D228F22A2" bold="true" box="[151,424,404,430]" italics="true" pageId="15" pageNumber="266">Triplocania manauara</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22757849B2DFF9CFE64978D22D522A2" box="[431,498,404,430]" pageId="15" pageNumber="266" rank="species">n. sp.</taxonomicNameLabel>
