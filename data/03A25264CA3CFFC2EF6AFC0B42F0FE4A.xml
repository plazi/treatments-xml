<taxonomicName id="4C0B98F1CA3EFFCDEDF8FA5042A6FA41" class="Insecta" family="Platycnemididae" genus="Allocnemis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="37" pageNumber="482" phylum="Arthropoda" rank="species" species="vicki">Allocnemis vicki</taxonomicName>
