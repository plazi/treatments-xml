<taxonomicName id="4C734D5A011CDA7CFE12FEDD3D802BFF" baseAuthorityName="Kellogg" baseAuthorityYear="1896" box="[386,502,367,392]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Phthiraptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="vulgata">
