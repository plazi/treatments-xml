<taxonomicName id="4C734D5A011CDA7CFC5AFEC338C32BFF" box="[970,1205,367,392]" class="Insecta" family="Philopteridae" genus="Degeeriella" kingdom="Animalia" order="Phthiraptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="vulgatus">
