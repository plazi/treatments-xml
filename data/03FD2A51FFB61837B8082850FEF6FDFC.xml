<paragraph id="8BEB9B47FFB61831B80828CDFC46FA78" blockId="10.[264,1325,1416,1802]" pageId="10" pageNumber="47">
<emphasis id="B9204755FFB61831B80828CDFE61FA10" bold="true" box="[264,449,1416,1442]" pageId="10" pageNumber="47">
<typeStatus id="54EF25E5FFB61831B80828CDFEE5FA10" box="[264,325,1416,1442]" pageId="10" pageNumber="47">Type</typeStatus>
