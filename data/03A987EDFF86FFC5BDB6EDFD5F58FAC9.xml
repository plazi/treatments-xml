<subSubSection id="C31A6570FF86FFC5BDB6EA0F5977FB5E" pageId="42" pageNumber="43" type="diagnosis">
<paragraph id="8BBF36FBFF86FFC5BDB6EA0F5AAAFB7A" blockId="42.[151,1436,1117,1322]" pageId="42" pageNumber="43">
<emphasis id="B974EAE9FF86FFC5BDB6EA0F5A77FB95" bold="true" box="[151,273,1117,1142]" pageId="42" pageNumber="43">Diagnosis.</emphasis>
