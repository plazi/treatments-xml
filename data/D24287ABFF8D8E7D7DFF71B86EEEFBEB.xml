<emphasis id="689FEAAFFF8E8E7D7DFF74BC6CF3FE6A" box="[151,204,420,443]" italics="true" pageId="86" pageNumber="87">2000</emphasis>
<taxonomicName id="9DEB4D3EFF8E8E7D7C4F74BC6EA0FE6A" authority="(Felix 1909)" baseAuthorityName="Felix" baseAuthorityYear="1909" box="[295,671,420,443]" class="Anthozoa" family="Flabellidae" genus="Flabellum" kingdom="Animalia" order="Scleractinia" pageId="86" pageNumber="87" phylum="Cnidaria" rank="species" species="anderssoni">
