<taxonomicName id="D0CA5097FF91FFC28AB10F63FE68FA53" authority="Enderlein 1922" authorityName="Enderlein" authorityYear="1922" box="[151,455,1390,1416]" class="Insecta" family="Tabanidae" genus="Chasmia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="61" phylum="Arthropoda" rank="genus">
