<taxonomicName id="9877C2D62A8BBF52B79F4CBF3D35C9AD" authorityName="Walker &amp; Wharton" authorityYear="2011" class="Hexapoda" family="Braconidae" genus="Eurytenes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="ormenus">Eurytenes ormenus</taxonomicName>
