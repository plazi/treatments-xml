, 
<bibRefCitation id="AD701EF50474FFF7FDCDFCA0751E13B0" author="Paterson" box="[617,780,876,903]" firstAuthor="Paterson" pageId="36" pageNumber="37" pagination="1 - 162" refId="ref195005" refString="Paterson, G. L. J. (1985) The deep-sea Ophiuroidea of the North Atlantic Ocean. Bulletin of the British Museum (Natural History), Zoology Series, 49 (1), 1 - 162." type="journal article" year="1985">Paterson 1985</bibRefCitation>
); 
<collectionCode id="AFF0FBC10474FFF7FCBAFCA0754E13B0" box="[798,860,877,903]" pageId="36" pageNumber="37">AZO</collectionCode>
