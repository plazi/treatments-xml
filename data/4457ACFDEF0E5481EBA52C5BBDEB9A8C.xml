<taxonomicName id="8C3E2687451894496C75610EA03E5356" class="Magnoliopsida" family="Adoxaceae" genus="Viburnum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Viburnum" order="Dipsacales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">Viburnum</taxonomicName>
