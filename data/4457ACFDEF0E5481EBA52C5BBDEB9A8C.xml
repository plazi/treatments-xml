<taxonomicName id="419BE3238401DAD6114B39BEE25DA007" authority="" authorityName="" class="Insecta" family="Caprifoliaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Caprifoliaceae</taxonomicName>
