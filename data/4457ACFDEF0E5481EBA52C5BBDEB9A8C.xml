<taxonomicName id="D139C2AEA3C270B40CB7D80F892A6E57" class="Magnoliopsida" family="Vitaceae" genus="Vitis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vitis" order="Vitales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">Vitis</taxonomicName>
