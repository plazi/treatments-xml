], 
<taxonomicName id="D069438B6EDE72687B247081FE82B851" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Juglans nigra" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="nigra">Juglans nigra</taxonomicName>
