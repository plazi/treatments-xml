<taxonomicName id="0A98FC3654429E7E55BBB56D68757126" class="Magnoliopsida" family="Vitaceae" genus="Ampelopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ampelopsis arborea" order="Vitales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="arborea">Ampelopsis arborea</taxonomicName>
(L.) Koehne [
<taxonomicName id="0B7788CDC85A4598E7C7B7C6537B2AC2" authority="" authorityName="" class="Insecta" family="Vitaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Vitaceae</taxonomicName>
], 
<taxonomicName id="F5C68998205A2B17B206C4ABAF2B10A4" class="Magnoliopsida" family="Betulaceae" genus="Alnus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Alnus serrulata" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="serrulata">Alnus serrulata</taxonomicName>
(Aiton) Willd. [
<taxonomicName id="EF9F11C311AF98D232973D5DC944F6C4" authority="" authorityName="" class="Insecta" family="Betulaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Betulaceae</taxonomicName>
], 
<taxonomicName id="95C677C295C57DD9D65170ADFAC0D118" class="Magnoliopsida" family="Araliaceae" genus="Aralia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Aralia spinosa" order="Apiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="spinosa">Aralia spinosa</taxonomicName>
L. [
<taxonomicName id="0782805BCCD6020B0301AF7CBF3E415D" authority="" authorityName="" class="Insecta" family="Araliaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Araliaceae</taxonomicName>
], 
<taxonomicName id="D4CC493E4196C15C43442A9619332CD6" class="Magnoliopsida" family="Cannabaceae" genus="Cannabis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cannabis sativa" order="Rosales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="sativa">Cannabis sativa</taxonomicName>
L. [
<taxonomicName id="3636BC0E097A34A30AC304EC156F33F4" authority="" authorityName="" class="Insecta" family="Cannabaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Cannabaceae</taxonomicName>
], 
<taxonomicName id="BA6A63465F7DA00FD8021EB98BEAE07F" class="Magnoliopsida" family="Cannabaceae" genus="Celtis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Celtis" order="Rosales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">Celtis</taxonomicName>
sp. [
<taxonomicName id="459F1F21E58E8CA02E79D59995B1A786" authority="" authorityName="" class="Insecta" family="Ulmaceae" genus="Heteroptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heteroptera" order="Hemiptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="family">Ulmaceae</taxonomicName>
], 
<taxonomicName id="13C9692BEE6885824FAD846AF6AA19BF" class="Liliopsida" family="Commelinaceae" genus="Commelina" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Commelina virginiana" order="Commelinales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="virginiana">Commelina virginiana</taxonomicName>
