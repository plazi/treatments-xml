<taxonomicName id="7E0202DDD9B126630B38538E11A3D031" class="Magnoliopsida" family="Oleaceae" genus="Fraxinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fraxinus" order="Lamiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">Fraxinus</taxonomicName>
