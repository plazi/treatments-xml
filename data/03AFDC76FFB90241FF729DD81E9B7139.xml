<taxonomicName id="4C0616E3FFB90241FBB29C841F38711C" authorityName="Yu, Yang and Gong" authorityYear="1982" box="[1111,1300,1008,1033]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="ejingshanense">
<emphasis id="B972B172FFB90241FBB29C841F38711C" box="[1111,1300,1008,1033]" italics="true" pageId="117" pageNumber="118">L. ejingshanense</emphasis>
</taxonomicName>
