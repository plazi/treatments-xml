<taxonomicName id="4C0616E3FFB90241FD669B60190B7138" authorityName="Yu, Yang, Zhang, Zi and Hu" authorityYear="1981" box="[643,807,1044,1069]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="yunlingense">
<emphasis id="B972B172FFB90241FD669B60190B7138" box="[643,807,1044,1069]" italics="true" pageId="117" pageNumber="118">L. yunlingense</emphasis>
</taxonomicName>
and N/N/NNB in 
<taxonomicName id="4C0616E3FFB90241FC119B601E9C7138" authorityName="Yu, Yang and Gong" authorityYear="1982" box="[1012,1200,1044,1069]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="ejingshanense">
