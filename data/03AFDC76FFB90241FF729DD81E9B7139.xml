<taxonomicName id="4C0616E3FFB90241FC9F9C841E33711C" authorityName="Yu, Yang, Zhang, Zi and Hu" authorityYear="1981" box="[890,1055,1008,1033]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="yunlingense">
