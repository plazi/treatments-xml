<geoCoordinate id="02C23EB6FFAAFFBDFE22FDA7FDC346C6" box="[470,624,630,657]" degrees="025" direction="east" minutes="09.876" orientation="longitude" pageId="5" pageNumber="472" precision="1" value="25.1646">025°09.876’E</geoCoordinate>
