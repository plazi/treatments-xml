<quantity id="A00EF594FFAAFFBDFD9DFE27FD054646" box="[617,694,502,529]" metricMagnitude="2" metricUnit="m" metricValue="6.56" pageId="5" pageNumber="472" unit="m" value="656.0">656 m</quantity>
; c) unnamed pitfall (
<geoCoordinate id="02C23EB6FFAAFFBDFED3FDC7FE094667" box="[295,442,534,560]" degrees="37" direction="north" minutes="01.410" orientation="latitude" pageId="5" pageNumber="472" precision="1" value="37.0235">37°01.410’N</geoCoordinate>
