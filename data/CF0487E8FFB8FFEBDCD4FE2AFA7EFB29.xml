”). First record from 
<collectingCountry id="3FBA766EFFB8FFEBDD42F969FE7AFBE9" box="[342,420,1026,1050]" name="China" pageId="6" pageNumber="5">China</collectingCountry>
, 
<collectingRegion id="8569F81CFFB8FFEBDDA4F969FD57FBE9" box="[432,649,1026,1050]" country="China" name="Yunnan" pageId="6" pageNumber="5">Yunnan Province</collectingRegion>
