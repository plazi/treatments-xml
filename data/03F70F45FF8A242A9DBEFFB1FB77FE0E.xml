<taxonomicName id="4C5EC5D0FF8D242B9C31FED7FD54FA1F" authority="Li &amp; Feng &amp; Luo, 2021" authorityName="Li &amp; Feng &amp; Luo" authorityYear="2021" box="[280,668,1409,1433]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="CoL" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="154" phylum="Arthropoda" rank="species" species="plumiopedella" status="sp. nov." subGenus="Gymnaeta">
