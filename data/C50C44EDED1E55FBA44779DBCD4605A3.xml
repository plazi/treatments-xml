<taxonomicName id="B1362898319423A13576265D3A87FEEE" authority="Cardoso, Bastos-Pereira, Souza &amp; Ferreira, 2021" authorityName="Cardoso 1 &amp; Bastos-Pereira 1 &amp; Ferreira 1" authorityYear="2022" class="Malacostraca" family="Styloniscidae" genus="Chaimowiczia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chaimowiczia obybytyra" order="Isopoda" pageId="0" pageNumber="139" phylum="Arthropoda" rank="species" species="obybytyra">
