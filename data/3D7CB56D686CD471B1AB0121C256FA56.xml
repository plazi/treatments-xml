<taxonomicName id="32B129E7D9B625C69B9B9EF0058CD33B" class="Magnoliopsida" family="Fabaceae" genus="Erythrina" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Erythrina" order="Fabales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">Erythrina</taxonomicName>
