<subSubSection id="C3696559FF90177FFF4C693DAE29FA38" pageId="24" pageNumber="135" type="diagnosis">
<paragraph id="8BCC36D2FF90177FFF4C693DABFFFCF8" blockId="24.[151,1437,860,1605]" box="[151,483,860,885]" pageId="24" pageNumber="135">
