<taxonomicName id="2108A33A2E915C5533C237177CAE92B4" genus="Nymphaeaceae" lsidName="Nymphaeaceae" pageId="7" pageNumber="356" rank="genus">Nymphaeaceae</taxonomicName>
), 
<taxonomicName id="E49FA1AE9824411421566052438E864D" class="Liliopsida" family="Cyperaceae" genus="Eleocharis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eleocharis" order="Poales" pageId="7" pageNumber="356" phylum="Tracheophyta" rank="genus">Eleocharis</taxonomicName>
and 
<taxonomicName id="3D9D37388E9BE3600C36E74C2A0FE237" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="7" pageNumber="356" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
(
<taxonomicName id="A2E1F6691D3C30B0F22275EC6A7BA16B" genus="Cyperaceae" lsidName="Cyperaceae" pageId="7" pageNumber="356" rank="genus">Cyperaceae</taxonomicName>
), and 
<taxonomicName id="CCF878DD97645D4D9803182A1CE251BF" class="Liliopsida" family="Potamogetonaceae" genus="Potamogeton" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Potamogeton" order="Alismatales" pageId="7" pageNumber="356" phylum="Tracheophyta" rank="genus">Potamogeton</taxonomicName>
(
<taxonomicName id="9AAA78AC7A579115D84943EC066BEBA8" genus="Potamogetonaceae" lsidName="Potamogetonaceae" pageId="7" pageNumber="356" rank="genus">Potamogetonaceae</taxonomicName>
) (
<bibRefCitation id="6A7369EF9F45818890C7CF730B476DD9" author="O'Brien, CW" journalOrPublisher="Southwestern Entomologist" pageId="23" pageNumber="372" pagination="141 - 149" title="U. S. Bagous, bionomic notes, a new species, and a new name (Bagoini, Erirhininae, Curculionidae, Coleoptera)." volume="4" year="1979">
<normalizedToken id="C8DDD44DEF6257040F3AAC59910A8F95" originalValue="O’Brien">O'Brien</normalizedToken>
and Marshall 1979
</bibRefCitation>
).The specimen of 
<taxonomicName id="0DF9C15F327DF634F4FAA3635869D9B3" class="Insecta" family="Curculionidae" genus="Bagous" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bagous obliquus" order="Coleoptera" pageId="7" pageNumber="356" phylum="Arthropoda" rank="species" species="obliquus">Bagous obliquus</taxonomicName>
from New Brunswick was swept from foliage in a brushy opening in a regenerating mixed forest (20 years old) near a small marsh in June.
</paragraph>
