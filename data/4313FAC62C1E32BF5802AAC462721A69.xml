<taxonomicName id="691629FAC81D1216D8EC7D7D775E2E52" class="Magnoliopsida" family="Nymphaeaceae" genus="Nymphaea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Nymphaea" order="Nymphaeales" pageId="7" pageNumber="356" phylum="Tracheophyta" rank="genus">Nymphaea</taxonomicName>
