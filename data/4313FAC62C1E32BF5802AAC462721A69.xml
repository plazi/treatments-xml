<paragraph id="A2C1BA4013C874B4AAF4E904B49CDA57" pageId="7" pageNumber="356">
<taxonomicName id="B67E046DBC1D409ADB076F7DE8DF7AD3" class="Insecta" family="Curculionidae" genus="Bagous" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bagous" order="Coleoptera" pageId="7" pageNumber="356" phylum="Arthropoda" rank="genus">Bagous</taxonomicName>
