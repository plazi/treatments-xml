<treatmentCitationGroup id="83FF11989E0CFFC4FD287693FBDD149F" pageId="8" pageNumber="8">
Now 
<taxonomicName id="64EF4D359E0CFFC4FD647693FB8C14D6" authority="fornsi Gundlach, 1858" authorityName="Gundlach" authorityYear="1858" box="[727,1146,648,667]" class="Aves" family="Parulidae" genus="Teretistris" kingdom="Animalia" order="Passeriformes" pageId="8" pageNumber="8" phylum="Chordata" rank="subSpecies" species="fornsi" subSpecies="fornsi">
<emphasis id="919BEAA49E0CFFC4FD647693FC3014D6" box="[727,966,648,667]" italics="true" pageId="8" pageNumber="8">Teretistris fornsi fornsi</emphasis>
<bibRefCitation id="C77E4B479E0CFFC4FC7C7692FB8C14D6" author="Gundlach, J." box="[975,1146,648,667]" pageId="8" pageNumber="8" pagination="267 - 275" refId="ref117426" refString="Gundlach, J. 1858. Notes on some Cuban birds, with descriptions of three new species. Annals of the Lyceum of Natural History of New York 6: 267 - 275." type="journal article" year="1858">Gundlach, 1858</bibRefCitation>
</taxonomicName>
