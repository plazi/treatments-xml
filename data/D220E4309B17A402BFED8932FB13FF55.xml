and figs 
<quantity id="9D71F8C39B17A40EBE068B03FE5CF934" box="[380,451,1662,1688]" metricMagnitude="-1" metricUnit="m" metricValue="2.159" metricValueMax="2.286" metricValueMin="2.032" pageId="5" pageNumber="572" unit="in" value="8.5" valueMax="9.0" valueMin="8.0">8–9 in</quantity>
