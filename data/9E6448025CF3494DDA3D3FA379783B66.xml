<taxonomicName id="C040853FC9D633D58A325BDB6A7305B6" class="Arachnida" family="Paratropididae" genus="Paratropis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paratropis scruposa" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="scruposa">Paratropis scruposa</taxonomicName>
