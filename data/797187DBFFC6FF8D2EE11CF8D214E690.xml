<taxonomicName id="36D84D4EFFC6FF8D2ED41D6DD005E649" baseAuthorityName="Mannerheim" baseAuthorityYear="1843" box="[185,376,307,333]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="98" phylum="Arthropoda" rank="species" species="picipennis">
<emphasis id="C3ACEADFFFC6FF8D2ED41D6DD005E649" box="[185,376,307,333]" italics="true" pageId="13" pageNumber="98">Atheta picipennis</emphasis>
</taxonomicName>
