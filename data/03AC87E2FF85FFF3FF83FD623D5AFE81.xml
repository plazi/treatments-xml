<materialsCitation id="3B6D3CA9FF85FFF3FDEEFA5B389AF9AB" collectingDate="1931-06-02" collectionCode="AMNH" pageId="62" pageNumber="62" specimenCode="AMNH 301046" specimenCount="1" typeStatus="paratype">
