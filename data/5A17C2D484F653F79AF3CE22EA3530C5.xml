<paragraph id="076628E5A83F7885F35D541AC03E7CFA" pageId="0" pageNumber="83">
<taxonomicName id="90749705B33BF7E67C841D2C5F1E604A" LSID="5A17C2D4-84F6-53F7-9AF3-CE22EA3530C5" authority="W. H. Chen &amp; Y. M. Shui" authorityName="W. H. Chen &amp; Y. M. Shui" authorityYear="2019" class="Insecta" family="Gesneriaceae" genus="Oreocharis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Oreocharis fulva" order="Lamiales" pageId="0" pageNumber="83" phylum="Arthropoda" rank="species" species="fulva" status="sp. nov.">Oreocharis fulva W.H.Chen &amp; Y.M.Shui</taxonomicName>
