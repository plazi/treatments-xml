<taxonomicName id="4C604D33FF85FFFE3E93FD8B1831DD95" authorityName="Schedl" authorityYear="1952" box="[1202,1350,1511,1533]" class="Insecta" family="Curculionidae" genus="Hexacolus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="99" phylum="Arthropoda" rank="species" species="pelicipennis">
