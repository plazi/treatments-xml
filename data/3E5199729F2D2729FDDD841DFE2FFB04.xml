<geoCoordinate id="D3CC4EA39F2D272AFF1681BBFEFAFB7B" box="[189,336,1197,1223]" degrees="30.23431" direction="south" orientation="latitude" pageId="44" pageNumber="45" precision="1" value="-30.23431">30.23431° S</geoCoordinate>
, 
<geoCoordinate id="D3CC4EA39F2D272AFEF681BBFE58FB7B" box="[349,498,1197,1223]" degrees="18.16491" direction="east" orientation="longitude" pageId="44" pageNumber="45" precision="1" value="18.16491">18.16491° E</geoCoordinate>
, 
<date id="C2460EA49F2D272AFE5481BBFD26FB7B" box="[511,652,1197,1223]" pageId="44" pageNumber="45" value="2002-10-07">7 Oct. 2002</date>
