<date id="FFB18C93A247FF89DFC3621A93F9FEC4" box="[415,456,260,284]" pageId="12" pageNumber="164">
V 
<number id="3DC7B874A247FF89DFE5621A93F5FEC4" box="[441,452,260,284]" pageId="12" pageNumber="164" value="1.0">1</number>
.
</date>
Setae 
<date id="FFB18C93A247FF89DC4F621A9006FEC4" box="[531,567,260,284]" pageId="12" pageNumber="164">
V 
<number id="3DC7B874A247FF89DC70621A9006FEC4" box="[556,567,260,284]" pageId="12" pageNumber="164" value="1.0">1</number>
</date>
, 
<date id="FFB18C93A247FF89DC1F621A90F1FEC4" box="[579,704,260,285]" pageId="12" pageNumber="164">
V 
<number id="3DC7B874A247FF89DC04621A9057FEC4" box="[600,614,260,284]" pageId="12" pageNumber="164" value="2.0">2</number>
and V 
<number id="3DC7B874A247FF89DCEF621A90F1FEC4" box="[691,704,260,284]" pageId="12" pageNumber="164" value="3.0">3</number>
</date>
, 
<date id="FFB18C93A247FF89DC90621A90DFFEC4" box="[716,750,260,284]" pageId="12" pageNumber="164">
V 
