<taxonomicName id="25A2301E72B56774133D3B94C564D8EC" authorityName="S.F.Blake" authorityYear="1925" class="Dicotyledoneae" family="Caryophyllaceae" genus="Aster" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Aster brachyactis" order="Centrospermae" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="brachyactis">
