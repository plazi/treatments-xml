<taxonomicName id="0F2A59A5FAD01698E02A777483C92F91" class="Magnoliopsida" family="Asteraceae" genus="Erigeron" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Erigeron philadelphicus" order="Asterales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="philadelphicus">
