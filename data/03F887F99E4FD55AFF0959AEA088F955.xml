<keyLead id="30A095DA9E4FD55AFF095E31A089FB23" pageId="14" pageNumber="524">
<number id="3D9924C89E4FD55AFF095E31A5B0FBE4" box="[151,165,1104,1125]" pageId="14" pageNumber="524" value="2.0">2</number>
