<paragraph id="AD6D3C2EFFDBFFE6FF3EFD42E854FD2E" blockId="10.[151,1437,582,861]" pageId="10" pageNumber="11">
<emphasis id="9FA6E03CFFDBFFE6FF3EFD42E8D6FD4B" bold="true" box="[151,461,654,680]" pageId="10" pageNumber="11">
Distribution in 
<collectingCountry id="D5C57CBEFFDBFFE6FEFBFD42E8D2FD4B" box="[338,457,654,680]" name="Colombia" pageId="10" pageNumber="11">Colombia</collectingCountry>
