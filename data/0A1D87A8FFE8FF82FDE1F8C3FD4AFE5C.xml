<taxonomicName id="45B44D3DFFE9FF82FF7AFE10FF50FE5D" box="[188,253,460,486]" class="Pinopsida" family="Pinaceae" genus="Abies" kingdom="Plantae" order="Pinales" pageId="85" pageNumber="86" phylum="Tracheophyta" rank="genus">
