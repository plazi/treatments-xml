<taxonomicName id="4C4638C7F30A7F6D1765A6F5FC1AFB22" box="[845,1001,1158,1181]" class="Insecta" family="Carabidae" genus="Paracallistoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="incrassatus">
