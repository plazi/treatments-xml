<subSubSection id="C329A688FFCB3A0AFF25BD3214E1FAB3" box="[197,993,1343,1370]" pageId="13" pageNumber="14" type="etymology">
<paragraph id="8B8CF503FFCB3A0AFF25BD3214E1FAB3" blockId="13.[151,1436,1231,1930]" box="[197,993,1343,1370]" pageId="13" pageNumber="14">
<emphasis id="B9472911FFCB3A0AFF25BD321655FAB0" bold="true" box="[197,341,1343,1369]" pageId="13" pageNumber="14">Etymology:</emphasis>
