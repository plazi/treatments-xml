<collectingCountry id="7BBF7616FF8BA711FEE1F8911F9A0E4F" box="[359,426,1842,1867]" name="China" pageId="9" pageNumber="536">China</collectingCountry>
(
<collectingRegion id="C16CF864FF8BA711FE3FF8911C2A0E4F" box="[441,538,1842,1867]" country="China" name="Sichuan" pageId="9" pageNumber="536">Sichuan</collectingRegion>
