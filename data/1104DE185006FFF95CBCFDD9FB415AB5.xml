<location id="9C7239D55006FFFA5DEFFC21FE355EA4" LSID="urn:lsid:plazi:treatment:1104DE185006FFF95CBCFDD9FB415AB5:9C7239D55006FFFA5DEFFC21FE355EA4" box="[452,499,873,899]" name="One" pageId="13" pageNumber="545" stateProvince="Kamchatka">One</location>
