<taxonomicName id="4C4AAE9AFF8FFFEFD3EA2843ADA38F80" authorityName="Hsiao &amp; Li &amp; Ren &amp; Pang" authorityYear="2021" box="[977,1206,1390,1411]" class="Insecta" family="Cantharidae" genus="Cretocantharis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="1281" phylum="Arthropoda" rank="species" species="veda">
