<geoCoordinate id="F4DF6C90D3411499BD990E0A3D8FA934" degrees="98" direction="east" minutes="3" orientation="longitude" precision="1" seconds="36.8064" value="98.06023">98°3'36.8064&quot;E</geoCoordinate>
