<taxonomicName id="4C03DED40F73FFFB07FC524EBE19FC24" authorityName="Wiiliams" authorityYear="1965" box="[482,592,872,896]" class="Malacostraca" family="Alpheidae" genus="Leptalpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="690" phylum="Arthropoda" rank="genus">
