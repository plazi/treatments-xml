<taxonomicName id="4C7AECD1FFF75A34FD6DB6BD0D2EFE31" authorityName="Uhler" authorityYear="1877" box="[733,861,441,467]" class="Insecta" family="Cicadellidae" genus="Pachyopsis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="525" phylum="Arthropoda" rank="genus">
