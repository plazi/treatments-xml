<quantity id="4C8936CDBDE611C3382DFB6AFB5FFB34" box="[1128,1161,1177,1198]" metricMagnitude="4" metricUnit="kg" metricValue="7.0" pageId="143" pageNumber="144" unit="t" value="70.0">70t</quantity>
</figureCitation>
