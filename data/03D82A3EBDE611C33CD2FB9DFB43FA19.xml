<taxonomicName id="4C71E0ABBDE611C33F06FA9DFC79FA19" box="[835,943,1390,1411]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="143" pageNumber="144" phylum="Tracheophyta" rank="genus">
