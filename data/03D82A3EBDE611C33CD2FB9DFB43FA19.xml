<collectingRegion id="49B555CABDE611C33806FAF0FB59FA82" box="[1091,1167,1283,1304]" country="New Zealand" name="Nelson" pageId="143" pageNumber="144">Nelson</collectingRegion>
, 
<collectingRegion id="49B555CABDE611C33EE8FAEDFCFCFAA9" box="[685,810,1310,1331]" country="New Zealand" name="Marlborough" pageId="143" pageNumber="144">Marlborough</collectingRegion>
