<taxonomicName id="4C704D7FFFFCFFF1FEE2F9F13C42F9C4" authority="M. Sars, 1851" authorityName="M. Sars" authorityYear="1851" box="[294,592,1544,1569]" class="Polychaeta" family="Spionidae" genus="Nerine" kingdom="Animalia" order="Spionida" pageId="6" pageNumber="259" phylum="Annelida" rank="species" species="cirrata">
