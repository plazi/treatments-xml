<heading id="D0D1B432FFC3FFC489B9C4EA230F9D63" bold="true" box="[151,837,761,787]" fontSize="11" level="1" pageId="114" pageNumber="115" reason="1">
<emphasis id="B952DF4CFFC3FFC489B9C4EA230F9D63" bold="true" box="[151,837,761,787]" pageId="114" pageNumber="115">
* 
<taxonomicName id="4C2678DDFFC3FFC4898BC4EA230F9D63" authority="(Creplin, 1825) Seurat, 1919" authorityName="(Creplin, 1825) Seurat" authorityYear="1919" box="[165,837,761,787]" class="Secernentea" family="Acuariidae" genus="Cosmocephalus" kingdom="Animalia" order="Spirurida" pageId="114" pageNumber="115" phylum="Nematoda" rank="species" species="obvelatus">
<emphasis id="B952DF4CFFC3FFC4898BC4EA21979D63" bold="true" box="[165,477,761,787]" italics="true" pageId="114" pageNumber="115">Cosmocephalus obvelatus</emphasis>
