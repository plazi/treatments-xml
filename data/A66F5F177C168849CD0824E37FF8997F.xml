<taxonomicName id="84934298773F3F95E671F30C93823F1A" authority="Schkuhr" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="540" phylum="Tracheophyta" rank="species" species="uliginosus">
