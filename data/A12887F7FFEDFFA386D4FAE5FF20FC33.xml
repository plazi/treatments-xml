<collectionCode id="4F90AE24FFEDFFA784F6FA5FFCD3FA54" box="[693,786,1431,1455]" httpUri="http://grbio.org/cool/zxpe-c0as" name="Virginia Museum of Natural History" pageId="1" pageNumber="64">VMNH</collectionCode>
): 
<collectingCountry id="51967671FFEDFFA78565FA50FC78FA4B" box="[806,953,1432,1456]" name="Tanzania" pageId="1" pageNumber="64">TANZANIA</collectingCountry>
: Lindi District, Rondo Plateau, 
<specimenCount id="3F87FD68FFEDFFA78365FA5FFAB9FA54" box="[1318,1400,1431,1455]" pageId="1" pageNumber="64" type="generic">
<quantity id="EE799B04FFEDFFA78365FA5FFAB5FA54" box="[1318,1396,1431,1455]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="1" pageNumber="64" unit="m" value="800.0">800 m</quantity>
