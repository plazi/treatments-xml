<quantity id="4CB99B6AFF9EFFC22841F97D495018FD" box="[1304,1389,1701,1727]" metricMagnitude="3" metricUnit="m" metricValue="2.1" pageId="34" pageNumber="35" unit="m" value="2100.0">2100m</quantity>
