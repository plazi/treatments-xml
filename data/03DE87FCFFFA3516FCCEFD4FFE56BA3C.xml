<taxonomicName id="4C774D69FFFA3528FC60FCC0FB32BA93" authority="Hebard, 1928 b" authorityName="Hebard" authorityYear="1928" box="[976,1201,890,914]" class="Insecta" family="Haglotettigoniidae" genus="Aclodes" kingdom="Animalia" order="Orthoptera" pageId="31" pageNumber="768" phylum="Arthropoda" rank="genus">
