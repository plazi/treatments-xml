<subSubSection id="219A81E3FF9CA445FF47FF63FDA6FF2B" box="[151,572,189,216]" pageId="6" pageNumber="7" type="nomenclature">
<paragraph id="693FD268FF9CA445FF47FF63FDA6FF2B" blockId="6.[151,653,153,216]" box="[151,572,189,216]" pageId="6" pageNumber="7">
<heading id="32776504FF9CA445FF47FF63FDA6FF2B" bold="true" box="[151,572,189,216]" fontSize="11" level="1" pageId="6" pageNumber="7" reason="1">
<emphasis id="5BF40E7AFF9CA445FF47FF63FDA6FF2B" bold="true" box="[151,572,189,216]" pageId="6" pageNumber="7">
Family 
<taxonomicName id="AE80A9EBFF9CA445FF27FF63FDA6FF2B" authority="Leach, 1818" authorityName="Leach" authorityYear="1818" box="[247,572,189,216]" class="Malacostraca" family="Cymothoidae" kingdom="Animalia" order="Isopoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="family">
Cymothoidae 
<bibRefCitation id="0D11AF99FF9CA445FE76FF60FDA6FF2B" author="Leach, W. E." box="[422,572,189,216]" pageId="6" pageNumber="7" pagination="338 - 354" refId="ref74972" refString="Leach, W. E. (1818) Cymothoadees. In: Cuvier, F. (Ed.), Dictionnaire des Sciences Naturelles. Vol. 12. Strasbourg et Levrault, Paris, pp. 338 - 354." type="book chapter" year="1818">Leach, 1818</bibRefCitation>
</taxonomicName>
</emphasis>
