<taxonomicName id="02FD62D6FFA11A78050EFE61EDF3FE09" box="[781,947,462,487]" class="Insecta" family="Staphylinidae" genus="Acylophorus" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="rufipennis">
