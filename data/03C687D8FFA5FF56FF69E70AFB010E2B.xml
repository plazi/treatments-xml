<date id="FFD1100EFFA5FF58FAFDE15FFA080A03" box="[1283,1423,1376,1402]" pageId="6" pageNumber="7" value="1992-10-30">
<number id="3DA724E9FFA5FF58FAFDE15FFAA50A03" box="[1283,1314,1376,1402]" pageId="6" pageNumber="7" value="30.0">30</number>
/ 
<number id="3DA724E9FFA5FF58FAD2E15FFACF0A03" box="[1324,1352,1376,1402]" pageId="6" pageNumber="7" value="10.0">10</number>
