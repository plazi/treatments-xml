<date id="FFD1100EFFA5FF58FE40E1E7FDC00A8B" box="[446,583,1496,1522]" pageId="6" pageNumber="7" value="1984-10-22">
<number id="3DA724E9FFA5FF58FE40E1E7FE5B0A8B" box="[446,476,1496,1522]" pageId="6" pageNumber="7" value="22.0">22</number>
/ 
<number id="3DA724E9FFA5FF58FE18E1E7FD850A8B" box="[486,514,1496,1522]" pageId="6" pageNumber="7" value="10.0">10</number>
/ 
<number id="3DA724E9FFA5FF58FDF5E1E7FDC00A8B" box="[523,583,1496,1522]" pageId="6" pageNumber="7" value="1984.0">1984</number>
