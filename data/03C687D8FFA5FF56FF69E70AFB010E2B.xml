<date id="FFD1100EFFA5FF58FCEEE12FFC1D0A53" box="[784,922,1296,1322]" pageId="6" pageNumber="7" value="1992-10-19">
<number id="3DA724E9FFA5FF58FCEEE12FFCAB0A53" box="[784,812,1296,1322]" pageId="6" pageNumber="7" value="19.0">19</number>
/ 
<number id="3DA724E9FFA5FF58FCC8E12FFCD40A53" box="[822,851,1296,1322]" pageId="6" pageNumber="7" value="10.0">10</number>
/ 
<number id="3DA724E9FFA5FF58FCA2E12FFC1D0A53" box="[860,922,1296,1322]" pageId="6" pageNumber="7" value="1992.0">1992</number>
</date>
), same locality, fine sand with shell gravel, upper eulittoral (
<date id="FFD1100EFFA5FF58FEA5E107FE610A2B" box="[347,486,1336,1362]" pageId="6" pageNumber="7" value="1992-10-19">
<number id="3DA724E9FFA5FF58FEA5E107FEF00A2B" box="[347,375,1336,1362]" pageId="6" pageNumber="7" value="19.0">19</number>
/ 
<number id="3DA724E9FFA5FF58FE7FE107FE190A2B" box="[385,414,1336,1362]" pageId="6" pageNumber="7" value="10.0">10</number>
/ 
<number id="3DA724E9FFA5FF58FE56E107FE610A2B" box="[424,486,1336,1362]" pageId="6" pageNumber="7" value="1992.0">1992</number>
</date>
