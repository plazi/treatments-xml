<date id="FFD1100EFFA5FF58FEFEE1E7FE020A8B" box="[256,389,1496,1522]" pageId="6" pageNumber="7" value="1984-09-30">
<number id="3DA724E9FFA5FF58FEFEE1E7FE9A0A8B" box="[256,285,1496,1522]" pageId="6" pageNumber="7" value="30.0">30</number>
