<date id="FFD1100EFFA5FF58FCB9E0D7FC550A7B" box="[839,978,1256,1282]" pageId="6" pageNumber="7" value="1992-10-12">
<number id="3DA724E9FFA5FF58FCB9E0D7FCE30A7B" box="[839,868,1256,1282]" pageId="6" pageNumber="7" value="12.0">12</number>
/ 
<number id="3DA724E9FFA5FF58FC90E0D7FC0C0A7B" box="[878,907,1256,1282]" pageId="6" pageNumber="7" value="10.0">10</number>
/ 
<number id="3DA724E9FFA5FF58FC6AE0D7FC550A7B" box="[916,978,1256,1282]" pageId="6" pageNumber="7" value="1992.0">1992</number>
</date>
