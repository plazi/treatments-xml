<date id="FFD1100EFFA5FF58FF21E15FFEEC0A03" box="[223,363,1376,1402]" pageId="6" pageNumber="7" value="1992-10-20">
<number id="3DA724E9FFA5FF58FF21E15FFF790A03" box="[223,254,1376,1402]" pageId="6" pageNumber="7" value="20.0">20</number>
/ 
<number id="3DA724E9FFA5FF58FEF6E15FFEA30A03" box="[264,292,1376,1402]" pageId="6" pageNumber="7" value="10.0">10</number>
/ 
<number id="3DA724E9FFA5FF58FED0E15FFEEC0A03" box="[302,363,1376,1402]" pageId="6" pageNumber="7" value="1992.0">1992</number>
</date>
