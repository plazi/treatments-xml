<date id="FFD1100EFFA5FF58FD74E0FFFC920BA3" box="[650,789,1216,1242]" pageId="6" pageNumber="7" value="1992-12-27">
<number id="3DA724E9FFA5FF58FD74E0FFFD2F0BA3" box="[650,680,1216,1242]" pageId="6" pageNumber="7" value="27.0">27</number>
/ 
<number id="3DA724E9FFA5FF58FD4CE0FFFD480BA3" box="[690,719,1216,1242]" pageId="6" pageNumber="7" value="12.0">12</number>
/ 
<number id="3DA724E9FFA5FF58FD26E0FFFC920BA3" box="[728,789,1216,1242]" pageId="6" pageNumber="7" value="1992.0">1992</number>
</date>
). McKenzie Point, 
<collectingCountry id="F378765EFFA5FF58FC05E0FFFBE80BA3" box="[1019,1135,1216,1242]" name="Kenya" pageId="6" pageNumber="7">Mombasa</collectingCountry>
(
