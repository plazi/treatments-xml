<date id="FF9D105E29621961FBC1415C0C89B174" box="[1138,1231,1850,1876]" pageId="2" pageNumber="63" value="1933-07-24">
<number id="3DEB24B929621961FBC1415C0CC6B174" box="[1138,1152,1850,1876]" pageId="2" pageNumber="63" value="7.0">7</number>
/ 
<number id="3DEB24B929621961FB3A415C0CE0B174" box="[1161,1190,1850,1876]" pageId="2" pageNumber="63" value="33.0">33</number>
/ 
<number id="3DEB24B929621961FB03415C0C89B174" box="[1200,1231,1850,1876]" pageId="2" pageNumber="63" value="24.0">24</number>
</date>
/ 
<number id="3DEB24B929621961FB6B415C0CB3B174" box="[1240,1269,1850,1876]" pageId="2" pageNumber="63" value="23.0">23</number>
/ 
<number id="3DEB24B929621961FB4C415C0D5CB174" box="[1279,1306,1850,1876]" pageId="2" pageNumber="63" value="13.0">13</number>
