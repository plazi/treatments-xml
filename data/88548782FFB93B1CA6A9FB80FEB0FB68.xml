<taxonomicName id="C7FD4D17FFB93B1CA6A9FB80FDDFFBBE" authority="Trybom" authorityName="Trybom" box="[151,572,1049,1075]" class="Insecta" family="Thripidae" genus="Megalurothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="272" phylum="Arthropoda" rank="species" species="sjostedti">
