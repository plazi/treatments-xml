<taxonomicName id="97BD4D2D813DFFBF7727F86AA0F3F829" authority="Hu &amp; Zhang, 1990" authorityName="Hu &amp; Zhang" authorityYear="1990" box="[296,733,1945,1967]" class="Arachnida" family="Hahniidae" genus="Hahnia" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="himalayaensis">
