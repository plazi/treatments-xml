<caption id="04C26626813CFFBE7698F86FA156F876" httpUri="https://zenodo.org/record/277832/files/figure.png" pageId="13" pageNumber="14" targetBox="[316,1270,155,1926]" targetPageId="13">
<paragraph id="500236AE813CFFBE7698F86FA156F876" blockId="13.[151,1437,1948,2032]" pageId="13" pageNumber="14">
<emphasis id="62C9EABC813CFFBE7698F86FA30EF837" bold="true" box="[151,288,1948,1971]" pageId="13" pageNumber="14">FIGURE 12.</emphasis>
<taxonomicName id="97BD4D2D813CFFBE7728F86FA0F8F834" authority="Hu &amp; Zhang, 1990" authorityName="Hu &amp; Zhang" authorityYear="1990" box="[295,726,1948,1970]" class="Arachnida" family="Hahniidae" genus="Hahnia" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="himalayaensis">
<emphasis id="62C9EABC813CFFBE7728F86FA024F834" box="[295,522,1948,1970]" italics="true" pageId="13" pageNumber="14">Hahnia himalayaensis</emphasis>
