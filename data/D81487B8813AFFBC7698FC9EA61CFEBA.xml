), 
<taxonomicName id="97BD4D2D813AFFB87737FAB7A070FADC" authority="Emerton, 1890" authorityName="Emerton" authorityYear="1890" box="[312,606,1346,1371]" class="Arachnida" family="Hahniidae" genus="Hahnia" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="cinerea">
