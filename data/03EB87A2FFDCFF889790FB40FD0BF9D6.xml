<taxonomicName id="4C424D37FFDCFF889549FA3AFC17FA10" authority="(Allan, 1947)" authorityName="Allan" authorityYear="1947" baseAuthorityName="Allan" baseAuthorityYear="1947" box="[590,928,1463,1488]" class="Gastropoda" family="Dorididae" genus="Doris" kingdom="Animalia" order="Nudibranchia" pageId="12" pageNumber="13" phylum="Mollusca" rank="species" species="cameroni">
