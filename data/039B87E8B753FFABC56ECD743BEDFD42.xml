<bibRefCitation id="EFA34B0FB750FFABC520CA963EF2FD1A" author="Pic, M." box="[326,453,644,670]" firstAuthor="Pic" journalOrPublisher="L'Echange Revue Linneenne" pageId="10" pageNumber="12" pagination="1 - 4" part="404" refId="ref6435" refString="Pic, M. (1921 a) Contribution a l'etude des Lycides. L'Echange Revue Linneenne, 404, 1 - 4." title="Contribution a l'etude des Lycides" type="journal article" year="1921" yearSuffix="a">
Pic, 
<number id="3DFA24D9B750FFABC518CA963E8FFD1A" box="[382,440,644,670]" pageId="10" pageNumber="11" value="1921.0">1921</number>
a
</bibRefCitation>
</taxonomicName>
with 
<taxonomicName id="4C324D7DB750FFABC677CA963D44FD19" authorityName="Bourgeois" authorityYear="1879" box="[529,627,644,669]" class="Insecta" family="Lycidae" genus="Plateros" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
