<taxonomicName id="A9BA4D13FF99FFBFB9F26F4080A8FA57" authorityName="Lehtinen" authorityYear="1967" box="[151,218,1398,1424]" class="Arachnida" family="Amaurobiidae" genus="Taira" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="481" phylum="Arthropoda" rank="genus">Taira</taxonomicName>
