<taxonomicName id="A9BA4D13FF99FFBFB8D26D5C822CF844" authorityName=": Lehtinen" authorityYear="1967" box="[439,606,1897,1923]" class="Arachnida" family="Amaurobiidae" genus="Taira" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="481" phylum="Arthropoda" rank="species" species="flavidorsalis">
