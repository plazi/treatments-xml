<taxonomicName id="0CAE4D55A851FF8EFEB2FD44FE000BBC" baseAuthorityName="Cory" baseAuthorityYear="1883" box="[295,445,657,678]" class="Aves" family="Thraupidae" genus="Calyptophilus" kingdom="Animalia" order="Passeriformes" pageId="73" pageNumber="73" phylum="Chordata" rank="species" species="frugivorus">
