<taxonomicName id="4C4F4D0CFFB0FFC6F3C2FCBF2F377763" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Bierman" baseAuthorityYear="1910" box="[151,319,834,860]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="pavo">
<emphasis id="B93BEA9DFFB0FFC6F3C2FCBF2F377763" bold="true" box="[151,319,834,860]" italics="true" pageId="83" pageNumber="84">Rhotana pavo</emphasis>
