<quantity id="4CA314D3ED10FFC4880BFBC10450FBB4" metricMagnitude="4" metricUnit="m" metricValue="1.9" pageId="24" pageNumber="25" unit="km" value="19.0">19 km</quantity>
N. Kalabakan, 
<quantity id="4CA314D3ED10FFC48CE2FBE9075EFBB4" box="[484,547,1084,1110]" metricMagnitude="1" metricUnit="m" metricValue="6.0" pageId="24" pageNumber="25" unit="m" value="60.0">60 m</quantity>
, 
<date id="FFE59FF6ED10FFC48F29FBE907C6FBB4" box="[559,699,1084,1110]" pageId="24" pageNumber="25" value="1962-11-04">
<number id="3D93AB11ED10FFC48F29FBE90743FBB4" box="[559,574,1084,1110]" pageId="24" pageNumber="25" value="4.0">4</number>
