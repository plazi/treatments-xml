<taxonomicName id="4C0E27CDF8046C516A9DFBD3B965AAC1" box="[402,525,1146,1171]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="silvatica">
