</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="08CCBFCB2C54FFABFDF253583D41FEB1" box="[608,769,268,288]" pageId="12" pageNumber="13">
<emphasis id="FDB8185A2C54FFABFDF253583D41FEB1" box="[608,769,268,288]" italics="true" pageId="12" pageNumber="13">
<collectionCode id="A9DD5C8D2C54FFABFDF253583C2FFE8E" box="[608,623,269,287]" name="Tavera, Department of Geology and Geophysics" pageId="12" pageNumber="13">T</collectionCode>
