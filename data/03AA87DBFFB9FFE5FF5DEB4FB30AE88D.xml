<taxonomicName id="4C034D4EFFB9FFE5FCF0EB8FB57EEFED" authority="Schumacher, 1817" authorityName="Schumacher" authorityYear="1817" box="[826,1307,958,981]" class="Bivalvia" family="Periplomatidae" genus="Periploma" kingdom="Animalia" order="Anomalodesmata" pageId="0" pageNumber="65" phylum="Mollusca" rank="species" species="inaequivalvis">
