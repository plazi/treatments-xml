<taxonomicName id="04B2FEDDFFA0AC49E7E801D1FD964C81" box="[151,572,451,477]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="490" phylum="Arthropoda" rank="species" species="guariba" status="sp. nov." subGenus="Dactylotrichia">
<emphasis id="F1C6594CFFA0AC49E7E801D1FEBA4C81" bold="true" box="[151,272,451,477]" italics="true" pageId="6" pageNumber="490">Oxyethira</emphasis>
