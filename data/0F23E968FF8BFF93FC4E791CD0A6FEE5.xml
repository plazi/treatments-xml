<materialsCitation id="37E25223FF8BFF91FBEE79D9D7EBF87F" ID-GBIF-Occurrence="3396082305" pageId="12" pageNumber="655">
<collectionCode id="E19BC0BBFF8BFF91FBEE79D9D085F808" box="[1108,1200,1924,1948]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="12" pageNumber="655">MNHN</collectionCode>
