<taxonomicName id="68B89A90FFE2785D6B661EBBFB5B7CF3" authorityName="Nielson" authorityYear="2011" box="[1055,1170,1411,1435]" class="Insecta" family="Cicadellidae" genus="Gracilidia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="biflangata">
