<subSubSection id="E7A2B298FFE2785D6FBE1E67FB9F7C1E" box="[199,1110,1374,1399]" pageId="23" pageNumber="24" type="etymology">
<paragraph id="AF07E113FFE2785D6FBE1E67FB9F7C1E" blockId="23.[151,1436,618,1470]" box="[199,1110,1374,1399]" pageId="23" pageNumber="24">
<emphasis id="9DCC3D01FFE2785D6FBE1E67FE877C1F" bold="true" box="[199,334,1375,1399]" inLineHeading="true" pageId="23" pageNumber="24" reason="1">Etymology.</emphasis>
