material examined.
</emphasis>
<typeStatus id="54D2D9889310FF8AFDEB2892FD8EF823" box="[513,617,1927,1951]" pageId="0" pageNumber="60">Holotype</typeStatus>
male and 
<number id="3DA1750D9310FF8AFD022892FD11F823" box="[744,758,1927,1951]" pageId="0" pageNumber="60" value="1.0">1</number>
<typeStatus id="54D2D9889310FF8AFCEA289DFC87F81C" box="[768,864,1928,1952]" pageId="0" pageNumber="60">paratype</typeStatus>
male, 
<collectingCountry id="F37E27BA9310FF8AFC5E2892FC1DF823" box="[948,1018,1927,1951]" name="Nepal" pageId="0" pageNumber="60">Nepal</collectingCountry>
, Amlekhganj (in the Eastern Terai), 
<quantity id="4C91CACF9310FF8AFF7D28BFFF30F87E" box="[151,215,1962,1986]" metricMagnitude="2" metricUnit="m" metricValue="5.2" pageId="0" pageNumber="60" unit="m" value="520.0">520m</quantity>
at light, 
<date id="FFD741EA9310FF8AFED228BFFE38F87E" box="[312,479,1962,1987]" pageId="0" pageNumber="60" value="1957-08-06">
<number id="3DA1750D9310FF8AFED228BFFEA1F87E" box="[312,326,1962,1986]" pageId="0" pageNumber="60" value="6.0">6</number>
August 
<number id="3DA1750D9310FF8AFE4E28BFFE38F87E" box="[420,479,1962,1986]" pageId="0" pageNumber="60" value="1957.0">1957</number>
</date>
; allotype female, same locality, 
<date id="FFD741EA9310FF8AFCA928BFFC32F87E" box="[835,981,1962,1986]" pageId="0" pageNumber="60" value="1957-07-21">
<number id="3DA1750D9310FF8AFCA928BFFCB8F87E" box="[835,863,1962,1986]" pageId="0" pageNumber="60" value="21.0">21</number>
July 
