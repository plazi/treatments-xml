</taxonomicName>
(
<taxonomicName id="4C204D72FFB8FFEFFF77F9B0D5DBF9F5" box="[782,965,1539,1563]" class="Magnoliopsida" family="Elaeocarpaceae" kingdom="Plantae" order="Oxalidales" pageId="15" pageNumber="14" phylum="Tracheophyta" rank="family">Elaeocarpaceae</taxonomicName>
