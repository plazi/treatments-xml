</taxonomicName>
(
<taxonomicName id="4C204D72FFB8FFEFF8C1F9B0D32BF9F5" box="[1208,1333,1539,1563]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="14" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
