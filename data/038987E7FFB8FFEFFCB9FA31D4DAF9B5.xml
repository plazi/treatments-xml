<taxonomicName id="4C204D72FFB8FFEFFF9DF9B7D2B3F9F5" box="[996,1197,1539,1563]" class="Magnoliopsida" family="Myrtaceae" genus="Eugenia" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="14" phylum="Tracheophyta" rank="species" species="axillaris">
