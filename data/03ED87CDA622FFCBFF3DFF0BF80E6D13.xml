<taxonomicName id="4C444D58A622FFCCFE4BF817F87F6EB3" authority="Zhang" authorityName="Zhang" box="[481,773,1924,1946]" pageId="27" pageNumber="28" rank="species" species="pollicium" subGenus="Tripodura">
<taxonomicName id="4C444D58A622FFCCFE4BF817F9C76EB3" box="[481,701,1924,1946]" pageId="27" pageNumber="28" rank="species" species="pollicium" subGenus="Tripodura">
(
<emphasis id="B930EAC9A622FFCCFE43F817F92A6EB3" box="[489,592,1924,1946]" italics="true" pageId="27" pageNumber="28">Tripodura</emphasis>
) 
<taxonomicName id="4C444D58A622FFCCFDF4F817F9C76EB3" authority="Zhang" authorityName="Zhang" box="[606,701,1924,1946]" pageId="27" pageNumber="28" rank="species" species="pollicium" status="sp. n.">
