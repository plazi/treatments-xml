, 
<number id="3DE124F80D06FFE1BF62BABEFEC513BB" box="[281,333,1765,1789]" pageId="11" pageNumber="458" value="2.08">2.08</number>
, 
<number id="3DE124F80D06FFE1BF22BABFFE0413BB" box="[345,396,1764,1789]" pageId="11" pageNumber="458" value="1.25">1.25</number>
, 
<number id="3DE124F80D06FFE1BFE2BABFFE4713BB" box="[409,463,1764,1789]" pageId="11" pageNumber="458" value="0.62">0.62</number>
); II 
<number id="3DE124F80D06FFE1BF85BABEFDA713BB" box="[510,559,1765,1789]" pageId="11" pageNumber="458" value="4.87">4.87</number>
(
<number id="3DE124F80D06FFE1BC3BBABFFDFC13BB" box="[576,628,1764,1789]" pageId="11" pageNumber="458" value="1.36">1.36</number>
, 
<number id="3DE124F80D06FFE1BCFABABFFD3D13BB" box="[641,693,1764,1789]" pageId="11" pageNumber="458" value="1.69">1.69</number>
, 
<number id="3DE124F80D06FFE1BCBABABEFD7D13BB" box="[705,757,1765,1789]" pageId="11" pageNumber="458" value="1.12">1.12</number>
, 
<number id="3DE124F80D06FFE1BD7ABABEFCBE13BB" box="[769,822,1765,1789]" pageId="11" pageNumber="458" value="0.7">0.70</number>
); III 
<number id="3DE124F80D06FFE1BD14BABEFC2813BB" box="[879,928,1765,1789]" pageId="11" pageNumber="458" value="4.29">4.29</number>
(
<number id="3DE124F80D06FFE1BDCABABEFC6E13BB" box="[945,998,1765,1789]" pageId="11" pageNumber="458" value="1.19">1.19</number>
