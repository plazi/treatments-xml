<number id="3DE124F80D06FFE1BD89BABEFBAE13BB" box="[1010,1062,1765,1789]" pageId="11" pageNumber="458" value="1.29">1.29</number>
, 
<number id="3DE124F80D06FFE1BA49BABEFBEE13BB" box="[1074,1126,1765,1789]" pageId="11" pageNumber="458" value="1.07">1.07</number>
, 
<number id="3DE124F80D06FFE1BA09BABEFB2013BB" box="[1138,1192,1765,1789]" pageId="11" pageNumber="458" value="0.74">0.74</number>
); IV 
<number id="3DE124F80D06FFE1BA9ABABFFA9A13BB" box="[1249,1298,1764,1789]" pageId="11" pageNumber="458" value="6.04">6.04</number>
(
