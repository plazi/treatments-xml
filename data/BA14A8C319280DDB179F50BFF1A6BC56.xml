<taxonomicName id="0934C1ACF1D916037AD02A1F79016AB6" LSID-ZBK="urn:lsid:zoobank.org:act:D25A861B-8DAE-4D3E-93DE-9BBB19B28535" class="Actinopterygii" family="Pimelodidae" genus="Pseudoplatystoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pseudoplatystoma Bleeker 1862:10" order="Siluriformes" pageNumber="5" phylum="Chordata" rank="genus">Pseudoplatystoma</taxonomicName>
