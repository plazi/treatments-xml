, 
<collectingRegion id="49ADF8384852FF99FDF7FB90FD642C11" box="[554,671,1086,1111]" country="Brazil" name="Maranhao" pageId="14" pageNumber="415">Maranhão</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FD6CFB91FCD12C11" box="[689,810,1087,1111]" country="Venezuela" name="Amazonas" pageId="14" pageNumber="415">Amazonas</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FCE1FB90FC642C11" box="[828,927,1086,1111]" country="Brazil" name="Roraima" pageId="14" pageNumber="415">Roraima</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FC6CFB90FBDA2C11" box="[945,1057,1086,1111]" country="Brazil" name="Rondonia" pageId="14" pageNumber="415">Rondônia</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FBEEFB91FB342C11" box="[1075,1231,1087,1111]" country="Brazil" name="Mato Grosso" pageId="14" pageNumber="415">Mato Grosso</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FB3CFB90FAAB2C11" box="[1249,1360,1086,1111]" country="Brazil" name="Tocantins" pageId="14" pageNumber="415">Tocantins</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FABFFB90FA672C11" box="[1378,1436,1086,1111]" country="Brazil" name="Piaui" pageId="14" pageNumber="415">Piauí</collectingRegion>
(Amazonia/Cerrado regions), 
