, 
<collectingRegion id="49ADF8384852FF99FD8EFBCCFD192C3D" box="[595,738,1122,1147]" country="Brazil" name="Pernambuco" pageId="14" pageNumber="415">Pernambuco</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FD2CFBCCFCB42C3D" box="[753,847,1122,1147]" country="Brazil" name="Alagoas" pageId="14" pageNumber="415">Alagoas</collectingRegion>
, 
<collectingRegion id="49ADF8384852FF99FC83FBCCFC482C3D" box="[862,947,1122,1147]" country="Brazil" name="Sergipe" pageId="14" pageNumber="415">Sergipe</collectingRegion>
