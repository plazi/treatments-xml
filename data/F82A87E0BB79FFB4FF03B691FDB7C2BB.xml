<typeStatus id="AF388854BB79FFB4FF53B7B8FE9EC1F6" box="[199,258,1479,1503]" pageId="1" pageNumber="64">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="B7834D75BB79FFB4FEFDB7B9FDF2C1F7" box="[361,622,1478,1502]" class="Holothuroidea" family="Sclerodactylidae" genus="Thandarum" kingdom="Animalia" order="Dendrochirotida" pageId="1" pageNumber="64" phylum="Echinodermata" rank="species" species="hernandezi" status="sp. nov.">
<emphasis id="42F7EAE4BB79FFB4FEFDB7B9FDF2C1F7" box="[361,622,1478,1502]" italics="true" pageId="1" pageNumber="64">Thandarum hernandezi</emphasis>
</taxonomicName>
<emphasis id="42F7EAE4BB79FFB4FDE2B7B8FD53C1F6" bold="true" box="[630,719,1479,1503]" pageId="1" pageNumber="64">
