<number id="3DE54140FFBCFFBED95EF86A94FB768A" box="[876,929,1935,1958]" pageId="8" pageNumber="41" value="1906.0">1906</number>
(A–D) and 
<taxonomicName id="4C2D28E4FFBCFFBEDE10F875920A768A" authority="Zhang, 1995" authorityName="Zhang" authorityYear="1995" box="[1058,1360,1935,1959]" class="Insecta" family="Attelabidae" genus="Euscelophilus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="41" phylum="Arthropoda" rank="species" species="denticulatus">
