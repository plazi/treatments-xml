<taxonomicName id="4E30182E9621D81D2FBFE9103589AA53" authorityName="C. M. Xie &amp; Lu L. Zhang" authorityYear="2022" class="Lecanoromycetes" family="Lecideaceae" genus="Immersaria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Immersaria shangrilaensis" order="Lecideales" pageId="0" pageNumber="99" phylum="Ascomycota" rank="species" species="shangrilaensis">Immersaria shangrilaensis</taxonomicName>
