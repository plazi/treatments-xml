<taxonomicNameLabel id="A23657DAFFB98901FD60FC31CD2CFC32" box="[700,837,1000,1023]" pageId="86" pageNumber="87" rank="species">new comb.</taxonomicNameLabel>
; 
<taxonomicName id="4C714D30FFB98901FC89FC31CD6AFBD1" authority="(Fouquette, 1958)" authorityName="FAIVOVICH &amp; HADDAD &amp; GARCIA &amp; FROST &amp; CAMPBELL &amp; WHEELER" authorityYear="2005" baseAuthorityName="Fouquette" baseAuthorityYear="1958" class="Amphibia" family="Hylidae" genus="Hypsiboas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="rufitelus" status="new comb.">
