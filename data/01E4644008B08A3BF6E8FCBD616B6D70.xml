<taxonomicName id="235B16ED935857044213DF7C9CF124B1" LSID="http://zoobank.org/15DD642E-6A90-4721-9C59-065C4A71F7EC" class="Insecta" family="Dytiscidae" genus="Novadessus" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Novadessus" order="Coleoptera" pageId="0" pageNumber="125" phylum="Arthropoda" rank="genus">Novadessus</taxonomicName>
