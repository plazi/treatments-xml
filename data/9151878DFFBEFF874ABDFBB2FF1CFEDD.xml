<subSubSection id="51E26510FFB8FF874B80FF6DFF1CFEDD" pageId="13" pageNumber="2757" type="diagnosis">
<paragraph id="1947369BFFB8FF874B80FF6DFEF3FF2E" blockId="13.[160,1156,141,340]" box="[160,276,141,167]" pageId="13" pageNumber="2757">
