<paragraph id="8BD28858C319FFFAFF7CB85FFED7FEE8" blockId="8.[151,537,295,354]" box="[151,372,330,354]" pageId="8" pageNumber="27">
(
<figureCitation id="135694DDC319FFFAFF74B85FFF4DFEE8" box="[159,238,330,354]" captionStart="FIGURES 3 – 9" captionStartId="2.[151,264,1760,1783]" captionTargetBox="[163,1415,199,1730]" captionTargetId="figure@2.[151,1436,193,1739]" captionTargetPageId="2" captionText="FIGURES 3 – 9. Female Apocephalus species, oviscapes, dorsal." httpUri="https://zenodo.org/record/281918/files/figure.png" pageId="8" pageNumber="27">Figs. 9</figureCitation>
, 
<figureCitation id="135694DDC319FFFAFF11B85FFEBBFEE8" box="[250,280,330,354]" captionStart="FIGURES 10 – 16" captionStartId="4.[151,264,1734,1757]" captionTargetBox="[165,1398,196,1704]" captionTargetId="figure@4.[151,1436,193,1713]" captionTargetPageId="4" captionText="FIGURES 10 – 16. Female Apocephalus species, oviscapes, ventral." httpUri="https://zenodo.org/record/281919/files/figure.png" pageId="8" pageNumber="27">16</figureCitation>
, 
<figureCitation id="135694DDC319FFFAFECFB85FFEE1FEE8" box="[292,322,330,354]" captionStart="FIGURES 17 – 23" captionStartId="5.[151,264,1993,2016]" captionTargetBox="[176,1398,550,1935]" captionTargetId="figure@5.[168,1405,545,1952]" captionTargetPageId="5" captionText="FIGURES 17 – 23. Female Apocephalus species, tergite 6, dorsal." httpUri="https://zenodo.org/record/281920/files/figure.png" pageId="8" pageNumber="27">23</figureCitation>
, 
<figureCitation id="135694DDC319FFFAFEA5B85FFEC8FEE8" box="[334,363,330,354]" captionStart="FIGURES 24 – 29" captionStartId="6.[151,264,1272,1295]" captionTargetBox="[220,1327,203,1230]" captionTargetId="figure@6.[215,1371,193,1249]" captionTargetPageId="6" captionText="FIGURES 24 – 29. Female Apocephalus species, segment 6, ventral." httpUri="https://zenodo.org/record/281921/files/figure.png" pageId="8" pageNumber="27">29</figureCitation>
