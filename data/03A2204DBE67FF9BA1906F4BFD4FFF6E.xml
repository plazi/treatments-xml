</taxonomicName>
; 2, 
<taxonomicName id="4C0BEAD8BE66FF9CA3D46F6DFA88F9DE" authorityName="Lepeletier &amp; Audinet-Serville in Latreille" authorityYear="1828" box="[723,944,1619,1642]" class="Insecta" family="Cerambycidae" genus="Euryptera" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="40" phylum="Arthropoda" rank="species" species="latipennis">
