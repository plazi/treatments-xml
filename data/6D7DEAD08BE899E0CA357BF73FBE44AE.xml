in 
<taxonomicName id="04491A9888F58A8110CFF4EBFB8DBBD9" class="Magnoliopsida" family="Fabaceae" genus="Centrosema" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Centrosema virginianum" order="Fabales" pageId="5" pageNumber="50" phylum="Tracheophyta" rank="species" species="virginianum">Centrosema virginianum</taxonomicName>
L. (
<taxonomicName id="DE0B4CB91D6C04F5A61219E5FE88594E" genus="Fabaceae" lsidName="Fabaceae" pageId="5" pageNumber="50" rank="genus">Fabaceae</taxonomicName>
