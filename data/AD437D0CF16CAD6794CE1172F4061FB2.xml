. Indo-West Pacific: 
<collectingCountry id="5DFD8C8AF16CAD679667132BF42F1F66" box="[574,636,1825,1850]" name="Palau" pageId="24" pageNumber="325">Palau</collectingCountry>
and 
<collectingCountry id="5DFD8C8AF16CAD6796EA132BF5731F66" box="[691,800,1825,1850]" name="Indonesia" pageId="24" pageNumber="325">Indonesia</collectingCountry>
(new record) (
<bibRefCitation id="417BB1EBF16CAD67979F1328F2261F66" author="De" box="[966,1141,1826,1850]" pageId="24" pageNumber="325" refString="De Grave, S. (2004) A new species of Salmoneus (Crustacea: Decapoda: Alpheidae) from Palau. Bulletin de l'Institut Royal des Sciences de Belgique, 74, 41 - 48." type="journal article" year="2004">
