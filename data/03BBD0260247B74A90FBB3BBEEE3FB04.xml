<paragraph id="8BAD61300241B74A90ABB1C8EC7FFBB4" blockId="68.[151,1437,151,1184]" pageId="68" pageNumber="69">
<materialsCitation id="3B7A6B6D0241B74A90ABB1C8EC64FBB4" ID-GBIF-Occurrence="2237851673" collectingDate="1919-01-20" collectionCode="BMNH" collectorName="Type &amp; New Zeal. &amp; Broun Coll. Brit. Mus. &amp; Coll. Brit. Mus. &amp; Martinborough" pageId="68" pageNumber="69" specimenCount="2" specimenCount-male="2" typeStatus="paratype">
