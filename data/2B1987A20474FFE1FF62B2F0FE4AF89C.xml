</emphasis>
<typeStatus id="7C0B88160474FFE6FE2AB3ADFD90FE7C" box="[479,588,458,484]" pageId="27" pageNumber="28">Holotype</typeStatus>
male and 
<typeStatus id="7C0B88160474FFE6FD25B3ADFCF2FE7C" box="[720,814,458,484]" pageId="27" pageNumber="28">allotype</typeStatus>
female [
<collectionCode id="C5A1AE710474FFE6FC6EB3ADFC2CFE7C" box="[923,1008,458,484]" pageId="27" pageNumber="28">MLPA</collectionCode>
] under the same coverslip on slide labeled: 1. “ 
<taxonomicName id="64B04D370474FFE6FED2B395FD50FD94" authority="A. Ogl." authorityName="A. Ogl." authorityYear="1846" box="[295,652,498,524]" class="Insecta" family="Mymaridae" genus="Lymaenon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="granulosus">
