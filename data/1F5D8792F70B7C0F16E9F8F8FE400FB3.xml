<subSubSection id="DFEE650FF70B7C0F16E9F866FBA00DC3" lastPageId="4" lastPageNumber="5" pageId="3" pageNumber="4" type="etymology">
<paragraph id="974B3684F70B7C0816E9F866FBEF0ED6" blockId="3.[151,1037,1945,1972]" box="[151,1037,1945,1972]" pageId="3" pageNumber="4">
<emphasis id="A580EA96F70B7C0816E9F866FEC60ED1" bold="true" box="[151,292,1945,1971]" pageId="3" pageNumber="4">Etymology.</emphasis>
