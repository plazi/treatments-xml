<paragraph blockId="24.[151,379,1398,1424]" box="[151,379,1398,1424]" pageId="24" pageNumber="481">
<heading bold="true" box="[151,379,1398,1424]" fontSize="11" level="1" pageId="24" pageNumber="481" reason="1">
<emphasis bold="true" box="[151,379,1398,1424]" pageId="24" pageNumber="481">
<taxonomicName authorityName="Lehtinen" authorityYear="1967" box="[151,218,1398,1424]" class="Arachnida" family="Amaurobiidae" genus="Taira" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="481" phylum="Arthropoda" rank="genus">Taira</taxonomicName>
