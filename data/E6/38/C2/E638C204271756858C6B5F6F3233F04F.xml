<taxonomicName class="Arachnida" family="Oonopidae" genus="Opopaea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opopaea makadara" order="Araneae" pageId="0" pageNumber="255" phylum="Arthropoda" rank="species" species="makadara">
