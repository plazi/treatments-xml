3. 
<taxonomicName authorityName="Germar" authorityYear="1821" box="[180,379,1545,1571]" class="Insecta" family="Tettigometridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="473" phylum="Arthropoda" rank="family">Tettigometridae</taxonomicName>
