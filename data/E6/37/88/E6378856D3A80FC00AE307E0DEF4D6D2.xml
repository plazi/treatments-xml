<subSubSection type="nomenclature">
<paragraph pageNumber="243">
<taxonomicName authority="Balogh, 1943" authorityName="Balogh" authorityYear="1943" class="Arachnida" family="Peloppiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageNumber="243" phylum="Arthropoda" rank="family">
