<paragraph blockId="30.[593,994,550,577]" box="[593,994,550,577]" pageId="30" pageNumber="31">
<heading box="[593,994,550,577]" centered="true" fontSize="11" level="2" pageId="30" pageNumber="31" reason="2">
<emphasis bold="true" box="[593,994,550,577]" pageId="30" pageNumber="31">
<taxonomicName authority="Sychra" authorityName="Sychra" box="[593,819,550,577]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Psocodea" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="alophoixi">
