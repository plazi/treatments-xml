<taxonomicName authority="L." class="Mammalia" family="Tenrecidae" genus="Echinops" kingdom="Animalia" order="Afrosoricida" pageId="null" pageNumber="404" phylum="Chordata" rank="species" species="sphaerocephalus">
<pageBreakToken pageId="null" pageNumber="404">Echinops</pageBreakToken>
