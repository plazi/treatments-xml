<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Melilotus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Melilotus officinalis" order="Fabales" pageId="3" pageNumber="114" phylum="Tracheophyta" rank="species" species="officinalis">Melilotus officinalis</taxonomicName>
