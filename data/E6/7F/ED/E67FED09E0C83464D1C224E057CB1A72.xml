<taxonomicName class="Magnoliopsida" family="Brassicaceae" genus="Rorippa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rorippa officinale" order="Brassicales" pageId="3" pageNumber="114" phylum="Tracheophyta" rank="species" species="officinale">Rorippa officinale</taxonomicName>
