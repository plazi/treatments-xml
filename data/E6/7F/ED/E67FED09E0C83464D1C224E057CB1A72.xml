<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Lathyrus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lathyrus pratensis" order="Fabales" pageId="3" pageNumber="114" phylum="Tracheophyta" rank="species" species="pratensis">Lathyrus pratensis</taxonomicName>
