<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Galium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Galium verum" order="Gentianales" pageId="3" pageNumber="114" phylum="Tracheophyta" rank="species" species="verum">Galium verum</taxonomicName>
