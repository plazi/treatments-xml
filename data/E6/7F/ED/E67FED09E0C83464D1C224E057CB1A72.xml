<taxonomicName class="Magnoliopsida" family="Apiaceae" genus="Conium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Conium maculatum" order="Apiales" pageId="3" pageNumber="114" phylum="Tracheophyta" rank="species" species="maculatum">Conium maculatum</taxonomicName>
