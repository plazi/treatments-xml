<taxonomicName authorityName="Channabasavanna" authorityYear="1966" box="[340,501,1286,1311]" class="Arachnida" family="Eriophyidae" genus="Aceria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="cajani">
