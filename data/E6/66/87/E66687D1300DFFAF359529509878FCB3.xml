<emphasis box="[512,597,868,893]" italics="true" pageId="36" pageNumber="37">
<taxonomicName authorityName="Keifer" authorityYear="1944" box="[512,594,868,893]" class="Arachnida" family="Eriophyidae" genus="Aceria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">Aceria</taxonomicName>
