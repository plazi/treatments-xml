<taxonomicName authorityName="Olmi" authorityYear="2017" box="[1181,1279,1241,1264]" class="Insecta" family="Embolemidae" genus="Ampulicomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="247" phylum="Arthropoda" rank="species" species="viator">
