<taxonomicName authority="Horvath, 1909" authorityName="Horvath" authorityYear="1909" box="[409,769,972,994]" class="Insecta" family="Reduviidae" genus="Sirthenea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="laevicollis">
<emphasis box="[409,616,972,994]" italics="true" pageId="64" pageNumber="65">Sirthenea laevicollis</emphasis>
<bibRefCitation author="Horvath, G." box="[622,769,972,994]" pageId="64" pageNumber="65" pagination="356 - 369" refId="ref44410" refString="Horvath, G. (1909) Species generis Reduviidarum Sirthenea Spinola. Annales Musei Nationalis Hungarici, 7, 356 - 369." type="journal article" year="1909">Horváth, 1909</bibRefCitation>
</taxonomicName>
(red circles) and 
<taxonomicName authority="(Stal, 1866)" authorityName="Stal" authorityYear="1866" baseAuthorityName="Stal" baseAuthorityYear="1866" box="[946,1256,972,994]" class="Insecta" family="Reduviidae" genus="Sirthenea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="obscura">
