<caption httpUri="https://zenodo.org/record/2609615/files/figure.png" pageId="53" pageNumber="54" startId="53.[151,264,661,684]" targetBox="[179,1393,218,610]" targetPageId="53">
<paragraph blockId="53.[151,1436,661,714]" pageId="53" pageNumber="54">
<emphasis bold="true" box="[151,368,661,684]" pageId="53" pageNumber="54">FIGURES 100–102.</emphasis>
<taxonomicName authority="(Miller, 1958)" authorityName="Miller" authorityYear="1958" baseAuthorityName="Miller" baseAuthorityYear="1958" box="[376,755,661,684]" class="Insecta" family="Reduviidae" genus="Sirthenea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="nigronitens">
