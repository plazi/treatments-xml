<taxonomicName authorityName="Böhlke" authorityYear="1949" box="[1302,1436,397,418]" class="Actinopterygii" family="Congridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="1" pageNumber="79" phylum="Chordata" rank="subFamily" subFamily="Bathymyrinae">Bathymyrinae</taxonomicName>
