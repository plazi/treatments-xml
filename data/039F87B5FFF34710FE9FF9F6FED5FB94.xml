</taxonomicName>
(
<taxonomicName id="4C364D20FFFC4710FCD3FD37FC36FCAC" box="[860,1020,751,775]" class="Magnoliopsida" family="Cucurbitaceae" kingdom="Plantae" order="Cucurbitales" pageId="56" pageNumber="155" phylum="Tracheophyta" rank="family">Cucurbitaceae</taxonomicName>
