<taxonomicName id="4C364D20FFFC4710FC84FD29FC85FCA3" box="[779,847,753,776]" class="Magnoliopsida" family="Cordiaceae" genus="Cordia" kingdom="Plantae" order="Boraginales" pageId="56" pageNumber="155" phylum="Tracheophyta" rank="genus">
