<taxonomicName id="4C314D2ADE717A04FD8A0DA939EBFB0B" authorityName="Mugrabi &amp; Azevedo" authorityYear="2016" box="[618,679,1206,1230]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="81" pageNumber="532" phylum="Arthropoda" rank="species" species="plaua">
