(
<typeStatus id="54AB887CFFD3FFB0FB8C7044FB9E576F" box="[1025,1127,1324,1349]" pageId="33" pageNumber="34">holotype</typeStatus>
, card)—head dorsal width, 
<number id="3DD824F9FFD3FFB0FF1A7038FF4F5742" box="[151,182,1360,1384]" pageId="33" pageNumber="34" value="31.0">31</number>
; frontovertex width, 
<number id="3DD824F9FFD3FFB0FE2C7038FE485742" box="[417,433,1360,1384]" pageId="33" pageNumber="34" value="9.0">9</number>
; head frontal height, 
<number id="3DD824F9FFD3FFB0FD127038FD475742" box="[671,702,1360,1384]" pageId="33" pageNumber="34" value="28.0">28</number>
; eye height, 
<number id="3DD824F9FFD3FFB0FCC17038FC925742" box="[844,875,1360,1384]" pageId="33" pageNumber="34" value="22.0">22</number>
; malar space, 
<number id="3DD824F9FFD3FFB0FB847038FBE05742" box="[1033,1049,1360,1384]" pageId="33" pageNumber="34" value="8.0">8</number>
