<taxonomicName id="0EC84D78EB7DFFDBFED84C738A9DFA23" authorityName="Metcalf" authorityYear="1946" box="[360,744,1828,1870]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="143" phylum="Arthropoda" rank="species" species="fuscovitta" status="new species">Empoasca fuscovitta</taxonomicName>
