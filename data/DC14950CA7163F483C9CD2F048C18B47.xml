(
<figureCitation id="CC86389FA7163F4A3C94D2AF48618BA2" box="[159,265,1217,1242]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURE 3" captionStartId-0="2.[151,250,1919,1941]" captionStartId-1="3.[151,250,894,916]" captionStartId-2="4.[151,250,1902,1924]" captionTargetBox-0="[303,1270,234,1881]" captionTargetBox-1="[298,1247,159,872]" captionTargetBox-2="[228,1339,290,1863]" captionTargetId-0="figure@2.[226,1360,193,1898]" captionTargetId-1="figure@3.[298,1247,159,872]" captionTargetId-2="figure@4.[212,1374,268,1880]" captionTargetPageId-0="2" captionTargetPageId-1="3" captionTargetPageId-2="4" captionText-0="FIGURE 1. Malostenopsocus lacteus sp. nov., holotype female. a. habitus; b. head, frontal view; c. head, dorsal view; d. head, lateral view. Scale bar = 0.5 mm." captionText-1="FIGURE 2. Forewing basal part of Malostenopsocus lacteus sp. nov., holotype female. Scale bar = 0.5 mm." captionText-2="FIGURE 3. Malostenopsocus lacteus sp. nov., holotype female. a. genitalia; b. epiproct, c. subgenital plate; d. gonapophyses; e. spermathecal sac. ep: epiproct, pp: paraproct, sp: subgenital plate, dv: dorsal valve, ev: external valve, vv: ventral valve. Scale bar = 0.05 mm." httpUri-0="https://zenodo.org/record/400222/files/figure.png" httpUri-1="https://zenodo.org/record/400223/files/figure.png" httpUri-2="https://zenodo.org/record/400224/files/figure.png" pageId="1" pageNumber="590">Figs. 1–3</figureCitation>
)
