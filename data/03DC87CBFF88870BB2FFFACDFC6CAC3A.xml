<taxonomicName id="4C754D5EFF88870BB275FA74FCDAAFFA" box="[324,403,1509,1532]" class="Magnoliopsida" family="Amaranthaceae" genus="Suaeda" kingdom="Plantae" order="Caryophyllales" pageId="22" pageNumber="391" phylum="Tracheophyta" rank="genus">
