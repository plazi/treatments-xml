<taxonomicName id="4C754D5EFF88870BB0BDFA74F94EAFFA" box="[908,1031,1509,1532]" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="22" pageNumber="391" phylum="Tracheophyta" rank="genus">
