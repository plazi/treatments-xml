<quantity id="4CD39B07FFDA1A0CCB1237C6401FF952" box="[862,1029,1007,1031]" metricMagnitude="-3" metricUnit="m" metricValue="4.775" metricValueMax="5.2" metricValueMin="4.35" pageId="20" pageNumber="21" unit="mm" value="4.775" valueMax="5.2" valueMin="4.35">4.35–5.20 mm</quantity>
