<quantity id="4CD39B07FFDA1A0CCAF8303D4740F979" box="[692,858,1044,1068]" metricMagnitude="-4" metricUnit="m" metricValue="3.55" metricValueMax="3.8" metricValueMin="3.3" pageId="20" pageNumber="21" unit="mm" value="0.355" valueMax="0.38" valueMin="0.33">0.33–0.38 mm</quantity>
