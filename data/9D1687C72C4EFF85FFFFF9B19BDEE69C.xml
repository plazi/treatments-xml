<taxonomicName id="D2BF4D522C41FF85FC8FFEAB9D8DE566" box="[999,1182,301,326]" class="Insecta" family="Mutillidae" genus="Photomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="66" phylum="Arthropoda" rank="species" species="myrmicoides">
