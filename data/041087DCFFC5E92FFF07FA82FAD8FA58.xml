<taxonomicName id="4BB94D49FFC5E92DFF07FA82FDB5FADE" authority="Lopes &amp; Menezes" authorityName="Lopes &amp; Menezes" authorityYear="2017" box="[151,595,1306,1333]" class="Insecta" family="Vespidae" genus="Synoeca" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="446" phylum="Arthropoda" rank="species" species="ilheensis" status="sp. nov.">
<emphasis id="BECDEAD8FFC5E92DFF07FA82FE8BFADF" bold="true" box="[151,365,1306,1332]" italics="true" pageId="1" pageNumber="446">Synoeca ilheensis</emphasis>
