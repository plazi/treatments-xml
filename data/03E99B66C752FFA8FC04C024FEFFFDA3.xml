<collectingCountry id="F3576AE0C751FFA8FEB8C481FE9CFE43" box="[261,325,465,488]" name="China" pageId="2" pageNumber="378">China</collectingCountry>
(
<collectingRegion id="4984E492C751FFA8FEE0C481FE77FE43" box="[349,430,465,488]" country="China" name="Hainan" pageId="2" pageNumber="378">Hainan</collectingRegion>
) and 
<collectingCountry id="F3576AE0C751FFA8FE43C481FD87FE43" box="[510,606,465,488]" name="Thailand" pageId="2" pageNumber="378">Thailand</collectingCountry>
(
<collectingRegion id="4984E492C751FFA8FDCBC481FD75FE43" box="[630,684,465,488]" country="Thailand" name="Loei" pageId="2" pageNumber="378">Loei</collectingRegion>
, 
<collectingRegion id="4984E492C751FFA8FD7DC481FD36FE43" box="[704,751,465,488]" country="Thailand" name="Nan" pageId="2" pageNumber="378">Nan</collectingRegion>
, 
<collectingRegion id="4984E492C751FFA8FF2AC4A1FEC3FDA3" box="[151,282,497,520]" country="Thailand" name="Chiang Mai" pageId="2" pageNumber="378">Chiang Mai</collectingRegion>
).
</paragraph>
