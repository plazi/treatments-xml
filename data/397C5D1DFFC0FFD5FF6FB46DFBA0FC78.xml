<quantity id="762D41EEFFC3FFD5FCB5B2A8FC64FC78" box="[845,1021,978,1005]" metricMagnitude="3" metricUnit="m" metricValue="3.025" metricValueMax="3.65" metricValueMin="2.4" pageId="10" pageNumber="11" unit="m" value="3025.0" valueMax="3650.0" valueMin="2400.0">2400 to 3650 m</quantity>
