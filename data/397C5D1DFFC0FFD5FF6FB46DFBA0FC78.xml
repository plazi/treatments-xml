<collectingCountry id="C9C2AC9BFFC3FFD5FBE3B2F0FBCCFC30" box="[1051,1109,907,933]" name="India" pageId="10" pageNumber="11">India</collectingCountry>
(
<collectingRegion id="731122E9FFC3FFD5FB9CB2F0FB6CFC33" box="[1124,1269,907,934]" country="India" name="Uttarakhand" pageId="10" pageNumber="11">Uttarakhand</collectingRegion>
