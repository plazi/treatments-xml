<number id="3DC793267702FF951A2BD42AEEB89D21" box="[1345,1432,296,320]" pageId="6" pageNumber="571" value="3.3499999999999996" valueMax="3.4" valueMin="3.3">3.3–3.4</number>
, female 
<number id="3DC793267702FF951F80D44EEA309D05" box="[234,272,332,356]" pageId="6" pageNumber="571" value="4.1">4.1</number>
; length of tegmen: male 
<number id="3DC793267702FF951D4CD44EE95C9D04" box="[550,636,332,357]" pageId="6" pageNumber="571" value="2.85" valueMax="3.1" valueMin="2.6">2.6–3.1</number>
, female 
<number id="3DC793267702FF951DB1D44EE8219D05" box="[731,769,332,356]" pageId="6" pageNumber="571" value="3.1">3.1</number>
; length of hind femur: male 
<number id="3DC793267702FF951B2AD44EEF859D05" box="[1088,1189,332,356]" pageId="6" pageNumber="571" value="9.95" valueMax="10.2" valueMin="9.7">9.7–10.2</number>
