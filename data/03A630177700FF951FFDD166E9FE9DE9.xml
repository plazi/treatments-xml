<number id="3DC793267702FF951D8CD425E8799D21" box="[742,857,295,320]" pageId="6" pageNumber="571" value="17.9" valueMax="18.2" valueMin="17.6">17.6–18.2</number>
, female 
<number id="3DC793267702FF951CA8D42AE8D69D21" box="[962,1014,296,320]" pageId="6" pageNumber="571" value="21.2">21.2</number>
