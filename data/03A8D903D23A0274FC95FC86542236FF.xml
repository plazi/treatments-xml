= DSM 15502 
<superScript id="7C74C55DD23A0274FB61FA9855D03664" attach="left" box="[1224,1234,1388,1402]" fontSize="6" pageId="53" pageNumber="2835">T</superScript>
= JCM 11969 
<superScript id="7C74C55DD23A0274FACFFA98546C3664" attach="none" box="[1382,1390,1388,1402]" fontSize="6" pageId="53" pageNumber="2835">T</superScript>
