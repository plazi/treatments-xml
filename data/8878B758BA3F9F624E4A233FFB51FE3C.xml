<location id="050E5095BA3F9F614E6525E1FC32F9C5" LSID="urn:lsid:plazi:treatment:8878B758BA3F9F624E4A233FFB51FE3C:050E5095BA3F9F614E6525E1FC32F9C5" box="[566,971,1516,1542]" country="Australia" name="Senckenberg Institute in Frankfurt" pageId="73" pageNumber="74" stateProvince="Victoria">Senckenberg Institute in Frankfurt</location>
, 
<collectingCountry id="78C646DEBA3F9F614F8425E0FBBDF9C4" box="[983,1092,1517,1543]" name="Germany" pageId="73" pageNumber="74">Germany</collectingCountry>
(
<specimenCode id="5077AE35BA3F9F61480025E0FAF7F9C4" box="[1107,1294,1517,1543]" pageId="73" pageNumber="74">SMF XXX 871</specimenCode>
