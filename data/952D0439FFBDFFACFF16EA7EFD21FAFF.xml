<quantity id="DA7C18CAFFBDFFACFD95ED46FD46FBC8" box="[625,700,1118,1141]" metricMagnitude="-2" metricUnit="m" metricValue="4.3" pageId="55" pageNumber="286" unit="mm" value="43.0">43 mm</quantity>
