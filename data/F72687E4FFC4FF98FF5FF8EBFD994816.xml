<emphasis id="4DFBEAE0FFC4FF9EFF5FF8B3FF154BA9" bold="true" box="[151,227,1905,1930]" pageId="83" pageNumber="84">
<typeStatus id="A0348850FFC4FF9EFF5FF8B3FF294BA9" box="[151,223,1905,1930]" pageId="83" pageNumber="84">Types</typeStatus>
.
</emphasis>
<typeStatus id="A0348850FFC4FF9EFF22F8B3FEA44BA9" box="[234,338,1905,1930]" pageId="83" pageNumber="84">Holotype</typeStatus>
female (
<collectionCode id="199EAE37FFC4FF9EFE7BF8B0FDF84BA9" box="[435,526,1906,1930]" country="Namibia" httpUri="http://grbio.org/cool/s32g-pczn" name="State Museum" pageId="83" pageNumber="84">SMWN</collectionCode>
) labelled “Hunkab 
<collectingRegion id="BD4BF810FFC4FF9EFD2FF8B3FCD14BA9" box="[743,807,1905,1930]" country="Nigeria" name="Rivers" pageId="83" pageNumber="84">River</collectingRegion>
, at 
