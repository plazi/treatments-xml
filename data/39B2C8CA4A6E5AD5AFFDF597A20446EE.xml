<taxonomicName id="352FE3815971A7C03F1DF794325AC365" authorityName="Zicsi" authorityYear="1996" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Haplotaxida" pageId="25" pageNumber="26" phylum="Annelida" rank="subSpecies" species="stuhlmanni" subSpecies="morogoronensis">
<emphasis id="023FCD45E4D96C688BB997E469C6ED97" italics="true" pageId="25" pageNumber="26">Glyphidrilus stuhlmanni morogoronensis</emphasis>
</taxonomicName>
