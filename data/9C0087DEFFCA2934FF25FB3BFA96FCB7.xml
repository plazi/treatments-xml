<taxonomicName id="D3A94D4BFFCB2933FAADFAD4FEE7FAF7" authorityName="Porch &amp; Smith &amp; Greig" authorityYear="2020" class="Insecta" family="Zopheridae" genus="Pycnomerus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="242" phylum="Arthropoda" rank="species" species="taralewisae">
<emphasis id="26DDEADAFFCB2933FAADFAD4FEE7FAF7" italics="true" pageId="3" pageNumber="242">Pycnomerus taralewisae</emphasis>
