<pageBreakToken id="67C6F56A3C1C7A703B77D42483B205A9" pageId="null" pageNumber="412" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="F5C387CC1D63AD62062783F0314F16E2" authority="Anderson" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="412" phylum="Tracheophyta" rank="species" species="suberectus">
Rubus 
<normalizedToken id="667B114BF2CCB5CEEE432652DA25C567" originalValue="suberéctus" pageId="null" pageNumber="412">suberectus</normalizedToken>
Anderson
</taxonomicName>
