<taxonomicName id="7AE14D6E105FFFC9F1DDFA9AB50AFABF" authority="Prena" authorityName="palliolatrix Prena" authorityYear="2006" box="[481,829,1284,1310]" class="Insecta" family="Curculionidae" genus="Palliolatrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="18" phylum="Arthropoda" rank="species" species="palliolatrix" status="sp. n.">
