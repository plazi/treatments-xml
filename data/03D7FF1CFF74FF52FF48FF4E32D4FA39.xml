<taxonomicName id="4C7E3589FF74FF52FA9CFE1C340AFA79" authorityName="Dechambre" authorityYear="1985" box="[1300,1398,1393,1417]" class="Insecta" family="Dynastidae" genus="Stenocrates" kingdom="Animalia" order="Coleoptera" pageId="191" pageNumber="190" phylum="Arthropoda" rank="species" species="bollei">
