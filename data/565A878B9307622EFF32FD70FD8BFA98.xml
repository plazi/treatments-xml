<taxonomicName id="19F34D1E9307622EFF5BFC00FE77FC30" baseAuthorityName="Melander" baseAuthorityYear="1902" box="[254,510,908,932]" class="Insecta" family="Mythicomyiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="family">MYTHICOMYIIDAE</taxonomicName>
