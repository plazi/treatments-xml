(
<figureCitation id="13462A30FFB737607DBB491A4259FDC1" box="[159,258,597,623]" captionStart="FIGURES 1–12" captionStartId="4.[151,271,1941,1965]" captionTargetBox="[315,1248,197,1922]" captionTargetId="figure-90@4.[311,1253,183,1922]" captionTargetPageId="4" captionText="FIGURES 1–12. Rhynchortalia spp., dorsal habitus: 1. R. australis sp. n.; 2. R. viridis sp. n.; 3. R. wau sp. n.; 4. R. papuana sp. n.; 5. R. iongai sp. n.; 6. R. moresby sp. n.; 7. R. purpurea sp. n.; 8. R.similis sp. n.; 9. R. anupama sp. n.; 10. R. riedeli sp. n.; 11. R. insueta Crotch; 12. R. iba sp. n." figureDoi="http://doi.org/10.5281/zenodo.5309135" httpUri="https://zenodo.org/record/5309135/files/figure.png" pageId="13" pageNumber="38">Figs. 10</figureCitation>
, 
<figureCitation id="13462A30FFB737607C2A491A4275FDC1" box="[270,302,597,623]" captionStart="FIGURES 18–29" captionStartId="7.[151,269,1353,1377]" captionTargetBox="[159,1436,200,1330]" captionTargetId="figure-295@7.[151,1436,197,1330]" captionTargetPageId="7" captionText="FIGURES 18–29. Male terminal abdominal ventrites: 18. R. australis; 19. R. viridis; 20. R. wau; 21. R. papuana; 22. R. iongai; 23. R. iba; 24. R. similis; 25. R. purpurea; 26. R. riedeli; 27. R. insueta; 28. R. anupama; 29. R. moresby." figureDoi="http://doi.org/10.5281/zenodo.5309139" httpUri="https://zenodo.org/record/5309139/files/figure.png" pageId="13" pageNumber="38">26</figureCitation>
, 
<figureCitation id="13462A30FFB737607C1E491A42DEFDC1" box="[314,389,597,623]" captionStart="FIGURES 57–76" captionStartId="14.[151,269,1881,1905]" captionTargetBox="[391,1187,191,1861]" captionTargetId="figure-102@14.[383,1213,183,1866]" captionTargetPageId="14" captionText="FIGURES 57–76. Male genitalia: 57–60. R. riedeli: 57. Tegmen, lateral view; 58. Tegmen, ventral view; 59. Penis; 60. Penis apex; 61–64. R. iba: 61. Tegmen, ventral view; 62. Tegmen, lateral view; 63. Penis; 64. Penis apex; 65–67. R. anupama: 65. Tegmen, ventral view; 66. Penis; 67. Penis apex; 68–72. R. insueta: 68. Tegmen, lateral view; 69. Tegmen, ventral view; 70, 71. Penis; 72. Penis apex; 73–76. R. moresby: 73. Tegmen, lateral view; 74. Tegmen, ventral view; 75. Penis; 76. Penis apex." figureDoi="http://doi.org/10.5281/zenodo.5309145" httpUri="https://zenodo.org/record/5309145/files/figure.png" pageId="13" pageNumber="38">57–60</figureCitation>
