<taxonomicName id="4C524D76FF8BFFA83F01F9C4FAA67AEB" authority="Gray, 1866b" authorityName="Gray" authorityYear="1866" box="[1145,1357,1662,1687]" class="Insecta" family="Tachinidae" genus="Pales" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FF8BFFA83F01F9C4FB537AEB" box="[1145,1208,1662,1687]" italics="true" pageId="43" pageNumber="44">Pales</emphasis>
<bibRefCitation id="EFC34B04FF8BFFA83FC7F9C5FAA67AEB" author="Gray, J. E." box="[1215,1357,1662,1687]" journalOrPublisher="Proceedings of the Meetings of the Zoological Society of London" pageId="43" pageNumber="44" pagination="233 - 240" refId="ref56124" refString="Gray, J. E. (1866 b) Notes on Zoanthinae, with descriptions of some new genera. Proceedings of the Meetings of the Zoological Society of London, 1866, 233 - 240. [September] [Dated from Duncan (1937).]" title="Notes on Zoanthinae, with descriptions of some new genera" type="book chapter" year="1866">Gray, 1866b</bibRefCitation>
</taxonomicName>
; junior synonym of 
<taxonomicName id="4C524D76FF8BFFA83A29F918FD6A7AC7" authority="Meigen, 1803" authorityName="Meigen" authorityYear="1803" box="[337,641,1698,1723]" class="Insecta" family="Tipulidae" genus="Nephrotoma" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAE7FF8BFFA83A29F918FE367AC7" box="[337,477,1698,1723]" italics="true" pageId="43" pageNumber="44">Nephrotoma</emphasis>
<bibRefCitation id="EFC34B04FF8BFFA83A9AF918FD6A7AC7" author="Meigen, J. W." box="[482,641,1698,1723]" journalOrPublisher="Magazin fur Insektenkunde" pageId="43" pageNumber="44" pagination="259 - 281" part="2" refId="ref61130" refString="Meigen, J. W. (1803) Versuch einer neuen Gattungs-Eintheilung der europaischen zweiflugligen Insekten. Magazin fur Insektenkunde, 2, 259 - 281. [after 17 May] [Dated from Thompson et al. (1999: 476).]" title="Versuch einer neuen Gattungs-Eintheilung der europaischen zweiflugligen Insekten" type="journal article" year="1803">Meigen, 1803</bibRefCitation>
</taxonomicName>
. 
<emphasis id="B926EAE7FF8BFFA839F4F918FCAF7AC7" bold="true" box="[652,836,1698,1723]" pageId="43" pageNumber="44">New Synonymy</emphasis>
.
</paragraph>
</subSubSection>
