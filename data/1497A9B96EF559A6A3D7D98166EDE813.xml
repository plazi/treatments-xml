<taxonomicName id="E32F22BF1F207213E2A31A8E2169729D" class="Magnoliopsida" family="Euphorbiaceae" genus="Croton" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Croton manampetsae subsp. var. var. lepidotus" order="Malpighiales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="variety" species="manampetsae" subSpecies="var." variety="lepidotus">Croton manampetsae var. lepidotus</taxonomicName>
