<taxonomicName id="4C074D158E16FF97FC8FEA5AFBC4BF36" authorityName="Spence Bate" authorityYear="1888" box="[882,1078,1807,1832]" class="Malacostraca" family="Acanthephyridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="16" pageNumber="217" phylum="Arthropoda" rank="family">Acanthephyridae</taxonomicName>
