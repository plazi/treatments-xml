<taxonomicName id="27CA4D35FE90FEF65B74FDCAFB986318" authorityName="Rondani" authorityYear="1861" box="[874,1067,583,609]" class="Insecta" family="Tachinidae" genus="Prosena" kingdom="Animalia" order="Diptera" pageId="349" pageNumber="350" phylum="Arthropoda" rank="species" species="sybarita">
<emphasis id="D2BEEAA4FE90FEF65B74FDCAFB986318" box="[874,1067,583,609]" italics="true" pageId="349" pageNumber="350">Prosena sybarita</emphasis>
</taxonomicName>
for 
<taxonomicName id="27CA4D35FE90FEF65C41FDCAFEBB63FC" authority="(Fabricius, 1775)" authorityName="Herting &amp; Dely-Draskovits" authorityYear="1993" baseAuthorityName="Fabricius" baseAuthorityYear="1775" class="Insecta" family="Tachinidae" genus="Prosena" kingdom="Animalia" order="Diptera" pageId="349" pageNumber="350" phylum="Arthropoda" rank="species" species="siberita">
<emphasis id="D2BEEAA4FE90FEF65C41FDCAFAA86318" box="[1119,1307,583,609]" italics="true" pageId="349" pageNumber="350">Prosena siberita</emphasis>
(
<bibRefCitation id="845B4B47FE90FEF65D35FDCAFEB363FC" author="Fabricius, J. C." pageId="349" pageNumber="350" refId="ref350134" refString="Fabricius, J. C. (1775) Systema entomologiae, sistens insectorum classes, ordines, genera, species, adiectis synonymis, locis, descriptionibus, observationibus. Officina Libraria Kortii, Flensburgi et Lipsiae [= Flensburg &amp; Leipzig], [xiv] + 832 pp. https: // doi. org / 10.5962 / bhl. title. 36510" type="book" year="1775">Fabricius, 1775</bibRefCitation>
)
</taxonomicName>
in reference to the locality “Sybaris” [= Sibari, 
<collectingCountry id="98DD7626FE90FEF65B3DFDE7FCE863FC" box="[803,859,618,645]" name="Italy" pageId="349" pageNumber="350">Italy</collectingCountry>
], which was not reported in the original description by 
<bibRefCitation id="845B4B47FE90FEF658F3FD02FE5A63D0" author="Fabricius, J. C." box="[237,489,654,681]" pageId="349" pageNumber="350" refId="ref350134" refString="Fabricius, J. C. (1775) Systema entomologiae, sistens insectorum classes, ordines, genera, species, adiectis synonymis, locis, descriptionibus, observationibus. Officina Libraria Kortii, Flensburgi et Lipsiae [= Flensburg &amp; Leipzig], [xiv] + 832 pp. https: // doi. org / 10.5962 / bhl. title. 36510" type="book" year="1775">Fabricius (1775: 798)</bibRefCitation>
. 
<bibRefCitation id="845B4B47FE90FEF659E8FD02FD5B63D0" author="Pandelle, L." box="[502,744,654,681]" pageId="349" pageNumber="350" pagination="221 - 230" refId="ref362363" refString="Pandelle, L. (1896) Etude sur le Muscides de France. IIIe partie. Revue d'Entomologie, 15, 1 - 219, 221 - 230." type="journal article" year="1896">Pandellé (1896: 161)</bibRefCitation>
considered 
<taxonomicName id="27CA4D35FE90FEF65B6AFD02FB8563D0" authorityName="Rondani" authorityYear="1861" box="[884,1078,655,681]" class="Insecta" family="Tachinidae" genus="Prosena" kingdom="Animalia" order="Diptera" pageId="349" pageNumber="350" phylum="Arthropoda" rank="species" species="sybarita">
<emphasis id="D2BEEAA4FE90FEF65B6AFD02FB8563D0" box="[884,1078,655,681]" italics="true" pageId="349" pageNumber="350">Prosena sybarita</emphasis>
</taxonomicName>
