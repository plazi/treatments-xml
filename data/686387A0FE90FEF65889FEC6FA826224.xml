<taxonomicName id="27CA4D35FE90FEF65914FD76FE7A626C" baseAuthorityName="Fabricius" baseAuthorityYear="1775" box="[266,457,763,789]" class="Insecta" family="Tachinidae" genus="Prosena" kingdom="Animalia" order="Diptera" pageId="349" pageNumber="350" phylum="Arthropoda" rank="species" species="sybarita">
