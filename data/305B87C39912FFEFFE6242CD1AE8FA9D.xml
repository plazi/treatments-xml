<quantity id="7F0A9B309912FFEFFD2940831C18FD00" box="[726,864,665,685]" metricMagnitude="-3" metricUnit="m" metricValue="8.95" metricValueMax="10.4" metricValueMin="7.5" pageId="3" pageNumber="298" unit="mm" value="8.95" valueMax="10.4" valueMin="7.5">7.5–10.4 mm</quantity>
