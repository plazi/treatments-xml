<quantity id="7F0A9B309912FFEFFD3241BB1C1BFC19" box="[717,867,929,948]" metricMagnitude="-2" metricUnit="m" metricValue="1.235" metricValueMax="1.43" metricValueMin="1.04" pageId="3" pageNumber="298" unit="mm" value="12.35" valueMax="14.3" valueMin="10.4">10.4–14.3 mm</quantity>
