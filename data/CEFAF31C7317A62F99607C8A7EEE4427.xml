<taxonomicName id="A46B5F6390F6D8904261B334CF2019C5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26917" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus maculatus (Fabricius)" order="Hymenoptera" pageNumber="213" phylum="Arthropoda" rank="species" species="maculatus">Camponotus maculatus</taxonomicName>
