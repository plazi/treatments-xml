<taxonomicName id="4C184D24B716FFA1269E30AFFDF52615" authorityName="Cook, 1897 sensu Hoffman" authorityYear="1980" box="[446,608,1772,1798]" class="Diplopoda" family="Pachybolidae" kingdom="Animalia" order="Spirobolida" pageId="2" pageNumber="109" phylum="Arthropoda" rank="family">Pachybolidae</taxonomicName>
. The closest relative of 
<taxonomicName id="4C184D24B716FFA1245530AFFBD42615" authority="Wesener, 2020" authorityName="Wesener" authorityYear="2020" box="[885,1089,1772,1798]" class="Diplopoda" family="Pachybolidae" genus="Dichromatobolus" kingdom="Animalia" order="Spirobolida" pageId="2" pageNumber="109" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B96CEAB5B716FFA1245530AFFBD42615" box="[885,1089,1772,1798]" italics="true" pageId="2" pageNumber="109">Dichromatobolus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25F57CEB716FFA1236930AEFB262614" box="[1097,1203,1773,1799]" pageId="2" pageNumber="109" rank="genus">gen. nov.</taxonomicNameLabel>
