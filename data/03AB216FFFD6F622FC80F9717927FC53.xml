<taxonomicName id="4C02EBFAFFD5F623FEF0F9197D9DF961" authorityName="VERON &amp; WALLACE" authorityYear="1984" box="[272,459,1736,1756]" class="Anthozoa" family="Acroporidae" genus="Acropora" kingdom="Animalia" order="Scleractinia" pageId="58" pageNumber="735" phylum="Cnidaria" rank="species" species="kirstyae">
