species: 
<taxonomicName id="4C654D052752AA086D00FAC36D71FAB7" authority="Santos, 1961" authorityName="Santos" authorityYear="1961" box="[294,637,1280,1304]" class="Insecta" family="Coenagrionidae" genus="Kiautagrion" kingdom="Animalia" order="Odonata" pageId="36" pageNumber="99" phylum="Arthropoda" rank="species" species="acutum">
