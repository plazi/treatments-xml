<taxonomicName id="CAB14798FA5EFFC25488FE09FF74420B" authorityName="Lacepede" authorityYear="1799" class="Mammalia" family="Hominidae" genus="Pongo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="35" pageNumber="2086" phylum="Chordata" rank="subSpecies" species="pygmaeus" subSpecies="borneo">
