<heading id="D0BA81C1FFDF2B03FF3CF94C3A3119B0" bold="true" box="[151,870,1751,1777]" fontSize="11" level="1" pageId="12" pageNumber="13" reason="1">
<emphasis id="B939EABFFFDF2B03FF3CF94C3A3119B0" bold="true" box="[151,870,1751,1777]" pageId="12" pageNumber="13">
<taxonomicName id="4C4D4D2EFFDF2B03FF3CF94C3B2019B0" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[151,631,1751,1777]" class="Insecta" family="Polycentropodidae" genus="Nyctiophylax" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="flavus" status="new species" subGenus="Paranyctiophylax">
<emphasis id="B939EABFFFDF2B03FF3CF94C386019B0" bold="true" box="[151,311,1751,1777]" italics="true" pageId="12" pageNumber="13">Nyctiophylax</emphasis>
(
<emphasis id="B939EABFFFDF2B03FEECF94C3B4B19B0" bold="true" box="[327,540,1751,1777]" italics="true" pageId="12" pageNumber="13">Paranyctiophylax</emphasis>
