<quantity id="4CD909B2165730750CA4E32201043756" box="[151,257,272,298]" metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="3" pageNumber="4" unit="mm" value="11.2">11.2 mm</quantity>
