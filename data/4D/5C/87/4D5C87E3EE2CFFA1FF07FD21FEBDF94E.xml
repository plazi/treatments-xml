<paragraph blockId="19.[151,1437,823,1739]" pageId="19" pageNumber="20">
<taxonomicName box="[197,285,1404,1429]" class="Adenophorea" family="Tripylidae" genus="Tripyla" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="sheri">
<emphasis box="[197,285,1404,1429]" italics="true" pageId="19" pageNumber="20">T. sheri</emphasis>
