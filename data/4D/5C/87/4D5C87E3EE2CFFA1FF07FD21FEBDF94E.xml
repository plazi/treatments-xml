<emphasis bold="true" box="[858,954,1403,1429]" pageId="19" pageNumber="20">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName box="[972,1082,1404,1429]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="tamaki" status="sp. nov.">
