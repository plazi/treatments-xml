<subSubSection pageId="19" pageNumber="20" type="description">
<paragraph blockId="19.[151,1437,823,1739]" pageId="19" pageNumber="20">
<taxonomicName box="[197,286,1520,1545]" class="Adenophorea" family="Tripylidae" genus="Tripyla" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="sheri">
<emphasis box="[197,286,1520,1545]" italics="true" pageId="19" pageNumber="20">T. sheri</emphasis>
</taxonomicName>
can be differentiated from 
<taxonomicName box="[621,744,1520,1545]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="tearoha" status="sp. nov.">
<emphasis box="[621,744,1520,1545]" italics="true" pageId="19" pageNumber="20">T. tearoha</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[753,850,1519,1545]" pageId="19" pageNumber="20" rank="species">
<emphasis bold="true" box="[753,850,1519,1545]" pageId="19" pageNumber="20">sp. nov.</emphasis>
