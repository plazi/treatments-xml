, 
<taxonomicName box="[1288,1432,1327,1352]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="stramenti">T. stramenti</taxonomicName>
, 
<taxonomicName box="[151,259,1365,1390]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="yeatesi" status="sp. nov.">T. yeatesi</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[268,362,1365,1391]" pageId="19" pageNumber="20" rank="species">
<emphasis bold="true" box="[268,362,1365,1391]" pageId="19" pageNumber="20">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName box="[419,552,1365,1390]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="kaikoura" status="sp. nov.">
