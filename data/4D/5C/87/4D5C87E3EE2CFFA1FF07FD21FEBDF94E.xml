and 
<taxonomicName box="[1250,1402,1404,1429]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="19" pageNumber="20" phylum="Nematoda" rank="species" species="macroseta">
