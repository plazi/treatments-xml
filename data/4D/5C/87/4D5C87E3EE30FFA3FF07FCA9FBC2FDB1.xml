<emphasis box="[379,493,229,254]" italics="true" pageId="16" pageNumber="17">T. ursulae</emphasis>
</taxonomicName>
and 
<taxonomicName box="[552,700,229,254]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="16" pageNumber="17" phylum="Nematoda" rank="species" species="macroseta">
