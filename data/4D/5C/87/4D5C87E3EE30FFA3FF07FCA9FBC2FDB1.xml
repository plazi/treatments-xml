, 
<taxonomicName box="[224,362,229,254]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="16" pageNumber="17" phylum="Nematoda" rank="species" species="arenicola">
