(
<figureCitation box="[159,231,1374,1399]" captionStart="FIGURES 1–7" captionStartId="4.[151,264,1476,1499]" captionTargetBox="[151,1436,194,1443]" captionTargetId="figure-229@4.[151,1436,193,1456]" captionTargetPageId="4" captionText="FIGURES 1–7. Sophiothrips from Australia. Heads and pronota: (1) canberrae sp.n.; (2) darwini sp.n.; (3) greensladei; (4) martinae sp.n.; (5) mongae sp.n.; (6) placodes sp.n.; (7) postlei sp.n." figureDoi="http://doi.org/10.5281/zenodo.4930181" httpUri="https://zenodo.org/record/4930181/files/figure.png" pageId="3" pageNumber="187">Figs 3</figureCitation>
, 
<figureCitation box="[243,273,1374,1399]" captionStart="FIGURES 8–13" captionStartId="6.[151,264,1235,1258]" captionTargetBox="[151,1434,339,1208]" captionTargetId="figure-381@6.[151,1436,339,1214]" captionTargetPageId="6" captionText="FIGURES 8–13. Sophiothrips from Australia, antennae. (8) canberrae sp.n.; (9) darwini sp.n. (10) greensladei; (11) martinae sp.n.; (12) placodes sp.n.; (13) postlei sp.n." figureDoi="http://doi.org/10.5281/zenodo.4930183" httpUri="https://zenodo.org/record/4930183/files/figure.png" pageId="3" pageNumber="187">10</figureCitation>
, 
<figureCitation box="[285,314,1374,1399]" captionStart="FIGURES 23–29" captionStartId="9.[151,264,1715,1738]" captionTargetBox="[151,1433,193,1682]" captionTargetId="figure-96@9.[151,1436,193,1694]" captionTargetPageId="9" captionText="FIGURES 23–29. Sophiothrips from Australia, tergite IX and tube. (23) darwini sp.n.; (24) greensladei; (25) mongae sp.n.; (26) placodes sp.n.; (27) postlei sp.n.; (28) martinae sp.n. female; (29) martinae sp.n. male." figureDoi="http://doi.org/10.5281/zenodo.4930189" httpUri="https://zenodo.org/record/4930189/files/figure.png" pageId="3" pageNumber="187">24</figureCitation>
