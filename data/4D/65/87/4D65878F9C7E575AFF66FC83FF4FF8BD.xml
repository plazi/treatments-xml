<geoCoordinate box="[216,285,1447,1472]" degrees="1" direction="south" minutes="25" orientation="latitude" pageId="7" pageNumber="191" precision="925" value="-1.4166666">S1 25</geoCoordinate>
, 
<geoCoordinate box="[298,366,1447,1472]" degrees="2" direction="south" minutes="20" orientation="latitude" pageId="7" pageNumber="191" precision="925" value="-2.3333333">S2 20</geoCoordinate>
