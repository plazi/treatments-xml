<taxonomicName class="Insecta" family="Mastotermitidae" genus="Mastotermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mastotermes" order="Blattodea" pageId="3" pageNumber="70" phylum="Arthropoda" rank="genus">Mastotermes</taxonomicName>
