Figs 
<number box="[209,270,1826,1852]" pageId="1" pageNumber="46" value="10.12" valueMax="19.24" valueMin="1.0">1–19</number>
</figureCitation>
, 
<figureCitation box="[282,355,1826,1852]" captionStart="FIGURES 21 – 25" captionStartId="5.[151,269,1904,1928]" captionTargetBox="[284,1319,505,1903]" captionTargetId="figure@5.[273,1320,505,1985]" captionTargetPageId="5" captionText="FIGURES 21 – 25. Rhynchocyrtus gen. nov.: 21, distribution of the new species of Rhynchocyrtus gen. nov. from Brazil; 22 – 23, habitus and cephalic lateral view of Rhynchocyrtus sp.; 24 – 25, habitus and cephalic lateral view of Rhynchocyrtus klausi" httpUri="https://zenodo.org/record/274035/files/figure.png" pageId="1" pageNumber="46">
24– 
<number box="[327,355,1826,1852]" pageId="1" pageNumber="46" value="25.0">25</number>
