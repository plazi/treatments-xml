<taxonomicName authority="Hunag &amp; Takaoka, 2009" authorityName="Hunag &amp; Takaoka" authorityYear="2009" box="[151,839,1019,1046]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="147" phylum="Arthropoda" rank="species" species="serenum" subGenus="Simulium">
<emphasis bold="true" box="[151,839,1019,1046]" pageId="6" pageNumber="147">
<emphasis bold="true" box="[151,271,1019,1045]" italics="true" pageId="6" pageNumber="147">Simulium</emphasis>
(
<emphasis bold="true" box="[287,406,1019,1045]" italics="true" pageId="6" pageNumber="147">Simulium</emphasis>
) 
<emphasis bold="true" box="[423,527,1019,1045]" italics="true" pageId="6" pageNumber="147">serenum</emphasis>
Hunag &amp; Takaoka, 2009
</emphasis>
</taxonomicName>
