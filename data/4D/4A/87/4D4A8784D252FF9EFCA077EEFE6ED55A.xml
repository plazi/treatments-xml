, pseudospikelet; 
<emphasis bold="true" box="[1042,1048,1657,1675]" pageId="2" pageNumber="57">I</emphasis>
, prophyll; 
<emphasis bold="true" box="[1133,1143,1657,1675]" pageId="2" pageNumber="57">J</emphasis>
, glume; 
<emphasis bold="true" box="[113,126,1680,1698]" pageId="2" pageNumber="57">K</emphasis>
, lemma; 
