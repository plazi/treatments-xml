<subSubSection type="reference_group">
<paragraph pageId="25" pageNumber="1482" type="mainText">
<taxonomicName authority="Forel, 1901: 66" authorityName="Forel" authorityPageNumber="66" authorityYear="1901" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hymenoptera" phylum="Arthropoda" rank="variety" species="cursor" variety="piliscapa">Myrmecocystus cursor var. piliscapa Forel, 1901: 66</taxonomicName>
