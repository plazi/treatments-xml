<subSubSection pageId="37" pageNumber="38" type="etymology">
<paragraph blockId="37.[151,1437,151,896]" pageId="37" pageNumber="38">
<emphasis bold="true" box="[199,334,836,861]" pageId="37" pageNumber="38">Etymology.</emphasis>
