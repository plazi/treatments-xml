<taxonomicName baseAuthorityName="Boucek" baseAuthorityYear="1988" box="[151,303,871,896]" class="Insecta" family="Chalcidoidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="family">Chalcidoidea</taxonomicName>
