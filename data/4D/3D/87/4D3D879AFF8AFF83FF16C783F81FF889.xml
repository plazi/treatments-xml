<taxonomicName baseAuthorityName="Boucek" baseAuthorityYear="1988" box="[151,303,1806,1831]" class="Insecta" family="Chalcidoidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="family">Chalcidoidea</taxonomicName>
