<taxonomicName authorityName="Cressey &amp; Cressey" authorityYear="1984" class="Maxillopoda" family="Bomolochidae" genus="Acantholochus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acantholochus" order="Poecilostomatoida" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus">
