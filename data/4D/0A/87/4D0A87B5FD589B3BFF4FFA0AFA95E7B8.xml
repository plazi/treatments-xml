<collectingCountry box="[696,756,151,177]" name="Laos" pageId="8" pageNumber="139">Laos</collectingCountry>
(
<collectingRegion box="[771,1011,150,177]" country="Laos" name="Houaphan" pageId="8" pageNumber="139">Houaphanh Province</collectingRegion>
