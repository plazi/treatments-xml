<taxonomicName authorityName="He &amp; Shen &amp; Wu" authorityYear="2020" box="[501,620,1839,1864]" class="Insecta" family="Trigonidiidae" genus="Emerasoma" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="123" phylum="Arthropoda" rank="species" species="curvicerca">
