<subSubSection pageId="19" pageNumber="20" type="biology_ecology">
<paragraph blockId="19.[151,1437,151,1653]" pageId="19" pageNumber="20">
<emphasis bold="true" box="[199,359,871,896]" pageId="19" pageNumber="20">Prey records.</emphasis>
