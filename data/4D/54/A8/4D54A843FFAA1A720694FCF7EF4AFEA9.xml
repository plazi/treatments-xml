<collectingCountry box="[791,953,1973,1999]" name="Ivory Coast" pageId="18" pageNumber="19">Côte d’Ivoire</collectingCountry>
, 
<collectingCountry box="[965,1088,1973,1999]" name="Cameroon" pageId="18" pageNumber="19">Cameroon</collectingCountry>
and 
<collectingCountry box="[1147,1225,1973,1999]" name="Gabon" pageId="18" pageNumber="19">Gabon</collectingCountry>
(
<figureCitation box="[1242,1346,1973,1999]" captionStart="FIGURE 143" captionStartId="45.[151,257,1953,1977]" captionTargetBox="[153,1436,996,1926]" captionTargetId="figure@45.[151,1436,996,1926]" captionTargetPageId="45" captionText="FIGURE 143. Distribution of species in the Acylophorus densipennis group = A. densipennis Bernhauer, = A. micans new species, = A. allardi Levasseur, = A. congoensis Cameron, = A. methneri Bernhauer)." httpUri="https://zenodo.org/record/275924/files/figure.png" pageId="18" pageNumber="19">
Fig. 
<number box="[1298,1346,1973,1999]" pageId="18" pageNumber="19" value="143.0">143</number>
</figureCitation>
