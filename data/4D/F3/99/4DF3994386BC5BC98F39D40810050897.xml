<taxonomicName class="Insecta" family="Cordycipitaceae" genus="C." kingdom="Animalia" lsidName="C. jingyuetanensis" order="Lepidoptera" pageId="0" pageNumber="161" phylum="Arthropoda" rank="species" species="jingyuetanensis">
<emphasis italics="true" pageId="0" pageNumber="161">C. jingyuetanensis</emphasis>
