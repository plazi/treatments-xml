<quantity id="890413999518FF8A224DFF7BFD8EE0CE" box="[363,513,153,179]" metricMagnitude="-3" metricUnit="m" metricValue="9.9" metricValueMax="10.4" metricValueMin="9.4" pageId="4" pageNumber="355" unit="mm" value="9.9" valueMax="10.4" valueMin="9.4">9.4–10.4 mm</quantity>
