<quantity id="4CEF9B25FFC7FFE398BBFF48FB4C30D0" box="[198,269,186,213]" metricMagnitude="-1" metricUnit="m" metricValue="1.5" pageId="86" pageNumber="87" unit="cm" value="15.0">15 cm</quantity>
