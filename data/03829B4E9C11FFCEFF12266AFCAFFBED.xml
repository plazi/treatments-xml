<emphasis id="B95FF64A9C12FFC8FF422732FEA3F93B" bold="true" box="[199,345,1653,1679]" pageId="2" pageNumber="281">Description.</emphasis>
Measurements: body length 
<quantity id="4CD387BD9C12FFC8FD272732FCF0F924" box="[674,778,1653,1680]" metricMagnitude="-2" metricUnit="m" metricValue="1.09" pageId="2" pageNumber="281" unit="mm" value="10.9">10.9 mm</quantity>
; head length 
<quantity id="4CD387BD9C12FFC8FC242732FC00F924" box="[929,1018,1653,1680]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="2" pageNumber="281" unit="mm" value="0.9">0.9 mm</quantity>
, head width (across compound eyes) 
<quantity id="4CD387BD9C12FFC8FF1227DEFF0AF900" box="[151,240,1689,1716]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="2" pageNumber="281" unit="mm" value="1.9">1.9 mm</quantity>
; pronotum length 
<quantity id="4CD387BD9C12FFC8FE3F27DEFDEBF900" box="[442,529,1689,1716]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="2" pageNumber="281" unit="mm" value="2.3">2.3 mm</quantity>
, pronotal maximum width 
<quantity id="4CD387BD9C12FFC8FCBB27DEFC6CF900" box="[830,918,1689,1716]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="2" pageNumber="281" unit="mm" value="4.5">4.5 mm</quantity>
; elytral length 
<quantity id="4CD387BD9C12FFC8FBBE27DEFB68F900" box="[1083,1170,1689,1716]" metricMagnitude="-3" metricUnit="m" metricValue="7.6" pageId="2" pageNumber="281" unit="mm" value="7.6">7.6 mm</quantity>
, elytral maximum combined width 
<quantity id="4CD387BD9C12FFC8FEA627FAFE86F96C" box="[291,380,1725,1752]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="2" pageNumber="281" unit="mm" value="4.2">4.2 mm</quantity>
