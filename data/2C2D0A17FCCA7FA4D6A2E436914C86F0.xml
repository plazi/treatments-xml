<taxonomicName id="7BC4BD5EBE97907ABF35BA3875EE5958" class="Polychaeta" family="Dorvilleidae" genus="Dorvillea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dorvillea rubrovittata" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="rubrovittata">Dorvillea rubrovittata</taxonomicName>
