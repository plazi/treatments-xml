<taxonomicName id="4C35DCDBFFB1FFBDDC4BFDDBFD1E7AFE" authority="(Benoit, 1956)" baseAuthorityName="Benoit" baseAuthorityYear="1956" box="[121,644,204,227]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="yangambicolus">
