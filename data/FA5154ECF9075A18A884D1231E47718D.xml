adult of 
<taxonomicName id="63851690FFDB8FE5C9EAE5287EE69E5A" class="Magnoliopsida" family="Poaceae" genus="M." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="M. plicata" order="Poales" pageId="0" pageNumber="23" phylum="Angiospermae" rank="species" species="plicata">
