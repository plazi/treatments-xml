<taxonomicName id="34CA4D31FF90FFF15F380F3CFD4651A9" authority="Jaschhof &amp; Jaschhof, 2018" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2018" box="[578,723,1147,1173]" class="Insecta" family="Cecidomyiidae" genus="Glemparon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="aotearoa" status="sp. nov.">
