<collectionCode id="ED31AE34FFB9FFEEFEF9F930D4D1F975" box="[640,719,1667,1691]" pageId="14" pageNumber="13">CZAC</collectionCode>
); Viñales, 
<date id="FF9E1031FFB9FFEEFF31F930D5DFF975" box="[840,961,1667,1691]" pageId="14" pageNumber="13" value="1969-02-21">21.ii.1969</date>
, col. 
<collectionCode id="ED31AE34FFB9FFEEFF80F937D20EF975" box="[1017,1040,1668,1691]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="14" pageNumber="13">R</collectionCode>
. Alayo, 
<taxonomicName id="4C204D72FFB9FFEEF813F937D316F975" box="[1130,1288,1667,1691]" class="Magnoliopsida" family="Malvaceae" genus="Urena" kingdom="Plantae" order="Malvales" pageId="14" pageNumber="13" phylum="Tracheophyta" rank="species" species="lobata">
