<taxonomicName id="27CA4D35FEEBFE8D5B0CFBB3FC4D6521" authorityName="Rondani" authorityYear="1859" box="[786,1022,1086,1112]" class="Insecta" family="Tachinidae" genus="Chetogena" kingdom="Animalia" order="Diptera" pageId="294" pageNumber="295" phylum="Arthropoda" rank="species" species="segregata">
