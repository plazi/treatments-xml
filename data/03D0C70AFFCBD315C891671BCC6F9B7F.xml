<paragraph id="8BC6761CFFCBD315C891644CCC6F9B7F" blockId="114.[151,1437,623,792]" pageId="114" pageNumber="115">
<collectionCode id="ED68EED9FFCBD315C891644CC8C19AE0" box="[151,246,624,648]" pageId="114" pageNumber="115">NOTES</collectionCode>
: (a) 
<taxonomicName id="4C790D9FFFCBD315C92E6453C98C9AE0" box="[296,443,623,648]" class="Arachnida" family="Ascidae" genus="Iphidozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="114" pageNumber="115" phylum="Arthropoda" rank="genus">
