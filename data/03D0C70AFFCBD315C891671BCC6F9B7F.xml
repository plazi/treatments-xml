<number id="3DB1643BFFCBD315CAE56484CB379AB8" box="[739,768,696,720]" pageId="114" pageNumber="115" value="34.0">34</number>
)
</bibRefCitation>
considered 
<taxonomicName id="4C790D9FFFCBD315CBA56484CC0E9AA7" box="[931,1081,696,719]" class="Arachnida" family="Ascidae" genus="Arctoseiopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="114" pageNumber="115" phylum="Arthropoda" rank="genus">
<emphasis id="B90DAA0EFFCBD315CBA56484CC0E9AA7" box="[931,1081,696,719]" italics="true" pageId="114" pageNumber="115">Arctoseiopsis</emphasis>
</taxonomicName>
to be a junior synonym of 
<taxonomicName id="4C790D9FFFCBD315C89164E0C91D9A9D" box="[151,298,732,757]" class="Arachnida" family="Ascidae" genus="Iphidozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="114" pageNumber="115" phylum="Arthropoda" rank="genus">
<emphasis id="B90DAA0EFFCBD315C89164E0C91D9A9D" box="[151,298,732,757]" italics="true" pageId="114" pageNumber="115">Iphidozercon</emphasis>
</taxonomicName>
