<number id="30784B44FF95FF94FF32FD7FFF7AFD6D" box="[222,232,652,676]" pageId="1" value="1.0">1</number>
–7
</figureCitation>
, 
<figureCitation id="1E8B45E6FF95FF94FEFAFD7FFEA1FD6C" box="[278,307,652,677]" captionStart="FIGURE 16" captionStartId="5.[151,250,1142,1164]" captionText="FIGURE 16. Distribution map of Brachypogon biradialis sp. n." pageId="1">16</figureCitation>
