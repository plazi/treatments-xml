<quantity id="D15FACB9FF895A62FED6FB82FE11FBB5" box="[375,473,1025,1048]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" pageId="6" pageNumber="35" unit="mm" value="6.1">6.10 mm</quantity>
