<specimenCount id="00A1CAD5FF895A62FEA1FBA2FEA2FB95" box="[256,362,1057,1080]" pageId="6" pageNumber="35" type="female">3 females</specimenCount>
<typeStatus id="C91CBFFEFF895A62FED2FBA2FE17FB95" box="[371,479,1057,1080]" pageId="6" pageNumber="35">paratypes</typeStatus>
