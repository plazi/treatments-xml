<taxonomicName id="428EE8C29E87E3FDFEE9F9E5D94B6562" box="[307,631,1564,1586]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="226" pageNumber="227" phylum="Arthropoda" rank="species" species="dresdeni" status="sp. nov." subGenus="Arcaleiodes">
