<date id="FF961063FFCC3F5E2BCDF90D142CF953" box="[1249,1378,1740,1764]" pageId="3" pageNumber="226" value="1984-05-14">
<number id="3DE02484FFCC3F5E2BCDF90D144EF953" box="[1249,1280,1740,1764]" pageId="3" pageNumber="226" value="14.0">14</number>
