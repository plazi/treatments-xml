<subSubSection id="C30F653F5D2AFFF1FF63BE1133498083" pageId="108" pageNumber="109" type="materials_examined">
<paragraph id="8BAA36B45D2AFFF1FF63BE1137D180D4" blockId="108.[189,1330,368,478]" box="[189,426,368,394]" pageId="108" pageNumber="109">
<emphasis id="B961EAA65D2AFFF1FF63BE1137D180D4" bold="true" box="[189,426,368,394]" pageId="108" pageNumber="109">Material examined</emphasis>
