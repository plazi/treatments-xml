<heading id="191DB74EFF9EFF9FAC894614FE0479E7" box="[178,477,1391,1413]" fontSize="8" level="2" pageId="22" pageNumber="1285" reason="2">
<emphasis id="709EDC30FF9EFF9FAC894614FE0479E7" bold="true" box="[178,477,1391,1413]" pageId="22" pageNumber="1285">
<taxonomicName id="85EA7BA1FF9EFF9FAC894614FE5379E7" authority="Malicky, 2012" authorityName="Malicky" authorityYear="2012" box="[178,394,1391,1413]" class="Insecta" family="Leptoceridae" genus="Mystacides" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="1285" phylum="Arthropoda" rank="species" species="dabrynos" status="nov.sp.">
<emphasis id="709EDC30FF9EFF9FAC894614FE5379E7" bold="true" box="[178,394,1391,1413]" italics="true" pageId="22" pageNumber="1285">Mystacides dabrynos</emphasis>
</taxonomicName>
<taxonomicNameLabel id="6BAD614BFF9EFF9FADAA4614FE0479E7" box="[401,477,1391,1413]" pageId="22" pageNumber="1285" rank="species">nov.sp.</taxonomicNameLabel>
