<collectingCountry id="F358767E2972FF87781A15E79EDAFD5F" box="[593,709,696,722]" name="Colombia" pageId="53" pageNumber="54">Colombia</collectingCountry>
and 
<collectingCountry id="F358767E2972FF8778B515E79F9CFD5F" box="[766,899,696,722]" name="Martinique" pageId="53" pageNumber="54">Martinique</collectingCountry>
