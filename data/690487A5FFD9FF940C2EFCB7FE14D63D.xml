<collectingCountry id="99BA7623FFD9FF940CAAFBEDFE11D769" box="[405,468,1024,1050]" name="India" pageId="26" pageNumber="218">India</collectingCountry>
, 
<collectingCountry id="99BA7623FFD9FF940CE0FBEDFDEDD769" box="[479,552,1024,1050]" name="Nepal" pageId="26" pageNumber="218">Nepal</collectingCountry>
, 
