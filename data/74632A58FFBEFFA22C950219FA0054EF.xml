<taxonomicName id="3BCAE0CDFFBDFFA22DB905D8FD2354EF" authority="Qin &amp; Liu &amp; Li, 2019" authorityName="Qin &amp; Liu &amp; Li" authorityYear="2019" box="[443,702,418,443]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="275" phylum="Arthropoda" rank="subGenus" sensu="stricto" status="s. str." subGenus="Tachycines">
<emphasis id="CEBE475CFFBDFFA22DB905D8FDAC54EF" box="[443,561,418,443]" italics="true" pageId="2" pageNumber="275">Tachycines</emphasis>
(
<emphasis id="CEBE475CFFBDFFA22E4205D8FD2B54EF" box="[576,694,418,443]" italics="true" pageId="2" pageNumber="275">Tachycines</emphasis>
)
</taxonomicName>
<taxonomicNameLabel id="D58DFA27FFBDFFA22EC605D8FD6254EF" box="[708,767,418,443]" pageId="2" pageNumber="275" sensu="stricto">
<emphasis id="CEBE475CFFBDFFA22EC605D8FD6254EF" bold="true" box="[708,767,418,443]" pageId="2" pageNumber="275">s. str.</emphasis>
</taxonomicNameLabel>
<bibRefCitation id="985BE6BFFFBDFFA22F0605D8FC3E54EF" author="Adelung, N." box="[772,931,418,443]" pageId="2" pageNumber="275" pagination="55 - 75" refId="ref15303" refString="Adelung, N. (1902) Beitrag zur Kenntnis der palaarktischen Stenopelmatiden (Orthoptera, Locustodea). Extrait L ' Annuaire Musee Zoologique L ' Academie Imperiale Science St. Petersbourg, 7, 55 - 75." type="journal article" year="1902">Adelung, 1902</bibRefCitation>
