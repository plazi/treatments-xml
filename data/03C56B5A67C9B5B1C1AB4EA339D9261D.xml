<taxonomicName id="4C6CA1CF67C9B5B1C09A4C603A062679" authorityName="Boheman" authorityYear="1844" box="[422,540,1807,1832]" class="Insecta" family="Curculionidae" genus="Alcides" kingdom="Animalia" order="Coleoptera" pageId="412" pageNumber="413" phylum="Arthropoda" rank="species" species="karelini">
<emphasis id="B918065E67C9B5B1C09A4C603A062679" box="[422,540,1807,1832]" italics="true" pageId="412" pageNumber="413">A. karelini</emphasis>
</taxonomicName>
