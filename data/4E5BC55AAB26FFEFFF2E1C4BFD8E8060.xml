Total length, 9.0– 
<number id="703A666BAB27FFECFD6D1D9DFC95869F" box="[724,771,1932,1955]" pageId="1" pageNumber="230" value="10.5">10.5</number>
; prothorax length, 
<number id="703A666BAB27FFECFC6F1D9DFBBF869F" box="[982,1065,1932,1955]" pageId="1" pageNumber="230" value="1.75" valueMax="2.2" valueMin="1.3">1.3–2.2</number>
; prothorax width, 
<number id="703A666BAB27FFECFB4F1D9DFADF869F" box="[1270,1353,1932,1955]" pageId="1" pageNumber="230" value="2.15" valueMax="2.5" valueMin="1.8">1.8–2.5</number>
; elytral length, 
<number id="703A666BAB27FFECFF5E1DBFFEAC86F9" box="[231,314,1966,1989]" pageId="1" pageNumber="230" value="7.1" valueMax="7.8" valueMin="6.4">6.4–7.8</number>
; humeral width, 
<number id="703A666BAB27FFECFE541DBFFDD686F9" box="[493,576,1966,1989]" pageId="1" pageNumber="230" value="3.15" valueMax="3.5" valueMin="2.8">2.8–3.5</number>
