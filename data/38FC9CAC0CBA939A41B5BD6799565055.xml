<normalizedToken id="D5F68E865CD33B1D61741A705FFD50EA" originalValue="Interlamellarhöckern">Interlamellarhoeckern</normalizedToken>
; ss distal einseitig mit 
<normalizedToken id="0789515A3EAED110FCF500BA1BC01923" originalValue="kräftigen">kraeftigen</normalizedToken>
