<normalizedToken id="7227D2C4E789694CA2BBE57B8788B1B7" originalValue="dünn">duenn</normalizedToken>
; 6 g, 
<normalizedToken id="F4C0ED78414E928E1A92AA5F60377A14" originalValue="Körperlänge">Koerperlaenge</normalizedToken>
