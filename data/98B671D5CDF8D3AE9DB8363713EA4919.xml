<taxonomicName id="391F76CDAA4A2004B7148EE79D4DCE40" class="Actinopterygii" family="Sparidae" genus="Diplodus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diplodus sargus" order="Perciformes" pageId="20" pageNumber="59" phylum="Chordata" rank="species" species="sargus">Diplodus sargus</taxonomicName>
