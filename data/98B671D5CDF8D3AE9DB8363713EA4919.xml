<taxonomicName id="906850CBFBDBE0A9E54DCD4E1C69AB24" class="Actinopterygii" family="Sparidae" genus="Lithognathus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lithognathus mormyrus" order="Perciformes" pageId="20" pageNumber="59" phylum="Chordata" rank="species" species="mormyrus">Lithognathus mormyrus</taxonomicName>
