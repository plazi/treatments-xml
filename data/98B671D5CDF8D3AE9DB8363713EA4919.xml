<taxonomicName id="4FCC92A9B29032C4245033500B6B5C34" class="Actinopterygii" family="Sparidae" genus="Pagellus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pagellus mormyrus" order="Perciformes" pageId="20" pageNumber="59" phylum="Chordata" rank="species" species="mormyrus">Pagellus mormyrus</taxonomicName>
