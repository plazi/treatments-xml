, 
<taxonomicName id="4C454D73FF92880D98AF1C43FDEA0BD3" authority="Szymczakowski, 1974" authorityName="Szymczakowski" authorityYear="1974" class="Insecta" family="Leiodidae" genus="Ptomaphaginus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="bucculentus">
