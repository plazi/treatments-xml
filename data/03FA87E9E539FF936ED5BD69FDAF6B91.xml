<taxonomicName id="4C534D7CE53BFF936D3CBB03FC416BB5" box="[894,1000,367,392]" class="Insecta" family="Triozidae" kingdom="Animalia" order="Hemiptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="family">Triozidae</taxonomicName>
