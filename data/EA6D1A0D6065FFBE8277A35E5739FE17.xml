considered 
<taxonomicName id="A5C4D0986065FFBC83CDA0385544F9EA" authorityName="Satchell" authorityYear="1953" box="[211,375,1564,1582]" class="Insecta" family="Psychodidae" genus="Nototelmatoscopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="240" phylum="Arthropoda" rank="genus">
