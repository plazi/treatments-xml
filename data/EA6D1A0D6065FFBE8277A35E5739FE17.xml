<taxonomicName id="A5C4D0986065FFBC81E8A03857C8F9EA" authority="Enderlein, 1935" authorityName="Enderlein" authorityYear="1935" box="[758,1019,1563,1582]" class="Insecta" family="Psychodidae" genus="Peripsychoda" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="240" phylum="Arthropoda" rank="genus">
<emphasis id="50B077096065FFBC81E8A0385758F9EA" box="[758,875,1564,1582]" italics="true" pageId="5" pageNumber="240">Peripsychoda</emphasis>
