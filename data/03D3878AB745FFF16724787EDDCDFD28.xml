<emphasis id="B90EEA8EB745FFF064AE7A99DE3CF85E" bold="true" box="[506,588,1963,1989]" pageId="20" pageNumber="21">Africa</emphasis>
(
<collectingCountry id="F36D760CB745FFF067097A99DEBEF85D" box="[605,718,1963,1990]" name="Tanzania" pageId="20" pageNumber="21">Zanzibar</collectingCountry>
), 
<emphasis id="B90EEA8EB745FFF067B17A99DF6EF85E" bold="true" box="[741,798,1963,1989]" pageId="20" pageNumber="21">Asia</emphasis>
(
<collectingCountry id="F36D760CB745FFF066647A99DFF6F85E" box="[816,902,1963,1989]" name="Myanmar" pageId="20" pageNumber="21">Burma</collectingCountry>
