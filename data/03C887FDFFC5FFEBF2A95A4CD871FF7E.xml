<subSubSection id="C37B6560FFC5FFEAF2A95A4CD8B7F9D4" box="[151,654,1591,1617]" pageId="2" pageNumber="249" type="nomenclature">
<paragraph id="8BDE36EBFFC5FFEAF2A95A4CD8B7F9D4" blockId="2.[151,654,1591,1617]" box="[151,654,1591,1617]" pageId="2" pageNumber="249">
<heading id="D0968187FFC5FFEAF2A95A4CD8B7F9D4" bold="true" box="[151,654,1591,1617]" fontSize="11" level="1" pageId="2" pageNumber="249" reason="1">
<emphasis id="B915EAF9FFC5FFEAF2A95A4CD8B7F9D4" bold="true" box="[151,654,1591,1617]" pageId="2" pageNumber="249">
Family 
<taxonomicName id="4C614D68FFC5FFEAF2C95A4CD8B7F9D4" authority="Fleming, 1828" authorityName="Fleming" authorityYear="1828" box="[247,654,1591,1617]" class="Crinoidea" family="Comatulidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="2" pageNumber="249" phylum="Echinodermata" rank="family">
COMATULIDAE 
<bibRefCitation id="EFF04B1AFFC5FFEAF3E05A4CD8B7F9D4" author="Fleming, J." box="[478,654,1591,1617]" pageId="2" pageNumber="249" refId="ref14648" refString="Fleming, J. (1828) History of British animals exhibiting the descriptive characters and systematical arrangement of the genera and species of quadrupeds, birds, reptiles, fishes, Mollusca and Radiata of the United Kingdom. Bell &amp; Bradfute, Edinburgh, xxiii + 565 pp. https: // doi. org / 10.5962 / bhl. title. 12859" type="book" year="1828">Fleming, 1828</bibRefCitation>
</taxonomicName>
</emphasis>
