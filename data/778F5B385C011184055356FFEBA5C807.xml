<taxonomicName id="5BF993D6DE2733741A29C80133F87C97" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:233576" authority="Roger, 1861" authorityName="Roger" authorityYear="1861" class="Insecta" family="Formicidae" genus="Syscia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Uploaded - Syscia autralis" lsidName-HNS="Syscia autralis" order="Hymenoptera" pageNumber="68" phylum="Arthropoda" rank="species" species="autralis">Syscia autralis</taxonomicName>
