<taxonomicName id="E7B04D49B85DFFAEFF6203FECF1AFB57" authority="Levichev" box="[151,583,1212,1241]" class="Liliopsida" family="Liliaceae" genus="Gagea" kingdom="Plantae" order="Liliales" pageId="4" pageNumber="48" phylum="Tracheophyta" rank="section" section="Bulbiferae">
