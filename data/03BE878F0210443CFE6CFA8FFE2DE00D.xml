<date id="FFA910590210443EFCF2FC5FFBF2E640" box="[873,1028,1293,1318]" pageId="20" pageNumber="162" value="1995-05-25">
<collectingDate id="EFEDE9B10210443EFCF2FC5FFBF2E640" box="[873,1028,1293,1318]" pageId="20" pageNumber="162" value="1995-05-25">25 May 1995</collectingDate>
