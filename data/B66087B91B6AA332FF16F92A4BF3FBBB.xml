<heading id="653E81C31B6AA33DFF16F92A4A07F8C1" bold="true" box="[151,578,1772,1798]" fontSize="11" level="1" pageId="6" pageNumber="451" reason="1">
<emphasis id="0CBDEABD1B6AA33DFF16F92A4A07F8C1" bold="true" box="[151,578,1772,1798]" pageId="6" pageNumber="451">
<taxonomicName id="F9C94D2C1B6AA33DFF16F92A491EF8C1" authority="orise" box="[151,347,1772,1798]" class="Insecta" family="Pieridae" genus="Patia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="451" phylum="Arthropoda" rank="subSpecies" species="orise" subSpecies="orise">
<emphasis id="0CBDEABD1B6AA33DFF16F92A491EF8C1" bold="true" box="[151,347,1772,1798]" italics="true" pageId="6" pageNumber="451">Patia orise orise</emphasis>
