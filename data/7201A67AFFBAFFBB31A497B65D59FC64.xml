<subSubSection id="B2B244E7FFBAFFB931A496235A41FE92" box="[151,1325,258,283]" pageId="4" pageNumber="559" type="etymology">
<paragraph id="FA17176CFFBAFFB931A496235A41FE92" blockId="4.[151,1437,258,2013]" box="[151,1325,258,283]" pageId="4" pageNumber="559">
<emphasis id="C8DCCB7EFFBAFFB931A496235E72FE92" bold="true" box="[151,286,258,283]" pageId="4" pageNumber="559">Etymology.</emphasis>
