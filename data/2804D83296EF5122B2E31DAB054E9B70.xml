<taxonomicName id="F3C9A05B953363BC28D1AB93B74C59F2" LSID="2804D832-96EF-5122-B2E3-1DAB054E9B70" authority="Schulze, 1885" authorityName="Schulze" authorityYear="1886" class="HEXACTINELLIDA" family="Rossellidae" genus="Caulophacus" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="" order="Lyssacinosida" pageId="0" pageNumber="33" phylum="PORIFERA" rank="family">Rossellidae Schulze, 1885</taxonomicName>
