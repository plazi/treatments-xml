<taxonomicName id="D8FE4D54FFCE155E4D5FFC42FC442EF9" authority="Habu" authorityName="Habu" authorityYear="1979" box="[773,1005,998,1019]" class="Insecta" family="Carabidae" genus="Nepalorthogonius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="91" phylum="Arthropoda" rank="genus">
