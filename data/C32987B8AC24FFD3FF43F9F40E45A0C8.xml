<number id="FD482489AC24FFD1FB35F91B098DA04B" box="[1249,1266,1789,1815]" pageId="12" pageNumber="113" value="2.0">2</number>
, pereonites 
<number id="FD482489AC24FFD1FAAAF91B0DD9A063" pageId="12" pageNumber="113" value="3.5" valueMax="5.0" valueMin="2.0">2– 5</number>
