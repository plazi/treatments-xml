, 
<collectingRegion id="EF6FF807FFD4FFDAFD75FEA1FCA8FEE6" box="[748,845,295,320]" country="Guinea-Bissau" name="Tombali" pageId="57" pageNumber="58">Tombali</collectingRegion>
