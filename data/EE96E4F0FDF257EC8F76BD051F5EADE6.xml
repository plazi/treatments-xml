<geoCoordinate id="5F4FCD4B738E282FAC3D0978D6FEA598" degrees="37" direction="north" minutes="00" orientation="latitude" precision="1" seconds="0.59" value="37.000164">37°00'0.59&quot;N</geoCoordinate>
