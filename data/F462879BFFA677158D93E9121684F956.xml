<taxonomicName id="BBCB4D0EFFA677158970EE1210F6FEFC" baseAuthorityName="TURNER" baseAuthorityYear="1956" box="[1131,1237,279,302]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="18" pageNumber="163" phylum="Tracheophyta" rank="family">
