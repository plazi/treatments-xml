<taxonomicName id="4C4C03ED8C1CFFA3FB40FE19DD17FE23" authorityName="Assing" authorityYear="2010" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="1138" phylum="Arthropoda" rank="species" species="polisitica" status="nov.sp." subGenus="Sibiota">
