<quantity id="4CFC29A6F708FFD9FBA4FAFFFB15FA8E" box="[1056,1157,1336,1363]" metricMagnitude="1" metricUnit="m" metricValue="1.375" metricValueMax="2.6" metricValueMin="0.15" pageId="27" pageNumber="124" unit="m" value="13.75" valueMax="26.0" valueMin="1.5">1.5-26 m</quantity>
