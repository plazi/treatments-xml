<subSubSection pageId="15" pageNumber="364" type="collection and habitat data">
<paragraph pageId="15" pageNumber="364">Collection and habitat data.</paragraph>
<paragraph pageId="15" pageNumber="364">
<taxonomicName class="Insecta" family="Curculionidae" genus="Magdalis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Magdalis hispoides" order="Coleoptera" pageId="15" pageNumber="364" phylum="Arthropoda" rank="species" species="hispoides">Magdalis hispoides</taxonomicName>
