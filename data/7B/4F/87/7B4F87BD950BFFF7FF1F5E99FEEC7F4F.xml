<quantity box="[395,501,410,436]" metricMagnitude="1" metricUnit="m" metricValue="2.85" metricValueMax="3.0" metricValueMin="2.7" pageId="29" pageNumber="30" unit="m" value="28.5" valueMax="30.0" valueMin="27.0">27–30 m</quantity>
