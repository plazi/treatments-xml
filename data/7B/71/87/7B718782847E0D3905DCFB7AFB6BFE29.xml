</taxonomicName>
, 
<taxonomicName class="Arachnida" family="Ctenidae" genus="Phoneutria" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="bahiensis">
<emphasis italics="true" pageId="2" pageNumber="3">P. bahiensis</emphasis>
</taxonomicName>
and 
<taxonomicName box="[325,487,1853,1878]" class="Arachnida" family="Ctenidae" genus="Phoneutria" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="eickstedtae" status="sp. nov.">
<emphasis box="[325,487,1853,1878]" italics="true" pageId="2" pageNumber="3">P. eickstedtae</emphasis>
