<taxonomicName box="[953,1112,1813,1838]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="nigriventer">
<emphasis box="[953,1112,1813,1838]" italics="true" pageId="2" pageNumber="3">P. nigriventer</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1127,1286,1813,1838]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="keyserlingi">
<emphasis box="[1127,1286,1813,1838]" italics="true" pageId="2" pageNumber="3">P. keyserlingi</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1301,1398,1813,1838]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="pertyi">
