male, 
<collectionCode box="[792,864,672,698]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34981" lsid="urn:lsid:biocol.org:col:34981" name="Western Australian Museum" pageId="16" pageNumber="17" type="Museum">WAM</collectionCode>
