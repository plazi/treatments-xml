<emphasis bold="true" box="[151,230,727,753]" italics="true" pageId="9" pageNumber="517">Atheta</emphasis>
(
<emphasis bold="true" box="[247,376,727,753]" italics="true" pageId="9" pageNumber="517">Datomicra</emphasis>
) 
<emphasis bold="true" box="[393,590,727,753]" italics="true" pageId="9" pageNumber="517">semidentiventris</emphasis>
Lee &amp; Ahn
</taxonomicName>
, 
<taxonomicNameLabel box="[750,844,728,754]" pageId="9" pageNumber="517" rank="species">sp. nov.</taxonomicNameLabel>
