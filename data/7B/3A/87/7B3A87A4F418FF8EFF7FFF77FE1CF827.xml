<subSubSection pageId="27" pageNumber="28" type="distribution">
<paragraph blockId="27.[151,1436,255,2018]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[198,512,1913,1939]" pageId="27" pageNumber="28">Distribution and habitat:</emphasis>
<taxonomicName authorityName="Edward &amp; Harvey" authorityYear="2010" box="[519,768,1913,1938]" class="Diplopoda" family="Iulomorphidae" genus="Atelomastix" kingdom="Animalia" order="Spirostreptida" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="montana">
<emphasis box="[519,768,1913,1938]" italics="true" pageId="27" pageNumber="28">Atelomastix montana</emphasis>
