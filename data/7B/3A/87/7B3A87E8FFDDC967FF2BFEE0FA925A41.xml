<emphasis box="[861,970,1559,1584]" italics="true" pageId="3" pageNumber="511">dadopora</emphasis>
</taxonomicName>
, 
<taxonomicName box="[985,1179,1559,1584]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="511" phylum="Arthropoda" rank="species" species="lewisiana" subGenus="Datostiba">
