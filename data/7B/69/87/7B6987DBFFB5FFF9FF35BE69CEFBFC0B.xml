<taxonomicName authorityName="Koch" authorityYear="1854" box="[657,795,1765,1790]" class="Insecta" family="Aphididae" genus="Hyalopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="genus">
