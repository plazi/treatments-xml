<paragraph blockId="13.[151,1436,392,618]" pageId="13" pageNumber="48">
<emphasis box="[198,306,432,457]" italics="true" pageId="13" pageNumber="48">
<typeStatus box="[198,306,432,457]" pageId="13" pageNumber="48">Holotype</typeStatus>
