<collectingCountry box="[1110,1323,1457,1481]" name="Taiwan" pageId="3" pageNumber="216">Republic of China</collectingCountry>
, 
<geoCoordinate box="[1336,1431,1454,1481]" direction="north" orientation="latitude" pageId="3" pageNumber="216" precision="925" value="22.466667">
<number box="[1336,1364,1457,1481]" pageId="3" pageNumber="216" value="22.0">22</number>
o 
<number box="[1371,1402,1457,1481]" pageId="3" pageNumber="216" value="28.0">28</number>
’N
</geoCoordinate>
, 
<geoCoordinate box="[151,257,1493,1519]" direction="east" orientation="longitude" pageId="3" pageNumber="216" precision="925" value="120.86667">
<number box="[151,193,1495,1519]" pageId="3" pageNumber="216" value="120.0">120</number>
o 
<number box="[200,232,1495,1519]" pageId="3" pageNumber="216" value="52.0">52</number>
’E
</geoCoordinate>
, 
<quantity box="[269,340,1495,1519]" metricMagnitude="2" metricUnit="m" metricValue="3.9" pageId="3" pageNumber="216" unit="m" value="390.0">390 m</quantity>
elevation, by light trapping, 
<date box="[663,887,1495,1520]" pageId="3" pageNumber="216" value="1996-08-13" valueMax="1996-08-14" valueMin="1996-08-13">
<number box="[663,734,1495,1519]" pageId="3" pageNumber="216" value="13.5" valueMax="14.0" valueMin="13.0">13–14</number>
August 
