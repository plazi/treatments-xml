<td align="left" pageId="null" pageNumber="205" valign="bottom">
<taxonomicName class="Liliopsida" family="Potamogetonaceae" genus="Potamogeton" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="205" phylum="Tracheophyta" rank="species" species="nodosus">
