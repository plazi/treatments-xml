<collectingCounty box="[264,424,1084,1110]" pageId="80" pageNumber="81">Indian Ocean</collectingCounty>
(
<geoCoordinate box="[444,505,1084,1110]" degrees="33" direction="south" orientation="latitude" pageId="80" pageNumber="81" precision="55555" value="-33.0">33°S</geoCoordinate>
