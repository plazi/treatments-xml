<location LSID="urn:lsid:plazi:treatment:7B1ABE13AB40FF82FEA6FD3CFC52C271:F66C59DEAB5DFF82FCE5FC54FC56C209" box="[843,1019,1004,1030]" country="Cape Verde" county="Indian Ocean" latitude="-7.225" longLatPrecision="923" longitude="111.816666" municipality="Rio de Janeiro." name="Bay of Biscay" pageId="80" pageNumber="81">Bay of Biscay</location>
, 
<location LSID="urn:lsid:plazi:treatment:7B1ABE13AB40FF82FEA6FD3CFC52C271:F66C59DEAB5DFF82FBA4FC54FB6DC209" box="[1034,1216,1004,1030]" country="Cape Verde" county="Indian Ocean" latitude="-7.225" longLatPrecision="923" longitude="111.816666" municipality="Rio de Janeiro." name="Canary Islands" pageId="80" pageNumber="81">Canary Islands</location>
, 
<location LSID="urn:lsid:plazi:treatment:7B1ABE13AB40FF82FEA6FD3CFC52C271:F66C59DEAB5DFF82FB7EFC54FA8BC209" box="[1232,1318,1004,1030]" country="Cape Verde" county="Indian Ocean" latitude="-7.225" longLatPrecision="923" longitude="111.816666" municipality="Rio de Janeiro." name="Azores" pageId="80" pageNumber="81">Azores</location>
, 
<collectingCountry box="[264,399,1044,1070]" name="Cape Verde" pageId="80" pageNumber="81">Cape Verde</collectingCountry>
<location LSID="urn:lsid:plazi:treatment:7B1ABE13AB40FF82FEA6FD3CFC52C271:F66C59DEAB5DFF82FE38FBACFE44C221" box="[406,489,1044,1070]" country="Cape Verde" county="Indian Ocean" latitude="-7.225" longLatPrecision="923" longitude="111.816666" municipality="Rio de Janeiro." name="Islands" pageId="80" pageNumber="81">Islands</location>
