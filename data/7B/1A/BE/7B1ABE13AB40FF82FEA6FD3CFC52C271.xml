<location LSID="urn:lsid:plazi:treatment:7B1ABE13AB40FF82FEA6FD3CFC52C271:F66C59DEAB5DFF82FEA6FC54FE10C209" box="[264,445,1004,1030]" country="Cape Verde" county="Indian Ocean" latitude="-7.225" longLatPrecision="923" longitude="111.816666" municipality="Rio de Janeiro." name="Atlantic Ocean" pageId="80" pageNumber="81">Atlantic Ocean</location>
