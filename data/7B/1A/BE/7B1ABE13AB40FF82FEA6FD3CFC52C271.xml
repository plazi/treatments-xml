<geoCoordinate box="[583,687,1124,1150]" degrees="7" direction="south" minutes="13.5" orientation="latitude" pageId="80" pageNumber="81" precision="92" value="-7.225">7º13.5’S</geoCoordinate>
, 
<geoCoordinate box="[700,809,1124,1150]" degrees="111" direction="east" minutes="49" orientation="longitude" pageId="80" pageNumber="81" precision="925" value="111.816666">111º49’E</geoCoordinate>
