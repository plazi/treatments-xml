<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298369" class="Insecta" family="Formicidae" genus="Hypoponera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera surda Bolton &amp; Fisher" order="Hymenoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="surda">Hypoponera surda</taxonomicName>
paratype worker CASENT0226544.
