<taxonomicName class="Pucciniomycetes" genus="A." higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="A. rutila" order="Pucciniales" pageId="0" pageNumber="139" phylum="Ascomycota" rank="species" species="rutila">
