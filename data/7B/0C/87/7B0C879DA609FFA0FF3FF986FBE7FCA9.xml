<subSubSection box="[196,1078,823,850]" pageId="6" pageNumber="47" type="etymology">
<paragraph blockId="6.[151,1437,152,850]" box="[196,1078,823,850]" pageId="6" pageNumber="47">
<emphasis bold="true" box="[196,340,823,849]" pageId="6" pageNumber="47">Etymology:</emphasis>
