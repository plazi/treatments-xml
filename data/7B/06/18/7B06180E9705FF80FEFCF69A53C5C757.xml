<taxonomicName authority="Mill." authorityName="Mill." box="[796,1071,1612,1638]" class="Magnoliopsida" family="Rosaceae" genus="Cydonia" kingdom="Animalia" order="Rosales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="oblonga">
