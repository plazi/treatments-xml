<collectingCountry box="[997,1073,1440,1464]" name="Zambia" pageId="23" pageNumber="618">Zambia</collectingCountry>
and northern 
<collectingCountry box="[677,785,1467,1491]" name="Zimbabwe" pageId="23" pageNumber="618">Zimbabwe</collectingCountry>
