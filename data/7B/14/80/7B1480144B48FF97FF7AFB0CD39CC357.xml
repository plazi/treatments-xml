<quantity box="[1073,1214,1582,1607]" metricMagnitude="3" metricUnit="m" metricValue="2.401" metricValueMax="4.192" metricValueMin="0.61" pageId="5" pageNumber="541" unit="m" value="2401.0" valueMax="4192.0" valueMin="610.0">610–4192 m</quantity>
