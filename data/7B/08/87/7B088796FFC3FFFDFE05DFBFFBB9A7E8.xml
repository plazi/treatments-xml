<taxonomicName authority="(De Man, 1895)" baseAuthorityName="De Man" baseAuthorityYear="1895" box="[335,803,1317,1339]" class="Malacostraca" family="Pilumnidae" genus="Aniptumnus" kingdom="Animalia" order="Decapoda" pageId="22" pageNumber="905" phylum="Arthropoda" rank="species" species="quadridentatus">
