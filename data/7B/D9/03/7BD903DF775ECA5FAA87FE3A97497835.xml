<taxonomicName infraspecific-rank="race" lsidName="angustior subsp. rubiginosa" pageId="1" pageNumber="132" race="rubiginosa" rank="race" species="angustior" subSpecies="rubiginosa">race rubiginosa</taxonomicName>
