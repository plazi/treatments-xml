<quantity box="[872,974,1516,1543]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="2" pageNumber="415" unit="mm" value="0.51">0.51 mm</quantity>
, FWL: 
<quantity box="[1058,1159,1517,1543]" metricMagnitude="-3" metricUnit="m" metricValue="3.21" pageId="2" pageNumber="415" unit="mm" value="3.21">3.21 mm</quantity>
