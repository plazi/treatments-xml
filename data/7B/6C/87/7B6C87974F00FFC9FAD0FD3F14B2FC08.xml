<taxonomicName authorityName="Horvath" authorityYear="1899" box="[784,933,979,1003]" class="Insecta" family="Hydrometridae" genus="Hydrometra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" phylum="Arthropoda" rank="species" species="gracilenta">
