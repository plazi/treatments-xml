<taxonomicName class="Insecta" family="Notonectidae" genus="Notonecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" phylum="Arthropoda" rank="variety" species="glauca" variety="furcata">
<emphasis italics="true" pageId="9">N. glauca</emphasis>
var. 
<emphasis box="[301,381,680,704]" italics="true" pageId="9">furcata</emphasis>
</taxonomicName>
