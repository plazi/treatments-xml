(Mazepova, 1984), comb.n., 
<taxonomicName genus="M." lsidName="M. navitarum" pageId="1" pageNumber="20" rank="species" species="navitarum">M. navitarum</taxonomicName>
(Mazepova, 1976), comb. n., 
<taxonomicName genus="M." lsidName="M. orbiculata" pageId="1" pageNumber="20" rank="species" species="orbiculata">M. orbiculata</taxonomicName>
(Mazepova, 1990), comb. n., 
<taxonomicName genus="M." lsidName="M. spicata" pageId="1" pageNumber="20" rank="species" species="spicata">M. spicata</taxonomicName>
