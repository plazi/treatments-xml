<taxonomicName authority="Ma &amp; Chen &amp; Li, 2017" authorityName="Ma, Chen &amp; Li" authorityYear="2017" box="[321,545,1953,1975]" class="Insecta" family="Eumenidae" genus="Euodynerus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="248" phylum="Arthropoda" rank="species" species="carinatus" status="sp. nov.">
<emphasis box="[321,545,1953,1975]" italics="true" pageId="3" pageNumber="248">Euodynerus carinatus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[551,632,1953,1974]" pageId="3" pageNumber="248" rank="species">
<emphasis bold="true" box="[551,632,1953,1974]" pageId="3" pageNumber="248">sp. nov.</emphasis>
</taxonomicNameLabel>
, 1, 4, 7, 
<typeStatus box="[722,813,1953,1975]" pageId="3" pageNumber="248">holotype</typeStatus>
, 
<specimenCount box="[824,844,1954,1975]" pageId="3" pageNumber="248" type="female">♀</specimenCount>
; 2–3, 5–6, 8–9, 
<typeStatus box="[1010,1100,1954,1975]" pageId="3" pageNumber="248">paratype</typeStatus>
, 
<specimenCount box="[1110,1131,1954,1975]" pageId="3" pageNumber="248" type="male">♂</specimenCount>
