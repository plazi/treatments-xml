<subSubSection box="[121,749,1600,1624]" pageId="7" pageNumber="36" type="type_taxon">
<paragraph blockId="7.[121,749,1600,1682]" box="[121,749,1600,1624]" pageId="7" pageNumber="36">
<emphasis bold="true" box="[121,263,1600,1624]" pageId="7" pageNumber="36">
<typeStatus box="[121,178,1600,1624]" pageId="7" pageNumber="36">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Weidig, 2010" authorityName="Weidig" authorityYear="2010" box="[276,574,1601,1624]" class="Aves" family="Zygodactylidae" genus="Eozygodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="36" phylum="Chordata" rank="species" species="americanus" status="n.gen. and n.sp.">
