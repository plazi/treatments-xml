<collectionCode box="[584,635,1287,1313]" country="Russia" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="9" pageNumber="10" type="Museum">ZIN</collectionCode>
); middle flow of 
<collectingMunicipality box="[830,1063,1287,1313]" pageId="9" pageNumber="10">Mamontovaya River</collectingMunicipality>
, N 
<location LSID="urn:lsid:plazi:treatment:7B7E785C646B9F1B57EEFCDAFAC3EF6D:F6089F91646B9F1B5336FAC3FB0EEC51" box="[1103,1220,1287,1313]" country="Russia" county="Babiy" municipality="Mamontovaya River" name="Perkatkun" pageId="9" pageNumber="10" stateProvince="Chukotka">Perkatkun</location>
, on emergent stone in river, 
<date box="[246,375,1323,1349]" pageId="9" pageNumber="10" value="1972-07-29">
<collectingDate box="[246,375,1323,1349]" pageId="9" pageNumber="10" value="1972-07-29">29.vii.1972</collectingDate>
</date>
, 
<collectionCode box="[387,446,1323,1349]" country="Japan" httpUri="http://grbio.org/cool/8tq1-rbs2" name="Kyoto Botanical Garden" pageId="9" pageNumber="10">KBG</collectionCode>
(
<specimenCount box="[463,507,1323,1349]" pageId="9" pageNumber="10" type="male">3 ♂</specimenCount>
, 
