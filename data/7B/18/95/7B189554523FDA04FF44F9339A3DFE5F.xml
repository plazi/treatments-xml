Fig. 
<number box="[817,832,192,218]" pageId="7" pageNumber="46" value="2.0">2</number>
</figureCitation>
F); from 
<taxonomicName box="[958,1087,192,217]" class="Insecta" family="Agromyzidae" genus="Phytomyza" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis box="[958,1087,192,217]" italics="true" pageId="7" pageNumber="46">Phytomyza</emphasis>
</taxonomicName>
sp. on 
<taxonomicName box="[1184,1288,192,217]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="genus">
