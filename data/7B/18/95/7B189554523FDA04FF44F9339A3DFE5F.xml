<subSubSection pageId="7" pageNumber="46" type="biology_ecology">
<paragraph blockId="7.[151,1436,151,498]" pageId="7" pageNumber="46">
<emphasis bold="true" box="[198,298,151,177]" pageId="7" pageNumber="46">Biology.</emphasis>
Reared from 
<taxonomicName box="[460,588,152,177]" class="Insecta" family="Agromyzidae" genus="Phytomyza" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="46" phylum="Arthropoda" rank="genus" status="n. sp.">
<emphasis box="[460,588,152,177]" italics="true" pageId="7" pageNumber="46">Phytomyza</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[596,659,152,178]" pageId="7" pageNumber="46" rank="species">n. sp.</taxonomicNameLabel>
<number box="[666,680,152,178]" pageId="7" pageNumber="46" value="1.0">1</number>
(
<emphasis box="[698,763,152,177]" italics="true" pageId="7" pageNumber="46">sensu</emphasis>
<bibRefCitation author="Scheffer" box="[771,1109,152,178]" pageId="7" pageNumber="48" refString="Scheffer, S. J. &amp; Wiegmann, B. M. (2000) Molecular phylogenetics of the holly leafminers (Diptera: Agromyzidae: Phytomyza): species limits, speciation, and dietary specialization. Molecular Phylogenetics and Evolution, 17, 244 - 255." type="journal article" year="2000">
Scheffer &amp; Wiegmann, 
<number box="[1049,1109,152,178]" pageId="7" pageNumber="46" value="2000.0">2000</number>
</bibRefCitation>
) on 
<taxonomicName box="[1161,1298,152,177]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="species" species="cassine">
<emphasis box="[1161,1298,152,177]" italics="true" pageId="7" pageNumber="46">Ilex cassine</emphasis>
</taxonomicName>
and 
<taxonomicName class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="species" species="myrtifolia">
<emphasis italics="true" pageId="7" pageNumber="46">I. myrtifolia</emphasis>
</taxonomicName>
(holly, 
<taxonomicName box="[320,488,192,218]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="7" pageNumber="46" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
; SJS reared material, 
