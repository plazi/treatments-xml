<taxonomicName id="4C58EECBFFDE53272DDCF8C52EA8FF40" box="[648,897,1883,1905]" class="Arachnida" family="Salticidae" genus="Onomastus" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="435" phylum="Arthropoda" rank="species" species="jamestaylori" status="sp. nov.">
<emphasis id="B92C495AFFDE53272DDCF8C52EA8FF40" box="[648,897,1883,1905]" italics="true" pageId="4" pageNumber="435">Onomastus jamestaylori</emphasis>
</taxonomicName>
