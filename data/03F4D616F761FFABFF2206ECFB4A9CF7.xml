<taxonomicName id="4C5D1C83F761FFABFD280512FCF19CF7" box="[669,811,621,644]" class="Insecta" family="Cydnidae" genus="Cimex" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="568" phylum="Arthropoda" rank="species" species="aterrimus">
