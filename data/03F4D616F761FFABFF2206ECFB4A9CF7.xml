<collectingCountry id="F34A2790F761FFABFD44055BFCF69C4F" box="[753,812,548,572]" name="United States of America" pageId="4" pageNumber="568">USA</collectingCountry>
, 
<collectingRegion id="4999A9E2F761FFABFC82055AFCB49C4F" box="[823,878,549,572]" country="Tanzania" name="Mjini Magharibi" pageId="4" pageNumber="568">West</collectingRegion>
