<heading id="D0A181D1FFF6FFA2FF3CF8CEFC88F8B7" bold="true" box="[151,874,1857,1883]" fontSize="11" level="1" pageId="2" pageNumber="42" reason="1">
<emphasis id="B922EAAFFFF6FFA2FF3CF8CEFC88F8B7" bold="true" box="[151,874,1857,1883]" pageId="2" pageNumber="42">
<taxonomicName id="4C564D3EFFF6FFA2FF3CF8CEFD89F8B7" box="[151,619,1857,1883]" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="2" pageNumber="42" phylum="Sipuncula" rank="subSpecies" species="vulgaris" subGenus="Golfingia" subSpecies="vulgaris">
<emphasis id="B922EAAFFFF6FFA2FF3CF8CEFEE9F8B7" bold="true" box="[151,267,1857,1883]" italics="true" pageId="2" pageNumber="42">Golfingia</emphasis>
(
<emphasis id="B922EAAFFFF6FFA2FEB7F8CEFE6DF8B7" bold="true" box="[284,399,1857,1883]" italics="true" pageId="2" pageNumber="42">Golfingia</emphasis>
) 
<emphasis id="B922EAAFFFF6FFA2FE0BF8CEFD89F8B7" bold="true" box="[416,619,1857,1883]" italics="true" pageId="2" pageNumber="42">vulgaris vulgaris</emphasis>
</taxonomicName>
(de Blainville, 
<number id="3D9E249AFFF6FFA2FC8DF8CEFC80F8B7" box="[806,866,1857,1883]" pageId="2" pageNumber="42" value="1827.0">1827</number>
)
</emphasis>
