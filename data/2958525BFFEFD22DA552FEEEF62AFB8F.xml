<taxonomicName id="66F198CEFFEFD22EA5C5FE92F04CFEA2" authority="Rakha &amp; McCoy, 1984" authorityName="Rakha &amp; McCoy" authorityYear="1984" box="[415,845,358,382]" class="Arachnida" family="Stigmaeidae" genus="Eryngiopus" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="22" phylum="Arthropoda" rank="species" species="citri">
