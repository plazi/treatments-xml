<taxonomicName id="622D8E66A53CFB702C25055173917C5A" authorityName="Dejean" authorityYear="1835" class="Insecta" family="Trogossitidae" genus="Gymnocheilis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
