<elevation id="21C8D1B92B31FFB3FD338F57FD376483" box="[684,744,1918,1942]" metricMagnitude="1" metricUnit="m" metricValue="3.4" pageId="70" pageNumber="71" unit="m" value="34.0">34 m</elevation>
