<taxonomicName id="4FB94D0B53689670C77FF918FE1678ED" box="[283,484,1685,1707]" class="Insecta" family="Braconidae" genus="Gelastagathis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="61" phylum="Arthropoda" rank="species" species="frosti">
