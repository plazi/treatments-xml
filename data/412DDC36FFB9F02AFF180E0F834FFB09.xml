<taxonomicName id="0E8416A3FFB9F02AFDF50E2E8220FC31" authority="Capuron." authorityName="Capuron." box="[576,768,888,912]" class="Magnoliopsida" family="Sapotaceae" genus="Tsebona" kingdom="Plantae" order="Ericales" pageId="7" pageNumber="979" phylum="Tracheophyta" rank="genus">
