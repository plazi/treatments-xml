<taxonomicName id="09A803A0FFC3F93F5A1BF91FF748F974" authority="Berglund &amp; Feldmann, 1989" authorityName="Berglund &amp; Feldmann" authorityYear="1989" box="[312,791,1706,1730]" class="Malacostraca" family="Lyreididae" genus="Rogueus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="orri">
