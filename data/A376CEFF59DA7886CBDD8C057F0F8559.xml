<subSubSection id="7A672A1089D8AFCBE620E7FF95D725B4" pageId="238" pageNumber="239" type="specimens examined">
<paragraph id="617E257B459439D11248CB6234A0F21E" pageId="238" pageNumber="239">Specimens examined.</paragraph>
<paragraph id="61AD82127B87807881527C3ED9C5F64C" pageId="238" pageNumber="239">
