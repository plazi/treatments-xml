<emphasis id="CB8B81141A33F46B3EB14E971882FE4C" box="[932,1220,413,438]" italics="true" pageId="35" pageNumber="36">
<taxonomicName id="3EFF26851A33F46B3EB14E97186AFE4C" box="[932,1068,413,438]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="saccharum">Saccharum</taxonomicName>
