<taxonomicName id="C6EFE1FDC3FDF4D62B1224DD8FDD2657" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179321" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sima mocquerysi emacerata Santschi" lsidName-HNS="Sima mocquerysi emacerata Santschi" order="Hymenoptera" pageNumber="57" phylum="Arthropoda" rank="species" species="emacerata">Sima emacerata Sants</taxonomicName>
