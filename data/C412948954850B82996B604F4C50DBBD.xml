<paragraph id="86B9D50BCC962ED7101AE9B937F0263F" pageNumber="57">
(
<taxonomicName id="170F75A9886C6B46D144661E2926C9DB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179321" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sima mocquerysi emacerata Santschi" lsidName-HNS="Sima mocquerysi emacerata Santschi" order="Hymenoptera" pageNumber="57" phylum="Arthropoda" rank="subSpecies" species="mocquerysi" subSpecies="emacerata">Sima mocquerysi Andre st. emacerata Santschi</taxonomicName>
