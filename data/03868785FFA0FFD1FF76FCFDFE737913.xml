<collectingCounty id="62F14E1FFFA0FFD5FC08FB80FB697F74" box="[1001,1167,1123,1149]" pageId="41" pageNumber="42">Highlands Co.</collectingCounty>
, 
<location id="8EF06048FFA0FFD5FB7CFB80FAC17F74" LSID="urn:lsid:plazi:treatment:03868785FFA0FFD1FF76FCFDFE737913:8EF06048FFA0FFD5FB7CFB80FAC17F74" box="[1181,1319,1123,1149]" country="United States of America" county="Highlands Co." municipality="Archbold Biological Station" name="Lake Placid" pageId="41" pageNumber="42">Lake Placid</location>
, 
<collectingMunicipality id="6BF4ACE9FFA0FFD5FAD2FB80FE8D7FA8" pageId="41" pageNumber="42">Archbold Biological Station</collectingMunicipality>
, 15/04/08, 
