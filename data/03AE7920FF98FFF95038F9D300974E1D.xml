<taxonomicName id="4C07B3B5FF9FFFF95247FE6B00EC4DDD" authority="Csiki" authorityName="Csiki" authorityYear="1909" box="[794,985,473,497]" class="Insecta" family="Staphylinidae" genus="Bironium" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="231" phylum="Arthropoda" rank="species" species="longipes">
