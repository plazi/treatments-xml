Both, 
<taxonomicName id="4C07B3B5FF9FFFF95343FE6B01EA4DDD" authority="(Pic)" authorityName=" Middle" baseAuthorityName="Pic" baseAuthorityYear="1956" box="[542,735,473,497]" class="Insecta" family="Staphylinidae" genus="Bironium" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="231" phylum="Arthropoda" rank="species" species="basicolle">
