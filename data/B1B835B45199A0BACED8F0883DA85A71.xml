<taxonomicName id="63A445E556E72972F83690F728D27A7D" LSID="http://zoobank.org/E1FE66AD-EFBB-4913-9F55-C7708AC8A80C" authority="Cao &amp; Li" class="Arachnida" family="Salticidae" genus="Carrhotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Carrhotus sarahcrewsae" order="Araneae" pageId="7" pageNumber="50" phylum="Arthropoda" rank="species" species="sarahcrewsae">Carrhotus sarahcrewsae Cao &amp; Li</taxonomicName>
