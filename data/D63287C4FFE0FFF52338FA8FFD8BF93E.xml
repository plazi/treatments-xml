<specimenCount id="489DFD5BFFE0FFF523BCFA60FDABFA57" box="[389,514,1468,1492]" pageId="12" pageNumber="393" type="generic">2 specimens</specimenCount>
: 
<emphasis id="6CEFEAC0FFE0FFF52037FA60FDEBFA57" bold="true" box="[526,578,1468,1492]" pageId="12" pageNumber="393">
<collectingCountry id="268C7642FFE0FFF52037FA60FDEBFA57" box="[526,578,1468,1492]" name="Togo" pageId="12" pageNumber="393">Togo</collectingCountry>
</emphasis>
. Misa-Höhe, 
<accessionNumber id="41C8AB31FFE0FFF520E9FA60FF6FFA6D" httpUri="http://www.ncbi.nlm.nih.gov/protein/ZMB13773" pageId="12" pageNumber="393">
<collectionCode id="388AAE17FFE0FFF520E9FA60FCAEFA50" box="[720,775,1468,1491]" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="12" pageNumber="393" type="Museum">ZMB</collectionCode>
13773
</accessionNumber>
(
<typeStatus id="81208870FFE0FFF522ECFA0AFE87FA6D" box="[213,302,1494,1518]" pageId="12" pageNumber="393">holotype</typeStatus>
of 
<taxonomicName id="999B4D51FFE0FFF5236BFA0AFD7DFA6D" authority="Werner, 1897" authorityName="Werner" authorityYear="1897" box="[338,724,1494,1518]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="393" phylum="Chordata" rank="species" species="intestinalis">
