<paragraph id="8B8AA758FFE6FFEADE83FE2FFB1B7953" blockId="103.[689,1153,824,847]" box="[689,1153,824,847]" pageId="103" pageNumber="104">
<heading id="D0C21034FFE6FFEADE83FE2FFB1B7953" box="[689,1153,824,847]" centered="true" fontSize="9" level="2" pageId="103" pageNumber="104" reason="2">
<taxonomicName id="4C35DCDBFFE6FFEADE83FE2FFC707953" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[689,1002,824,847]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="shimenensis" status="new species">
<emphasis id="B9417B4AFFE6FFEADE83FE2FFC707953" bold="true" box="[689,1002,824,847]" italics="true" pageId="103" pageNumber="104">Paramblynotus shimenensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFE6FFEADFC9FE2EFB1B7953" box="[1019,1153,825,847]" pageId="103" pageNumber="104" rank="species">new species</taxonomicNameLabel>
