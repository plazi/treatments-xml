<subSubSection id="41956560D7073966FF5AFDB6FBC19ACF" pageId="97" pageNumber="98" type="discussion">
<paragraph id="093036EBD7073966FF5AFDB6FD7B9A87" blockId="97.[151,1436,151,716]" pageId="97" pageNumber="98">
<emphasis id="3BFBEAF9D7073966FF5AFDB6FEAC9A63" bold="true" box="[199,367,583,608]" pageId="97" pageNumber="98">COMMENTS</emphasis>
. 
<taxonomicName id="CE8F4D68D7073966FE19FDB8FE3F9A63" box="[388,508,584,608]" class="Insecta" family="Pseudococcidae" genus="Rhizoecus" kingdom="Animalia" order="Hemiptera" pageId="97" pageNumber="98" phylum="Arthropoda" rank="species" species="albidus">
