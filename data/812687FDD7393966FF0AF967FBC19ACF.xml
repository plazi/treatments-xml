<collectingCountry id="7198767BD7073966FC2BFE81FBDA998B" box="[950,1049,368,392]" name="Hungary" pageId="97" pageNumber="98">Hungary</collectingCountry>
, 
<collectingCountry id="7198767BD7073966FBB9FE81FB979984" box="[1060,1108,368,391]" name="Iran" pageId="97" pageNumber="98">Iran</collectingCountry>
, 
<collectingCountry id="7198767BD7073966FBFDFE9EFB579984" box="[1120,1172,367,391]" name="Italy" pageId="97" pageNumber="98">Italy</collectingCountry>
, 
<collectingCountry id="7198767BD7073966FB02FE9EFAE79984" box="[1183,1316,367,391]" name="Kazakhstan" pageId="97" pageNumber="98">Kazakhstan</collectingCountry>
