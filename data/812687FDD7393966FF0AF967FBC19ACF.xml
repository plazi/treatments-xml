<taxonomicName id="CE8F4D68D7073966FB69FDB6FAA59A5C" box="[1268,1382,583,607]" class="Magnoliopsida" family="Cactaceae" kingdom="Plantae" order="Caryophyllales" pageId="97" pageNumber="98" phylum="Tracheophyta" rank="family">Cactaceae</taxonomicName>
