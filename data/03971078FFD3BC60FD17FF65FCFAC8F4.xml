<collectingRegion id="49FA6F8CFFD3BC67FEB4F90BFE6DC8F4" box="[366,464,1709,1730]" country="United States of America" name="California" pageId="35" pageNumber="36">California</collectingRegion>
, 
<collectingCountry id="F329E1FEFFD3BC67FE06F90BFDB2C8F4" box="[476,527,1709,1730]" name="United States of America" pageId="35" pageNumber="36">USA</collectingCountry>
