<quantity id="4CEF9B25FFD4FFF098BBFF49FB6130D0" box="[198,288,187,213]" metricMagnitude="3" metricUnit="m" metricValue="3.82" pageId="69" pageNumber="70" unit="m" value="3820.0">3,820 m</quantity>
(
<quantity id="4CEF9B25FFD4FFF09950FF48FBD430D3" box="[301,405,186,214]" metricMagnitude="3" metricUnit="m" metricValue="3.819144" pageId="69" pageNumber="70" unit="ft" value="12530.0">12,530 ft</quantity>
