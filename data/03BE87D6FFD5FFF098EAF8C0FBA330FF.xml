<quantity id="4CEF9B25FFD5FFF19A16F831F8F437D8" box="[619,693,1987,2013]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="68" pageNumber="69" unit="m" value="800.0">800 m</quantity>
(
<quantity id="4CEF9B25FFD5FFF19ABBF830F96237DB" box="[710,803,1986,2014]" metricMagnitude="2" metricUnit="m" metricValue="8.001" pageId="68" pageNumber="69" unit="ft" value="2625.0">2,625 ft</quantity>
