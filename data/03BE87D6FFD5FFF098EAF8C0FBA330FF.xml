<geoCoordinate id="EE235007FFD5FFF199FBF889FBA83790" box="[390,489,1915,1941]" degrees="32" direction="north" minutes="07" orientation="latitude" pageId="68" pageNumber="69" precision="925" value="32.116665">32°07’N</geoCoordinate>
