<subSubSection id="32ED6BBD85669600FCD67C3BFBDEF932" pageId="11" pageNumber="218" type="biology_ecology">
<paragraph id="7A48383685669600FCD67C3BFBDEF932" blockId="11.[828,1437,1390,1732]" pageId="11" pageNumber="218">
<emphasis id="4883E42485669600FCD67C3BFC25F992" bold="true" box="[828,919,1549,1572]" pageId="11" pageNumber="218">Biology.</emphasis>
