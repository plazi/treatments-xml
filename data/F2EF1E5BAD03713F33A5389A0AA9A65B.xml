<paragraph id="8237C538BA404545EA7DF722EDFC0E2D" pageId="null" pageNumber="391">
<taxonomicName id="2ECD0386EBB4A087AF2A6F787B154D84" class="Magnoliopsida" family="Campanulaceae" genus="Campanula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="391" phylum="Tracheophyta" rank="genus">
