<taxonomicName id="FDE0848EFF9CB26EF56CF9BAFD62A385" authority="Dendy, 1892" authorityName="Dendy" authorityYear="1892" box="[293,646,1654,1679]" class="Calcarea" family="Lelapiidae" genus="Grantiopsis" kingdom="Animalia" order="Leucosolenida" pageId="39" pageNumber="242" phylum="Porifera" rank="species" species="cylindrica">
