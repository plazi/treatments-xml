<taxonomicName id="4C2F4D6ADC5BFF93FDD4F97184DB719C" box="[553,754,1728,1751]" class="Arachnida" family="Salticidae" genus="Pellenes" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="montanus">
