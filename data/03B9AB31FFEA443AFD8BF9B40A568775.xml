<taxonomicName id="4C1061A4FFE9443AFE28FE44085E84EF" authority="Kolliker, 1880" authorityName="* Kolliker" authorityYear="1880" box="[450,869,447,473]" class="Anthozoa" family="Umbellulidae" genus="Umbellula" kingdom="Animalia" order="Pennatulacea" pageId="6" pageNumber="127" phylum="Cnidaria" rank="species" species="durissima">
<emphasis id="B964C635FFE9443AFE28FE44098C84EF" box="[450,695,447,473]" italics="true" pageId="6" pageNumber="127">Umbellula durissima</emphasis>
<bibRefCitation id="EF8167D6FFE9443AFD56FE44085E84EF" author="Kolliker" box="[700,869,447,473]" firstAuthor="Kolliker" pageId="6" pageNumber="127" pagination="1 - 41" refId="ref9265" refString="Kolliker R. A. von 1880. Report on the PENNATULIDA dredged by H. M. S. Challenger during the years 1873 - 1876. Report on the scientific results of the voyage of H. M. S. Challenger during the years 1873 - 1876. Zoology 1 (2): 1 - 41." type="journal article" year="1880">Kölliker, 1880</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="4C1061A4FFE9443AFC73FE440BC384CA" authority="Thomson &amp; Henderson, 1906" authorityName="* Thomson &amp; Henderson" authorityYear="1906" class="Anthozoa" family="Umbellulidae" genus="Umbellula" kingdom="Animalia" order="Pennatulacea" pageId="6" pageNumber="127" phylum="Cnidaria" rank="species" species="rosea">
<emphasis id="B964C635FFE9443AFC73FE440F6184EF" box="[921,1114,447,473]" italics="true" pageId="6" pageNumber="127">Umbellula rosea</emphasis>
<bibRefCitation id="EF8167D6FFE9443AFB8AFE440BC384CA" author="Thomson &amp; Henderson" firstAuthor="Thomson" pageId="6" pageNumber="127" refId="ref11013" refString="Thomson J. A. &amp; Henderson W. D. 1906. An Account of the Alcyonarians Collected by the Royal Indian Marine Survey Ship ' Investigator ' in the Indian Ocean. 1. The Alcyonarians of the Deep Sea. Trustees of the Indian Museum, Calcuta." type="book" year="1906">Thomson &amp; Henderson, 1906</bibRefCitation>
</taxonomicName>
