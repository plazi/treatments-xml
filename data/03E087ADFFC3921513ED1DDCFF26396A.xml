Colony encrusting (
<figureCitation id="13722A3EFFC2921511401B3FFD563A49" box="[570,661,321,345]" captionStart="FIGURE 4" captionStartId="7.[151,250,1671,1693]" captionTargetBox="[151,1436,1189,1658]" captionTargetId="figure@7.[151,1436,1189,1658]" captionTargetPageId="7" captionText="FIGURE 4. Celleporaria mordax (MARCUS, 1937A), UFBA 1639. A. OVERVIEW Of A COLONY.B. CLOSE-UP Of AUTOZOOIDS, SHOWINg THE D-SHAPED PRIMARY ORIfICE, BASES Of ORAL SPINES, SUBORAL AVICULARIUM WITH SERRATE ROSTRUM, AND A LARgE, SPATULATE AVICULARIUM. SCALE BARS: A, 200 µM; B, 100 µM." httpUri="https://zenodo.org/record/1292027/files/figure.png" pageId="7" pageNumber="408">Fig. 4A</figureCitation>
), unilaminar. 
<collectionCode id="ED58AE7EFFC2921510541B3FFC6C3A49" box="[814,943,321,345]" pageId="7" pageNumber="408">Autozooids</collectionCode>
<collectionCode id="ED58AE7EFFC2921510CF1B3FFBE83A49" box="[949,1067,321,345]" pageId="7" pageNumber="408">irregularly</collectionCode>
<collectionCode id="ED58AE7EFFC2921517481B3FFB473A49" box="[1074,1156,321,345]" pageId="7" pageNumber="408">shaped</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921517F71B3FFAC83A49" box="[1165,1291,321,345]" pageId="7" pageNumber="408">surrounded</collectionCode>
by 
<collectionCode id="ED58AE7EFFC29215164E1B3CFA9D3A49" box="[1332,1374,322,345]" pageId="7" pageNumber="408">few</collectionCode>
<collectionCode id="ED58AE7EFFC29215161F1B3FFA5F3A49" box="[1381,1436,321,345]" pageId="7" pageNumber="408">large</collectionCode>
<collectionCode id="ED58AE7EFFC2921513ED1B18FF393A6E" box="[151,250,358,382]" pageId="7" pageNumber="408">marginal</collectionCode>
<collectionCode id="ED58AE7EFFC2921512791B18FE873A6E" box="[259,324,358,382]" pageId="7" pageNumber="408">pores</collectionCode>
. 
<collectionCode id="ED58AE7EFFC29215122A1B18FE623A6E" box="[336,417,358,382]" pageId="7" pageNumber="408">Frontal</collectionCode>
<collectionCode id="ED58AE7EFFC2921512D01B18FE193A6E" box="[426,474,358,382]" pageId="7" pageNumber="408">wall</collectionCode>
<collectionCode id="ED58AE7EFFC29215129E1B18FDA83A6E" box="[484,619,358,382]" pageId="7" pageNumber="408">imperforate</collectionCode>
, 
<collectionCode id="ED58AE7EFFC29215110D1B18FD7E3A6E" box="[631,701,358,382]" pageId="7" pageNumber="408">rough</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921511B21B18FD393A6D" box="[712,762,358,381]" pageId="7" pageNumber="408">with</collectionCode>
<collectionCode id="ED58AE7EFFC2921510791B18FC833A6E" box="[771,832,358,382]" pageId="7" pageNumber="408">small</collectionCode>
<collectionCode id="ED58AE7EFFC2921510331B18FC663A6E" box="[841,933,358,382]" pageId="7" pageNumber="408">nodules</collectionCode>
. 
<collectionCode id="ED58AE7EFFC2921510CB1B18FBC83A6E" box="[945,1035,358,382]" pageId="7" pageNumber="408">Primary</collectionCode>
<collectionCode id="ED58AE7EFFC29215176E1B18FB9E3A6E" box="[1044,1117,358,382]" pageId="7" pageNumber="408">orifice</collectionCode>
(
<figureCitation id="13722A3EFFC2921517141B18FB043A6E" box="[1134,1223,358,382]" captionStart="FIGURE 4" captionStartId="7.[151,250,1671,1693]" captionTargetBox="[151,1436,1189,1658]" captionTargetId="figure@7.[151,1436,1189,1658]" captionTargetPageId="7" captionText="FIGURE 4. Celleporaria mordax (MARCUS, 1937A), UFBA 1639. A. OVERVIEW Of A COLONY.B. CLOSE-UP Of AUTOZOOIDS, SHOWINg THE D-SHAPED PRIMARY ORIfICE, BASES Of ORAL SPINES, SUBORAL AVICULARIUM WITH SERRATE ROSTRUM, AND A LARgE, SPATULATE AVICULARIUM. SCALE BARS: A, 200 µM; B, 100 µM." httpUri="https://zenodo.org/record/1292027/files/figure.png" pageId="7" pageNumber="408">Fig. 4B</figureCitation>
) 
<collectionCode id="ED58AE7EFFC2921517A31B18FA8B3A6E" box="[1241,1352,358,382]" pageId="7" pageNumber="408">D-shaped</collectionCode>
, 
<collectionCode id="ED58AE7EFFC29215162F1B18FA443A6D" box="[1365,1415,358,381]" pageId="7" pageNumber="408">with</collectionCode>
a 
<collectionCode id="ED58AE7EFFC2921513ED1BF7FF1B3AB1" box="[151,216,393,417]" pageId="7" pageNumber="408">small</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921513981BF7FE8E3AB1" box="[226,333,393,417]" pageId="7" pageNumber="408">U-shaped</collectionCode>
<collectionCode id="ED58AE7EFFC29215122C1BF7FE533AB1" box="[342,400,393,417]" pageId="7" pageNumber="408">sinus</collectionCode>
(
<collectionCode id="ED58AE7EFFC2921512E61BF7FDEE3AB1" box="[412,557,393,417]" pageId="7" pageNumber="408">occasionally</collectionCode>
<collectionCode id="ED58AE7EFFC29215114E1BF4FDA53AB1" box="[564,614,394,417]" pageId="7" pageNumber="408">with</collectionCode>
<collectionCode id="ED58AE7EFFC2921511141BF7FD5B3AB1" box="[622,664,393,417]" pageId="7" pageNumber="408">two</collectionCode>
<collectionCode id="ED58AE7EFFC2921511E51BF7FCF23AB1" box="[671,817,393,417]" pageId="7" pageNumber="408">indentations</collectionCode>
); 2–4 
<collectionCode id="ED58AE7EFFC2921510091BF7FC5C3AB1" box="[883,927,393,417]" pageId="7" pageNumber="408">oral</collectionCode>
<collectionCode id="ED58AE7EFFC2921510DC1BF7FC323AB1" box="[934,1009,393,417]" pageId="7" pageNumber="408">spines</collectionCode>
. Suboral 
<collectionCode id="ED58AE7EFFC2921517201BF7FB233AB1" box="[1114,1248,393,417]" pageId="7" pageNumber="408">avicularium</collectionCode>
<collectionCode id="ED58AE7EFFC2921517921BF7FAEA3AB1" box="[1256,1321,393,417]" pageId="7" pageNumber="408">small</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921516491BF7FA5A3AB1" box="[1331,1433,393,417]" pageId="7" pageNumber="408">elliptical</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921513ED1BD0FF2D3AD6" box="[151,238,430,454]" pageId="7" pageNumber="408">rostrum</collectionCode>
<collectionCode id="ED58AE7EFFC29215138F1BD0FE913AD6" box="[245,338,430,454]" pageId="7" pageNumber="408">serrated</collectionCode>
, obliquely 
<collectionCode id="ED58AE7EFFC2921512B41BD0FDE93AD6" box="[462,554,430,454]" pageId="7" pageNumber="408">oriented</collectionCode>
<collectionCode id="ED58AE7EFFC29215114B1BD0FDBB3AD5" box="[561,632,430,453]" pageId="7" pageNumber="408">within</collectionCode>
<collectionCode id="ED58AE7EFFC2921511FA1BD0FD613AD5" box="[640,674,430,453]" pageId="7" pageNumber="408">the</collectionCode>
<collectionCode id="ED58AE7EFFC2921511D01BD0FCDE3AD6" box="[682,797,430,454]" pageId="7" pageNumber="408">peristome</collectionCode>
. 
<collectionCode id="ED58AE7EFFC29215105D1BD0FC6D3AD6" box="[807,942,430,454]" pageId="7" pageNumber="408">Interzooidal</collectionCode>
<collectionCode id="ED58AE7EFFC2921510CC1BD0FBF83AD6" box="[950,1083,430,454]" pageId="7" pageNumber="408">avicularium</collectionCode>
<collectionCode id="ED58AE7EFFC2921517391BD0FBBC3AD6" box="[1091,1151,430,454]" pageId="7" pageNumber="408">large</collectionCode>
, 
<collectionCode id="ED58AE7EFFC2921517F21BD0FB333AD6" box="[1160,1264,430,454]" pageId="7" pageNumber="408">spatulate</collectionCode>
, 
