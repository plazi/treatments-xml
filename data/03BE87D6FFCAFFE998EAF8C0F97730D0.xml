<quantity id="4CEF9B25FFCAFFEE9CB0F86DFF4737BC" box="[1229,1286,1951,1977]" metricMagnitude="1" metricUnit="m" metricValue="2.9" pageId="91" pageNumber="92" unit="m" value="29.0">29 m</quantity>
(
<quantity id="4CEF9B25FFCAFFEE9D69F86CFF0A37BF" box="[1300,1355,1950,1978]" metricMagnitude="1" metricUnit="m" metricValue="2.8956" pageId="91" pageNumber="92" unit="ft" value="95.0">95 ft</quantity>
