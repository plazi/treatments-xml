<taxonomicName id="8BA66B231DB0F5437076B64CC32DD7CF" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pinus yunnanensis" order="Pinales" pageId="18" pageNumber="111" phylum="Tracheophyta" rank="species" species="yunnanensis">Pinus yunnanensis</taxonomicName>
