1. 
<normalizedToken id="3EA1DC9D507E51DD7BFBA10FC4510F7C" originalValue="Fruchtschläuche" pageId="null" pageNumber="487">Fruchtschlaeuche</normalizedToken>
<normalizedToken id="1518D969C335DC5175510D292AB6AE9F" originalValue="über" pageId="null" pageNumber="487">ueber</normalizedToken>
