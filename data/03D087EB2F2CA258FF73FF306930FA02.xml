<taxonomicName id="4C794D7E2F2AA25BFEFFF99C6E1FF9D5" box="[283,532,1588,1610]" class="Aves" family="Furnariidae" genus="Tyrannidectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="314" phylum="Chordata" rank="species" species="synallaxis" status="sp. n.">
