<taxonomicName id="D0D34D56FFC5FFED0392FC1EF39AFC14" authority="Stoeckhert, 1942" authorityName="Stoeckhert" authorityYear="1942" box="[151,749,908,935]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="alutacea" subGenus="Proxiandrena">
<emphasis id="25A7EAC7FFC5FFED0392FC1EF39AFC14" bold="true" box="[151,749,908,935]" pageId="18" pageNumber="219">
<emphasis id="25A7EAC7FFC5FFED0392FC1EF18BFC14" bold="true" box="[151,252,908,935]" italics="true" pageId="18" pageNumber="219">Andrena</emphasis>
(
<emphasis id="25A7EAC7FFC5FFED0208FC1EF0DAFC14" bold="true" box="[269,429,908,935]" italics="true" pageId="18" pageNumber="219">Proxiandrena</emphasis>
) 
<emphasis id="25A7EAC7FFC5FFED02B8FC1EF368FC14" bold="true" box="[445,543,908,935]" italics="true" pageId="18" pageNumber="219">alutacea</emphasis>
Stoeckhert, 1942
</emphasis>
</taxonomicName>
