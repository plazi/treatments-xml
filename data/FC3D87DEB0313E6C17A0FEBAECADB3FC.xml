<paragraph id="742B36C8B0313E6B17A0FE0CEC0CB49F" blockId="67.[178,1086,395,656]" pageId="67" pageNumber="816">
<taxonomicName id="B3944D4BB0313E6B17A0FE0CEE70B7F2" authority="Schwarz, 2015" authorityName="Schwarz" authorityYear="2015" box="[178,359,488,509]" class="Insecta" family="Ichneumonidae" genus="Cryptus" kingdom="Animalia" order="Hymenoptera" pageId="67" pageNumber="816" phylum="Arthropoda" rank="species" species="adversus" status="nov.sp.">
