<taxonomicName id="B3944D4BB0313E6B16B6FDE1ED83B414" authority="(GEOFFROY)" baseAuthorityName="GEOFFROY" box="[420,660,517,539]" class="Insecta" family="Ichneumonidae" genus="Cryptus" kingdom="Animalia" order="Hymenoptera" pageId="67" pageNumber="816" phylum="Arthropoda" rank="species" species="spiralis">
