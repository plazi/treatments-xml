<heading id="B63F9C02FE4FFFD3FD16467FFB5BF915" box="[732,1196,1640,1664]" centered="true" fontSize="36" level="2" pageId="10" pageNumber="11" reason="8">
<taxonomicName id="2AC850EDFE4FFFD3FD16467FFB5BF915" authority="Ameghino, 1901" authorityName="Ameghino" authorityYear="1901" box="[732,1196,1640,1664]" class="Mammalia" family="Isotemnidae" genus="Thomashuxleya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Notoungulata" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="externa">
<emphasis id="DFBCF77CFE4FFFD3FD16467FFC1FF915" box="[732,1000,1640,1664]" italics="true" pageId="10" pageNumber="11">Thomashuxleya externa</emphasis>
<bibRefCitation id="8959569FFE4FFFD3FC3B467FFB5BF915" author="Ameghino, F." box="[1009,1196,1640,1664]" pageId="10" pageNumber="11" pagination="1 - 349" refId="ref14857" refString="Ameghino, F. 1901. Notices preliminaries sur des ongules nouveaux des terrains Cretaces de Patagonie. Boletin de la Academia Nacional de Ciencias de Cordoba 16: 1 - 349." type="journal article" year="1901">Ameghino, 1901</bibRefCitation>
