<materialsCitation id="7DE75A14FF92AC56B7B2FC6A8BC5FC55" collectingDate="1999-03-03" collectionCode="TYPUS" collectorName="de Mafate" country="Réunion" location="Cirque de Mafate" pageId="1" pageNumber="22" specimenCount="1" typeStatus="holotype">
