. — 
<emphasis id="FFFB8C5BFF92AC56B601FC558C81FC32" box="[855,988,927,952]" italics="true" pageId="1" pageNumber="22">Castillon s.n.</emphasis>
, 
<collectingCountry id="B59810D9FF92AC56B6BAFC6A8B37FC3C" box="[1004,1130,927,952]" name="Réunion" pageId="1" pageNumber="22">La Réunion</collectingCountry>
, 
<location id="C8500692FF92AC56B121FC558C4FFC55" LSID="urn:lsid:plazi:treatment:4526E15FFF92AC53B79CFD638C6EFD93:C8500692FF92AC56B121FC558C4FFC55" country="Réunion" name="Cirque de Mafate" pageId="1" pageNumber="22">
Cirque 
<collectorName id="607A359FFF92AC56B191FC558C4FFC55" pageId="1" pageNumber="22">de Mafate</collectorName>
</location>
, fleuri au Tampon, 
<date id="B9317689FF92AC56B683FC4C8B0DFC55" box="[981,1104,952,977]" pageId="1" pageNumber="22" value="1999-03-03">
<collectingDate id="A9758F61FF92AC56B683FC4C8B0DFC55" box="[981,1104,952,977]" pageId="1" pageNumber="22" value="1999-03-03">3 mars 1999</collectingDate>
