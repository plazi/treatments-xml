<collectingRegion id="49EEF847371AB953FC52544EFB560B79" box="[973,1157,524,547]" country="United States of America" name="North Carolina" pageId="87" pageNumber="88">North Carolina</collectingRegion>
, 
<collectingRegion id="49EEF847371AB953FB09544EFCD90B18" country="United States of America" name="Tennessee" pageId="87" pageNumber="88">Tennessee</collectingRegion>
, and 
<collectingRegion id="49EEF847371AB953FCD45469FC7A0B18" box="[843,937,555,578]" country="United States of America" name="Georgia" pageId="87" pageNumber="88">Georgia</collectingRegion>
