<number id="3DA624B4FF94CA4C37A6FE96FD05802F" box="[681,696,338,364]" pageId="9" pageNumber="10" value="2.0">2</number>
</figureCitation>
E, 
<number id="3DA624B4FF94CA4C37D7FE96FD5B802F" box="[728,742,338,364]" pageId="9" pageNumber="10" value="3.0">3</number>
C, 
<tableCitation id="C6EC0328FF94CA4C3606FE96FCE2802F" box="[777,863,338,364]" captionStart="TABLE 1" captionStartId="2.[151,239,151,175]" captionText="TABLE 1. Locations of surveyed aquatic sites." pageId="9" pageNumber="10">
