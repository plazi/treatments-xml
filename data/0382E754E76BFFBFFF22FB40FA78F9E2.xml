<taxonomicName id="4C2B2DC1E76AFFBFFA9AFE35FA78F9E2" authorityName="Aube" authorityYear="1838" box="[1327,1437,476,497]" class="Insecta" family="Dytiscidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="128" phylum="Arthropoda" rank="species" species="rimosus">
