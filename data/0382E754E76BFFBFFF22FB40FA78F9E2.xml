<taxonomicName id="4C2B2DC1E76BFFBEFA9AFA69FA78FD86" authorityName="Tremouilles" authorityYear="1996" box="[1327,1437,1407,1429]" class="Insecta" family="Dytiscidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="127" phylum="Arthropoda" rank="species" species="devexus">
