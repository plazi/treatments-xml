<taxonomicName id="4C2B2DC1E76BFFBEFA9BFA52FA79FDC3" authorityName="Laporte" authorityYear="1835" box="[1326,1436,1466,1488]" class="Insecta" family="Dytiscidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="127" phylum="Arthropoda" rank="species" species="lateralis">
