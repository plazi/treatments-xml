<taxonomicName id="4C2B2DC1E76BFFBEFAB6F9D9FA78FE56" authorityName="Laporte" authorityYear="1835" box="[1283,1437,1583,1605]" class="Insecta" family="Dytiscidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="127" phylum="Arthropoda" rank="species" species="subfasciatus">
<emphasis id="B95F8A50E76BFFBEFAB6F9D9FA78FE56" bold="true" box="[1283,1437,1583,1605]" italics="true" pageId="14" pageNumber="127">H. subfasciatus</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8B945642E76BFFBEFF22F9A7FA78FE8E" blockId="14.[151,1437,1261,2016]" pageId="14" pageNumber="127">
<keyLead id="30DAF577E76BFFBEFF22F9A7FA78FE8E" pageId="14" pageNumber="127">
- TL 
<quantity id="4CD3FBA7E76BFFBEFEB1F9A4FE63FE70" box="[260,390,1613,1635]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.04" metricValueMin="0.96" pageId="14" pageNumber="127" unit="mm" value="10.0" valueMax="10.4" valueMin="9.6">9.6–10.4 mm</quantity>
