<taxonomicName id="6DFD27A9BF62FFDE5A77FA0B34A14D6A" box="[264,463,1516,1542]" class="Magnoliopsida" family="Chenopodiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="family">Chenopodiaceae</taxonomicName>
