<taxonomicName id="FE89B6E0FF98A27D59C57947FDB1F9BC" authority="Handschin, 1942" authorityName="Handschin" authorityYear="1942" box="[238,593,1546,1573]" class="Entognatha" family="Neanuridae" genus="Aethiopella" kingdom="Animalia" order="Collembola" pageId="1" pageNumber="40" phylum="Arthropoda" rank="genus">
