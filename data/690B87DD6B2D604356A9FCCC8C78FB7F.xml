Ex 
<taxonomicName id="26A24D486B2D60435791FCB78A5AFC9F" authority="(Vieillot, 1818)" authorityName="Vieillot" authorityYear="1818" baseAuthorityName="Vieillot" baseAuthorityYear="1818" box="[431,889,869,894]" class="Aves" family="Thraupidae" genus="Trichothraupis" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="174" phylum="Chordata" rank="species" species="melanops">
<emphasis id="D3D6EAD96B2D60435791FCB78BEBFC9F" box="[431,712,869,894]" italics="true" pageId="13" pageNumber="174">Trichothraupis melanops</emphasis>
