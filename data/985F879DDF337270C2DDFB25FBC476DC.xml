<taxonomicName id="D7F64D08DF317270C079FE30FD8576C3" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,613,441,464]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="juno">
