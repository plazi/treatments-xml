<taxonomicName id="4C304D39FFC1FF8EFEF373ABC1B39238" authorityName="Cheng, Gao &amp; Chen" authorityYear="2009" box="[341,724,1522,1545]" class="Insecta" family="Drosophilidae" genus="Stegana" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="40" phylum="Arthropoda" rank="species" species="albiventralis" status="sp. nov." subGenus="Steganina">
