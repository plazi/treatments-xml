<subSubSection id="1A8E3098FFCD7A79450EFCD1DDF90C3D" box="[312,1003,804,830]" pageId="125" pageNumber="126" type="etymology">
<paragraph id="522B6313FFCD7A79450EFCD1DDF90C3D" blockId="125.[264,1324,284,910]" box="[312,1003,804,830]" pageId="125" pageNumber="126">
<emphasis id="60E0BF01FFCD7A79450EFCD1DFAF0C3D" bold="true" box="[312,445,804,830]" pageId="125" pageNumber="126">Etymology</emphasis>
