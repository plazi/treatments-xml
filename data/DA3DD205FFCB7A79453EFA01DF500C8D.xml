<taxonomicName id="95941890FFCD7A794671FCD1DCAB0C3E" authorityName="Paṡnik" authorityYear="2006" box="[583,697,804,829]" class="Insecta" family="Staphylinidae" genus="Tachyusa" kingdom="Animalia" order="Coleoptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="harbinica">
