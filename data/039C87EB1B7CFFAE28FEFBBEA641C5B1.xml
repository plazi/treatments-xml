<caption id="DF4A66751B7AFFAD28FEF8A4A2A9C0CE" ID-DOI="http://doi.org/10.5281/zenodo.5881222" ID-Zenodo-Dep="5881222" httpUri="https://zenodo.org/record/5881222/files/figure.png" pageId="5" pageNumber="323" startId="5.[151,250,1874,1898]" targetBox="[151,1435,1299,1850]" targetPageId="5">
