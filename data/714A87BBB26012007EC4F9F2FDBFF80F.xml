<taxonomicName id="3EE34D2EB27F12007B6DF8E7FE5EF86B" authority="Hampson" authorityName="Hampson" class="Insecta" family="Crambidae" genus="Acigona" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="517" phylum="Arthropoda" rank="species" species="steniellus">
