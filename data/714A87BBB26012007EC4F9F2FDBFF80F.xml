<taxonomicName id="3EE34D2EB27F12007D27F8E7FB24F8B7" authority="Walker" authorityName="Walker" box="[884,1276,1895,1920]" class="Insecta" family="Crambidae" genus="Scirpophaga" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="517" phylum="Arthropoda" rank="species" species="excerptalis">
