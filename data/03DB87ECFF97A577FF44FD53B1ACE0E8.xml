<taxonomicName id="4C724D79FF97A577FC3FFB9EB512E05C" box="[1004,1117,1041,1067]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="26" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
) (
<bibRefCitation id="EFE34B0BFF97A577FBA0FB9EB5BEE05C" author="Ruiz" box="[1139,1265,1041,1067]" pageId="10" pageNumber="30" refString="Ruiz, F. (1935) Algunas notas entomologicas. Revista Chilena de Historia Natural, 39, 271 - 278." type="journal article" year="1935">
Ruíz 
<number id="3DBA24DDFF97A577FB60FB9EB5BEE05C" box="[1203,1265,1041,1067]" pageId="10" pageNumber="26" value="1935.0">1935</number>
