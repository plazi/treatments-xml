(
<bibRefCitation id="EF864B31FF1AFF3E9936FEB9F80E3160" author="Jordan &amp; Starks" box="[331,591,330,357]" firstAuthor="Jordan" pageId="139" pageNumber="140" pagination="785 - 855" refId="ref288677" refString="Jordan, D. S. &amp; Starks, E. C. (1895) The fishes of Puget Sound. Proceedings of the California Academy of Sciences, 5, 785 - 855." type="journal article" year="1895">
