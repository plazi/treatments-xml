<number id="5B17A0239929FFAEEC4F9D56DA01F933" box="[1362,1376,1642,1666]" pageId="26" pageNumber="27" value="1.0">1</number>
long preapical 
<emphasis id="DFAB6E169929FFAEE81B9DB2DE43F917" box="[262,290,1678,1702]" italics="true" pageId="26" pageNumber="27">ad</emphasis>
and 
<number id="5B17A0239929FFAEE8469DB3DE08F916" box="[347,361,1679,1703]" pageId="26" pageNumber="27" value="1.0">1</number>
short 
<emphasis id="DFAB6E169929FFAEE8AF9DB3DEB8F917" box="[434,473,1679,1702]" italics="true" pageId="26" pageNumber="27">apv</emphasis>
. Mid femur with 
<number id="5B17A0239929FFAEEBB99DB3DDAFF916" box="[676,718,1679,1703]" pageId="26" pageNumber="27" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
<emphasis id="DFAB6E169929FFAEEBCB9DB3DD85F917" box="[726,740,1679,1702]" italics="true" pageId="26" pageNumber="27">a</emphasis>
and 
<number id="5B17A0239929FFAEEA039DB3DC4DF916" box="[798,812,1679,1703]" pageId="26" pageNumber="27" value="1.0">1</number>
<emphasis id="DFAB6E169929FFAEEA299DB3DC3FF917" box="[820,862,1679,1702]" italics="true" pageId="26" pageNumber="27">app</emphasis>
; mid tibia with 
<number id="5B17A0239929FFAEED0A9DB3DB44F916" box="[1047,1061,1679,1703]" pageId="26" pageNumber="27" value="1.0">1</number>
strong preapical 
<emphasis id="DFAB6E169929FFAEEDF79DB2DA67F917" box="[1258,1286,1678,1702]" italics="true" pageId="26" pageNumber="27">ad</emphasis>
and 
<number id="5B17A0239929FFAEEC229DB3DA2CF916" box="[1343,1357,1679,1703]" pageId="26" pageNumber="27" value="2.0">2</number>
strong 
<emphasis id="DFAB6E169929FFAEE98A9D8FDFDFF97B" box="[151,190,1715,1738]" italics="true" pageId="26" pageNumber="27">apv</emphasis>
. 
<collectingCountry id="95C8F2949929FFAEE9D09D8EDE64F97A" box="[205,261,1714,1739]" name="India" pageId="26" pageNumber="27">Hind</collectingCountry>
