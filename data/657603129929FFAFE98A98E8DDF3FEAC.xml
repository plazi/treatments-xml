<number id="5B17A0239929FFAEEB7A9D56DD14F932" box="[615,629,1642,1667]" pageId="26" pageNumber="27" value="5.0">5</number>
<emphasis id="DFAB6E169929FFAEEB629D57DDF9F933" box="[639,664,1643,1666]" italics="true" pageId="26" pageNumber="27">pv</emphasis>
, 
<number id="5B17A0239929FFAEEBB49D56DDD6F933" box="[681,695,1642,1666]" pageId="26" pageNumber="27" value="8.0">8</number>
<emphasis id="DFAB6E169929FFAEEBDC9D56DDBCF933" box="[705,733,1642,1666]" italics="true" pageId="26" pageNumber="27">pd</emphasis>
