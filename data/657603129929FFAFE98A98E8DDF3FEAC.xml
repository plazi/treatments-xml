<number id="5B17A0239929FFAEED1E9C30DB6BF8AB" box="[1027,1034,1804,1818]" pageId="26" pageNumber="27" value="3.0">3</number>
and R 
<number id="5B17A0239929FFAEED4E9C30DB3AF8AB" box="[1107,1115,1804,1818]" pageId="26" pageNumber="27" value="4.0">4</number>
