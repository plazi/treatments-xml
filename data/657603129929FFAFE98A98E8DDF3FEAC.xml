+ 
<number id="5B17A0239929FFAEEA359C30DC4EF8AB" box="[808,815,1804,1818]" pageId="26" pageNumber="27" value="3.0">3</number>
), 
<number id="5B17A0239929FFAEEA5B9DC0DC33F8A4" box="[838,850,1788,1813]" pageId="26" pageNumber="27" value="3.0">3</number>
