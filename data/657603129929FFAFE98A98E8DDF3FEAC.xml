<quantity id="2A271FE19929FFAEEC659C0EDAFDF88D" box="[1400,1436,1827,1856]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="26" pageNumber="27" unit="in" value="1.0">1 in</quantity>
