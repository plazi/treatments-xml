<number id="5B17A0239929FFAEE8889D8EDEC2F97B" box="[405,419,1714,1738]" pageId="26" pageNumber="27" value="1.0">1</number>
weak preapical 
<emphasis id="DFAB6E169929FFAEEB429D8EDD1AF97B" box="[607,635,1714,1738]" italics="true" pageId="26" pageNumber="27">ad</emphasis>
; hind tibia with 
<number id="5B17A0239929FFAEEA219D8EDC2BF97B" box="[828,842,1714,1738]" pageId="26" pageNumber="27" value="1.0">1</number>
preapical 
<emphasis id="DFAB6E169929FFAEEADF9D8EDCBFF97B" box="[962,990,1714,1738]" italics="true" pageId="26" pageNumber="27">ad</emphasis>
and 
<number id="5B17A0239929FFAEED059D8EDB47F97B" box="[1048,1062,1714,1738]" pageId="26" pageNumber="27" value="1.0">1</number>
