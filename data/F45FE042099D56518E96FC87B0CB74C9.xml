<taxonomicName id="3BAEE728F4E65E539EBDDF625400B4E4" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia melanoxylon" order="Fabales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="melanoxylon">
<emphasis id="B1AC354A2A3AD5DB82845413C2910313" italics="true" pageId="0" pageNumber="53690">Acacia melanoxylon</emphasis>
</taxonomicName>
, 
<taxonomicName id="3CF56685FA5B8981E5B187C3278AE0C5" class="Magnoliopsida" family="Betulaceae" genus="Alnus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Alnus glutinosa" order="Fagales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="glutinosa">
<emphasis id="7ADF72DA1269274069E1FB5065A2D166" italics="true" pageId="0" pageNumber="53690">Alnus glutinosa</emphasis>
</taxonomicName>
, 
<taxonomicName id="510126E5573FF3EE4E2E6BE8597BA36B" class="Magnoliopsida" family="Clethraceae" genus="Clethra" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Clethra arborea" order="Ericales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="arborea">
<emphasis id="CA8D7C2ECE6AC85B314BA10E90E22BE3" italics="true" pageId="0" pageNumber="53690">Clethra arborea</emphasis>
</taxonomicName>
