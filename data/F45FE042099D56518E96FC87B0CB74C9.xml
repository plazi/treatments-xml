<emphasis id="47FA5CE4B498B7C3A3F06DBFD227D2D1" italics="true" pageId="0" pageNumber="53690">
<taxonomicName id="E314F8538599BF50DC26D0475F08B3D8" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ilex perado" order="Aquifoliales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="perado">Ilex perado</taxonomicName>
, 
<taxonomicName id="5F3D6F378271A85ABD0E22B7A8B184EC" class="Magnoliopsida" family="Pittosporaceae" genus="Pittosporum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pittosporum undulatum" order="Apiales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="undulatum">Pittosporum undulatum</taxonomicName>
