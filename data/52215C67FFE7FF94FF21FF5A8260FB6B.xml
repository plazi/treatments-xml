<emphasis id="E8FC3163FFE7FF94FF21FCA481CDFCF3" bold="true" box="[151,172,834,859]" pageId="22" pageNumber="23">1.</emphasis>
<emphasis id="E8FC3163FFE7FF94FF05FCA48024FCF3" box="[179,325,834,859]" italics="true" pageId="22" pageNumber="23">Endeolophus</emphasis>
<taxonomicName id="1D8896F2FFE7FF94FEFAFCA5826EFCF3" authority="(Wieser, 1954) Boucher, 1976" authorityName="Boucher" authorityYear="1976" baseAuthorityName="Wieser" baseAuthorityYear="1954" box="[332,783,834,859]" class="Chromadorea" family="Chromadoridae" genus="Endeolophos" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chromadorida" pageId="22" pageNumber="23" phylum="Nematoda" rank="species" species="fossiferus">
