<taxonomicName id="4C2A4D02FF99562CFDB98A5C7774AD46" box="[570,799,1300,1325]" class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="13" pageNumber="372" phylum="Tracheophyta" rank="species" species="murorum">
<emphasis id="B95EEA93FF99562CFDB98A5C7774AD46" box="[570,799,1300,1325]" italics="true" pageId="13" pageNumber="372">Hieracium murorum</emphasis>
