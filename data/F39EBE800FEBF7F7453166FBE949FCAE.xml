<taxonomicName id="21793DE91143C5229A4612484DC5A2C6" authority="Li, 2002" authorityName="Li" authorityYear="2002" class="Insecta" family="Psocidae" genus="Pentablaste" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pentablaste lanceolata" order="Psocodea" pageId="11" pageNumber="38" phylum="Arthropoda" rank="species" species="lanceolata">Pentablaste lanceolata</taxonomicName>
