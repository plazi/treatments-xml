<taxonomicName id="6B9D2CD8C656FFD6FF5F1F15FDC21D6B" authority="Berest, 1988" authorityName="Berest" authorityYear="1988" box="[151,566,542,567]" class="Insecta" family="Cecidomyiidae" genus="Bryomyia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="257" phylum="Arthropoda" rank="species" species="multispinata">
<emphasis id="9EE98B49C656FFD6FF5F1F15FE6A1D6B" bold="true" box="[151,414,542,567]" italics="true" pageId="2" pageNumber="257">Bryomyia multispinata</emphasis>
Berest, 
<number id="1A55457CC656FFD6FE371F15FDC21D6B" box="[511,566,542,567]" pageId="2" pageNumber="257" value="1988.0">1988</number>
</taxonomicName>
—new junior synonym of 
<taxonomicName id="6B9D2CD8C656FFD6FCA41F15FB3E1D6B" authority="Felt, 1907" authorityName="Felt" authorityYear="1907" box="[876,1226,542,567]" class="Insecta" family="Cecidomyiidae" genus="Bryomyia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="257" phylum="Arthropoda" rank="species" species="gibbosa">
<emphasis id="9EE98B49C656FFD6FCA41F15FBC81D6B" bold="true" box="[876,1084,542,567]" italics="true" pageId="2" pageNumber="257">Bryomyia gibbosa</emphasis>
(Felt, 
<number id="1A55457CC656FFD6FB411F15FB361D6B" box="[1161,1218,542,567]" pageId="2" pageNumber="257" value="1907.0">1907</number>
)
</taxonomicName>
</emphasis>
</heading>
</paragraph>
