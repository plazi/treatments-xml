<number id="3D83722A834B9131F1269463FEC6FA67" box="[334,348,1405,1431]" pageId="8" pageNumber="53" value="5.0">5</number>
A, 
<number id="3D83722A834B9131F1E89463FE15FA67" box="[384,399,1405,1431]" pageId="8" pageNumber="53" value="6.0">6</number>
A, 
<number id="3D83722A834B9131F1DB9463FE5BFA67" box="[435,449,1405,1431]" pageId="8" pageNumber="53" value="7.0">7</number>
A–C, 
