(23) 
<emphasis id="B92CEAD8FF9D277FB523DC6224F9F89F" box="[315,444,1839,1862]" italics="true" pageId="18" pageNumber="19">
<taxonomicName id="4C584D49FF9D277FB523DC6224FDF89F" box="[315,440,1839,1862]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="zambiae">D. zambiae</taxonomicName>
