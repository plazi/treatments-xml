<taxonomicName id="4C584D49FF9D277FB0B9DC412060F8FA" box="[1185,1317,1804,1827]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="papuana">D. papuana</taxonomicName>
