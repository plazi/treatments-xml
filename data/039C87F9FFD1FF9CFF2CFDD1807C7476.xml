<taxonomicName id="4C354D6CFFD1FF9DFDFBF9AA810871EF" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" box="[576,826,1657,1681]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="365" phylum="Arthropoda" rank="species" species="sofielundensis">
