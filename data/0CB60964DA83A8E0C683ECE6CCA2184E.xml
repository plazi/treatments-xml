15. 
<taxonomicName id="AEC10816F1996547A96DDC9C1852645F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143855" class="Insecta" family="Formicidae" genus="Pseudomyrma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Pseudomyrma flavidula Smith" order="Hymenoptera" pageId="157" pageNumber="157" phylum="Arthropoda" rank="species" species="flavidula">Pseudomyrma flavidula</taxonomicName>
