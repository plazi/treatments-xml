<taxonomicName id="4C39CEFBFFA5D667E751FE32FB84FDAF" authority="(Rich.) Vahl." authorityName="Vahl." baseAuthorityName="Rich." box="[631,1094,504,531]" class="Magnoliopsida" family="Verbenaceae" genus="Stachytarpheta" kingdom="Plantae" order="Lamiales" pageId="9" pageNumber="160" phylum="Tracheophyta" rank="species" species="cayennensis">
