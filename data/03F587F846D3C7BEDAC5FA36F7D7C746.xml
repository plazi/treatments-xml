<taxonomicName id="4C5C4D6D46D3C7B0DAC6F9D9F7EBC04C" authority=": HEINRICH 1974" authorityName="HEINRICH" authorityYear="1974" box="[178,443,1573,1593]" class="Insecta" family="Ichneumonidae" genus="Pristicerops" kingdom="Animalia" order="Hymenoptera" pageId="199" pageNumber="1516" phylum="Arthropoda" rank="genus">
