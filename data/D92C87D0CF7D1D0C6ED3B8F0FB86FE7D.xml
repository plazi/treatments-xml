<taxonomicName id="96854D45CF7D1D0C6D51BC16FC6CFEDD" authorityName="Mendoza-Garfias &amp; García-Prieto &amp; León" authorityYear="2017" box="[813,922,311,337]" class="Monogenea" family="Ancyrocephalidae" genus="Scutogyrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dactylogyridea" pageId="26" pageNumber="527" phylum="Platyhelminthes" rank="genus">
