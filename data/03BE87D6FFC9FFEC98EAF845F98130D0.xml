</quantity>
(
<quantity id="4CEF9B25FFC8FFEC99F1FF49FBB730D3" box="[396,502,187,214]" metricMagnitude="3" metricUnit="m" metricValue="3.5692079999999997" pageId="89" pageNumber="90" unit="ft" value="11710.0">
