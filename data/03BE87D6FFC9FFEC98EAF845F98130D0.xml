<elevation id="003AD1F3FFC8FFEC9B19FF65FE6630B7" box="[868,1063,151,178]" metricMagnitude="3" metricUnit="m" metricValue="3.6236148" metricValueMax="3.6700968" metricValueMin="3.5771328" pageId="89" pageNumber="90" unit="ft" value="11888.5" valueMax="12041.0" valueMin="11736.0">11,736–12,041 ft</elevation>
</quantity>
