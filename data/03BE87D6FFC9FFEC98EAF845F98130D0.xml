<quantity id="4CEF9B25FFC8FFEC9AD2FF64F91630B4" box="[687,855,150,177]" metricMagnitude="3" metricUnit="m" metricValue="3.6245" metricValueMax="3.671" metricValueMin="3.578" pageId="89" pageNumber="90" unit="m" value="3624.5" valueMax="3671.0" valueMin="3578.0">
