<quantity id="4CEF9B25FFC8FFEC995FFF48FB3F30D0" box="[290,382,186,213]" metricMagnitude="3" metricUnit="m" metricValue="3.57" pageId="89" pageNumber="90" unit="m" value="3570.0">
