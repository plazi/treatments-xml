</taxonomicName>
(or 
<taxonomicName id="4C0974AE643BFF8EFD5AFBB9FCC8532D" baseAuthorityName="Kastshenko" baseAuthorityYear="1905" box="[642,777,1070,1095]" class="Mammalia" family="Vespertilionidae" genus="Myotis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Chiroptera" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="sibiricus">
