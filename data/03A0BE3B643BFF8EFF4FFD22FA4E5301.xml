<taxonomicName id="4C0974AE643BFF8EFE73FBB9FD91532C" authorityName=": Thomas" authorityYear="1907" box="[427,592,1071,1094]" class="Mammalia" family="Vespertilionidae" genus="Myotis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Chiroptera" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="mystacinus">
