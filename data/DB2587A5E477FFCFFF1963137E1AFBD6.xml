<caption id="07F3663BE474FFCEFF19677F7B4DF84F" ID-DOI="http://doi.org/10.5281/zenodo.5270906" ID-Zenodo-Dep="5270906" httpUri="https://zenodo.org/record/5270906/files/figure.png" pageId="26" pageNumber="27" startId="26.[151,264,1883,1905]" targetBox="[253,1320,216,1819]" targetPageId="26">
