<taxonomicName id="4C0435BDF8624940FDC3FEC33F92FEC2" authority="Kuster, 1851" authorityName="Kuster" authorityYear="1851" box="[586,920,260,285]" class="Insecta" family="Tenebrionidae" genus="Odocnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="tuberculata">
