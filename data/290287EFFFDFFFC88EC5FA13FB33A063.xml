<emphasis id="93DFEAEBFFD8FFC88D63FE4FFDF7A2C9" box="[328,516,480,503]" italics="true" pageId="60" pageNumber="61">
<taxonomicName id="66AB4D7AFFD8FFC88D63FE4FFDF2A2C9" box="[328,513,480,503]" class="Reptilia" family="Elapidae" genus="Elaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="60" pageNumber="61" phylum="Chordata" rank="species" species="corallinus">Elaps corallinus</taxonomicName>
