<location id="8B406F92FFAB7261FD2857FCFC998121" LSID="urn:lsid:plazi:treatment:0636885FFFAB7261FF74576CFAB08191:8B406F92FFAB7261FD2857FCFC998121" box="[715,788,1015,1040]" municipality="Mentawai Islands" name="Sipura" pageId="26" pageNumber="27" stateProvince="Sumatra">Sipura</location>
