<paragraph id="8E203949FFAB7261FF2457FCFC2E8121" blockId="26.[151,1437,943,1184]" box="[199,931,1015,1040]" pageId="26" pageNumber="27">
<materialsCitation id="3EF73314FFAB7261FF2457FCFC2E8121" ID-GBIF-Occurrence="1913870742" box="[199,931,1015,1040]" location="Sipura" municipality="Mentawai Islands" pageId="26" pageNumber="27" specimenCount="1" stateProvince="Sumatra" typeStatus="holotype">
<emphasis id="BCEBE55BFFAB7261FF2457FCFED28121" bold="true" box="[199,351,1015,1040]" pageId="26" pageNumber="27">
<typeStatus id="512487EBFFAB7261FF2457FCFE8F8121" box="[199,258,1015,1040]" pageId="26" pageNumber="27">Type</typeStatus>
locality
</emphasis>
: 
<collectingMunicipality id="6E44A333FFAB7261FE8D57FCFDB98121" box="[366,564,1015,1040]" pageId="26" pageNumber="27">Mentawai Islands</collectingMunicipality>
