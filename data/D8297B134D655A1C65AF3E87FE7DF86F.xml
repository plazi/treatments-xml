<taxonomicName id="9780B1864D635A1C64153AD0FD4BFB1C" authority="Krapp-Schickel, 2003" authorityName="Krapp-Schickel" authorityYear="2003" box="[301,743,1215,1237]" class="Malacostraca" family="Melitidae" genus="Linguimaera" kingdom="Animalia" order="Amphipoda" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="pirloti">
