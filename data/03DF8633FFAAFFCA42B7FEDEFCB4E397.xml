<emphasis id="B902EB37FFAAFFCA407BF8FDFE13E3F3" bold="true" pageId="15" pageNumber="16">
<taxonomicName id="4C764CA6FFAAFFCA407BF8FDFC3AE311" authority="Heslop-Harrison" authorityName="Heslop-Harrison" box="[603,984,1869,1894]" class="Insecta" family="Psyllidae" genus="Euglyptoneura" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
<emphasis id="B902EB37FFAAFFCA407BF8FDFCEAE311" bold="true" box="[603,776,1869,1894]" italics="true" pageId="15" pageNumber="16">Euglyptoneura</emphasis>
Heslop-Harrison
