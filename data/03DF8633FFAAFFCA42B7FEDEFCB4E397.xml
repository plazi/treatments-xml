</taxonomicName>
spp., including 
<emphasis id="B902EB37FFAAFFCA412AFE7CFF1CE676" italics="true" pageId="15" pageNumber="16">
<taxonomicName id="4C764CA6FFAAFFCA412AFE7CFC29E594" box="[778,971,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="camaldulensis">E. camaldulensis</taxonomicName>
, 
<taxonomicName id="4C764CA6FFAAFFCA41F6FE7CFBBAE594" box="[982,1112,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="globulus">E. globulus</taxonomicName>
, 
<taxonomicName id="4C764CA6FFAAFFCA4642FE7CFB3BE594" box="[1122,1241,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="grandis">E. grandis</taxonomicName>
, 
<taxonomicName id="4C764CA6FFAAFFCA46C3FE7CFA96E594" box="[1251,1396,459,483]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="parvifolia">E. parvifolia</taxonomicName>
, 
<taxonomicName id="4C764CA6FFAAFFCA475EFE7CFF1CE676" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="viminalis">E. viminalis</taxonomicName>
</emphasis>
.
