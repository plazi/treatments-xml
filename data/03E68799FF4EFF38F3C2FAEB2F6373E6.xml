<taxonomicName id="4C4F4D0CFF4EFF38F04BF8842DEE73AE" authority="Zelazny" authorityName="Zelazny" authorityYear="1981" box="[798,998,1913,1937]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="173" pageNumber="174" phylum="Arthropoda" rank="species" species="lutea">
