<typeStatus id="4908883DFFA7FF92FD58988FFD26F9C8" box="[640,738,1172,1198]" pageId="25" pageNumber="384">Allotype</typeStatus>
<specimenCount id="80B5FD16FFA7FF92FD30988EFD3BF9C9" box="[744,767,1173,1199]" pageId="25" pageNumber="384" type="female">♀</specimenCount>
