<typeStatus id="4908883DFFA4FF91FC099D32FBFAFC25" box="[977,1086,297,323]" pageId="26" pageNumber="385">Paratypes</typeStatus>
<specimenCount id="80B5FD16FFA4FF91FB909D32FB9BFC25" box="[1096,1119,297,323]" pageId="26" pageNumber="385" type="female">♀</specimenCount>
