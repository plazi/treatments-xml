<date id="FFAFBBA5FFEC017C33FFFDB6FDECF057" box="[490,631,586,611]" pageId="81" pageNumber="82" value="1915-01-01" valueMax="1915-03-02" valueMin="1915-01-01">
<number id="3DD98F42FFEC017C33FFFDB6FD8EF057" box="[490,533,586,611]" pageId="81" pageNumber="82" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
