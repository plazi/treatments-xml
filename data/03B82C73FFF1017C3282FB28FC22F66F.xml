<date id="FFAFBBA5FFEC017C32ACFDB7FED3F057" box="[185,328,586,611]" pageId="81" pageNumber="82" value="1915-03-01" valueMax="1915-04-04" valueMin="1915-03-01">
<number id="3DD98F42FFEC017C32ACFDB7FF7AF057" box="[185,225,586,611]" pageId="81" pageNumber="82" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
– 
<number id="3DD98F42FFEC017C32E4FDB7FE86F057" box="[241,285,586,611]" pageId="81" pageNumber="82" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
– 
