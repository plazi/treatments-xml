<taxonomicName id="4C5B4D086C67FFFEB044FE77EFC0FE40" authorityName="Mocsary" authorityYear="1884" box="[1052,1218,475,501]" class="Insecta" family="Megachilidae" genus="Anthidium" kingdom="Animalia" order="Hymenoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="dalmaticum">
