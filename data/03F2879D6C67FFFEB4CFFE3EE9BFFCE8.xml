<taxonomicName id="4C5B4D086C67FFFEB56CFE77E91BFE41" authorityName="Perez" authorityYear="1912" box="[308,537,475,501]" class="Insecta" family="Megachilidae" genus="Anthidium" kingdom="Animalia" order="Hymenoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="syriacum">
