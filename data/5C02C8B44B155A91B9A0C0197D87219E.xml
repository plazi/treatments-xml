<taxonomicName id="9A4B33D8612F3507F3D9520232867424" class="Insecta" family="Crambidae" genus="Nagia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nagia" order="Lepidoptera" pageId="0" pageNumber="143" phylum="Arthropoda" rank="genus">Nagia</taxonomicName>
