<taxonomicName id="4C795DCAFFFA58438D4DD242BF1DFD55" authority="J. V. Lamouroux" authorityName="J. V. Lamouroux" box="[1067,1337,1802,1828]" class="Ulvophyceae" family="Udoteaceae" genus="Udotea" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Bryopsidales" pageId="8" pageNumber="59" phylum="Chlorophyta" rank="genus">
