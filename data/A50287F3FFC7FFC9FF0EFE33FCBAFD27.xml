<taxonomicName id="EAAB4D66FFC7FFC9FECCFE5CFE34FE54" box="[341,465,473,498]" class="Insecta" family="Nymphalidae" genus="Cymothoe" kingdom="Animalia" order="Lepidoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="mabillei">
