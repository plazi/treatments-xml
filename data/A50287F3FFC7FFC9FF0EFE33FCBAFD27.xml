<quantity id="EA539B00FFC7FFC9FE99FE5FFEA9FE57" box="[256,332,473,497]" metricMagnitude="-2" metricUnit="m" metricValue="2.7" pageId="42" pageNumber="43" unit="mm" value="27.0">27mm</quantity>
