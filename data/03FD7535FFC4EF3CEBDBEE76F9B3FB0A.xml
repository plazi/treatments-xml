<collectorName id="26A1A1F5FFC4EF3DE849EE5EFBC7F98B" box="[872,1045,1593,1627]" pageId="2" pageNumber="306">THH+IJC</collectorName>
(178), 
<specimenCount id="9D520FAAFFC4EF3DEFA3EE5EFB6AF98B" box="[1154,1208,1593,1627]" pageId="2" pageNumber="306" type="male">
<emphasis id="B9201831FFC4EF3DEFA3EE5EFB44F98B" bold="true" box="[1154,1174,1593,1627]" pageId="2" pageNumber="306">2</emphasis>
