<taxonomicName id="ECE9B3A8FFB1FFA1C6ADFF565789FE8B" authority="Fraser-Brunner, 1955" authorityName="Fraser-Brunner" authorityYear="1955" box="[247,664,252,277]" class="Actinopterygii" family="Ambassidae" genus="Gymnochanda" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="689" phylum="Chordata" rank="genus">
