<taxonomicName id="4C284D0DFF9BB564FE48021AF8473C00" authority="ssp. osyroides" box="[506,847,1066,1090]" class="Magnoliopsida" family="Fabaceae" genus="Teline" kingdom="Plantae" order="Fabales" pageId="30" pageNumber="426" phylum="Tracheophyta" rank="subSpecies" species="osyroides" subSpecies="osyroides">
