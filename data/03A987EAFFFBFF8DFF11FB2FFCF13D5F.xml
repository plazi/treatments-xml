<taxonomicName id="4C004D7FFFFBFF8AFEB6FA89FD733AF8" authority="(FORSTER, 1855)" baseAuthorityName="FORSTER" baseAuthorityYear="1855" box="[274,645,1392,1420]" class="Insecta" family="Ichneumonidae" genus="Erigorgus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="252" phylum="Arthropoda" rank="species" species="melanops">
