<taxonomicName id="E9F24D07F31E1F1E6458BDBA8BE6ABA4" box="[891,974,1976,1999]" class="Magnoliopsida" family="Fabaceae" genus="Cytisus" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="genus">
