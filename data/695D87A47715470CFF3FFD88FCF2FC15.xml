<taxonomicName id="26F44D317715470CFEC5FD89FE2AFDBB" box="[365,450,597,620]" class="Magnoliopsida" family="Asteraceae" genus="Cirsium" kingdom="Plantae" order="Asterales" pageId="20" pageNumber="209" phylum="Tracheophyta" rank="species" species="arvense">
