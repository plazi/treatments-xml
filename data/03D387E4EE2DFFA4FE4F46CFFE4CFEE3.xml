<taxonomicName id="4C7A4D71EE2DFFA2FC8F4373FC2FF9ED" authorityName="Wittmer" authorityYear="1970" box="[772,929,1542,1565]" class="Insecta" family="Cantharidae" genus="Malthodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="202" phylum="Arthropoda" rank="species" species="seleucianus">
