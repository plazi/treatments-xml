, 
<number id="3DA324E1FFD7D679FCD011A69926F800" box="[778,831,1972,1996]" pageId="2" pageNumber="51" value="0.27">0.27</number>
), IV 
<number id="3DA324E1FFD7D679FCA011A699B2F800" box="[890,939,1972,1996]" pageId="2" pageNumber="51" value="2.75">2.75</number>
(
<number id="3DA324E1FFD7D679FC6611A699E9F800" box="[956,1008,1972,1996]" pageId="2" pageNumber="51" value="0.81">0.81</number>
, 
<number id="3DA324E1FFD7D679FC2711A69E28F800" box="[1021,1073,1972,1996]" pageId="2" pageNumber="51" value="0.27">0.27</number>
, 
<number id="3DA324E1FFD7D679FBE711A69E68F800" box="[1085,1137,1972,1996]" pageId="2" pageNumber="51" value="0.63">0.63</number>
, 
<number id="3DA324E1FFD7D679FBA711A69EA8F800" box="[1149,1201,1972,1996]" pageId="2" pageNumber="51" value="0.63">0.63</number>
