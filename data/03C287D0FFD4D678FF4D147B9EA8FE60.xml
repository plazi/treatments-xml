<number id="3DA324E1FFD7D679FB6311829EF7F864" box="[1209,1262,1936,1960]" pageId="2" pageNumber="51" value="0.36">0.36</number>
), II 
<number id="3DA324E1FFD7D679FAFA11829F48F864" box="[1312,1361,1936,1960]" pageId="2" pageNumber="51" value="2.67">2.67</number>
(
<number id="3DA324E1FFD7D679FAB911829F81F864" box="[1379,1432,1936,1960]" pageId="2" pageNumber="51" value="0.86">0.86</number>
, 
<number id="3DA324E1FFD7D679FF4D11A69AD2F800" box="[151,203,1972,1996]" pageId="2" pageNumber="51" value="0.27">0.27</number>
, 
<number id="3DA324E1FFD7D679FF0D11A69B12F800" box="[215,267,1972,1996]" pageId="2" pageNumber="51" value="0.59">0.59</number>
, 
<number id="3DA324E1FFD7D679FEC211A69B52F800" box="[280,331,1972,1996]" pageId="2" pageNumber="51" value="0.59">0.59</number>
, 
