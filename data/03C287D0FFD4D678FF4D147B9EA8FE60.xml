<number id="3DA324E1FFD4D67AFAF9106E9F4DF958" box="[1315,1364,1660,1684]" pageId="1" pageNumber="50" value="2.62">2.62</number>
(
<number id="3DA324E1FFD4D67AFAB9106E9F81F958" box="[1379,1432,1660,1684]" pageId="1" pageNumber="50" value="0.9">0.90</number>
, 
<number id="3DA324E1FFD4D67AFF4D10B29AD2F974" box="[151,203,1696,1720]" pageId="1" pageNumber="50" value="0.23">0.23</number>
, 
<number id="3DA324E1FFD4D67AFF0D10B29B12F974" box="[215,267,1696,1720]" pageId="1" pageNumber="50" value="0.54">0.54</number>
, 
<number id="3DA324E1FFD4D67AFEC210B29B52F974" box="[280,331,1696,1720]" pageId="1" pageNumber="50" value="0.54">0.54</number>
, 
<number id="3DA324E1FFD4D67AFE8210B29B95F974" box="[344,396,1696,1720]" pageId="1" pageNumber="50" value="0.41">0.41</number>
), III 
<number id="3DA324E1FFD4D67AFE1C10B29BEEF974" box="[454,503,1696,1720]" pageId="1" pageNumber="50" value="1.86">1.86</number>
(
<number id="3DA324E1FFD4D67AFDDD10B29825F974" box="[519,572,1696,1720]" pageId="1" pageNumber="50" value="0.54">0.54</number>
, 
<number id="3DA324E1FFD4D67AFD9310B29864F974" box="[585,637,1696,1720]" pageId="1" pageNumber="50" value="0.14">0.14</number>
, 
<number id="3DA324E1FFD4D67AFD5310B298A4F974" box="[649,701,1696,1720]" pageId="1" pageNumber="50" value="0.45">0.45</number>
, 
<number id="3DA324E1FFD4D67AFD1010B298E7F974" box="[714,766,1696,1720]" pageId="1" pageNumber="50" value="0.36">0.36</number>
, 
<number id="3DA324E1FFD4D67AFCD010B29926F974" box="[778,831,1696,1720]" pageId="1" pageNumber="50" value="0.32">0.32</number>
), IV 
<number id="3DA324E1FFD4D67AFCA010B299B2F974" box="[890,939,1696,1720]" pageId="1" pageNumber="50" value="2.17">2.17</number>
(
<number id="3DA324E1FFD4D67AFC6110B299E9F974" box="[955,1008,1696,1720]" pageId="1" pageNumber="50" value="0.63">0.63</number>
, 
<number id="3DA324E1FFD4D67AFC2710B29E28F974" box="[1021,1073,1696,1720]" pageId="1" pageNumber="50" value="0.23">0.23</number>
, 
<number id="3DA324E1FFD4D67AFBE710B29E68F974" box="[1085,1137,1696,1720]" pageId="1" pageNumber="50" value="0.45">0.45</number>
