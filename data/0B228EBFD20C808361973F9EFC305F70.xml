<taxonomicName id="C9B08D4CB7CF163050FD71617C4381DE" authorityName="Letourneux &amp; Bourguignat" authorityYear="1887" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis olivula" order="Pteropoda" pageId="200" pageNumber="201" phylum="Mollusca" rank="species" species="olivula">
