<taxonomicName id="4C4E4D6D41119E7FE96E429CFCE8FD87" box="[596,824,510,535]" class="Insecta" family="Sphingidae" genus="Macroglossum" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="414" phylum="Arthropoda" rank="species" species="saga">
