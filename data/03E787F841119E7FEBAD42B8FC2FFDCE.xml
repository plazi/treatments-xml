<taxonomicName id="4C4E4D6D41119E7FEAAE4125FE3FFDCF" box="[404,495,583,606]" class="Insecta" family="Sphingidae" genus="Macroglossum" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="414" phylum="Arthropoda" rank="species" species="saga">
