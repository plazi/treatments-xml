<emphasis id="B9650718FFB0FFB098F70F4BFE638A5F" bold="true" box="[151,486,332,358]" pageId="7" pageNumber="92">
<taxonomicName id="4C11A089FFB0FFB098F70F4BFE058A5F" authority="Gonçalves &amp; Mejdalani, 2017" authorityName="Gonçalves &amp; Mejdalani" authorityYear="2017" box="[151,384,332,358]" class="Insecta" family="Cicadellidae" genus="Nelidina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="92" phylum="Arthropoda" rank="species" species="platypenis" status="sp. nov.">
<emphasis id="B9650718FFB0FFB098F70F4BFE058A5F" bold="true" box="[151,384,332,358]" italics="true" pageId="7" pageNumber="92">Nelidina platypenis</emphasis>
