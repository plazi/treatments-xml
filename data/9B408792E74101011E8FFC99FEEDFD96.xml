and the 
<typeStatus id="CC528826E74101031D29F8DEFC4CF8B6" box="[870,969,1859,1883]" pageId="12" pageNumber="11">allotype</typeStatus>
from 
<collectingCountry id="6BFE7614E74101031A5FF8D9FBF2F8B6" box="[1040,1143,1860,1883]" name="Panama" pageId="12" pageNumber="11">Panama</collectingCountry>
