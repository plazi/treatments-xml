<collectionCode id="ED09AE284C4EA967B649F8B2C53AF8EE" box="[530,617,1871,1897]" httpUri="http://grbio.org/cool/m2jc-wmeq" name="University of California, Riverside" pageId="6" pageNumber="7">UCRC</collectionCode>
