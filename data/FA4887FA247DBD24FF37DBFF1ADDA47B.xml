<quantity id="B5199B09247DBD24FA91D8031E49A559" box="[1329,1398,1601,1624]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="99" pageNumber="100" unit="m" value="200.0">200 m</quantity>
