<geoCoordinate id="17D5502B247DBD24FC6BD9511F06A42C" box="[971,1081,1811,1837]" degrees="77" direction="west" minutes="30" orientation="longitude" pageId="99" pageNumber="100" precision="925" value="-77.5">77°30’W</geoCoordinate>
