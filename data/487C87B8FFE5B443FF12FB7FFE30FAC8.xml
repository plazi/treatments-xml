<collectingMunicipality id="200EACD4FFE7B443FCEEFC01FDF5FC25" box="[159,610,970,994]" pageId="78" pageNumber="336">Agricultural Scientific Collections Unit</collectingMunicipality>
(Acarology), 
<collectingCounty id="290B4E22FFE7B443FCEEFC26FE6EFBC2" box="[159,505,1005,1029]" pageId="78" pageNumber="336">Orange Agricultural Institute</collectingCounty>
, 
<collectingCountry id="B8C2763EFFE7B443FE75FC26FDE3FBC2" box="[516,628,1005,1029]" name="Australia" pageId="78" pageNumber="336">Australia</collectingCountry>
