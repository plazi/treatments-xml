<collectingCountry id="F31B763E2A6279732A20DDEAF5502ED6" box="[768,836,1710,1735]" name="Brazil" pageId="61" pageNumber="62">Brazil</collectingCountry>
and 
<collectingCountry id="F31B763E2A6279732A5BDDEAF5FD2ED6" box="[891,1001,1710,1735]" name="Colombia" pageId="61" pageNumber="62">Colombia</collectingCountry>
(
<bibRefCitation id="EF9D4B5F2A6279732AD8DDEAF28B2ED6" box="[1016,1183,1710,1735]" pageId="61" pageNumber="62" refId="ref62259" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii + 1 - 585." type="journal article">Metcalf 1963a</bibRefCitation>
