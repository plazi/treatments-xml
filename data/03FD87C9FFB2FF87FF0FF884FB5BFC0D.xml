<paragraph id="8BEB36DFFFB2FF86FF0FF884FD21F8AE" blockId="18.[192,684,1793,1818]" box="[192,684,1793,1818]" pageId="18" pageNumber="17">
<heading id="D0A381B3FFB2FF86FF0FF884FD21F8AE" bold="true" box="[192,684,1793,1818]" fontSize="10" level="2" pageId="18" pageNumber="17" reason="3">
<emphasis id="B920EACDFFB2FF86FF0FF884FD21F8AE" bold="true" box="[192,684,1793,1818]" pageId="18" pageNumber="17">
Tribe 
<taxonomicName id="4C544D5CFFB2FF86FEDCF884FD21F8AE" authority="Bolton, 2003" authorityName="Bolton" authorityYear="2003" box="[275,684,1793,1818]" class="Insecta" family="Formicidae" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="tribe" tribe="Haidomyrmecini">
Haidomyrmecini 
<bibRefCitation id="EFC54B2EFFB2FF86FDCBF887FD21F8AE" author="Bolton, B." box="[516,684,1793,1818]" pageId="18" pageNumber="17" pagination="1 - 370" refId="ref20331" refString="Bolton, B. 2003. Synopsis and classification of Formicidae. Memoirs of the American Entomological Institute 71: 1 - 370." type="journal article" year="2003">Bolton, 2003</bibRefCitation>
