s.n.
</emphasis>
(
<accessionNumber id="94513FC3AF5BFF94D513FBC8BDEAC107" box="[335,448,1078,1100]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/BP707539" pageId="0" pageNumber="118">
<collectionCode id="ED133AE5AF5BFF94D513FBC8BD47C107" box="[335,365,1079,1100]" country="Hungary" lsid="urn:lsid:biocol.org:col:13261" name="Hungarian Natural History Museum" pageId="0" pageNumber="118" type="Herbarium">BP</collectionCode>
707539
</accessionNumber>
!) (
<figureCitation id="1339BEA5AF5BFF94D584FBC9BE25C106" box="[472,527,1078,1101]" captionStart="FIGURE 1" captionStartId="1.[136,229,1137,1159]" captionTargetBox="[208,1380,183,1106]" captionTargetId="figure-27@1.[208,1380,183,1106]" captionTargetPageId="1" captionText="FIGURE 1. A. Lectotype of Polycnemum heuffelii Láng (BP 707539!). B. Lectotype of Polycnemum verrucosum Láng (BP 707552!)." figureDoi="http://doi.org/10.5281/zenodo.5147227" httpUri="https://zenodo.org/record/5147227/files/figure.png" pageId="0" pageNumber="118">Fig. 1</figureCitation>
