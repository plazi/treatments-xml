<collectingCountry id="F315E2B0AF5BFF94D5F2FBEDBE03C163" box="[430,553,1042,1064]" name="Hungary" pageId="0" pageNumber="118">HUNGARY</collectingCountry>
. 
<location id="8EDDF4FBAF5BFF94D66DFBEDBE80C163" LSID="urn:lsid:plazi:treatment:03AB1336AF5BFF94D4D4FC39B81CC20A:8EDDF4FBAF5BFF94D66DFBEDBE80C163" box="[561,682,1042,1064]" country="Hungary" municipality="Heuffel" name="Veresegyhaz" pageId="0" pageNumber="118">Veresegyház</location>
