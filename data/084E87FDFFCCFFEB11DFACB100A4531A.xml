; EL: 
<quantity id="471F9B0EFFCBFFEA104BAF6B029C5540" box="[259,424,1827,1854]" metricMagnitude="-3" metricUnit="m" metricValue="2.41" metricValueMax="2.5" metricValueMin="2.32" pageId="4" pageNumber="235" unit="mm" value="2.41" valueMax="2.5" valueMin="2.32">2.32–2.50 mm</quantity>
