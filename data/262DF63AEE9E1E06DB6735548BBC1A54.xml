<taxonomicName id="B6049C098E3981D48997E98C12B8395A" authority="Grant, 1836" authorityName="Grant" authorityYear="1836" class="Gastropoda" family="Rissoidae" genus="Porifera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Porifera" order="Littorinimorpha" pageId="13" pageNumber="14" phylum="Mollusca" rank="genus">Porifera</taxonomicName>
: 
<taxonomicName id="6CA2818FBA12086C138EBA4CA891272A" family="Ancorinidae" lsidName="" pageId="13" pageNumber="14" rank="family">Ancorinidae</taxonomicName>
