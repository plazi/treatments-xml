<date id="FFC7FB98A70CB36DA07BFC6DFAB68945" box="[1336,1392,1020,1049]" pageId="9" pageNumber="543" value="1922-05-01">
<emphasis id="B90D014AA70CB36DA07BFC6DFA828944" box="[1336,1348,1021,1044]" italics="true" pageId="9" pageNumber="543">v</emphasis>
<emphasis id="B90D014AA70CB36DA007FB9BFA8A8949" box="[1348,1356,1035,1049]" italics="true" pageId="9" pageNumber="543">1</emphasis>
<emphasis id="B90D014AA70CB36DA017FC6CFAB68945" bold="true" box="[1364,1392,1020,1045]" pageId="9" pageNumber="543">22</emphasis>
</date>
, 22 (20 
<emphasis id="B90D014AA70CB36DA5FFFBB4FF0C896B" box="[188,202,1060,1083]" italics="true" pageId="9" pageNumber="543">–</emphasis>
23), 
<emphasis id="B90D014AA70CB36DA5BDFBB4FED3896B" box="[254,277,1060,1083]" italics="true" pageId="9" pageNumber="543">sc</emphasis>
<date id="FFC7FB98A70CB36DA456FBA2FEAA896B" box="[277,364,1059,1088]" pageId="9" pageNumber="543" value="2014-01-15">
<emphasis id="B90D014AA70CB36DA456FBA2FEDB8910" box="[277,285,1074,1088]" italics="true" pageId="9" pageNumber="543">1</emphasis>
