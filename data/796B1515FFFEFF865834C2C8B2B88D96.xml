<taxonomicName id="36C2DF80FFFFFF865B9DC011B0FA8DB7" authorityName="Ariyama and Azuma" authorityYear="2011" class="Malacostraca" family="Paracalliopiidae" genus="Mucrocalliope" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="88" phylum="Arthropoda" rank="genus">
