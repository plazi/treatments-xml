<geoCoordinate id="EE3CDDA1FFD4FF8AFB92F890E8B5F8A1" box="[1104,1166,1889,1917]" direction="south" orientation="latitude" pageId="15" pageNumber="257" precision="925" value="-5.4166665">
<emphasis id="B97C6774FFD4FF8AFB92F890E857F8A0" box="[1104,1132,1891,1917]" italics="true" pageId="15" pageNumber="257">S5</emphasis>
25
</geoCoordinate>
