<geoCoordinate id="EE3CDDA1FFD4FF8AFE68F891EDDEF8A1" box="[426,485,1890,1916]" direction="south" orientation="latitude" pageId="15" pageNumber="257" precision="925" value="-4.55">
<emphasis id="B97C6774FFD4FF8AFE68F891EDF8F8A1" box="[426,451,1890,1916]" italics="true" pageId="15" pageNumber="257">s4</emphasis>
