35
</geoCoordinate>
(33–37); 
<geoCoordinate id="EE3CDDA1FFD4FF8AFD38F891EF03F8A1" box="[762,824,1890,1916]" direction="south" orientation="latitude" pageId="15" pageNumber="257" precision="925" value="-2.6">
<emphasis id="B97C6774FFD4FF8AFD38F891EF2DF8A1" box="[762,790,1890,1916]" italics="true" pageId="15" pageNumber="257">S2</emphasis>
36
</geoCoordinate>
(34–37); 
<geoCoordinate id="EE3CDDA1FFD4FF8AFC67F891EFD8F8A1" box="[933,995,1890,1916]" direction="south" orientation="latitude" pageId="15" pageNumber="257" precision="925" value="-4.55">
<emphasis id="B97C6774FFD4FF8AFC67F891EFFAF8A1" box="[933,961,1890,1916]" italics="true" pageId="15" pageNumber="257">S4</emphasis>
33
