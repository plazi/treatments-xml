<collectingCountry id="F34069AD2725253F63BE9B1AFDDEE23E" box="[477,583,306,330]" name="Indonesia" pageId="5" pageNumber="183">Indonesia</collectingCountry>
, 
<collectingCountry id="F34069AD2725253F60319B1AFD2FE23E" box="[594,694,306,330]" name="Malaysia" pageId="5" pageNumber="183">Malaysia</collectingCountry>
, 
<collectingCountry id="F34069AD2725253F60A29B1AFCA5E23E" box="[705,828,306,330]" name="Philippines" pageId="5" pageNumber="183">Philippines</collectingCountry>
, 
<collectingCountry id="F34069AD2725253F61259B1AFC2BE23E" box="[838,946,306,330]" name="Singapore" pageId="5" pageNumber="183">Singapore</collectingCountry>
