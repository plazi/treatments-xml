<taxonomicName id="B29147DEFFE3FFE5FEDEF9A2FC82F9C8" authority="Jager &amp; Ono 2001" authorityName="Jager &amp; Ono" authorityYear="2001" box="[362,779,1624,1648]" class="Arachnida" family="Sparassidae" genus="Sinopoda" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="exspectata">
