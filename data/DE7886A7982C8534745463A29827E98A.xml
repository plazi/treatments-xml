<taxonomicName id="90EA905B3E9CE465E554EAE8FE12D067" class="Polychaeta" family="Sabellidae" genus="Sabella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sabella bipunctata" order="Sabellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="bipunctata">Sabella bipunctata</taxonomicName>
