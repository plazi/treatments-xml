<taxonomicName id="4C754D3CFFBEFFE64E69F961FCB940E8" authority="Spaeth, 1923" authorityName="Spaeth" authorityYear="1923" box="[575,826,1743,1765]" class="Insecta" family="Chrysomelidae" genus="Paratrikona" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="424" phylum="Arthropoda" rank="genus">
