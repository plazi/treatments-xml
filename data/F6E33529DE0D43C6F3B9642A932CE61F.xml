<taxonomicName id="643FA4C4B61AA3B7E0D2569851175377" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="alpinum">
