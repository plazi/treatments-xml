<emphasis id="B942EAF9FFAE0965FF6AF80CFEB9F81E" bold="true" box="[151,290,1975,1997]" pageId="27" pageNumber="28">FIGURE 16.</emphasis>
<taxonomicName id="4C364D68FFAE0965FED1F803FE7FF81F" authority="Alipanah &amp; Khodadad &amp; Rajaei &amp; Haseli, 2018" authorityName="Alipanah &amp; Khodadad &amp; Rajaei &amp; Haseli" authorityYear="2018" box="[300,484,1975,1997]" class="Insecta" family="Crambidae" genus="Evergestis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="pazukii" status="sp. n.">
<emphasis id="B942EAF9FFAE0965FED1F803FE7FF81F" box="[300,484,1975,1997]" italics="true" pageId="27" pageNumber="28">Evergestis pazukii</emphasis>
</taxonomicName>
<emphasis id="B942EAF9FFAE0965FE12F80CFDB0F81E" bold="true" box="[495,555,1975,1996]" pageId="27" pageNumber="28">
<taxonomicNameLabel id="A2715782FFAE0965FE12F80CFDB0F81E" box="[495,555,1975,1996]" pageId="27" pageNumber="28" rank="species">sp. n.</taxonomicNameLabel>
