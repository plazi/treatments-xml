<taxonomicName id="4C230FBBFFD4A46FFEF2FD73FE537C42" box="[344,475,764,789]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="clarencei" status="sp. nov.">
