<quantity id="5A261FAD811AFEB9EF84CCB91A190CDB" box="[751,836,656,680]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="2.0" metricValueMin="0.1" pageId="286" pageNumber="287" unit="m" value="10.5" valueMax="20.0" valueMin="1.0">1–20 m</quantity>
