[
<taxonomicName id="4C5C4D14FF9FEB5454A2FA48B60B3E1A" authority="(Simon, 1884)" baseAuthorityName="Simon" baseAuthorityYear="1884" box="[161,629,1530,1556]" class="Arachnida" family="Linyphiidae" genus="Palliduphantes" kingdom="Animalia" order="Araneae" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="cernuus">
