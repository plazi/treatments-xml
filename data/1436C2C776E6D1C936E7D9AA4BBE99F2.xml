<subSubSection id="8593CB48AE62BE737070BD06DF14ABBB" pageId="1" pageNumber="26" type="nomenclature">
<paragraph id="1F4DB59E88857791053A57E72B95FDE4" pageId="1" pageNumber="26">
<taxonomicName id="873B8E6DC00FA5323B4508EF92D6E88F" LSID="http://zoobank.org/66F44A5A-A630-4C7B-8566-239342221E33" authority="Fernandez-Triana" authorityName="Fernandez-Triana" authorityYear="2018" class="Insecta" family="Braconidae" genus="Agupta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Agupta" order="Hymenoptera" pageId="1" pageNumber="26" phylum="Arthropoda" rank="genus">Agupta Fernandez-Triana</taxonomicName>
