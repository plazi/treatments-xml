<taxonomicName id="169E4D014875FF932F50F78E4921262C" box="[889,1111,2077,2098]" class="Actinopterygii" family="Serranidae" genus="Cephalopholis" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="158" phylum="Chordata" rank="species" species="aurantia">
