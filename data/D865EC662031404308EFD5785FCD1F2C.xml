<taxonomicName id="97CC26F3203140430C06D5B5584D1E9C" box="[1150,1355,1730,1755]" class="Insecta" family="Cleridae" genus="Clerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="314" phylum="Arthropoda" rank="species" species="pallipennis">
