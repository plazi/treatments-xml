<taxonomicName id="97CC26F3203140430AE4D5B55EBE1E9C" box="[668,952,1730,1755]" class="Insecta" family="Cleridae" genus="Phyllobaenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="314" phylum="Arthropoda" rank="species" species="pallipennis">
