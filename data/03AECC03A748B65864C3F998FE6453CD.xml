<taxonomicName id="4C070696A747B6586687FB9FFCA45326" box="[723,834,1112,1137]" class="Insecta" family="Therevidae" genus="Taenogera" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="63" phylum="Arthropoda" rank="species" species="luteola" status="sp. nov.">
<emphasis id="B973A107A747B6586687FB9FFCA45326" box="[723,834,1112,1137]" italics="true" pageId="8" pageNumber="63">T. luteola</emphasis>
</taxonomicName>
<emphasis id="B973A107A747B658671EFB90FC4E5326" bold="true" box="[842,936,1111,1137]" pageId="8" pageNumber="63">
