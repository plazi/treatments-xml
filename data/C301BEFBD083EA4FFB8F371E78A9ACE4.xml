<taxonomicName id="1AF25F7CBE7BCEA42FB84E29AD026C8F" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus floryae" order="Hymenoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="floryae">Euplectrus floryae</taxonomicName>
