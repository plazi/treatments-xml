<taxonomicName id="4C2B4D41BB5A5D1B9687080FFD73385C" baseAuthorityName="Westwood" baseAuthorityYear="1842" box="[496,686,866,892]" class="Insecta" family="Formicidae" genus="Eciton" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="burchellii">
