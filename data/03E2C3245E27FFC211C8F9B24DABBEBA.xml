<taxonomicName id="4C4B09B15E27FFC310AEF9474BACBAC1" authorityName="Cowley" authorityYear="1934" box="[497,671,1723,1747]" class="Insecta" family="Liassogomphidae" genus="Liassogomphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="0" pageNumber="108" phylum="Arthropoda" rank="genus">
