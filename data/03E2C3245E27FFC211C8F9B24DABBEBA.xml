<taxonomicName id="4C4B09B15E27FFC313B8F9474D7EBAC1" authority="Handlirsch, 1906" authorityName="Handlirsch" authorityYear="1906" box="[743,1101,1722,1747]" class="Insecta" family="Liassogomphidae" genus="Heterothemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="0" pageNumber="108" phylum="Arthropoda" rank="genus">
