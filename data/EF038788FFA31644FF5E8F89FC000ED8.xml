(Marivan), 
<collectingRegion id="A56EF87CFFA11644FD6788FEFCFD0EF0" box="[686,788,952,978]" country="Iran" name="Lorestan" pageId="15" pageNumber="16">Lorestan</collectingRegion>
(Ghelaei) and 
<collectingRegion id="A56EF87CFFA11644FC0888FEFBBC0EF0" box="[961,1109,952,978]" country="Iran" name="Kermanshah" pageId="15" pageNumber="16">Kermanshah</collectingRegion>
(Gahvareh) provinces; 
<collectingCountry id="1FBD760EFFA11644FAA088FEFF2B0ED8" name="Turkey" pageId="15" pageNumber="16">Turkey</collectingCountry>
