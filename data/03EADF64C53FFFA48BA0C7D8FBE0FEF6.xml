<typeStatus id="54F8D0D0C53FFFA68889C67BFB89F9F2" box="[929,1033,1614,1640]" pageId="1" pageNumber="605">Holotype</typeStatus>
<collectionCode id="ED52F6B7C53FFFA68F3AC67AFBC2F9F3" box="[1042,1090,1615,1641]" country="Russia" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="1" pageNumber="605">PIN</collectionCode>
