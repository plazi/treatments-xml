(
<bibRefCitation id="0C224B37FFD4FFDD4992FC92FAEFFC73" author="NOTMAN, H." box="[1221,1383,892,916]" pageId="25" pageNumber="256" refId="ref84737" refString="NOTMAN, H. 1925: A synoptic review of the beetles of" type="book" year="1925">NOTMAN, 1925</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
