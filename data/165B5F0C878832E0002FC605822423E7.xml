<taxonomicName id="C5EE969B9AA936DE84B84694BFE78A43" LSID="http://zoobank.org/AE368D52-FA6F-48BB-A701-59B727AC3448" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus dulcus" order="Hymenoptera" pageId="347" pageNumber="348" phylum="Arthropoda" rank="species" species="dulcus">Heterospilus dulcus Marsh</taxonomicName>
