<caption id="99E18BAB3276B060FF7CF8AC2779F856" ID-DOI="http://doi.org/10.5281/zenodo.4545885" ID-Zenodo-Dep="4545885" httpUri="https://zenodo.org/record/4545885/files/figure.png" pageId="33" pageNumber="334" startId="33.[151,250,1898,1922]" targetBox="[157,1432,404,1868]" targetPageId="33">
