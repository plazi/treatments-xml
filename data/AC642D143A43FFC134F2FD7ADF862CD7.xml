<quantity id="E33531E73A43FFC13746FCD0DED92856" box="[803,898,860,885]" metricMagnitude="3" metricUnit="m" metricValue="1.536" pageId="10" pageNumber="303" unit="m" value="1536.0">1,536 m</quantity>
