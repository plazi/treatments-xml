<emphasis id="D1F1EA97426BFFEC592FFB4B01A087AB" bold="true" box="[832,980,1161,1185]" pageId="1" pageNumber="46">
<typeStatus id="3C3E8827426BFFEC592FFB4B010D87AB" box="[832,889,1161,1185]" pageId="1" pageNumber="46">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="24854D06426BFFEC5989FB4B06FA87AA" authorityName="Zanata &amp; Akama" authorityYear="2004" box="[998,1166,1161,1184]" class="Actinopterygii" family="Characidae" genus="Myxiops" kingdom="Animalia" order="Characiformes" pageId="1" pageNumber="46" phylum="Chordata" rank="species" species="aphos">
