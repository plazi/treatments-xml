<paragraph id="E533F9080F560FC0F49A2D48A6D86B1F" pageId="580" pageNumber="581">
<taxonomicName id="7E5349CF5F059CC082551003B3B7B3EB" authorityName="Casey" authorityYear="1918" class="Hexapoda" family="Carabidae" genus="Celia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="580" pageNumber="581" phylum="Arthropoda" rank="species" species="shoemakeri">Celia shoemakeri</taxonomicName>
