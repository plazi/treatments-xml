, depth range 
<quantity id="4CF91CE377E6FF1DFC64A6149425FA0D" box="[999,1112,1407,1432]" metricMagnitude="2" metricUnit="m" metricValue="3.655" metricValueMax="6.5" metricValueMin="0.8099999999999999" pageId="177" pageNumber="178" unit="m" value="365.5" valueMax="650.0" valueMin="81.0">81–650 m</quantity>
(
<collectingCountry id="F316F19677E6FF1DFBE5A6EB94C2FA02" box="[1126,1215,1408,1432]" name="Guyana" pageId="177" pageNumber="178">Guyana</collectingCountry>
Shelf 
<quantity id="4CF91CE377E6FF1DFA8AA6EB9538FA02" box="[1289,1349,1408,1432]" metricMagnitude="1" metricUnit="m" metricValue="8.1" pageId="177" pageNumber="178" unit="m" value="81.0">81 m</quantity>
