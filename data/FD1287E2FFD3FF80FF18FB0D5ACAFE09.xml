<emphasis id="47CFEAE6FFD3FF9FFEFDF8585A72F84C" box="[365,518,1922,1948]" italics="true" pageId="15" pageNumber="190">L. maculatum</emphasis>
, UC
</taxonomicName>
1408205!); Siskiyou Co.: Mt. Shasta, 
<quantity id="B2439B11FFD3FF9FFC4CF8595C40F84C" box="[988,1076,1922,1948]" metricMagnitude="3" metricUnit="m" metricValue="2.31648" pageId="15" pageNumber="190" unit="ft" value="7600.0">7600 ft.</quantity>
, on dead twig, 
<date id="01051034FFD3FF9FFB4AF8595D1FF84C" box="[1242,1387,1922,1949]" pageId="15" pageNumber="190" value="1967-07-25">25 July 1967</date>
, 
<emphasis id="47CFEAE6FFD3FF9FFAE5F85858B4F810" italics="true" pageId="15" pageNumber="190">DTK 7396</emphasis>
(as 
<taxonomicName id="B2BB4D77FFD3FF9FFF7FF87C59B5F811" authority=", UC" authorityName="UC" box="[239,449,1958,1985]" class="Myxomycetes" family="Stemonitidaceae" genus="Lamproderma" kingdom="Protozoa" order="Stemonitales" pageId="15" pageNumber="190" phylum="Mycetozoa" rank="species" species="maculatum">
<emphasis id="47CFEAE6FFD3FF9FFF7FF87C59FDF810" box="[239,393,1958,1984]" italics="true" pageId="15" pageNumber="190">L. maculatum</emphasis>
, UC
</taxonomicName>
1408215!); Mt. Rainer National Park, Bench Lake Trail, 
<quantity id="B2439B11FFD3FF9FFBACF87D5CE1F810" box="[1084,1173,1958,1985]" metricMagnitude="3" metricUnit="m" metricValue="1.3716" pageId="15" pageNumber="190" unit="ft" value="4500.0">4500 ft.</quantity>
, on twigs, 
<date id="01051034FFD3FF9FFA9FF87D5DD3F810" box="[1295,1447,1958,1985]" pageId="15" pageNumber="190" value="1968-06-13">13 June 1968</date>
, 
<emphasis id="47CFEAE6FFD3FF9FFF18F810588AF834" box="[136,254,1994,2020]" italics="true" pageId="15" pageNumber="190">DTK 8664</emphasis>
