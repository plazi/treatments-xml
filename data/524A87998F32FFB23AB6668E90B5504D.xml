<taxonomicName id="1DE34D0C8F35FFB239C963F4923F52A5" authority="disconotatus (Lichtenstein, 1884)" authorityName="disconotatus (Lichtenstein" authorityYear="1884" baseAuthorityName="Lichtenstein" baseAuthorityYear="1884" class="Insecta" family="Eumenidae" genus="Euodynerus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="48" phylum="Arthropoda" rank="subSpecies" species="disconotatus" subSpecies="disconotatus">
