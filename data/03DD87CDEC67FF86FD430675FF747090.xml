<taxonomicName id="4C744D58EC64FF88FE930451FE3F777A" authority="Platnick &amp; Dupérré &amp; Berniker &amp; Bonaldo, 2013" authorityName="Platnick &amp; Dupérré &amp; Berniker &amp; Bonaldo" authorityYear="2013" box="[290,498,1603,1623]" class="Arachnida" family="Oonopidae" genus="Prodysderina" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="22" phylum="Arthropoda" rank="species" species="rasgon" status="new species">
<emphasis id="B900EAC9EC64FF88FE930451FE3F777A" box="[290,498,1603,1623]" italics="true" pageId="22" pageNumber="22">Prodysderina rasgon</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A23357B2EC64FF88FDB00456FDB6777B" box="[513,635,1604,1623]" pageId="22" pageNumber="22" rank="species">new species</taxonomicNameLabel>
