<taxonomicName id="4C744D58EC65FF89FC6207D5FBF374F6" authority="Platnick &amp; Dupérré &amp; Berniker &amp; Bonaldo, 2013" authorityName="Platnick &amp; Dupérré &amp; Berniker &amp; Bonaldo" authorityYear="2013" box="[979,1086,1478,1498]" class="Arachnida" family="Oonopidae" genus="Prodysderina" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="23" phylum="Arthropoda" rank="species" species="filandia" status="new species">
