<paragraph id="DF2ABFFCB498559E3E394646C34C0CB3" pageId="5" pageNumber="18">
<taxonomicName id="3C5993265270836718131855CBF3838C" LSID="60EDB58D-FFB3-87AF-C167-23437AD95DA4" authority="(Kirby, 1808)" authorityName="Douglas &amp; Bouchard &amp; Anderson &amp; Tonnancour &amp; Vigneault &amp; Webster" authorityYear="2013" baseAuthorityName="Kirby" baseAuthorityYear="1808" class="Hexapoda" family="Brentidae" genus="Stenopterapion" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Stenopterapion meliloti" order="Coleoptera" pageId="5" pageNumber="18" phylum="Arthropoda" rank="species" species="meliloti" status="new to Canada">Stenopterapion meliloti (Kirby, 1808)</taxonomicName>
