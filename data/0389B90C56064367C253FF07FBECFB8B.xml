, 
<geoCoordinate id="EE146EDD56064367C389FDC3FE7BFD89" box="[427,510,521,544]" degrees="48" direction="east" minutes="25" orientation="longitude" pageId="9" pageNumber="122" precision="925" value="48.416668">48°25’E</geoCoordinate>
, fl., fr., 
<date id="FF9E2EDA56064367C076FDC3FF21FD93" pageId="9" pageNumber="122" value="1911-11">
