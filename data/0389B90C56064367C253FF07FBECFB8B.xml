<collectorName id="26D56DCC56064367C352FE3AFDC1FDAE" box="[368,580,496,519]" pageId="9" pageNumber="122">du l’Onive-Mangoro</collectorName>
, Forêt d’Andasibe, 
<quantity id="4CD8A5FF56064367C2CCFDC3FEC5FD89" box="[238,320,521,544]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="9" pageNumber="122" unit="m" value="1400.0">
