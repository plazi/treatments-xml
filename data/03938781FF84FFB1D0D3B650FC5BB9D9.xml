<collectionCode id="ED2BAE52FF85FFB1D402B42FFB0DBF26" box="[1094,1185,333,356]" httpUri="http://grbio.org/cool/k3qp-615s" name="Zoological Museum, Moscow Lomonosov State University" pageId="19" pageNumber="20">ZMMU</collectionCode>
