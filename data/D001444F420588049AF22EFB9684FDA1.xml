<number id="EE60E77E420488049E182EB49098FE29" box="[1149,1166,432,458]" pageId="5" pageNumber="54" value="4.0">4</number>
+ 
<number id="EE60E77E420488049EC52EB490A7FE29" box="[1184,1201,432,458]" pageId="5" pageNumber="54" value="4.0">4</number>
