<number id="EE60E77E420488049E762EB49033FE29" box="[1043,1061,432,458]" pageId="5" pageNumber="54" value="4.0">4</number>
+ 
<number id="EE60E77E420488049E5D2EB49051FE29" box="[1080,1095,432,458]" pageId="5" pageNumber="54" value="3.0">3</number>
