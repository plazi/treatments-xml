<taxonomicName id="4C764D0DFFB41D38FC0DF97DFB6B49FD" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[933,1203,1665,1682]" class="Insecta" family="Melolonthidae" genus="Serica" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="790" phylum="Arthropoda" rank="species" species="brunnea">
