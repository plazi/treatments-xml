<taxonomicName id="27E985B1EB8FB58DE66CF07299DCD997" class="Magnoliopsida" family="Ulmaceae" genus="Celtis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Rosales" pageId="0" pageNumber="49" phylum="Tracheophyta" rank="family">Ulmaceae</taxonomicName>
