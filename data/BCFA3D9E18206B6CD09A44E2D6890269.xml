<taxonomicName id="1904E354A737BE2B158E4982F64F1C33" authority="Sharp, 1903" authorityName="Sharp" authorityYear="1903" class="Insecta" family="Carabidae" genus="Thriscothorax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Thriscothorax subconstrictus" order="Coleoptera" pageId="145" pageNumber="146" phylum="Arthropoda" rank="species" species="subconstrictus">
