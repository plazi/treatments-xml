<taxonomicName id="4C1F00F2FFF0FFE0FF3FFF0F7E6AFF59" authorityName="Vigors" authorityYear="1825" box="[151,301,153,179]" class="Aves" family="Phasianidae" kingdom="Animalia" order="Galliformes" pageId="7" pageNumber="118" phylum="Chordata" rank="subFamily" subFamily="Tetraoninae">Tetraoninae</taxonomicName>
sp.
