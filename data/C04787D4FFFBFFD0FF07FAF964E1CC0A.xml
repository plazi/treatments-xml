<quantity id="8F169B27FFFBFFD0FE4EFAF86777CE3E" box="[478,564,1375,1401]" metricMagnitude="1" metricUnit="m" metricValue="5.12064" pageId="18" pageNumber="19" unit="in" value="2016.0">2016 in</quantity>
