<heading id="FD747C67FF81E01EFED567E5FD7DF926" bold="true" box="[264,692,1735,1761]" fontSize="11" level="4" pageId="81" pageNumber="82" reason="6">
<emphasis id="94F71719FF81E01EFED567E5FD7DF926" bold="true" box="[264,692,1735,1761]" pageId="81" pageNumber="82">
<taxonomicName id="6183B088FF81E01EFED567E5FDDCF926" box="[264,533,1735,1761]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="virgulae" status="new species">
<emphasis id="94F71719FF81E01EFED567E5FDDCF926" bold="true" box="[264,533,1735,1761]" italics="true" pageId="81" pageNumber="82">Glyphidocera virgulae</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="8FC4AA62FF81E01EFDF967E5FD7DF926" box="[548,692,1735,1761]" pageId="81" pageNumber="82" rank="species">new species</taxonomicNameLabel>
