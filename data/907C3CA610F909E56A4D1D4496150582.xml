<taxonomicName id="2DF8FE1455FA523CA28E65A33F075DE0" baseAuthorityName="Pfeiffer" baseAuthorityYear="1847" class="Gastropoda" family="Odontostomidae" genus="Bahiensis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="neglecta">
