<taxonomicName id="6ADB4D158120FFC472BDD4207BFDFE84" authority="de Saussure" authorityName="de Saussure" box="[683,994,331,358]" class="Insecta" family="Eumenidae" genus="Anterhynchium" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
<emphasis id="9FAFEA848120FFC472BDD4207B49FE87" box="[683,854,331,357]" italics="true" pageId="26" pageNumber="27">Anterhynchium</emphasis>
