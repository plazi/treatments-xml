<taxonomicName id="6ADB4D158120FFC474D7D4047D73FE6B" box="[1217,1388,367,393]" class="Insecta" family="Eumenidae" genus="Anterhynchium" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
