<taxonomicName id="E811CF7E841625ED4E41E5F2C253B221" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34109" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole yaqui Creighton &amp; Gregg" lsidName-HNS="Pheidole yaqui Creighton &amp; Gregg" order="Hymenoptera" pageNumber="591" phylum="Arthropoda" rank="species" species="yaqui">yaqui</taxonomicName>
