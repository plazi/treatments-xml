<taxonomicName id="97FC8711D59650F0BC22B90F259D77AF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181978" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole yucatana Wilson" lsidName-HNS="Pheidole yucatana Wilson" order="Hymenoptera" pageNumber="591" phylum="Arthropoda" rank="species" species="yucatana">yucatana</taxonomicName>
