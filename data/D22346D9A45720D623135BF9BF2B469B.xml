<paragraph id="EF6AEEC74491175664F3FE7D603E7B38" pageId="null" pageNumber="391">
<pageBreakToken id="6D57F49C7715037D9199E049FF6BBC74" pageId="null" pageNumber="391">Artengruppe</pageBreakToken>
der 
<taxonomicName id="D310A8EABE6EFCF847C78490C2CAB1E5" authority="L." class="Magnoliopsida" family="Rosaceae" genus="Potentilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="391" phylum="Tracheophyta" rank="species" species="argentea">
Potentilla 
