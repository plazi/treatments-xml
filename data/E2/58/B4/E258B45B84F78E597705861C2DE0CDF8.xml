<taxonomicName class="Insecta" family="Curculionidae" genus="Sphadasmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphadasmus" order="Coleoptera" pageId="259" pageNumber="260" phylum="Arthropoda" rank="genus">Sphadasmus</taxonomicName>
