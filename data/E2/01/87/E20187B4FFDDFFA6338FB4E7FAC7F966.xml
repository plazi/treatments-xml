<typeStatus box="[1031,1079,1560,1584]" pageId="5" pageNumber="120">type</typeStatus>
locality, “La Clara”, in 
<collectingRegion box="[1353,1432,1558,1583]" country="Colombia" name="Caldas" pageId="5" pageNumber="120">Caldas</collectingRegion>
, 
<collectingRegion box="[151,266,1594,1619]" country="Colombia" name="Antioquia" pageId="5" pageNumber="120">Antioquia</collectingRegion>
, 
<collectingCountry box="[277,391,1594,1619]" name="Colombia" pageId="5" pageNumber="120">Colombia</collectingCountry>
