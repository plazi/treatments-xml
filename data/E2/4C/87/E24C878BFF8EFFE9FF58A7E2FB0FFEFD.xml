<collectingRegion box="[893,999,223,247]" country="United States of America" name="Maryland" pageId="1" pageNumber="144">Maryland</collectingRegion>
, 
<collectingCountry box="[1010,1067,223,247]" name="United States of America" pageId="1" pageNumber="144">USA</collectingCountry>
.
</paragraph>
