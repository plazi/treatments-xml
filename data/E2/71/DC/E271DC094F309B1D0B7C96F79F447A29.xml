<taxonomicName class="Hexapoda" family="Vespidae" genus="Odynerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="1" pageNumber="132" phylum="Arthropoda" rank="species" species="troglodytes">
