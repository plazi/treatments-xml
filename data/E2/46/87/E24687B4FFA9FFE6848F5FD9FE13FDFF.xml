<taxonomicName box="[1052,1151,522,547]" class="Diplopoda" family="Parajulidae" genus="Paraiulus" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="tarascus">
