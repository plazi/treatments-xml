</bibRefCitation>
); coordinates are 
<geoCoordinate box="[420,534,482,508]" direction="north" orientation="latitude" pageId="8" pageNumber="9" precision="925" value="19.616667">
<number box="[420,451,482,508]" pageId="8" pageNumber="9" value="19.0">19</number>
° 
<number box="[468,500,482,508]" pageId="8" pageNumber="9" value="37.0">37</number>
' N
</geoCoordinate>
, 
<geoCoordinate box="[546,680,482,508]" direction="west" orientation="longitude" pageId="8" pageNumber="9" precision="925" value="-100.28333">
<number box="[546,592,482,508]" pageId="8" pageNumber="9" value="100.0">100</number>
