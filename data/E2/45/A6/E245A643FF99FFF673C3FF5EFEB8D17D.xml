<taxonomicName authority="Biodiversity Inventory" authorityName="Biodiversity Inventory" box="[670,1007,247,271]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="150" phylum="Arthropoda" rank="order">Diptera Biodiversity Inventory</taxonomicName>
