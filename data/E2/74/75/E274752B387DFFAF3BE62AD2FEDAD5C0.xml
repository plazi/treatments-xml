<collectingCountry box="[846,930,863,887]" name="Guinea-Bissau" pageId="14" pageNumber="15">Guinea</collectingCountry>
, 
<collectingCountry box="[944,1082,863,889]" name="Ivory Coast" pageId="14" pageNumber="15">Ivory Coast</collectingCountry>
; West Africa: 
<collectingCountry box="[1249,1335,863,887]" name="Angola" pageId="14" pageNumber="15">Angola</collectingCountry>
; 
<quantity box="[1348,1436,863,888]" metricMagnitude="1" metricUnit="m" metricValue="3.0" metricValueMax="6.0" metricValueMin="0.0" pageId="14" pageNumber="15" unit="m" value="30.0" valueMax="60.0" valueMin="0.0">0–60 m</quantity>
