<collectingCountry box="[590,700,152,178]" name="Bermuda" pageId="35" pageNumber="36">Bermuda</collectingCountry>
, Florida, Antilles, 
<collectingCountry box="[924,1043,152,178]" name="Colombia" pageId="35" pageNumber="36">Colombia</collectingCountry>
, and 
<collectingCountry box="[1107,1179,152,178]" name="Brazil" pageId="35" pageNumber="36">Brazil</collectingCountry>
(from Pará to Rio de Janeiro) (
<bibRefCitation author="Melo" box="[260,393,192,218]" pageId="35" pageNumber="42" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do litoral Brasileiro. Editora Pleiade, Sao Paulo, 603 pp." type="book" year="1996">
