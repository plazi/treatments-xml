<collectingCountry box="[1094,1186,301,327]" name="Mexico" pageId="32" pageNumber="33">Mexico</collectingCountry>
, Antilles, 
<collectingCountry box="[1313,1431,301,327]" name="Colombia" pageId="32" pageNumber="33">Colombia</collectingCountry>
, 
<collectingCountry box="[151,274,341,367]" name="Venezuela" pageId="32" pageNumber="33">Venezuela</collectingCountry>
, Guyanas, and 
<collectingCountry box="[452,524,341,367]" name="Brazil" pageId="32" pageNumber="33">Brazil</collectingCountry>
(from Amapá to Rio Grande do Sul) (
<bibRefCitation author="Melo" box="[971,1105,341,367]" pageId="32" pageNumber="42" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do litoral Brasileiro. Editora Pleiade, Sao Paulo, 603 pp." type="book" year="1996">
