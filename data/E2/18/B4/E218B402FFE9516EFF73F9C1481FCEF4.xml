<collectingCountry box="[966,1058,1770,1796]" name="Mexico" pageId="26" pageNumber="27">Mexico</collectingCountry>
, Antilles, 
<collectingCountry box="[1178,1297,1770,1796]" name="Colombia" pageId="26" pageNumber="27">Colombia</collectingCountry>
, 
<collectingCountry box="[1308,1431,1770,1796]" name="Venezuela" pageId="26" pageNumber="27">Venezuela</collectingCountry>
, Guyanas, 
<collectingCountry box="[268,340,1810,1836]" name="Brazil" pageId="26" pageNumber="27">Brazil</collectingCountry>
(from Amapá to Rio Grande do Sul), 
<collectingCountry box="[786,906,1810,1836]" name="Argentina" pageId="26" pageNumber="27">Argentina</collectingCountry>
, and 
<collectingCountry box="[967,1069,1810,1836]" name="Uruguay" pageId="26" pageNumber="27">Uruguay</collectingCountry>
(
