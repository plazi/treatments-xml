<collectingCountry box="[1072,1126,873,897]" name="India" pageId="10" pageNumber="643">India</collectingCountry>
(
<collectingRegion box="[158,292,905,929]" country="India" name="Tamil Nadu" pageId="10" pageNumber="643">Tamil Nadu</collectingRegion>
), 
<collectingCountry box="[311,377,905,929]" name="Nepal" pageId="10" pageNumber="643">Nepal</collectingCountry>
, 
<collectingCountry box="[390,513,905,929]" name="Philippines" pageId="10" pageNumber="643">Philippines</collectingCountry>
, 
<collectingCountry box="[524,632,905,929]" name="Sri Lanka" pageId="10" pageNumber="643">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[644,739,905,929]" name="Thailand" pageId="10" pageNumber="643">Thailand</collectingCountry>
