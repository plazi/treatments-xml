<collectingRegion box="[408,540,505,529]" country="India" name="Uttarakhand" pageId="11" pageNumber="644">Uttarakhand</collectingRegion>
and 
<collectingRegion box="[598,730,505,529]" country="India" name="Tamil Nadu" pageId="11" pageNumber="644">Tamil Nadu</collectingRegion>
