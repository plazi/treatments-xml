<collectingCountry box="[921,1026,473,497]" name="Sri Lanka" pageId="11" pageNumber="644">Sri Lanka</collectingCountry>
(
<typeStatus box="[1041,1088,473,497]" pageId="11" pageNumber="644">type</typeStatus>
