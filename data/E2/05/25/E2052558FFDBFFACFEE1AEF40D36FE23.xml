<collectingCountry box="[735,846,266,290]" name="Cambodia" pageId="6" pageNumber="639">Cambodia</collectingCountry>
, 
<collectingCountry box="[855,909,267,290]" name="Laos" pageId="6" pageNumber="639">Laos</collectingCountry>
, 
<collectingCountry box="[917,1022,267,291]" name="Myanmar" pageId="6" pageNumber="639">Myanmar</collectingCountry>
, 
<collectingCountry box="[1030,1122,266,290]" name="Pakistan" pageId="6" pageNumber="639">Pakistan</collectingCountry>
, 
<collectingCountry box="[151,246,298,322]" name="Thailand" pageId="6" pageNumber="639">Thailand</collectingCountry>
and 
<collectingCountry box="[305,396,298,322]" name="Vietnam" pageId="6" pageNumber="639">Vietnam</collectingCountry>
(
<bibRefCitation author="BALTHASAR V." box="[414,598,298,322]" pageId="6" pageNumber="639" refId="ref6497" refString="BALTHASAR V. 1964: Monographie der Scarabaeidae der palaearktischen und orientalischen Region. Bd. 3. Verlag der Tschechoslowakischen Akademie der Wissenschaften, Prag, 652 pp." type="book" year="1964">BALTHASAR 1964</bibRefCitation>
; DELLACASA &amp; DELLACASA 2006; 
<bibRefCitation author="PAULIAN R." box="[968,1122,298,322]" pageId="6" pageNumber="639" refId="ref7579" refString="PAULIAN R. 1945: Coleopteres Scarabeides de l'Indochine. Faune de l'Empire Francais III. Librairie Larose, Paris, 225 pp." type="book" year="1945">PAULIAN 1945</bibRefCitation>
