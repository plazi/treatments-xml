), from 
<taxonomicName box="[745,881,260,284]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="12" pageNumber="55" phylum="Tracheophyta" rank="species" species="lenta">
