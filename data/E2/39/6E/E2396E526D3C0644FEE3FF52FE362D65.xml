<emphasis box="[504,518,866,889]" italics="true" pageId="27" pageNumber="28">S</emphasis>
. (
<emphasis box="[542,561,866,889]" italics="true" pageId="27" pageNumber="28">N</emphasis>
.) 
<emphasis box="[584,713,865,889]" italics="true" pageId="27" pageNumber="28">borneoense</emphasis>
</taxonomicName>
and 
<taxonomicName box="[767,985,865,890]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="nigripilosum" subGenus="Simulium">
