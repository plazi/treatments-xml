<subSubSection pageId="null" pageNumber="129" type="vernacular_name">
<paragraph pageId="null" pageNumber="129">
<normalizedToken originalValue="Großlippiger" pageId="null" pageNumber="129">Grosslippiger</normalizedToken>
Ziest
</paragraph>
