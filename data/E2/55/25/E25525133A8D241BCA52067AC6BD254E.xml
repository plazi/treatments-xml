<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147183" authority="Emery" authorityName="Emery" class="Insecta" family="Formicidae" genus="Parholcomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Parholcomyrmex Emery" order="Hymenoptera" pageNumber="162" phylum="Arthropoda" rank="genus">Parholcomyrmex</taxonomicName>
