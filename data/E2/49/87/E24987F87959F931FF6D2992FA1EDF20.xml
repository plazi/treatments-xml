</taxonomicName>
(
<taxonomicName box="[1249,1366,251,272]" class="Anthozoa" kingdom="Animalia" order="Scleractinia" pageId="21" pageNumber="56" phylum="Cnidaria" rank="order">Scleractinia</taxonomicName>
, 
<taxonomicName class="Anthozoa" family="Pocilloporidae" kingdom="Animalia" order="Scleractinia" pageId="21" pageNumber="56" phylum="Cnidaria" rank="family">Pocilloporidae</taxonomicName>
)............................................................ 
