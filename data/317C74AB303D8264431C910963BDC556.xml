<subSubSection id="1AD85DAAC460DA3C965A1D0C62C2603A" pageId="5" pageNumber="46" type="etymology">
<paragraph id="1BB390459F3BF1ED194693EA2A967D89" pageId="5" pageNumber="46">Etymology.</paragraph>
<paragraph id="4260E9EF38168A4DCE8FED5B714D890E" pageId="5" pageNumber="46">
Named after 
<normalizedToken id="3F1E46640173B6ECD7ABAB05DFDE0B81" originalValue="Lodève">Lodeve</normalizedToken>
