<taxonomicName id="E3A7FE88FF938363FE045F74FDF1F8D0" authority="R. Br." authorityName="R. Br." box="[448,609,1812,1835]" class="Magnoliopsida" family="Brassicaceae" genus="Eutrema" kingdom="Plantae" order="Brassicales" pageId="46" pageNumber="46" phylum="Tracheophyta" rank="genus">
