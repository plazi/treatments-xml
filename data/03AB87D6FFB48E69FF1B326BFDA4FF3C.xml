<emphasis id="B976EAD2FFB58E77FF1B364CFE8DF86F" bold="true" box="[151,341,1902,1925]" pageId="15" pageNumber="538">FIGURES 71–84.</emphasis>
<taxonomicName id="4C024D43FFB58E77FED7364DFE0FF86E" box="[347,471,1903,1925]" class="Entognatha" family="Entomobryidae" genus="Sinella" kingdom="Animalia" order="Collembola" pageId="15" pageNumber="538" phylum="Arthropoda" rank="species" species="tiani" status="sp. nov.">
<emphasis id="B976EAD2FFB58E77FED7364DFE0FF86E" box="[347,471,1903,1925]" italics="true" pageId="15" pageNumber="538">Sinella tiani</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24557A9FFB58E77FE53364DFDE9F86F" box="[479,561,1903,1924]" pageId="15" pageNumber="538" rank="species">
