[
<normalizedToken id="E4E9E5B164DD6AF4393D2FA819D22687" originalValue="Dégé">Dege</normalizedToken>
, handwritten] / 3200m [handwritten] / 
<normalizedToken id="9553359AD6470201A31EB8806F92C65A" originalValue="中国科学院">中国科学院</normalizedToken>
[
<normalizedToken id="342529CB992BDB095A18F51235381442" originalValue="Zhōngguó">Zhōngguo</normalizedToken>
<normalizedToken id="30B2A6E0F9132C79F4A713D5C342B727" originalValue="Kēxuéyuàn">Kēxueyuan</normalizedToken>
