<taxonomicName id="20D83383FF8AFD2BFEFBBD8259FCF878" authority="Gnezdilov, 2019" authorityName="Gnezdilov" authorityYear="2019" box="[264,453,2007,2026]" class="Insecta" family="Cicadellidae" genus="Stenogiffardia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="551" phylum="Arthropoda" rank="species" species="bifurca" status="sp. nov.">
