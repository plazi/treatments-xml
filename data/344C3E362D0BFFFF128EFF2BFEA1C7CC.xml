<taxonomicName id="7BE5F4A32D0AFFFF130BFBFDFEABC720" box="[274,352,1102,1126]" class="Insecta" family="Sepsidae" genus="Mucha" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="254" phylum="Arthropoda" rank="genus">
