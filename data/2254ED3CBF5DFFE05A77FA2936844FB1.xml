</taxonomicName>
, 
<taxonomicName id="6DFD27A9BF5CFFE058D7FBAC31724F09" box="[936,1052,1099,1125]" class="Magnoliopsida" family="Ericaceae" kingdom="Plantae" order="Ericales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="family">Ericaceae</taxonomicName>
