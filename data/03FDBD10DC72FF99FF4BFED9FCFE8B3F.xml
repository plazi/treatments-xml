<taxonomicName id="4C547785DC72FF99FF32FED9FDE18C79" authority="Burmeister" authorityName="Burmeister" box="[238,580,301,327]" class="Insecta" family="Dynastidae" genus="Heterogomphus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="B920D014DC72FF99FF32FED9FE158C79" bold="true" box="[238,432,301,327]" italics="true" pageId="5" pageNumber="39">Heterogomphus</emphasis>
