<treatmentCitationGroup id="9DB811FFFFA92A7B84ADF953FDFE66A9" box="[151,620,1744,1768]" pageId="5" pageNumber="34">
<treatmentCitation id="3C0910C0FFA92A7B84ADF953FDFA66A9" ID-DOI="http://doi.org/10.5281/zenodo.6077775" ID-Zenodo-Dep="6077775" author="Minaei, K." box="[151,616,1744,1768]" httpUri="http://treatment.plazi.org/id/492C87E4C926FF88FF37FF5FFE8DC8B9" page="371" pageId="5" pageNumber="34" year="2016">
