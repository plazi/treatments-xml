<paragraph id="102536C8FFE6ED21CF55FBC837F2FBD8" blockId="10.[264,1323,399,1185]" box="[312,924,1039,1065]" pageId="10" pageNumber="11">
<typeStatus id="CF21886AFFE6ED21CF55FBC835CCFBD8" box="[312,418,1039,1065]" pageId="10" pageNumber="11">Paratype</typeStatus>
