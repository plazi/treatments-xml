<th id="475C0E032C320037FC8F8C1EFC0AFCA6" box="[863,939,805,827]" gridcol="5" gridrow="0" pageId="19" pageNumber="20">LFIII</th>
<th id="475C0E032C320037FC3B8C1EFB96FCA6" box="[1003,1079,805,827]" gridcol="6" gridrow="0" pageId="19" pageNumber="20">wFIII</th>
<th id="475C0E032C320037FBA88C1EFB0CFCA6" box="[1144,1197,805,827]" gridcol="7" gridrow="0" pageId="19" pageNumber="20">LTIII</th>
<th id="475C0E032C320037FB3F8C1EFAF3FCA6" box="[1263,1362,805,827]" gridcol="8" gridrow="0" pageId="19" pageNumber="20">File</th>
</tr>
<tr id="048D677F2C320037FF4F8C75FAF3FCF9" box="[159,1362,846,868]" gridrow="1" pageId="19" pageNumber="20" rowspan-8="1">
<th id="475C0E032C320037FF4F8C75FEB5FCF9" box="[159,276,846,868]" gridcol="0" gridrow="1" pageId="19" pageNumber="20">Holotype</th>
<td id="475C0E032C320037FEBA8C75FE09FCF9" box="[362,424,846,868]" gridcol="1" gridrow="1" pageId="19" pageNumber="20">2.1</td>
<td id="475C0E032C320037FE398C75FD94FCF9" box="[489,565,846,868]" gridcol="2" gridrow="1" pageId="19" pageNumber="20">3.2</td>
<td id="475C0E032C320037FDA58C75FD0BFCF9" box="[629,682,846,868]" gridcol="3" gridrow="1" pageId="19" pageNumber="20">6.8</td>
<td id="475C0E032C320037FD388C75FC80FCF9" box="[744,801,846,868]" gridcol="4" gridrow="1" pageId="19" pageNumber="20">5</td>
