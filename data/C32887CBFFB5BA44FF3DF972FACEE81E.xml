<heading id="107681B1FFB5BA46FF3DF972FDD6ECBD" bold="true" box="[151,571,1669,1695]" fontSize="11" level="1" pageId="37" pageNumber="38" reason="1">
<taxonomicName id="8C814D5EFFB5BA46FF3DF972FDD6ECBD" authority="(Wagner 1842)" authorityName="Wagner" authorityYear="1842" baseAuthorityName="Wagner" baseAuthorityYear="1842" box="[151,571,1669,1695]" class="Mammalia" family="Muridae" genus="Gerbillus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="dasyurus">
