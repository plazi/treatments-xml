<quantity id="BF279B36FF95D273FD69F90FFD488B49" box="[686,761,1773,1799]" metricMagnitude="2" metricUnit="m" metricValue="1.6" pageId="10" pageNumber="311" unit="m" value="160.0">160 m</quantity>
