<paragraph id="04F8653464902E5751E6B654A3B451C5" pageId="42" pageNumber="43">
<taxonomicName id="42C1F6DF0C6D29AA72E325591749C0F9" class="Arachnida" family="Linyphiidae" genus="Vittatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vittatus fencha" order="Araneae" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="fencha">Vittatus fencha</taxonomicName>
