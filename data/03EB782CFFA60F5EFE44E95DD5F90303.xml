<collectingCountry id="F35589AAFFA70F5EFEF0E991D54700A3" box="[266,372,412,436]" name="Indonesia" pageId="3" pageNumber="614">Indonesia</collectingCountry>
, 
<collectingRegion id="498607D8FFA70F5EFE85E991D59C00A3" box="[383,431,412,436]" country="Indonesia" name="Bali" pageId="3" pageNumber="614">Bali</collectingRegion>
