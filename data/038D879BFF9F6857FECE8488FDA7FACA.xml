<taxonomicName id="4C244D0EFF9E6857FD9384C6FC96FC4D" box="[609,802,909,933]" class="Insecta" family="Capniidae" genus="Allocapnia" kingdom="Animalia" order="Plecoptera" pageId="23" pageNumber="93" phylum="Arthropoda" rank="species" species="rickeri">
