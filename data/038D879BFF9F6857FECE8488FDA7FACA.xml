<taxonomicName id="4C244D0EFF9E6857FC9184C6FBA8FC4D" box="[867,1052,909,933]" class="Insecta" family="Capniidae" genus="Allocapnia" kingdom="Animalia" order="Plecoptera" pageId="23" pageNumber="93" phylum="Arthropoda" rank="species" species="starki">
