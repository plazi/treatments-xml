<subSubSection id="23243D5B9BC4D90370120AFD0EA72F9A" pageId="null" pageNumber="801" type="nomenclature">
<paragraph id="37D298C77A946B3593DEE8381D387ACF" pageId="null" pageNumber="801">
<taxonomicName id="1D89DF0B2C2AC0DEF950837168D05F51" authority="L." class="Gastropoda" family="Xenophoridae" genus="Stellaria" kingdom="Animalia" order="Littorinimorpha" pageId="null" pageNumber="801" phylum="Mollusca" rank="species" species="nemorum">
Stellaria 
<normalizedToken id="636DBF4463FD2B73F6F122DD0B2590F7" originalValue="némorum" pageId="null" pageNumber="801">nemorum</normalizedToken>
<authorityName id="4557D7947298189DF9C8BA6B68D6BE79" pageId="null" pageNumber="801">L.</authorityName>
</taxonomicName>
</paragraph>
