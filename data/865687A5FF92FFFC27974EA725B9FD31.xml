<taxonomicName id="C9FF4D30FF92FFFC27974EA7232DFE34" authority="Muller, 1941" authorityName="Muller" authorityYear="1941" box="[151,568,402,429]" class="Insecta" family="Scarabaeidae" genus="Catharsius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="285" phylum="Arthropoda" rank="species" species="omoensis">
