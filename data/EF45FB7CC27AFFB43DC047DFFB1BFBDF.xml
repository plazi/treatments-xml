<taxonomicName id="A0EC31E9C27BFFB43DC043ACFEFAFE12" box="[151,316,385,409]" class="Actinopterygii" family="Oneirodidae" genus="Chaenophryne" kingdom="Animalia" order="Lophiiformes" pageId="25" pageNumber="292" phylum="Chordata" rank="genus">
