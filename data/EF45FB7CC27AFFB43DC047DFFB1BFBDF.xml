<subSubSection id="2FF619E1C27AFFB43D904496FECBFE9E" lastPageId="25" lastPageNumber="292" pageId="24" pageNumber="291" type="description">
<paragraph id="67534A6AC27AFFB53D904496FD10F89C" blockId="24.[151,1436,1623,2015]" pageId="24" pageNumber="291">
<emphasis id="55989678C27AFFB53D904496FE9FF95F" bold="true" box="[199,345,1723,1748]" pageId="24" pageNumber="291">Description.</emphasis>
Dorsal-fin rays 
<number id="D124584DC27AFFB53F584496FDD9F95F" box="[527,543,1723,1748]" pageId="24" pageNumber="291" value="6.0">6</number>
