<paragraph id="E27E368FFFD0054FFF56B0CDFDE8FB24" blockId="7.[151,1015,1203,1256]" box="[151,548,1203,1226]" pageId="7" pageNumber="46">
<taxonomicName id="25C14D0CFFD0054FFF56B0CDFDD3FB24" authority="Marais &amp; Loots, 1969: 1" authorityName="Marais &amp; Loots" authorityPageNumber="1" authorityYear="1969" box="[151,543,1203,1226]" class="Arachnida" family="Dermanyssidae" genus="Laelaspisella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="D0B5EA9DFFD0054FFF56B0CDFED2FB27" box="[151,286,1204,1226]" italics="true" pageId="7" pageNumber="46">Laelaspisella</emphasis>
