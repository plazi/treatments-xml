<taxonomicName id="646686760554434706E4C5AB07845C00" class="Insecta" family="Cicadellidae" genus="Arboridia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arboridia (Arboridia) hussaini" order="Hemiptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="species" species="hussaini" subGenus="Arboridia">Arboridia (Arboridia) hussaini</taxonomicName>
