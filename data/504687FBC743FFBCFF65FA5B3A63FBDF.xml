<taxonomicName id="1FEF4D6EC743FFBDFF65FA5B3B63FA40" authority="Pitts" authorityName="Pitts" box="[151,540,1455,1481]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="arnalduri" status="NEW SPECIES">
<emphasis id="EA9BEAFFC743FFBDFF65FA5B38A5FA40" bold="true" box="[151,474,1455,1481]" italics="true" pageId="30" pageNumber="31">Sphaeropthalma arnalduri</emphasis>
Pitts
</taxonomicName>
, 
<taxonomicNameLabel id="F1A85784C743FFBDFDDBFA5B3B8FFA40" box="[553,752,1455,1481]" pageId="30" pageNumber="31" rank="species">
<collectionCode id="BEFEAE28C743FFBDFDDBFA5B3B10FA40" box="[553,623,1455,1481]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="30" pageNumber="31">NEW</collectionCode>
<collectionCode id="BEFEAE28C743FFBDFD84FA5B3B8FFA40" box="[630,752,1455,1481]" pageId="30" pageNumber="31">SPECIES</collectionCode>
