<taxonomicName id="C2C04D3C707CFFBDBD409812FD2CFDD1" box="[525,650,516,541]" class="Magnoliopsida" family="Asteraceae" genus="Carthamus" kingdom="Plantae" order="Asterales" pageId="14" pageNumber="165" phylum="Tracheophyta" rank="genus">
