<taxonomicName id="C7F14D37FFE98A0FFC15FD0FFAC4DBB6" authorityName="Thomas" authorityYear="1896" box="[985,1345,686,725]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="creaghi">
<emphasis id="3285EAA6FFE98A0FFC15FD0FFAC4DBB6" box="[985,1345,686,725]" italics="true" pageId="38" pageNumber="317">Rhinolophus creaghi</emphasis>
</taxonomicName>
is included in the 
<taxonomicName id="C7F14D37FFE98A0FF90BFD0FFBB3DA69" authorityName="Temminck" authorityYear="1835" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="euryotis">
<emphasis id="3285EAA6FFE98A0FF90BFD0FFBB3DA69" italics="true" pageId="38" pageNumber="317">euryotis</emphasis>
</taxonomicName>
species group, along with 
<emphasis id="3285EAA6FFE98A0FF989FD42FBB8DD77" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FF989FD42F96BDA69" authorityName="Thomas &amp; Wroughton" authorityYear="1909" box="[1605,1774,739,778]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="canuti">R. canuti</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFC15FCB8FB37DA23" authorityName="Hill &amp; Schlitter" authorityYear="1982" box="[985,1202,793,832]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="mcintyrei">R. mcintyrei</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFB02FCB8FA0BDA23" authorityName="Temminck" authorityYear="1835" box="[1230,1422,793,832]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="euryotis">R. euryotis</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFA60FCB8F9B8DA23" authorityName="Bergmans &amp; Rozendaal" authorityYear="1982" box="[1452,1597,793,832]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="tatar">R. tatar</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FF996FCB8FBB8DA16" authorityName="Patrick, McCulloch &amp; Ruedas" authorityYear="2013" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="belligerator">R. belligerator</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFB9BFCEFFAD4DA16" authorityName="Hill" authorityYear="1959" box="[1111,1361,846,885]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="proconsulis">R. proconsulis</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFAA7FCEFF9B8DA16" authorityName="Peters" authorityYear="1871" box="[1387,1597,846,885]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="arcuatus">R. arcuatus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FF994FCEFF968DA16" authorityName="K Andersen" authorityYear="1905" box="[1624,1773,846,885]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="inops">R. inops</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFC15FC22FB21DAC9" authorityName="K. Andersen" authorityYear="1905" box="[985,1188,899,938]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="subrufus">R.subrufus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFB7BFC22FACDDAC9" authorityName="Eydoux &amp; Gervais" authorityYear="1836" box="[1207,1352,899,938]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="rufus">R.rufus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFA97FC22FA7ADAC9" authorityName="Horsfield" authorityYear="1823" box="[1371,1535,899,938]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="affinis">R affinis</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FF9DDFC22FBA8DABC" authorityName="Dobson" authorityYear="1872" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="andamanensis">R.andamanensis</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFB8FFC19FAC0DABC" authorityName="Peters" authorityYear="1867" box="[1091,1349,952,991]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="coelophyllus">R. coelophyllus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFA97FC19F991DABC" authorityName="Tate" authorityYear="1943" box="[1371,1556,952,991]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="shameli">R. shameli</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FF9E6FC19FBB2DD77" authorityName="Csorba &amp; Jenkins" authorityYear="1998" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="microglobosus">R. microglobosus</taxonomicName>
,
</emphasis>
and 
<emphasis id="3285EAA6FFE98A0FFB65FC4CFAD1DD77" box="[1193,1364,1005,1045]" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FFB65FC4CFACADD77" authorityName="K. Andersen" authorityYear="1905" box="[1193,1359,1005,1045]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="stheno">R. stheno</taxonomicName>
.
</emphasis>
The 
<taxonomicName id="C7F14D37FFE98A0FFA0AFC4CF9CCDD77" authorityName="Temminck" authorityYear="1835" box="[1478,1609,1005,1044]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="euryotis">
<emphasis id="3285EAA6FFE98A0FFA0AFC4CF9CCDD77" box="[1478,1609,1005,1044]" italics="true" pageId="38" pageNumber="317">euryotis</emphasis>
</taxonomicName>
group is currendy not well defined and includes a number of taxa that have typically been placed in other species groups (particularly the 
<taxonomicName id="C7F14D37FFE98A0FFF22FB2DFE3BDDD0" authorityName="J. E. Gray" authorityYear="1834" box="[238,446,1164,1203]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="megaphyllus">
<emphasis id="3285EAA6FFE98A0FFF22FB2DFE3BDDD0" box="[238,446,1164,1203]" italics="true" pageId="38" pageNumber="317">megaphyllus</emphasis>
</taxonomicName>
group). The 
<emphasis id="3285EAA6FFE98A0FFD1CFB2DFCE5DDD0" box="[720,864,1164,1203]" italics="true" pageId="38" pageNumber="317">current</emphasis>
arrangement is based mainly on genetic data, but further sampling is needed to achieve a better understanding of the relationships of the species within this group. 
<emphasis id="3285EAA6FFE98A0FFCC1FB57FC22DC7E" box="[781,935,1270,1309]" italics="true" pageId="38" pageNumber="317">Current</emphasis>
genetic data indicate that the 
<taxonomicName id="C7F14D37FFE98A0FFA39FB57F9FDDC7E" authorityName="Temminck" authorityYear="1835" box="[1525,1656,1270,1309]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="euryotis">
<emphasis id="3285EAA6FFE98A0FFA39FB57F9FDDC7E" box="[1525,1656,1270,1309]" italics="true" pageId="38" pageNumber="317">euryotis</emphasis>
</taxonomicName>
group is probably either close to the 
<emphasis id="3285EAA6FFE98A0FFD2FFA8AFAAADC31" box="[739,1327,1323,1362]" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FFD2FFA8AFC06DC31" authorityName="Horsfield" authorityYear="1851" box="[739,899,1323,1362]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="pearsonii">pearsonii</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFC50FA8AFBC7DC31" authorityName="Temminck" authorityYear="1834" box="[924,1090,1323,1362]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="trifoliatus">trifoliatus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFB96FA8AFAACDC31" authorityName="Borkhausen" authorityYear="1797" box="[1114,1321,1323,1362]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="hipposideros">hipposideros</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="C7F14D37FFE98A0FFA5AFA8AFA79DC31" authorityName="Temminck" authorityYear="1835" box="[1430,1532,1323,1362]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="rouxii">
<emphasis id="3285EAA6FFE98A0FFA5AFA8AFA79DC31" box="[1430,1532,1323,1362]" italics="true" pageId="38" pageNumber="317">rouxii</emphasis>
</taxonomicName>
groups, or is sister to a clade including the 
<emphasis id="3285EAA6FFE98A0FFD2FFAC0FAE2DCEB" box="[739,1383,1377,1416]" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FFD2FFAC0FCEBDCEB" authorityName="Temminck" authorityYear="1834" box="[739,878,1377,1416]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="pusillus">pusillus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFC4BFAC0FBDCDCEB" authorityName="J. E. Gray" authorityYear="1834" box="[903,1113,1377,1416]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="megaphyllus">megaphyllus</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFBA1FAC0FAE4DCEB" authorityName="Waterhouse" authorityYear="1843" box="[1133,1377,1377,1416]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="philippinensis">philippinensis</taxonomicName>
,
</emphasis>
and 
<taxonomicName id="C7F14D37FFE98A0FFA1CFAC0F9D9DCEB" authorityName="Blyth" authorityYear="1844" box="[1488,1628,1377,1416]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="macrotis">
<emphasis id="3285EAA6FFE98A0FFA1CFAC0F9D9DCEB" box="[1488,1628,1377,1416]" italics="true" pageId="38" pageNumber="317">macrotis</emphasis>
</taxonomicName>
groups, but genetic data are still limited. Genetic data have placed. 
<taxonomicName id="C7F14D37FFE98A0FFAE1FA37FA20DCDE" authorityName="Thomas" authorityYear="1896" box="[1325,1445,1430,1469]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="creaghi">
<emphasis id="3285EAA6FFE98A0FFAE1FA37FA20DCDE" box="[1325,1445,1430,1469]" italics="true" pageId="38" pageNumber="317">creaghi</emphasis>
</taxonomicName>
close to 
<emphasis id="3285EAA6FFE98A0FF986FA37FDBDDC90" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FF986FA37F968DCDE" authorityName="Horsfield" authorityYear="1823" box="[1610,1773,1430,1469]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="affinis">R. affinis</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFF50FA6DFED7DC90" authorityName="Tate" authorityYear="1943" box="[156,338,1484,1523]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="shameli">R shameli</taxonomicName>
, 
<taxonomicName id="C7F14D37FFE98A0FFEA9FA6DFDB7DC90" authorityName="Peters" authorityYear="1871" box="[357,562,1484,1523]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="arcuatus">R arcuatus</taxonomicName>
,
</emphasis>
and 
<emphasis id="3285EAA6FFE98A0FFD56FA6DFCEEDC90" box="[666,875,1484,1523]" italics="true" pageId="38" pageNumber="317">
<taxonomicName id="C7F14D37FFE98A0FFD56FA6DFCE3DC90" authorityName="K. Andersen" authorityYear="1905" box="[666,870,1484,1523]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="38" pageNumber="317" phylum="Chordata" rank="species" species="subrufus">R subrufus</taxonomicName>
.
</emphasis>
Two subspecies recognized.
</paragraph>
</subSubSection>
