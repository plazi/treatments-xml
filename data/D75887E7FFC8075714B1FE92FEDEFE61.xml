<materialsCitation id="EF993CACFFC807571437FB2AFB29FAD5" ID-GBIF-Occurrence="3038495303" collectionCode="MNHN" county="Society Islands" elevation="25" location="Living" municipality="Tuamotu Archipelago" pageId="21" pageNumber="354" specimenCount="1" stateProvince="Iles Marquises">
