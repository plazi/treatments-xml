, 
<collectingMunicipality id="BF2AAC8BFFC8075715FEFB2AF8C4FB77" box="[440,646,1199,1223]" pageId="21" pageNumber="354">Tuamotu Archipelago</collectingMunicipality>
and 
<collectingRegion id="9D35F813FFC8075714DAFB4CFB0DFB51" box="[156,335,1225,1249]" country="French Polynesia" name="Iles Marquises" pageId="21" pageNumber="354">Marquesas Islands</collectingRegion>
(
<bibRefCitation id="3B604B00FFC807571518FB4CF85CFB51" author="DIJKSTRA H. H." box="[350,542,1225,1249]" pageId="21" pageNumber="354" pagination="11 - 19" refId="ref20433" refString="DIJKSTRA H. H. 1989 a. - Les Pectinidae de Polynesie Francaise (expose preliminaire) / Pectinidae from French Polynesia (a preliminary report). Xenophora 48: 11 - 19." type="journal article" year="1989">Dijkstra 1989a: 15</bibRefCitation>
; 
<collectionCode id="39E0AE34FFC80757166DFB4CF8C3FB51" box="[555,641,1225,1249]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="21" pageNumber="354">MNHN</collectionCode>
, unpubl. data). 
<location id="5A2E602AFFC807571540FB61FB07FB4C" LSID="urn:lsid:plazi:treatment:D75887E7FFC8075714B1FE92FEDEFE61:5A2E602AFFC807571540FB61FB07FB4C" box="[262,325,1252,1276]" county="Society Islands" municipality="Tuamotu Archipelago" name="Living" pageId="21" pageNumber="354" stateProvince="Iles Marquises">Living</location>
sublittorally under coral slabs or amongst coral rubble on sandy bottoms (
<collectionCode id="39E0AE34FFC8075715BBFB7BF811FAA6" box="[509,595,1278,1302]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="21" pageNumber="354">MNHN</collectionCode>
