<paragraph id="E68CF156FB09DF200E7C50C2B546E8ED" pageId="14" pageNumber="107">
<taxonomicName id="5E9DE9400B73C783B7EE9F599E1D7965" LSID="http://zoobank.org/747FAD7E-4637-4F8E-8E35-93BCFD8D5739" authority="Kang" authorityName="Förster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lytopylus eliethcantillanoae" order="Hymenoptera" pageId="14" pageNumber="107" phylum="Arthropoda" rank="species" species="eliethcantillanoae">Lytopylus eliethcantillanoae Kang</taxonomicName>
<taxonomicNameLabel id="D39F0CB109D9F765E7B789099F980718" pageId="14" pageNumber="107">sp. n.</taxonomicNameLabel>
