<taxonomicName id="227B9B6A703FB1484089BCAF0E9355FF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33765" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole gouldi Forel" order="Hymenoptera" pageNumber="304" phylum="Arthropoda" rank="species" species="gouldi">Ph. Gouldi</taxonomicName>
