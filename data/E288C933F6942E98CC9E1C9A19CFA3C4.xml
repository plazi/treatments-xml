<taxonomicName id="42B5828546486900BD1D73768451A90A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27097" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus radovae Forel" order="Hymenoptera" pageNumber="45" phylum="Arthropoda" rank="species" species="radovae">CAMPONOTUS RADOVAE</taxonomicName>
