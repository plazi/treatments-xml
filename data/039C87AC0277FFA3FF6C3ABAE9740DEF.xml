<number id="3DFD249D0277FFA3FC983C23E9BD0F33" box="[867,916,1094,1118]" pageId="9" pageNumber="10" value="0.48">0.48</number>
long, 
<number id="3DFD249D0277FFA3FC203C23EE250F33" box="[987,1036,1094,1118]" pageId="9" pageNumber="10" value="0.4">0.40</number>
