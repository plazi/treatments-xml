<taxonomicName id="6105BB4F6786FD340D12AFFA4FE0ADD3" authorityName="Kobelt &amp; Moellendorff" authorityYear="1900" class="Gastropoda" family="Alycaeidae" genus="Dicharax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dicharax" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="genus">
