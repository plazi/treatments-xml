<materialsCitation id="3B342F14FF911E7BFB94FE61FB4EC78B" collectionCode="BNHS" pageId="58" pageNumber="432" specimenCode="BNHS 4558" specimenCount="1">
<specimenCode id="DBFA8D32FF911E7BFB94FE61FA8AC7E8" box="[1149,1286,412,434]" collectionCode="BNHS" country="India" httpUri="http://grbio.org/cool/8g20-8f2r" name="Bombay Natural History Society" pageId="58" pageNumber="432">BNHS 4558</specimenCode>
