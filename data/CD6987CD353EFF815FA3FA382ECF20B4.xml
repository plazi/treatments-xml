<taxonomicName id="82C04D58353CFF815DCFFEC82BF2207C" authority="Chang, 1960" authorityName="Chang" authorityYear="1960" box="[763,1180,330,356]" class="Insecta" family="Cerambycidae" genus="Anoplophora" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="272" phylum="Arthropoda" rank="species" species="wusheana">
