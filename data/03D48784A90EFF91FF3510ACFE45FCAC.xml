<taxonomicName id="4C7D4D11A90CFF91FF4A1491FEB9FC8F" box="[232,317,765,788]" class="Arachnida" family="Aturidae" genus="Javalbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="sunyi">
