<number id="3DB524B5A90CFF91FB1D14D8FB73FD56" box="[1215,1271,692,717]" pageId="39" pageNumber="40" value="1967.0">1967</number>
</bibRefCitation>
</taxonomicName>
(
<collectingCountry id="F36A7602A90CFF91FAAA14D8FAC1FD56" box="[1288,1349,692,717]" name="India" pageId="39" pageNumber="40">India</collectingCountry>
) and 
<taxonomicName id="4C7D4D11A90CFF91FA2A14D9FEE1FD6B" authority="Wiles, 1991" authorityName="Wiles" authorityYear="1991" class="Arachnida" family="Aturidae" genus="Javalbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="sunyi">
