<subSubSection id="C347CDFEFFBA9220FF2AF9E15E9FF925" box="[199,1008,1653,1678]" pageId="1" pageNumber="120" type="etymology">
<paragraph id="8BE29E75FFBA9220FF2AF9E15E9FF925" blockId="1.[151,1437,1473,2038]" box="[199,1008,1653,1678]" pageId="1" pageNumber="120">
<emphasis id="B9294267FFBA9220FF2AF9E15C21F925" bold="true" box="[199,334,1653,1678]" pageId="1" pageNumber="120">Etymology.</emphasis>
