<subSubSection id="5103D160EDB792E1C4CD41A14E73583B" pageId="0" pageNumber="77" type="morphological data">
<paragraph id="218A491BB62BA12CCB7FCD21AEC7B4E2" pageId="0" pageNumber="77">Morphological data.</paragraph>
<paragraph id="91690A6421A4F61970472BB13251704D" pageId="0" pageNumber="77">
