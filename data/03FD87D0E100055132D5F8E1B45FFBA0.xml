<number id="3D9C24E1E10705513142FF1CB649FF68" box="[768,855,151,175]" pageId="20" pageNumber="21" value="5.5" valueMax="5.6" valueMin="5.4">5.4–5.6</number>
; 
<emphasis id="B920EAD4E10705513120FF12B670FF77" box="[866,878,153,176]" italics="true" pageId="20" pageNumber="21">c</emphasis>
= 
<number id="3D9C24E1E107055131CFFF1CB11EFF68" box="[909,1024,151,175]" pageId="20" pageNumber="21" value="16.85" valueMax="19.3" valueMin="14.4">14.4–19.3</number>
