: 
<taxonomicName id="00F44D1E254EE067FD19B997FC85FC6D" box="[759,872,981,1007]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="hirsuta">
