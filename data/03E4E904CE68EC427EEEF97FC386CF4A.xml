<typeStatus id="54F6E6B0CE68EC427EEEF891C7D0CF1C" box="[151,209,1802,1828]" pageId="7" pageNumber="8">Type</typeStatus>
locality: 
<collectingCountry id="F35A1882CE68EC427F3BF891C694CF1C" box="[322,405,1802,1828]" name="France" pageId="7" pageNumber="8">France</collectingCountry>
