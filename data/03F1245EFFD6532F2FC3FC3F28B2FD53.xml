<taxonomicName id="4C58EECBFFD6532F2B97FB6D286DFD3B" box="[1219,1348,1268,1290]" class="Arachnida" family="Salticidae" genus="Onomastus" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="443" phylum="Arthropoda" rank="species" species="maskeliya" status="sp. nov.">
