<taxonomicName id="45DC91BEA76A22595661ABA500537F2E" class="Magnoliopsida" family="Rosaceae" genus="Prunus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Prunus davidiana" order="Rosales" pageId="0" pageNumber="67" phylum="Tracheophyta" rank="species" species="davidiana">
