<geoCoordinate id="358A2E77FDE723F8216E8AFC47D7B33C" degrees="115" direction="east" minutes="29" orientation="longitude" precision="1" seconds="20.52" value="115.48903">115°29'20.52&quot;E</geoCoordinate>
