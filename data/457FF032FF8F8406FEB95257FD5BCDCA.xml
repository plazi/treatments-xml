<taxonomicName id="0AD63AA7FF8F8406FE535055FDA3CD6E" authorityName="Thomson" authorityYear="1876" box="[489,607,1597,1621]" class="Insecta" family="Aphelinidae" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="136" phylum="Arthropoda" rank="family">Aphelinidae</taxonomicName>
) and 
<taxonomicName id="0AD63AA7FF8F8406FD295055FE8FCD4B" authority="Howard, 1906" authorityName="Howard" authorityYear="1906" class="Insecta" family="Aphelinidae" genus="Compariella" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="136" phylum="Arthropoda" rank="species" species="bifasciata">
<emphasis id="FFA29D36FF8F8406FD295055FF21CD4B" italics="true" pageId="7" pageNumber="136">Compariella bifasciata</emphasis>
Howard, 1906
</taxonomicName>
(
<taxonomicName id="0AD63AA7FF8F8406FE3A5030FDF1CD4B" box="[384,525,1624,1648]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="136" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
: 
<taxonomicName id="0AD63AA7FF8F8406FDAF5030FD7FCD4B" baseAuthorityName="Forster" baseAuthorityYear="1856" box="[533,643,1624,1648]" class="Insecta" family="Encyrtidae" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="136" phylum="Arthropoda" rank="family">Encyrtidae</taxonomicName>
