<taxonomicName id="0AD63AA7FF8F8406FD9853AAFDB7CEE2" box="[546,587,1474,1497]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="7" pageNumber="136" phylum="Tracheophyta" rank="genus">
