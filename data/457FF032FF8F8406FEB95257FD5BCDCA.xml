<taxonomicName id="0AD63AA7FF8F8406FF3C53E4FF3ECE98" box="[134,194,1420,1443]" class="Magnoliopsida" family="Rutaceae" genus="Citrus" kingdom="Plantae" order="Sapindales" pageId="7" pageNumber="136" phylum="Tracheophyta" rank="genus">
