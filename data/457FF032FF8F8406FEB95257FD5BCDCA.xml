<taxonomicName id="0AD63AA7FF8F8406FEA153CFFEB3CE85" box="[283,335,1447,1470]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="7" pageNumber="136" phylum="Tracheophyta" rank="genus">
