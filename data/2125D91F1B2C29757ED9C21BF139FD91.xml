<taxonomicName id="6E8C138A1B2D29767FB6C4A3F691FB39" box="[504,650,1178,1200]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="13" pageNumber="14" phylum="Porifera" rank="species" species="axifera">
