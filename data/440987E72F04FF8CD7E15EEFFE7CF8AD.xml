<emphasis id="FED4EAE32F05FF8CD4D85A18FC7FF8F5" box="[942,967,1809,1833]" italics="true" pageId="17" pageNumber="318">
z 
<number id="7A6824D62F05FF8CD4CF5A18FC7FF8F5" box="[953,967,1809,1833]" pageId="17" pageNumber="318" value="4.0">4</number>
</emphasis>
, 
<emphasis id="FED4EAE32F05FF8CD4A15A18FC4DF8F5" box="[983,1013,1809,1833]" italics="true" pageId="17" pageNumber="318">
Z 
<number id="7A6824D62F05FF8CD49E5A18FC4DF8F5" box="[1000,1013,1809,1833]" pageId="17" pageNumber="318" value="4.0">4</number>
</emphasis>
and 
<emphasis id="FED4EAE32F05FF8CD3585A18FBF4F8F4" box="[1070,1100,1809,1832]" italics="true" pageId="17" pageNumber="318">
Z 
<number id="7A6824D62F05FF8CD3495A18FBF4F8F4" box="[1087,1100,1809,1832]" pageId="17" pageNumber="318" value="5.0">5</number>
</emphasis>
(
<number id="7A6824D62F05FF8CD32B5A18FB1EF8F5" box="[1117,1190,1809,1833]" pageId="17" pageNumber="318" value="20.39">20, 39</number>
and 
<number id="7A6824D62F05FF8CD3965A19FB44F8F5" box="[1248,1276,1808,1833]" pageId="17" pageNumber="318" value="59.0">59</number>
respectively). 
<taxonomicName id="0BA04D722F05FF8CD7E15A3DFE2BF891" box="[151,403,1844,1869]" class="Arachnida" family="Phytoseiidae" genus="Amblydromalus" kingdom="Animalia" order="Mesostigmata" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="laetus">
<emphasis id="FED4EAE32F05FF8CD7E15A3DFE2BF891" box="[151,403,1844,1869]" italics="true" pageId="17" pageNumber="318">Amblydromalus laetus</emphasis>
</taxonomicName>
