<emphasis id="B95F56751902FF89FF7686A052931101" bold="true" box="[192,1202,1278,1303]" pageId="3" pageNumber="2">
Subgenus 
<taxonomicName id="4C2BF1E41902FF89FEF886A1550F1101" authority="Klimaszewski" authorityName="Klimaszewski" box="[334,814,1278,1303]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="subGenus" subGenus="Hydrosmectomorpha">
<emphasis id="B95F56751902FF89FEF886A154481100" bold="true" box="[334,617,1278,1302]" italics="true" pageId="3" pageNumber="2">Hydrosmectomorpha</emphasis>
Klimaszewski
</taxonomicName>
and 
<taxonomicName id="4C2BF1E41902FF89FCC786A055C31101" authority="Klimaszewski &amp; Webster &amp; Davies &amp; Bourdon, 2018" authorityName="Klimaszewski &amp; Webster &amp; Davies &amp; Bourdon" authorityYear="2018" box="[881,994,1279,1303]" pageId="3" pageNumber="2" rank="subGenus" status="new subgenus" subGenus="Webster">Webster</taxonomicName>
