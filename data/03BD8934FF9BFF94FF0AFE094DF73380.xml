</taxonomicName>
, 
<taxonomicName id="4C1443A1FF9BFF94FE77FC6A4CE63118" authority="Arita &amp; Gorbunov, 2002" authorityName="Arita &amp; Gorbunov" authorityYear="2002" box="[490,952,1014,1039]" class="Insecta" family="Sesiidae" genus="Melittia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="166" phylum="Arthropoda" rank="species" species="formosana">
