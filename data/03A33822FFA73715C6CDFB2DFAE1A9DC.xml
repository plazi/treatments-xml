<taxonomicName id="4C0AF2B7FFA03715C74AFABEFC9AAB1E" authority="Sinev &amp; Hollwedel, 2002" authorityName="Sinev &amp; Hollwedel" authorityYear="2002" box="[272,793,1300,1322]" class="Branchiopoda" family="Chydoridae" genus="Anthalona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="4" pageNumber="228" phylum="Arthropoda" rank="species" species="brandorffi">
