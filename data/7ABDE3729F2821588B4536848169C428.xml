<taxonomicName id="423CF47DCC808D04B36A8B383CBFDC00" class="Insecta" family="Braconidae" genus="Dinotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dinotrema" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Dinotrema</taxonomicName>
