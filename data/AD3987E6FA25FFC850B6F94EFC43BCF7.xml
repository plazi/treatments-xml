<taxonomicName id="E2904D73FA2BFFC8526EFE8CFD1CBFB3" box="[591,689,367,393]" class="Magnoliopsida" family="Cactaceae" genus="Pfeiffera" kingdom="Plantae" order="Caryophyllales" pageId="25" pageNumber="26" phylum="Tracheophyta" rank="genus">
