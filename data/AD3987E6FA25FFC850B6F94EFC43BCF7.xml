</taxonomicName>
[sic] (?) 
<taxonomicName id="E2904D73FA2BFFC8533DFE8CFCD5BFB3" box="[796,888,367,393]" class="Gastropoda" family="Charopidae" genus="Pyrrha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="25" pageNumber="26" phylum="Mollusca" rank="species" species="cressida">
<emphasis id="17E4EAE2FA2BFFC8533DFE8CFCD5BFB3" box="[796,888,367,393]" italics="true" pageId="25" pageNumber="26">cressida</emphasis>
</taxonomicName>
