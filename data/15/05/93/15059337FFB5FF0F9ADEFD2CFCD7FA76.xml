<quantity box="[1257,1431,1378,1404]" metricMagnitude="2" metricUnit="m" metricValue="8.875" metricValueMax="15.93" metricValueMin="1.8199999999999998" pageId="12" pageNumber="37" unit="m" value="887.5" valueMax="1593.0" valueMin="182.0">182 to 1593 m</quantity>
