<quantity box="[872,1037,1061,1087]" metricMagnitude="2" metricUnit="m" metricValue="6.215" metricValueMax="6.29" metricValueMin="6.14" pageId="10" pageNumber="35" unit="m" value="621.5" valueMax="629.0" valueMin="614.0">
