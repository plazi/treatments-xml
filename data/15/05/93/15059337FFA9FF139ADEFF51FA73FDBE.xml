<quantity box="[1257,1431,448,474]" metricMagnitude="2" metricUnit="m" metricValue="6.245" metricValueMax="10.16" metricValueMin="2.33" pageId="16" pageNumber="41" unit="m" value="624.5" valueMax="1016.0" valueMin="233.0">233 to 1016 m</quantity>
