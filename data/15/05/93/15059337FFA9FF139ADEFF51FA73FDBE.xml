<geoCoordinate box="[1024,1145,485,511]" direction="west" orientation="longitude" pageId="16" pageNumber="41" precision="925" value="-25.166666">
<number box="[1024,1057,485,511]" pageId="16" pageNumber="41" value="25.0">25</number>
