<typeStatus box="[478,587,1037,1063]" pageId="18" pageNumber="19">Holotype</typeStatus>
female macroptera, 
<emphasis bold="true" box="[834,1034,1037,1063]" pageId="18" pageNumber="19">
<collectingRegion box="[834,1034,1037,1063]" country="Australia" name="South Australia" pageId="18" pageNumber="19">South Australia</collectingRegion>
</emphasis>
