<taxonomicName authority="Gorham, 1876" authorityName="Gorham" authorityYear="1876" class="Insecta" family="Cleridae" genus="Callimerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Callimerus" order="Coleoptera" pageId="18" pageNumber="27" phylum="Arthropoda" rank="genus">Callimerus</taxonomicName>
