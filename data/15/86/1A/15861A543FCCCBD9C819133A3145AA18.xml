C 
<taxonomicName class="Liliopsida" family="Araceae" genus="Gonatopus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Gonatopus kanyawarus" order="Alismatales" pageId="26" pageNumber="71" phylum="Tracheophyta" rank="species" species="kanyawarus">Gonatopus kanyawarus</taxonomicName>
