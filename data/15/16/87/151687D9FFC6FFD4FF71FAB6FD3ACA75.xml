<paragraph blockId="10.[151,487,1302,1366]" box="[151,487,1302,1329]" pageId="10" pageNumber="497">
<heading bold="true" box="[151,487,1302,1329]" fontSize="11" level="1" pageId="10" pageNumber="497" reason="1">
<emphasis bold="true" box="[151,487,1302,1329]" pageId="10" pageNumber="497">
<taxonomicName authority="Cutrim &amp; Neto &amp; García Aldrete &amp; Rafael, 2021" authorityName="Cutrim &amp; Neto &amp; García Aldrete &amp; Rafael" authorityYear="2021" box="[151,343,1302,1329]" class="Insecta" family="Ptiloneuridae" genus="Loneura" kingdom="Animalia" order="Psocodea" pageId="10" pageNumber="497" phylum="Arthropoda" rank="species" species="duckei" status="n. sp.">
<emphasis bold="true" box="[151,343,1302,1329]" italics="true" pageId="10" pageNumber="497">Loneura duckei</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[350,416,1303,1329]" pageId="10" pageNumber="497" rank="species">n. sp.</taxonomicNameLabel>
Male
</emphasis>
