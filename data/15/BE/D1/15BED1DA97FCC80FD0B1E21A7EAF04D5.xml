<subSubSection pageId="0" pageNumber="1" type="type species">
<paragraph pageId="0" pageNumber="1">Type species.</paragraph>
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Eggers" authorityYear="1923" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus puer" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="puer">
