<taxonomicName authority="Morley, 1913" authorityName="Morley" authorityYear="1913" box="[279,365,267,293]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="subFamily" subFamily="Rhyssinae">species</taxonomicName>
