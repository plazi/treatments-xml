<collectingMunicipality box="[972,1160,969,993]" pageId="1" pageNumber="114">Chinese province</collectingMunicipality>
<collectingCounty box="[1167,1253,970,993]" pageId="1" pageNumber="114">Yunnan</collectingCounty>
, not far from the border with 
<collectingCountry box="[983,1075,1001,1025]" name="Vietnam" pageId="1" pageNumber="114">Vietnam</collectingCountry>
(
<bibRefCitation author="ASSING, V." box="[1094,1237,1001,1025]" pageId="1" pageNumber="114" pagination="31 - 39" refId="ref3218" refString="ASSING, V. 2015: Three new species of Domene from China (Coleoptera: Staphylinidae: Paederinae). - Contributions to Entomology 65 (1): 31 - 39." type="journal article" year="2015">ASSING 2015</bibRefCitation>
)
</materialsCitation>
and approximately 
<quantity box="[876,942,1033,1057]" metricMagnitude="4" metricUnit="m" metricValue="6.0" pageId="1" pageNumber="114" unit="km" value="60.0">60 km</quantity>
away from the locality where the above male was collected. 
<taxonomicName authorityName="ASSING" authorityYear="2015" box="[1021,1238,1065,1089]" class="Insecta" family="Staphylinidae" genus="Domene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="114" phylum="Arthropoda" rank="species" species="affimbriata">
<emphasis box="[1021,1238,1065,1089]" italics="true" pageId="1" pageNumber="114">Domene affimbriata</emphasis>
</taxonomicName>
