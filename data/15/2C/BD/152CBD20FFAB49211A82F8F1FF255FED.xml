<collectingCountry box="[978,1043,223,247]" name="Switzerland" pageId="10" pageNumber="49">Suiza</collectingCountry>
, 
<quantity box="[1054,1142,223,247]" metricMagnitude="3" metricUnit="m" metricValue="1.22" pageId="10" pageNumber="49" unit="m" value="1220.0">1220 m</quantity>
), female, 
<date box="[1252,1368,223,247]" pageId="10" pageNumber="49" value="2000-06-05">
<number box="[1252,1265,223,247]" pageId="10" pageNumber="49" value="5.0">5</number>
