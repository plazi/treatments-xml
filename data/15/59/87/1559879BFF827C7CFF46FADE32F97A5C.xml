<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[874,947,997,1018]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="8" pageNumber="55">USNM</collectionCode>
