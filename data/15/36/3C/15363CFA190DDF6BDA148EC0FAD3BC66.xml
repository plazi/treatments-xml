<subSubSection pageId="53" pageNumber="54" type="distribution">
<paragraph pageId="53" pageNumber="54">Distribution.</paragraph>
<paragraph pageId="53" pageNumber="54">Brazil, Colombia, Ecuador, Peru, Guyana, and Surinam.</paragraph>
</subSubSection>
<subSubSection pageId="53" pageNumber="54" type="records examined">
<paragraph pageId="53" pageNumber="54">Records examined.</paragraph>
<paragraph pageId="53" pageNumber="54">
