<taxonomicName class="Insecta" family="Liviidae" genus="Psyllopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psyllopsis fraxini" order="Hemiptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="species" species="fraxini">Psyllopsis fraxini</taxonomicName>
