<taxonomicName class="Insecta" family="Triozidae" genus="Trioza" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trioza erytreae" order="Hemiptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="species" species="erytreae">Trioza erytreae</taxonomicName>
