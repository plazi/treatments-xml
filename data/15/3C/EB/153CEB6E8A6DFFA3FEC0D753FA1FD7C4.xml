<paragraph blockId="43.[264,1323,284,725]" pageId="43" pageNumber="44">
<number box="[264,282,284,310]" pageId="43" pageNumber="44" value="2.0">2</number>
(
<number box="[293,305,284,310]" pageId="43" pageNumber="44" value="1.0">1</number>
