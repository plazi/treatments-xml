<collectingCountry box="[151,218,1812,1835]" name="Japan" pageId="132" pageNumber="133">Japan</collectingCountry>
, south to 
<collectingRegion box="[327,459,1811,1836]" country="Australia" name="Queensland" pageId="132" pageNumber="133">Queensland</collectingRegion>
(
<collectingCountry box="[475,581,1811,1836]" name="Australia" pageId="132" pageNumber="133">Australia</collectingCountry>
