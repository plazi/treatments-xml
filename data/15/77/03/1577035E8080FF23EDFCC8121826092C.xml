<quantity metricMagnitude="1" metricUnit="m" metricValue="2.65" metricValueMax="5.0" metricValueMin="0.3" pageId="132" pageNumber="133" unit="m" value="26.5" valueMax="50.0" valueMin="3.0">3– 50 m</quantity>
