<location LSID="urn:lsid:plazi:treatment:1577035E807FFFDCEDFCC8B51BD209CD:9801E493807FFFDCE932C9211C03095C" box="[1113,1374,1800,1825]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="Lissenung Island" name="southern New Hanover" pageId="123" pageNumber="124" stateProvince="New">southern New Hanover</location>
