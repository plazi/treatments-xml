<quantity box="[1164,1263,188,213]" metricMagnitude="1" metricUnit="m" metricValue="5.25" metricValueMax="9.5" metricValueMin="1.0" pageId="195" pageNumber="196" unit="m" value="52.5" valueMax="95.0" valueMin="10.0">10–95 m</quantity>
