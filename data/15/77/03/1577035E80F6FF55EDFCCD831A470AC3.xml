<collectingCountry box="[742,845,1154,1179]" name="Australia" pageId="242" pageNumber="243">Australia</collectingCountry>
and 
<collectingCountry box="[904,1084,1154,1179]" name="New Caledonia" pageId="242" pageNumber="243">New Caledonia</collectingCountry>
. Found in lagoon and seaward coral reefs, 
<quantity box="[282,367,1190,1215]" metricMagnitude="1" metricUnit="m" metricValue="1.25" metricValueMax="2.5" metricValueMin="0.0" pageId="242" pageNumber="243" unit="m" value="12.5" valueMax="25.0" valueMin="0.0">0–25 m</quantity>
