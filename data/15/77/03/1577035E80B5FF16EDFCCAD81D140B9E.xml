<location LSID="urn:lsid:plazi:treatment:1577035E80B5FF16EDFCCAD81D140B9E:9801E49380B5FF16EC18CB101B6D0B2E" box="[371,560,1337,1362]" country="Ireland" county="Serge Andrefouet" municipality="Bauddisson Island" name="Kavieng District" pageId="177" pageNumber="178" stateProvince="New">Kavieng District</location>
