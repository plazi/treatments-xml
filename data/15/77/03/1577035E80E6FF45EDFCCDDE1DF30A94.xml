<quantity box="[942,1027,1231,1256]" metricMagnitude="1" metricUnit="m" metricValue="1.35" metricValueMax="2.5" metricValueMin="0.2" pageId="226" pageNumber="227" unit="m" value="13.5" valueMax="25.0" valueMin="2.0">2–25 m</quantity>
