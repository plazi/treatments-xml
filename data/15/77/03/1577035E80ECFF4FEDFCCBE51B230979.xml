<quantity box="[636,721,1628,1653]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="232" pageNumber="233" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">
