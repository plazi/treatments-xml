<location LSID="urn:lsid:plazi:treatment:1577035E80ECFF4FEDFCCBE51B230979:9801E49380ECFF4FEC4FC8751B0B0808" box="[292,598,1628,1653]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="Bismarck Sea" name="southeastern New Hanover" pageId="232" pageNumber="233" stateProvince="New">southeastern New Hanover</location>
