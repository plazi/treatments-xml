<location LSID="urn:lsid:plazi:treatment:1577035E80ECFF4FEDFCCBE51B230979:9801E49380ECFF4FED81C81E1B54082C" box="[234,521,1591,1616]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="Bismarck Sea" name="northern Kavieng District" pageId="232" pageNumber="233" stateProvince="New">northern Kavieng District</location>
