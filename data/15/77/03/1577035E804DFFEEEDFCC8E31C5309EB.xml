<quantity box="[668,739,1919,1943]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="73" pageNumber="74" unit="m" value="1.5" valueMax="3.0" valueMin="0.0">0–3 m</quantity>
