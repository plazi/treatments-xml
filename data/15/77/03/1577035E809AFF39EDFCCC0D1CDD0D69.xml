<collectingCountry box="[181,268,727,752]" name="Kiribati" pageId="158" pageNumber="159">Kiribati</collectingCountry>
(
<collectingRegion box="[284,428,727,752]" country="Kiribati" name="Line Islands" pageId="158" pageNumber="159">Line Islands</collectingRegion>
