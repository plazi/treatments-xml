<quantity metricMagnitude="1" metricUnit="m" metricValue="1.3" metricValueMax="2.5" metricValueMin="0.1" pageId="255" pageNumber="256" unit="m" value="13.0" valueMax="25.0" valueMin="1.0">1–25 m</quantity>
