<collectingCountry box="[473,620,188,213]" name="South Africa" pageId="323" pageNumber="324">South Africa</collectingCountry>
, 
<collectingCountry box="[632,780,188,213]" name="Mozambique" pageId="323" pageNumber="324">Mozambique</collectingCountry>
Channel, 
<collectingCountry box="[896,1019,188,213]" name="Seychelles" pageId="323" pageNumber="324">Seychelles</collectingCountry>
, 
<collectingCountry box="[1031,1166,188,213]" name="Madagascar" pageId="323" pageNumber="324">Madagascar</collectingCountry>
