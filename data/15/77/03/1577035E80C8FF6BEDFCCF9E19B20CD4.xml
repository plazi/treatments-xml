<collectingCountry box="[1262,1388,583,608]" name="Philippines" pageId="204" pageNumber="205">Philippines</collectingCountry>
and 
<collectingCountry box="[151,246,621,644]" name="Vanuatu" pageId="204" pageNumber="205">Vanuatu</collectingCountry>
