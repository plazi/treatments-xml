<quantity box="[1275,1360,620,645]" metricMagnitude="1" metricUnit="m" metricValue="3.3" metricValueMax="6.5" metricValueMin="0.1" pageId="204" pageNumber="205" unit="m" value="33.0" valueMax="65.0" valueMin="1.0">1–65 m</quantity>
