<collectingCountry box="[455,558,620,645]" name="Australia" pageId="204" pageNumber="205">Australia</collectingCountry>
and 
<collectingCountry box="[613,789,620,645]" name="New Caledonia" pageId="204" pageNumber="205">New Caledonia</collectingCountry>
