<collectingCountry box="[366,451,188,213]" name="Taiwan" pageId="298" pageNumber="299">Taiwan</collectingCountry>
, south to northern 
<collectingCountry box="[681,784,188,213]" name="Australia" pageId="298" pageNumber="299">Australia</collectingCountry>
and 
<collectingCountry box="[848,1030,188,213]" name="New Caledonia" pageId="298" pageNumber="299">New Caledonia</collectingCountry>
