<collectingCountry box="[1200,1385,151,176]" name="Micronesia" pageId="298" pageNumber="299">Caroline Islands</collectingCountry>
and 
<collectingCountry box="[151,246,189,212]" name="Vanuatu" pageId="298" pageNumber="299">Vanuatu</collectingCountry>
