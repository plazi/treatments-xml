<collectingCountry box="[1215,1318,1698,1723]" name="Australia" pageId="70" pageNumber="71">Australia</collectingCountry>
and 
<collectingCountry name="New Caledonia" pageId="70" pageNumber="71">New Caledonia</collectingCountry>
