<quantity box="[592,677,511,536]" metricMagnitude="1" metricUnit="m" metricValue="2.0" metricValueMax="3.5" metricValueMin="0.5" pageId="165" pageNumber="166" unit="m" value="20.0" valueMax="35.0" valueMin="5.0">5–35 m</quantity>
