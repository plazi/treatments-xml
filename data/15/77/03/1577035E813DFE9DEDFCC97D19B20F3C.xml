<geoCoordinate box="[434,573,1984,2009]" direction="south" orientation="latitude" pageId="313" pageNumber="314" precision="1" value="-2.61755">02°37.053’S</geoCoordinate>
<geoCoordinate box="[581,737,1984,2009]" direction="east" orientation="longitude" pageId="313" pageNumber="314" precision="1" value="150.7369">150°44.214’E</geoCoordinate>
, 
<quantity box="[749,820,1984,2009]" metricMagnitude="0" metricUnit="m" metricValue="5.5" metricValueMax="6.0" metricValueMin="5.0" pageId="313" pageNumber="314" unit="m" value="5.5" valueMax="6.0" valueMin="5.0">5–6 m</quantity>
depth, 
<date box="[904,1059,1984,2009]" pageId="313" pageNumber="314" value="2014-06-25">25 June 2014</date>
), and NTUM 11457 (
<specimenCount box="[1304,1432,1984,2009]" pageId="313" pageNumber="314" type="generic">1 specimen</specimenCount>
,
</subSubSection>
