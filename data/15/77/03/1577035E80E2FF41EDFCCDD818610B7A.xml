<collectingMunicipality box="[151,342,1081,1106]" pageId="230" pageNumber="231">Kavieng District</collectingMunicipality>
, 
<quantity box="[355,442,1081,1106]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="230" pageNumber="231" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">
