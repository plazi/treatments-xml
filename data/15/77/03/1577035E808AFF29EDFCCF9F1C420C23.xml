<quantity box="[785,885,583,607]" metricMagnitude="1" metricUnit="m" metricValue="5.05" metricValueMax="10.0" metricValueMin="0.1" pageId="142" pageNumber="143" unit="m" value="50.5" valueMax="100.0" valueMin="1.0">1–100 m</quantity>
