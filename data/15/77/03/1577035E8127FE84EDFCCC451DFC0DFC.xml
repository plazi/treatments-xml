<location LSID="urn:lsid:plazi:treatment:1577035E8127FE84EDFCCC451DFC0DFC:9801E4938127FE84EDFCCCF1184C0C93" box="[151,273,728,752]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="Senta Pass" pageId="291" pageNumber="292" stateProvince="New">Senta Pass</location>
