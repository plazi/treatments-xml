<location LSID="urn:lsid:plazi:treatment:1577035E8127FE84EDFCCC451DFC0DFC:9801E4938127FE84EC06CCF11B540C93" box="[365,521,728,751]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="New Hanover" pageId="291" pageNumber="292" stateProvince="New">New Hanover</location>
at 
<elevation box="[555,640,727,752]" metricMagnitude="0" metricUnit="m" metricValue="8.5" metricValueMax="12.0" metricValueMin="5.0" pageId="291" pageNumber="292" unit="m" value="8.5" valueMax="12.0" valueMin="5.0">
<quantity box="[555,640,727,752]" metricMagnitude="0" metricUnit="m" metricValue="8.5" metricValueMax="12.0" metricValueMin="5.0" pageId="291" pageNumber="292" unit="m" value="8.5" valueMax="12.0" valueMin="5.0">5–12 m</quantity>
</elevation>
depth, 
<collectorName box="[723,801,728,752]" pageId="291" pageNumber="292">St. KR</collectorName>
