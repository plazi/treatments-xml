<collectingMunicipality box="[1230,1352,655,680]" pageId="291" pageNumber="292">Jeff Kinch</collectingMunicipality>
during ACIAR 
<location LSID="urn:lsid:plazi:treatment:1577035E8127FE84EDFCCC451DFC0DFC:9801E4938127FE84ED9ECC9D18E00CB0" box="[245,445,692,717]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="Aquarium Survey" pageId="291" pageNumber="292" stateProvince="New">Aquarium Survey</location>
