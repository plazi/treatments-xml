<subSubSection pageId="125" pageNumber="126" type="distribution">
<paragraph blockId="125.[151,1437,727,1077]" pageId="125" pageNumber="126">
<emphasis bold="true" box="[199,495,836,861]" pageId="125" pageNumber="126">Distribution and habitat.</emphasis>
<collectingRegion box="[502,645,836,861]" country="Papua New Guinea" name="New Ireland" pageId="125" pageNumber="126">New Ireland</collectingRegion>
