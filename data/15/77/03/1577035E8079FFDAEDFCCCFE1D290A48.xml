<quantity box="[597,724,871,896]" metricMagnitude="2" metricUnit="m" metricValue="1.9899999999999998" metricValueMax="2.69" metricValueMin="1.29" pageId="125" pageNumber="126" unit="m" value="199.0" valueMax="269.0" valueMin="129.0">129–269 m</quantity>
