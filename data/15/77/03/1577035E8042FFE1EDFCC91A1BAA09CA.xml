<taxonomicName authority="Minding, 1832" authorityName="Minding" authorityYear="1832" box="[1051,1175,1913,1938]" class="Actinopterygii" family="Lophiidae" genus="Lophioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lophiiformes" pageId="70" pageNumber="71" phylum="Chordata" rank="genus">
