<collectingCountry name="Marshall Islands" pageId="159" pageNumber="160">Marshall Islands</collectingCountry>
and 
<collectingCountry box="[286,328,1196,1221]" name="Fiji" pageId="159" pageNumber="160">Fiji</collectingCountry>
