, east of 
<collectingMunicipality pageId="195" pageNumber="196">New Hanover</collectingMunicipality>
, 
<collectorName box="[260,339,1416,1441]" pageId="195" pageNumber="196">St. KR</collectorName>
154, at 
<quantity box="[419,504,1416,1441]" metricMagnitude="1" metricUnit="m" metricValue="1.3" metricValueMax="2.0" metricValueMin="0.6" pageId="195" pageNumber="196" unit="m" value="13.0" valueMax="20.0" valueMin="6.0">
