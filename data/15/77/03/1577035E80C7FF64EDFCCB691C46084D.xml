<quantity box="[378,463,1452,1477]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="2.4" metricValueMin="0.6" pageId="195" pageNumber="196" unit="m" value="15.0" valueMax="24.0" valueMin="6.0">
<elevation box="[378,463,1452,1477]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="2.4" metricValueMin="0.6" pageId="195" pageNumber="196" unit="m" value="15.0" valueMax="24.0" valueMin="6.0">6–24 m</elevation>
</quantity>
