<quantity box="[447,532,1087,1111]" metricMagnitude="1" metricUnit="m" metricValue="1.65" metricValueMax="3.0" metricValueMin="0.3" pageId="253" pageNumber="254" unit="m" value="16.5" valueMax="30.0" valueMin="3.0">3–30 m</quantity>
