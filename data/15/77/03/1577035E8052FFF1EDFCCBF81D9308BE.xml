<quantity box="[960,1059,1705,1729]" metricMagnitude="1" metricUnit="m" metricValue="6.15" metricValueMax="12.2" metricValueMin="0.1" pageId="86" pageNumber="87" unit="m" value="61.5" valueMax="122.0" valueMin="1.0">1–122 m</quantity>
