<collectingRegion box="[233,277,728,751]" country="Micronesia" name="Yap" pageId="218" pageNumber="219">Yap</collectingRegion>
(
<collectingCountry box="[293,481,727,752]" name="Micronesia" pageId="218" pageNumber="219">Caroline Islands</collectingCountry>
) and 
<collectingCountry box="[547,589,727,752]" name="Fiji" pageId="218" pageNumber="219">Fiji</collectingCountry>
