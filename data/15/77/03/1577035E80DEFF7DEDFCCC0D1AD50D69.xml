<collectingCountry box="[1094,1197,727,752]" name="Australia" pageId="218" pageNumber="219">Australia</collectingCountry>
and 
<collectingCountry box="[1254,1432,727,752]" name="New Caledonia" pageId="218" pageNumber="219">New Caledonia</collectingCountry>
. Found in coastal, lagoon and seaward reefs, 
<quantity box="[648,733,764,788]" metricMagnitude="1" metricUnit="m" metricValue="1.6" metricValueMax="3.2" metricValueMin="0.0" pageId="218" pageNumber="219" unit="m" value="16.0" valueMax="32.0" valueMin="0.0">0–32 m</quantity>
