<quantity box="[1067,1151,151,176]" metricMagnitude="1" metricUnit="m" metricValue="1.25" metricValueMax="2.5" metricValueMin="0.0" pageId="297" pageNumber="298" unit="m" value="12.5" valueMax="25.0" valueMin="0.0">0–25 m</quantity>
