<quantity box="[280,365,1591,1616]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.6" metricValueMin="0.5" pageId="137" pageNumber="138" unit="m" value="10.5" valueMax="16.0" valueMin="5.0">
