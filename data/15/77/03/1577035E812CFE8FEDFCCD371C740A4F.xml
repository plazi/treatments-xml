<collectingCountry box="[989,1092,1014,1039]" name="Australia" pageId="296" pageNumber="297">Australia</collectingCountry>
and 
<collectingCountry box="[1160,1344,1014,1039]" name="New Caledonia" pageId="296" pageNumber="297">New Caledonia</collectingCountry>
