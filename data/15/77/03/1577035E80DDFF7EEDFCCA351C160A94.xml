<quantity box="[1098,1183,1232,1256]" metricMagnitude="0" metricUnit="m" metricValue="5.5" metricValueMax="10.0" metricValueMin="1.0" pageId="217" pageNumber="218" unit="m" value="5.5" valueMax="10.0" valueMin="1.0">1–10 m</quantity>
