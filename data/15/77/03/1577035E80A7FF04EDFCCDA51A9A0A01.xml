<quantity box="[711,796,1124,1149]" metricMagnitude="1" metricUnit="m" metricValue="1.8" metricValueMax="3.5" metricValueMin="0.1" pageId="163" pageNumber="164" unit="m" value="18.0" valueMax="35.0" valueMin="1.0">1–35 m</quantity>
