<subSubSection pageId="50" pageNumber="51" type="distribution">
<paragraph blockId="50.[151,1437,973,1214]" pageId="50" pageNumber="51">
<emphasis bold="true" box="[199,500,1081,1106]" pageId="50" pageNumber="51">Distribution and habitat.</emphasis>
<collectingRegion box="[509,655,1081,1106]" country="Papua New Guinea" name="New Ireland" pageId="50" pageNumber="51">New Ireland</collectingRegion>
