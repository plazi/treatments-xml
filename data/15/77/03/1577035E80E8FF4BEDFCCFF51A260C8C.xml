<taxonomicName authority="(Bleeker 1857)" authorityName="Bleeker" authorityYear="1857" baseAuthorityName="Bleeker" baseAuthorityYear="1857" box="[151,651,476,501]" class="Actinopterygii" family="Labridae" genus="Pseudocheilinus" kingdom="Animalia" order="Perciformes" pageId="236" pageNumber="237" phylum="Chordata" rank="species" species="hexataenia">
