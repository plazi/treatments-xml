<quantity box="[775,860,800,824]" metricMagnitude="1" metricUnit="m" metricValue="1.25" metricValueMax="2.3" metricValueMin="0.2" pageId="216" pageNumber="217" unit="m" value="12.5" valueMax="23.0" valueMin="2.0">2–23 m</quantity>
