<collectingCountry box="[661,812,1879,1904]" name="New Zealand" pageId="237" pageNumber="238">New Zealand</collectingCountry>
and 
<collectingCountry box="[867,940,1881,1905]" name="Tonga" pageId="237" pageNumber="238">Tonga</collectingCountry>
