<quantity box="[735,820,1916,1941]" metricMagnitude="0" metricUnit="m" metricValue="7.5" metricValueMax="15.0" metricValueMin="0.0" pageId="237" pageNumber="238" unit="m" value="7.5" valueMax="15.0" valueMin="0.0">0–15 m</quantity>
