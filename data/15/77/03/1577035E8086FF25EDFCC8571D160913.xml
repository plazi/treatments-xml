<collectingMunicipality box="[151,341,1734,1759]" pageId="130" pageNumber="131">Kavieng District</collectingMunicipality>
, at 
<quantity box="[383,470,1734,1759]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="2.6" metricValueMin="0.4" pageId="130" pageNumber="131" unit="m" value="15.0" valueMax="26.0" valueMin="4.0">
