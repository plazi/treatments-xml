<quantity box="[555,640,1770,1795]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="16.0" metricValueMin="3.0" pageId="130" pageNumber="131" unit="m" value="9.5" valueMax="16.0" valueMin="3.0">
