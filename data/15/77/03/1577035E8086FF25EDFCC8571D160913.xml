<location LSID="urn:lsid:plazi:treatment:1577035E8086FF25EDFCC8571D160913:9801E4938086FF25E854C8EF1983097F" country="Ireland" county="Nago Island" municipality="Kavieng District" name="Anelaua Island" pageId="130" pageNumber="131" stateProvince="New">Anelaua Island</location>
