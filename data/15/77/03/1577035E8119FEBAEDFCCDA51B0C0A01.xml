<quantity box="[555,640,1016,1040]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="3.2" metricValueMin="0.3" pageId="285" pageNumber="286" unit="m" value="17.5" valueMax="32.0" valueMin="3.0">
