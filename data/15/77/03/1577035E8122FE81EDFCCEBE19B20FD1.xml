<collectingMunicipality box="[151,338,223,248]" pageId="294" pageNumber="295">Kavieng District</collectingMunicipality>
at 
<elevation box="[374,459,224,248]" metricMagnitude="0" metricUnit="m" metricValue="6.0" metricValueMax="11.0" metricValueMin="1.0" pageId="294" pageNumber="295" unit="m" value="6.0" valueMax="11.0" valueMin="1.0">
