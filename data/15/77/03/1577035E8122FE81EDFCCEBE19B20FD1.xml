<quantity box="[636,721,260,285]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="294" pageNumber="295" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">
<elevation box="[636,721,260,285]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="294" pageNumber="295" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">5–14 m</elevation>
</quantity>
