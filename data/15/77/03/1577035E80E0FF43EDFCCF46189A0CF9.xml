<quantity box="[213,284,620,644]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="228" pageNumber="229" unit="m" value="1.5" valueMax="3.0" valueMin="0.0">0–3 m</quantity>
