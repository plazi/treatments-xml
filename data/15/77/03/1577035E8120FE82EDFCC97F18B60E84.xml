<location LSID="urn:lsid:plazi:treatment:1577035E8120FE82EDFCC97F18B60E84:9801E4938120FE83EDD9C9EB180409A7" box="[178,345,1986,2011]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="Nubilus Island" pageId="292" pageNumber="293" stateProvince="New">Nubilus Island</location>
