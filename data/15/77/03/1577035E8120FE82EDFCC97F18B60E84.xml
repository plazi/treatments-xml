<collectingMunicipality box="[1231,1352,1914,1939]" pageId="292" pageNumber="293">Jeff Kinch</collectingMunicipality>
during ACIAR 
<location LSID="urn:lsid:plazi:treatment:1577035E8120FE82EDFCC97F18B60E84:9801E4938120FE83ED9EC9B718E009CB" box="[245,445,1950,1975]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="Aquarium Survey" pageId="292" pageNumber="293" stateProvince="New">Aquarium Survey</location>
