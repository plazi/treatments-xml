<location LSID="urn:lsid:plazi:treatment:1577035E8120FE82EDFCC97F18B60E84:9801E4938120FE83ECE0C9EA1BDD09A6" box="[395,640,1987,2010]" country="Ireland" county="Kavieng Lagoon" municipality="Jeff Kinch" name="eastern New Hanover" pageId="292" pageNumber="293" stateProvince="New">eastern New Hanover</location>
, at 
<quantity box="[678,763,1987,2011]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="3.2" metricValueMin="0.3" pageId="292" pageNumber="293" unit="m" value="17.5" valueMax="32.0" valueMin="3.0">
<elevation box="[678,763,1987,2011]" metricMagnitude="1" metricUnit="m" metricValue="1.75" metricValueMax="3.2" metricValueMin="0.3" pageId="292" pageNumber="293" unit="m" value="17.5" valueMax="32.0" valueMin="3.0">3–32 m</elevation>
</quantity>
