<quantity box="[522,607,1489,1513]" metricMagnitude="0" metricUnit="m" metricValue="6.0" metricValueMax="10.0" metricValueMin="2.0" pageId="279" pageNumber="280" unit="m" value="6.0" valueMax="10.0" valueMin="2.0">2–10 m</quantity>
