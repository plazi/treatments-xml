<quantity box="[473,558,1382,1406]" metricMagnitude="1" metricUnit="m" metricValue="2.4" metricValueMax="4.1" metricValueMin="0.7" pageId="235" pageNumber="236" unit="m" value="24.0" valueMax="41.0" valueMin="7.0">7–41 m</quantity>
