<collectingCountry box="[663,730,1511,1534]" name="Japan" pageId="109" pageNumber="110">Japan</collectingCountry>
, south to northwestern 
<collectingCountry box="[1003,1108,1510,1535]" name="Australia" pageId="109" pageNumber="110">Australia</collectingCountry>
, 
<collectingRegion box="[1122,1254,1510,1535]" country="Australia" name="Queensland" pageId="109" pageNumber="110">Queensland</collectingRegion>
(
<collectingCountry box="[1272,1378,1510,1535]" name="Australia" pageId="109" pageNumber="110">Australia</collectingCountry>
