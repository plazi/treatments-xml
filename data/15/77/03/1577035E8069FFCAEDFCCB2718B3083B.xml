<collectingCountry box="[151,339,1510,1535]" name="Marshall Islands" pageId="109" pageNumber="110">Marshall Islands</collectingCountry>
and 
<collectingCountry box="[400,442,1510,1535]" name="Fiji" pageId="109" pageNumber="110">Fiji</collectingCountry>
