<subSubSection pageId="277" pageNumber="278" type="distribution">
<paragraph blockId="277.[151,1436,151,357]" pageId="277" pageNumber="278">
<emphasis bold="true" box="[199,500,260,285]" pageId="277" pageNumber="278">Distribution and habitat.</emphasis>
<collectingRegion box="[508,654,260,285]" country="Papua New Guinea" name="New Ireland" pageId="277" pageNumber="278">New Ireland</collectingRegion>
