<quantity box="[636,721,908,933]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="233" pageNumber="234" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">
<elevation box="[636,721,908,933]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="233" pageNumber="234" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">5–14 m</elevation>
</quantity>
