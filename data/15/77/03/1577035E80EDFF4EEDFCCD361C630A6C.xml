<collectingCounty box="[1290,1432,836,861]" pageId="233" pageNumber="234">Nago Island</collectingCounty>
, 
<collectingMunicipality box="[151,341,871,896]" pageId="233" pageNumber="234">Kavieng District</collectingMunicipality>
, at 
<quantity box="[382,454,872,896]" metricMagnitude="0" metricUnit="m" metricValue="5.5" metricValueMax="9.0" metricValueMin="2.0" pageId="233" pageNumber="234" unit="m" value="5.5" valueMax="9.0" valueMin="2.0">
