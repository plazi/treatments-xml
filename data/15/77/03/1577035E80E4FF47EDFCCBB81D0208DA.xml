<collectingRegion box="[269,473,1641,1666]" country="Australia" name="New South Wales" pageId="224" pageNumber="225">New South Wales</collectingRegion>
(
<collectingCountry box="[490,599,1641,1666]" name="Australia" pageId="224" pageNumber="225">Australia</collectingCountry>
