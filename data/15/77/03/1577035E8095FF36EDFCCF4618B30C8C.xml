<subSubSection box="[151,868,367,392]" pageId="145" pageNumber="146" type="nomenclature">
<paragraph blockId="145.[151,1437,367,752]" box="[151,868,367,392]" pageId="145" pageNumber="146">
<heading bold="true" box="[151,868,367,392]" fontSize="10" level="2" pageId="145" pageNumber="146" reason="2">
<emphasis bold="true" box="[151,868,367,392]" pageId="145" pageNumber="146">
<taxonomicName authority="(Forsskal in Niebuhr 1775)" authorityName="Forsskal in Niebuhr" authorityYear="1775" baseAuthorityName="Forsskal in Niebuhr" baseAuthorityYear="1775" box="[151,671,367,392]" class="Actinopterygii" family="Carangidae" genus="Caranx" kingdom="Animalia" order="Perciformes" pageId="145" pageNumber="146" phylum="Chordata" rank="species" species="ignobilis">
