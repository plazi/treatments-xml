<quantity box="[461,558,1628,1653]" metricMagnitude="1" metricUnit="m" metricValue="3.05" metricValueMax="5.0" metricValueMin="1.1" pageId="134" pageNumber="135" unit="m" value="30.5" valueMax="50.0" valueMin="11.0">11–50 m</quantity>
