<collectingCountry box="[272,334,1591,1616]" name="Palau" pageId="134" pageNumber="135">Palau</collectingCountry>
and 
<collectingCountry box="[393,586,1591,1616]" name="Solomon Islands" pageId="134" pageNumber="135">Solomon Islands</collectingCountry>
