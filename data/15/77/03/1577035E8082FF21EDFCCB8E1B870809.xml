<collectingCountry box="[995,1104,1556,1581]" name="Indonesia" pageId="134" pageNumber="135">Indonesia</collectingCountry>
to 
<collectingCountry box="[1153,1282,1556,1581]" name="Philippines" pageId="134" pageNumber="135">Philippines</collectingCountry>
, 
<collectingCountry name="Papua New Guinea" pageId="134" pageNumber="135">Papua New Guinea</collectingCountry>
