as 
<taxonomicName baseAuthorityName="non Bleeker" baseAuthorityYear="1854" box="[924,1130,1158,1183]" class="Actinopterygii" family="Eleotridae" genus="Ophiocara" kingdom="Animalia" order="Perciformes" pageId="267" pageNumber="268" phylum="Chordata" rank="species" species="aporos">
