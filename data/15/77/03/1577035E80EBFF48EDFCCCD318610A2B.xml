<collectingCountry box="[151,329,1014,1039]" name="Kiribati" pageId="239" pageNumber="240">Phoenix islands</collectingCountry>
and 
<collectingCountry box="[386,465,1015,1039]" name="Samoa" pageId="239" pageNumber="240">Samoa</collectingCountry>
