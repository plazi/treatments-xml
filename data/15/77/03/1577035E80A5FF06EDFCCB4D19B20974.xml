on 
<date box="[851,1089,1452,1477]" pageId="161" pageNumber="162" value="2014-06-07" valueMax="2014-06-21" valueMin="2014-06-07">
<collectingDate box="[851,1089,1452,1477]" pageId="161" pageNumber="162" value="2014-06-07" valueMax="2014-06-21" valueMin="2014-06-07">7 and 21 June 2014</collectingDate>
</date>
