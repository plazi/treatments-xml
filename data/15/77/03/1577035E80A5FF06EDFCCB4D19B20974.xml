<location LSID="urn:lsid:plazi:treatment:1577035E80A5FF06EDFCCB4D19B20974:9801E49380A5FF06E826CBF918680870" country="Ireland" county="Kavieng" municipality="Jeff Kinch" name="eastern New Hanover" pageId="161" pageNumber="162" stateProvince="New">eastern New Hanover</location>
