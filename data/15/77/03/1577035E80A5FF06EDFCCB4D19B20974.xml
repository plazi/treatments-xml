<quantity box="[320,405,1524,1549]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.6" metricValueMin="0.5" pageId="161" pageNumber="162" unit="m" value="10.5" valueMax="16.0" valueMin="5.0">
<elevation box="[320,405,1524,1549]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.6" metricValueMin="0.5" pageId="161" pageNumber="162" unit="m" value="10.5" valueMax="16.0" valueMin="5.0">5–16 m</elevation>
