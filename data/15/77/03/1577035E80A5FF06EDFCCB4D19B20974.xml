<location LSID="urn:lsid:plazi:treatment:1577035E80A5FF06EDFCCB4D19B20974:9801E49380A5FF06EDFCCBE6180B0B94" box="[151,342,1487,1512]" country="Ireland" county="Kavieng" municipality="Jeff Kinch" name="Lissenung Island" pageId="161" pageNumber="162" stateProvince="New">Lissenung Island</location>
by 
<location LSID="urn:lsid:plazi:treatment:1577035E80A5FF06EDFCCB4D19B20974:9801E49380A5FF06ECEACBE61B760B94" box="[385,555,1487,1512]" country="Ireland" county="Kavieng" municipality="Jeff Kinch" name="Dietmar Amon" pageId="161" pageNumber="162" stateProvince="New">Dietmar Amon</location>
on 
<date box="[598,747,1487,1512]" pageId="161" pageNumber="162" value="2010-07-24">
