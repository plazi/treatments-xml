<paragraph blockId="115.[151,1436,151,285]" pageId="115" pageNumber="116">
<emphasis bold="true" box="[199,504,151,176]" pageId="115" pageNumber="116">Distribution and habitat.</emphasis>
<collectingRegion box="[514,662,151,176]" country="Papua New Guinea" name="New Ireland" pageId="115" pageNumber="116">New Ireland</collectingRegion>
