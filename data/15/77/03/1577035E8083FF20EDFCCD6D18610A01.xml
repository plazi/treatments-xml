<quantity box="[1351,1436,1087,1112]" metricMagnitude="1" metricUnit="m" metricValue="2.55" metricValueMax="5.0" metricValueMin="0.1" pageId="135" pageNumber="136" unit="m" value="25.5" valueMax="50.0" valueMin="1.0">1–50 m</quantity>
