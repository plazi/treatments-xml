<collectingCountry box="[1223,1299,296,320]" name="Samoa" pageId="204" pageNumber="205">Samoa</collectingCountry>
and 
<collectingCountry box="[1359,1431,296,320]" name="Tonga" pageId="204" pageNumber="205">Tonga</collectingCountry>
, north to southern 
<collectingCountry box="[359,426,333,356]" name="Japan" pageId="204" pageNumber="205">Japan</collectingCountry>
, south to 
<collectingRegion box="[543,749,332,357]" country="Australia" name="Western Australia" pageId="204" pageNumber="205">Western Australia</collectingRegion>
, 
<collectingRegion box="[764,971,332,357]" country="Australia" name="New South Wales" pageId="204" pageNumber="205">New South Wales</collectingRegion>
(
<collectingCountry box="[989,1098,332,357]" name="Australia" pageId="204" pageNumber="205">Australia</collectingCountry>
), and northern 
<collectingCountry box="[1276,1433,332,357]" name="New Zealand" pageId="204" pageNumber="205">New Zealand</collectingCountry>
. Found on inshore rocky reefs and lagoons and outer reef slopes, 
<quantity box="[874,959,367,392]" metricMagnitude="0" metricUnit="m" metricValue="7.5" metricValueMax="15.0" metricValueMin="0.0" pageId="204" pageNumber="205" unit="m" value="7.5" valueMax="15.0" valueMin="0.0">0–15 m</quantity>
