<subSubSection pageId="204" pageNumber="205" type="distribution">
<paragraph blockId="204.[151,1437,151,392]" pageId="204" pageNumber="205">
<emphasis bold="true" box="[199,503,295,320]" pageId="204" pageNumber="205">Distribution and habitat.</emphasis>
<collectingRegion box="[513,659,295,320]" country="Papua New Guinea" name="New Ireland" pageId="204" pageNumber="205">New Ireland</collectingRegion>
