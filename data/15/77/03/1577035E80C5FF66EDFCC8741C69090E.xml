<location LSID="urn:lsid:plazi:treatment:1577035E80C5FF66EDFCC8741C69090E:9801E49380C5FF66EC7BC8C71B580979" box="[272,517,1774,1797]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="Nubilus Island" name="eastern New Hanover" pageId="193" pageNumber="194" stateProvince="New">eastern New Hanover</location>
, 
<quantity box="[528,613,1773,1798]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="193" pageNumber="194" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">
<elevation box="[528,613,1773,1798]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="193" pageNumber="194" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">3–25 m</elevation>
</quantity>
