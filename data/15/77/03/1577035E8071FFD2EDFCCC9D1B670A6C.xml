<quantity box="[1334,1436,980,1005]" metricMagnitude="1" metricUnit="m" metricValue="7.75" metricValueMax="15.0" metricValueMin="0.5" pageId="117" pageNumber="118" unit="m" value="77.5" valueMax="150.0" valueMin="5.0">5–150 m</quantity>
