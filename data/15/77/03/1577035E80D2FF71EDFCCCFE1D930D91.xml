<subSubSection pageId="214" pageNumber="215" type="distribution">
<paragraph blockId="214.[151,1436,727,1005]" pageId="214" pageNumber="215">
<emphasis bold="true" box="[199,495,871,896]" pageId="214" pageNumber="215">Distribution and habitat.</emphasis>
<collectingRegion box="[502,645,871,896]" country="Papua New Guinea" name="New Ireland" pageId="214" pageNumber="215">New Ireland</collectingRegion>
