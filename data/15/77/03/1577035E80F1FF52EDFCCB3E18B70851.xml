<quantity box="[616,701,1412,1437]" metricMagnitude="1" metricUnit="m" metricValue="2.45" metricValueMax="4.6" metricValueMin="0.3" pageId="245" pageNumber="246" unit="m" value="24.5" valueMax="46.0" valueMin="3.0">
