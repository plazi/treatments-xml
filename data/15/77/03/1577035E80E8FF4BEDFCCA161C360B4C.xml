<collectorName box="[676,867,1159,1184]" pageId="236" pageNumber="237">Barry C. Russell</collectorName>
); another taken off 
<location LSID="urn:lsid:plazi:treatment:1577035E80E8FF4BEDFCCA161C360B4C:9801E49380E8FF4BE956CAAE1C1C0AE3" box="[1085,1345,1159,1184]" country="Ireland" county="Nusa Island" municipality="Kavieng District" name="southern New Hanover" pageId="236" pageNumber="237" stateProvince="New">southern New Hanover</location>
