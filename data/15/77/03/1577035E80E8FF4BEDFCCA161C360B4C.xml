<quantity metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="4.7" metricValueMin="0.3" pageId="236" pageNumber="237" unit="m" value="25.0" valueMax="47.0" valueMin="3.0">
<elevation metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="4.7" metricValueMin="0.3" pageId="236" pageNumber="237" unit="m" value="25.0" valueMax="47.0" valueMin="3.0">3–47 m</elevation>
