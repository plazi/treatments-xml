<heading bold="true" box="[151,1067,1124,1149]" fontSize="10" level="2" pageId="145" pageNumber="146" reason="2">
<emphasis bold="true" box="[151,1067,1124,1149]" pageId="145" pageNumber="146">
<taxonomicName authority="Cuvier in Cuvier &amp; Valenciennes 1833" authorityName="Cuvier in Cuvier &amp; Valenciennes" authorityYear="1833" box="[151,852,1124,1149]" class="Actinopterygii" family="Carangidae" genus="Caranx" kingdom="Animalia" order="Perciformes" pageId="145" pageNumber="146" phylum="Chordata" rank="species" species="melampygus">
