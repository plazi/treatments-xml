<collectingCountry box="[1161,1240,548,572]" name="Samoa" pageId="131" pageNumber="132">Samoa</collectingCountry>
, north to 
<collectingCountry box="[1347,1432,548,573]" name="Taiwan" pageId="131" pageNumber="132">Taiwan</collectingCountry>
