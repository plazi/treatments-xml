, 
<quantity box="[528,613,1561,1586]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="136" pageNumber="137" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">
<elevation box="[528,613,1561,1586]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="136" pageNumber="137" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">3–25 m</elevation>
