<location LSID="urn:lsid:plazi:treatment:1577035E812AFE89EDFCCC4518CA0DD9:9801E493812AFE89EDFCCCD518180D69" box="[151,325,764,789]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="northwestern Bauddisson Island" name="Tungelo Island" pageId="302" pageNumber="303" stateProvince="New">Tungelo Island</location>
