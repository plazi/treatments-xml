<location LSID="urn:lsid:plazi:treatment:1577035E812AFE89EDFCCC4518CA0DD9:9801E493812AFE89EF12CCF11A1E0C8C" box="[633,835,727,752]" country="Ireland" county="Aquarium Survey in Kavieng District" municipality="northwestern Bauddisson Island" name="Serge Andrefouet" pageId="302" pageNumber="303" stateProvince="New">Serge Andréfouët</location>
