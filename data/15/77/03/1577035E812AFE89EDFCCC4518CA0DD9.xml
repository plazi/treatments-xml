<quantity box="[746,835,764,789]" metricMagnitude="1" metricUnit="m" metricValue="2.45" metricValueMax="4.6" metricValueMin="0.3" pageId="302" pageNumber="303" unit="m" value="24.5" valueMax="46.0" valueMin="3.0">
