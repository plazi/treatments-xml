, 
<quantity box="[260,345,1735,1760]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.5" metricValueMin="0.3" pageId="218" pageNumber="219" unit="m" value="14.0" valueMax="25.0" valueMin="3.0">
