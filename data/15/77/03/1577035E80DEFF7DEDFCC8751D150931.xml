<quantity box="[151,236,1700,1725]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="2.3" metricValueMin="0.5" pageId="218" pageNumber="219" unit="m" value="14.0" valueMax="23.0" valueMin="5.0">
