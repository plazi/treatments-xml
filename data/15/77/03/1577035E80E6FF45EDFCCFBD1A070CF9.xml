<quantity box="[281,366,511,536]" metricMagnitude="0" metricUnit="m" metricValue="8.5" metricValueMax="12.0" metricValueMin="5.0" pageId="226" pageNumber="227" unit="m" value="8.5" valueMax="12.0" valueMin="5.0">
<elevation box="[281,366,511,536]" metricMagnitude="0" metricUnit="m" metricValue="8.5" metricValueMax="12.0" metricValueMin="5.0" pageId="226" pageNumber="227" unit="m" value="8.5" valueMax="12.0" valueMin="5.0">5–12 m</elevation>
</quantity>
