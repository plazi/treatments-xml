<quantity box="[348,433,368,392]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="2.0" metricValueMin="0.1" pageId="286" pageNumber="287" unit="m" value="10.5" valueMax="20.0" valueMin="1.0">1–20 m</quantity>
