<quantity box="[151,278,654,679]" metricMagnitude="2" metricUnit="m" metricValue="3.09" metricValueMax="4.6899999999999995" metricValueMin="1.49" pageId="126" pageNumber="127" unit="m" value="309.0" valueMax="469.0" valueMin="149.0">149–469 m</quantity>
