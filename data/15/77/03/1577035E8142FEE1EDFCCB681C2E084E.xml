<quantity box="[1138,1224,1562,1586]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.8" metricValueMin="0.3" pageId="326" pageNumber="327" unit="m" value="10.5" valueMax="18.0" valueMin="3.0">3–18 m</quantity>
