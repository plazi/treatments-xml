<collectingCountry box="[1309,1432,1489,1514]" name="Seychelles" pageId="326" pageNumber="327">Seychelles</collectingCountry>
, 
<collectingCountry box="[151,286,1525,1550]" name="Madagascar" pageId="326" pageNumber="327">Madagascar</collectingCountry>
