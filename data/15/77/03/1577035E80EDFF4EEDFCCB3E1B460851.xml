<quantity box="[282,367,1556,1580]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="233" pageNumber="234" unit="m" value="15.0" valueMax="30.0" valueMin="0.0">0–30 m</quantity>
