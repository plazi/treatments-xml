<quantity box="[252,337,1807,1832]" metricMagnitude="1" metricUnit="m" metricValue="2.55" metricValueMax="5.0" metricValueMin="0.1" pageId="163" pageNumber="164" unit="m" value="25.5" valueMax="50.0" valueMin="1.0">1–50 m</quantity>
