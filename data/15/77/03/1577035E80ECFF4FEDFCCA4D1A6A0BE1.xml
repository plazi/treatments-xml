<quantity box="[566,651,1412,1436]" metricMagnitude="1" metricUnit="m" metricValue="2.05" metricValueMax="4.0" metricValueMin="0.1" pageId="232" pageNumber="233" unit="m" value="20.5" valueMax="40.0" valueMin="1.0">1–40 m</quantity>
