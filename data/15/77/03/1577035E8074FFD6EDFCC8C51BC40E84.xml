<quantity box="[408,493,223,248]" metricMagnitude="1" metricUnit="m" metricValue="2.6" metricValueMax="5.0" metricValueMin="0.2" pageId="113" pageNumber="114" unit="m" value="26.0" valueMax="50.0" valueMin="2.0">2–50 m</quantity>
