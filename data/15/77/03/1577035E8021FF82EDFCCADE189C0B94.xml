<quantity box="[151,278,1487,1512]" metricMagnitude="2" metricUnit="m" metricValue="4.279999999999999" metricValueMax="6.16" metricValueMin="2.4" pageId="37" pageNumber="38" unit="m" value="428.0" valueMax="616.0" valueMin="240.0">240–616 m</quantity>
