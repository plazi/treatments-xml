<quantity box="[636,721,1159,1184]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="14.0" metricValueMin="5.0" pageId="227" pageNumber="228" unit="m" value="9.5" valueMax="14.0" valueMin="5.0">
