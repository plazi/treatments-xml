<treatmentCitationGroup id="AB52119A094AE172DBEEFB11FE53FAB5" pageId="5" pageNumber="100">
<taxonomicName id="4C424D37094AE172DBEEFB11FDDFFB4F" authority=": d'Orchymont 1926: 208" authorityName="d'Orchymont" authorityPageNumber="208" authorityYear="1926" box="[148,633,1244,1265]" class="Insecta" family="Hydrophilidae" genus="Armostus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="100" phylum="Arthropoda" rank="species" species="crenulatus">
