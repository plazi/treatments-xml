<td id="5C4A5DC7FFA0000B8DCFFA28FDFCA6A6" box="[484,527,1415,1432]" gridcol="2" gridrow="1" pageId="68" pageNumber="69">1098</td>
<td id="5C4A5DC7FFA0000B8E63FA28FD80A6A6" box="[584,627,1415,1432]" gridcol="3" gridrow="1" pageId="68" pageNumber="69">1097</td>
