<emphasis id="B971EABFFF90FFF461DEC2A1FD9C73DB" bold="true" box="[151,591,917,943]" pageId="11" pageNumber="12">
<typeStatus id="54BE880FFF90FFF461DEC2A1FF0573DB" box="[151,214,917,943]" pageId="11" pageNumber="12">Type</typeStatus>
material. 
<typeStatus id="54BE880FFF90FFF46015C2A1FE0573DB" box="[348,470,917,943]" pageId="11" pageNumber="12">Holotype</typeStatus>
: 
<collectingCountry id="F312763DFF90FFF460ACC2A1FD9973DB" box="[485,586,917,943]" name="Japan" pageId="11" pageNumber="12">JAPAN</collectingCountry>
:
