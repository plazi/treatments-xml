<heading id="D09581FCFFA3FF9576D8266DFB56C20A" bold="true" box="[949,1262,1338,1363]" centered="true" fontSize="10" level="2" pageId="1" pageNumber="2" reason="2">
<taxonomicName id="4C624D13FFA3FF9576D8266DFB56C20A" authority="Kirkaldy, 1913" authorityName="Kirkaldy" authorityYear="1913" box="[949,1262,1338,1363]" class="Insecta" family="Dictyopharidae" genus="Cuernavaca" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis id="B916EA82FFA3FF9576D8266DFB56C20A" bold="true" box="[949,1262,1338,1363]" pageId="1" pageNumber="2">
<emphasis id="B916EA82FFA3FF9576D8266DFB86C20A" bold="true" box="[949,1086,1338,1363]" italics="true" pageId="1" pageNumber="2">Cuernavaca</emphasis>
<bibRefCitation id="EFF34B61FFA3FF957129266DFB56C20A" author="Kirkaldy, G. W." box="[1092,1262,1338,1363]" pageId="1" pageNumber="2" pagination="7 - 27" refId="ref7401" refString="Kirkaldy, G. W. 1913. On some new species of leafhoppers. Part 1. Bulletin Hawaiian Sugar Planter's Association Experiment Station. Division of Entomology, 12: 7 - 27." type="journal article" year="1913">Kirkaldy, 1913</bibRefCitation>
