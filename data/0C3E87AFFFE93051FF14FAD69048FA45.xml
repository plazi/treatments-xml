<taxonomicName id="43974D3AFFE93051FAB5F9139633FBB8" box="[1334,1387,1751,1774]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="genus">
