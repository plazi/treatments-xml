<taxonomicName id="43974D3AFFE93051FF1DF93F924EFA45" authorityName="" authorityYear="1789" box="[158,278,1787,1811]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
