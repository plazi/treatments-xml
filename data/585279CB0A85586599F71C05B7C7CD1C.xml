<taxonomicName id="08C903AE4C8BE43EFB257B391B73A0BB" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1855" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="125" pageNumber="126" phylum="Mollusca" rank="species" species="convexus" subGenus="Drymaeus">
