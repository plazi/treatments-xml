<geoCoordinate id="71583FD246CAA5B35C67EDFDC9A225D5" degrees="24" direction="north" minutes="23" orientation="latitude" precision="925" value="24.383333">24°23'N</geoCoordinate>
, 
<geoCoordinate id="CAC9C0BFAB599D23EF5B3CC0CEF85D03" degrees="100" direction="east" minutes="47" orientation="longitude" precision="925" value="100.78333">100°47'E</geoCoordinate>
