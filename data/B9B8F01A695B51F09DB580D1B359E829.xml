<taxonomicName id="2EEEBBC28FECED0BC52A88554A792317" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Castanopsis" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
