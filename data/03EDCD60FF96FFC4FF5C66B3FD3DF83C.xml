<taxonomicName id="4C4407F5FF97FFC7FBB56498FB10FB60" box="[1150,1246,1247,1269]" pageId="9" pageNumber="284" rank="species" species="gobiensis">gobiensis</taxonomicName>
</emphasis>
</taxonomicName>
(28–29, ♀) and 
<taxonomicName id="4C4407F5FF97FFC7FA4D64A6FF1DFA81" class="Arachnida" family="Thomisidae" genus="Xysticus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="284" phylum="Arthropoda" rank="species" species="courti" status="sp. n.">
