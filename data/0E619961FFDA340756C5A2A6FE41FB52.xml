<collectingCountry id="FEDF68E7FFDB34075770A208FE0CFB52" box="[352,419,1148,1173]" name="Egypt" pageId="13" pageNumber="2501">Egypt</collectingCountry>
, 
<collectingCountry id="FEDF68E7FFDB340757BEA208FE44FB52" box="[430,491,1148,1173]" name="Israel" pageId="13" pageNumber="2501">Israel</collectingCountry>
