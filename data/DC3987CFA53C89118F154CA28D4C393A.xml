<taxonomicName id="93904D5AA53C89118FAC4E9B8C6236DA" baseAuthorityName="WARNCKE" baseAuthorityYear="1992" box="[988,1134,1961,1985]" class="Insecta" family="Colletidae" genus="Hylaeus" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="307" phylum="Arthropoda" rank="species" species="monedulus">
<emphasis id="66E4EACBA53C89118FAC4E9B8C6236DA" box="[988,1134,1961,1985]" italics="true" pageId="34" pageNumber="307">H. monedulus</emphasis>
</taxonomicName>
was described from 
<collectingRegion id="9654F83BA53C891189164E988D9036DA" box="[1382,1436,1961,1985]" country="Turkey" name="Bitlis" pageId="34" pageNumber="307">Bitlis</collectingRegion>
(Nemrut Mt, 
<quantity id="93689B3CA53C89118FA94EF88C2636FA" box="[985,1066,1993,2017]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="34" pageNumber="307" unit="m" value="2000.0">2000 m</quantity>
) as indigenous in 
