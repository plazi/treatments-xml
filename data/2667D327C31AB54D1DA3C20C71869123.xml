<geoCoordinate id="D5B7880A12DC23035E7325D17628F54D" direction="north" orientation="latitude" precision="925" value="10.433333">10°26'N</geoCoordinate>
, 
<geoCoordinate id="751C14BC9D43839468798BE937D1F3C4" direction="west" orientation="longitude" precision="925" value="-84.01667">84°01'W</geoCoordinate>
, 100m [;] Canopy fogging 19 [;] 
<pageBreakToken id="6B4803377DBD8550F424679E029EA8CC" pageId="427" pageNumber="428" start="start">8</pageBreakToken>
