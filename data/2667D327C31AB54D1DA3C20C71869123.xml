<taxonomicName id="03EAFE7AAA2CAC27BFC37CDA2C46872D" LSID="http://zoobank.org/2F0B383C-CE09-4E48-A216-48F1666F8ED3" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus washingtoni" order="Hymenoptera" pageId="426" pageNumber="427" phylum="Arthropoda" rank="species" species="washingtoni">
<pageBreakToken id="F7B07BDD83794355EC081C52CAA1E8AA" pageId="426" pageNumber="427" start="start">Heterospilus</pageBreakToken>
washingtoni Marsh
</taxonomicName>
<taxonomicNameLabel id="6C5DCF0B7A034A942AEC5EC5E70935B2" pageId="426" pageNumber="427">sp. n.</taxonomicNameLabel>
Figures 296, 297
</paragraph>
