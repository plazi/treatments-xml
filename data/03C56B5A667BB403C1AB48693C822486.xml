<subSubSection id="C37689C7667BB403C1FB4E063C822486" pageId="46" pageNumber="47" type="biology_ecology">
<paragraph id="8BD3DA4C667BB403C1FB4E063C822486" blockId="46.[151,1437,930,1495]" pageId="46" pageNumber="47">
<emphasis id="B918065E667BB403C1FB4E06393D24DE" bold="true" box="[199,295,1398,1423]" pageId="46" pageNumber="47">Biology.</emphasis>
