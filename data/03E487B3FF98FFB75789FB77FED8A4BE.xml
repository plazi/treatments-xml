<taxonomicName id="4C4D4D26FF9DFFB756CCFEE4FE86A4FE" box="[252,373,362,386]" class="Chilopoda" family="Scolopendridae" genus="Rhysida" kingdom="Animalia" order="Scolopendromorpha" pageId="14" pageNumber="91" phylum="Arthropoda" rank="species" species="longipes">
