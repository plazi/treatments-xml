<taxonomicName id="4C4D4D26FF9EFFB45474F9E8FD56A302" authorityName="Lewis" authorityYear="2002" box="[580,677,1638,1662]" class="Chilopoda" family="Scolopendridae" genus="Rhysida" kingdom="Animalia" order="Scolopendromorpha" pageId="13" pageNumber="90" phylum="Arthropoda" rank="species" species="jonesi">
<emphasis id="B939EAB7FF9EFFB45474F9E8FDA4A302" box="[580,599,1639,1662]" italics="true" pageId="13" pageNumber="90">R</emphasis>
. 
<emphasis id="B939EAB7FF9EFFB45457F9E8FD56A302" box="[615,677,1639,1662]" italics="true" pageId="13" pageNumber="90">jonesi</emphasis>
</taxonomicName>
. 
