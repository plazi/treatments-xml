<quantity id="4CB59B40FF9EFFB45585FA1EFBF5A0DA" box="[949,1030,1424,1447]" metricMagnitude="-2" metricUnit="m" metricValue="5.1" pageId="13" pageNumber="90" unit="mm" value="51.0">51 mm</quantity>
