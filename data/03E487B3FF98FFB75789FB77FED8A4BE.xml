<taxonomicName id="4C4D4D26FF9EFFB45538FA69FE55A362" authority="Khanna and Tripathi, 1986" authorityName="Khanna and Tripathi" authorityYear="1986" class="Chilopoda" family="Scolopendridae" genus="Rhysida" kingdom="Animalia" order="Scolopendromorpha" pageId="13" pageNumber="90" phylum="Arthropoda" rank="species" species="longicarinulata">
