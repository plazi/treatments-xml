<taxonomicName id="2C9B4D27FF89B40862707810FB01FA9A" box="[1053,1251,1373,1397]" class="Magnoliopsida" family="Lamiaceae" genus="Gmelina" kingdom="Plantae" order="Lamiales" pageId="1" pageNumber="160" phylum="Tracheophyta" rank="species" species="arborea">
