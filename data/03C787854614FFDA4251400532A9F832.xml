<paragraph id="8BD136934614FFDA40BF40E033FFFC02" blockId="24.[189,711,961,987]" box="[189,711,961,987]" pageId="24" pageNumber="25">
<taxonomicName id="4C6E4D104614FFDA40BF40E03337FC02" authority="Herbert, 2020" authorityName="Herbert" authorityYear="2020" box="[189,527,961,987]" class="Gastropoda" family="Charopidae" genus="Biomphalodonta" kingdom="Animalia" order="Stylommatophora" pageId="24" pageNumber="25" phylum="Mollusca" rank="species" species="forticostata" status="gen. et sp. nov.">
