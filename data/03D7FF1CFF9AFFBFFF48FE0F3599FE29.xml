<taxonomicName id="4C7E3589FF9AFFBCFC11FEA134DCFA16" authority="(Harold, 1867)" baseAuthorityName="Harold" baseAuthorityYear="1867" box="[921,1440,1486,1510]" class="Insecta" family="Scarabaeidae" genus="Malagoniella" kingdom="Animalia" order="Coleoptera" pageId="81" pageNumber="80" phylum="Arthropoda" rank="subSpecies" species="astyanax" subGenus="Malagoniella" subSpecies="columbica">
