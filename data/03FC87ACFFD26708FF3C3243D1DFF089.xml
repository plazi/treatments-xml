<taxonomicName id="4C554D39FFD16709FDC436A9D1DBF6B1" box="[623,746,1959,1983]" class="Insecta" family="Anthicidae" genus="Lemodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="albertisi">
