<geoCoordinate id="3B6DB1ADB6BAA63EF8683CFC5F7361C5" degrees="22" direction="north" minutes="45" orientation="latitude" precision="15" seconds="18" value="22.755">22°45'18&quot;N</geoCoordinate>
