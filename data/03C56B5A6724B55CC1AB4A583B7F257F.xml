<taxonomicName id="4C6CA1CF6724B55CC56148823CC5255B" baseAuthorityName="Ter-Minasian" baseAuthorityYear="1967" box="[1117,1247,1010,1034]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="369" pageNumber="370" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
