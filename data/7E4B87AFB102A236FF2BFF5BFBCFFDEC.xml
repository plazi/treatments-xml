<materialsCitation id="468A3CE4B102A236FF7BFE19FBCFFDEC" ID-GBIF-Occurrence="1500194404" country="Vietnam" location="Java" pageId="52" pageNumber="53" specimenCount="1" stateProvince="Java">
<emphasis id="C496EAABB102A236FF7BFE19FEA2FE34" bold="true" box="[199,308,469,494]" pageId="52" pageNumber="53">Remarks</emphasis>
