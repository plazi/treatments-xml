<collectingCountry id="BAE4761DFF3668CFFBEFFBE4CB6BFBAF" box="[1048,1148,1036,1062]" name="United Kingdom" pageId="176" pageNumber="177">Orcadas</collectingCountry>
</emphasis>
, cruise 575, Stn 54, 
<geoCoordinate id="A7C7504AFF3668CFFF4AFBD8CE35FBC2" box="[189,290,1072,1100]" degrees="57" direction="south" minutes="39" orientation="latitude" pageId="176" pageNumber="177" precision="925" value="-57.65">57°39ʹ S</geoCoordinate>
, 
<geoCoordinate id="A7C7504AFF3668CFFEDAFBD8CEA6FBC5" box="[301,433,1072,1100]" degrees="26" direction="west" minutes="00.4" orientation="longitude" pageId="176" pageNumber="177" precision="92" value="-26.006666">26°00.4ʹ W</geoCoordinate>
[
<collectingCountry id="BAE4761DFF3668CFFE4AFBD9CDC9FBC5" box="[445,734,1073,1100]" name="South Georgia and the South Sandwich Islands" pageId="176" pageNumber="177">South Sandwich Islands</collectingCountry>
