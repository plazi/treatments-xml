<taxonomicName id="4C29D290127FFF94FEC123FDFF3A87E2" authority="Switchenska, 1968" authorityName="Switchenska" authorityYear="1968" box="[313,812,472,497]" class="Actinopterygii" family="Perciformes (awaiting allocation)" genus="Parasphyraena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="5" pageNumber="402" phylum="Chordata" rank="species" species="apscheronica">
