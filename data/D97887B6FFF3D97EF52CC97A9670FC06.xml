<taxonomicName id="96D14D23FFF3D97FF7C6CBC89752FB05" authority="(Denis, 1945)" baseAuthorityName="Denis" baseAuthorityYear="1945" box="[955,1325,1217,1239]" class="Arachnida" family="Linyphiidae" genus="Pecado" kingdom="Animalia" order="Araneae" pageId="38" pageNumber="397" phylum="Arthropoda" rank="species" species="impudicus">
<emphasis id="63A5EAB2FFF3D97FF7C6CBC896F5FB04" box="[955,1162,1217,1238]" italics="true" pageId="38" pageNumber="397">Pecado impudicus</emphasis>
(
<bibRefCitation id="35404B51FFF3D97FF0E7CBC8975BFB05" author="Denis J" box="[1178,1316,1217,1239]" pageId="38" pageNumber="397" pagination="41 - 57" refId="ref17953" refString="Denis J. 1945. Descriptions d'araignees nord-africaines. Bulletin de la Societe d'Histoire Naturelle de Toulouse 79: 41 - 57." type="journal article" year="1945">Denis, 1945</bibRefCitation>
)
</taxonomicName>
