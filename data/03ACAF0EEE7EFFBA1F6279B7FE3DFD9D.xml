<taxonomicName id="4C05659BEE7EFFBE1DB67E43FE86FB83" box="[189,347,1089,1115]" class="Diplopoda" family="Julidae" genus="Ommatoiulus" kingdom="Animalia" order="Julida" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
