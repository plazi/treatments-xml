<taxonomicName id="F4874D0C7932E12D2CF23F07FEB9FABE" authority="Cobb, 1898" authorityName="Cobb" authorityYear="1898" box="[243,351,1294,1315]" class="Chromadorea" family="Hystrignathidae" genus="Lepidonema" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rhabditida" pageId="7" pageNumber="8" phylum="Nematoda" rank="genus">Lepidonema</taxonomicName>
