, Amazonas, 20 km SW Manaus, 
<geoCoordinate id="53620E6D8ECF3E3F2EE6CD21DB757E43" degrees="3.166" direction="south" orientation="latitude" precision="55" value="-3.166">3.166°S</geoCoordinate>
, 
<geoCoordinate id="C844EC457B5D3F1055B7A50C3C1A304A" degrees="60.234" direction="west" orientation="longitude" precision="55" value="-60.234">60.234°W</geoCoordinate>
