<taxonomicName id="769AAE5CBCA9FB35A91413AC96B70E78" genus="Ichneumonoidea" lsidName="Ichneumonoidea" order="Hymenoptera" pageId="0" pageNumber="992" rank="order">Hymenoptera</taxonomicName>
