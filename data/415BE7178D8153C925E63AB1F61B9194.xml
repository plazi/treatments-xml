<taxonomicName id="65D4CB0DF8A5D9277989BA45CF754404" class="Insecta" family="Ichneumonidae" genus="Zatypota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zatypota anomala" order="Hymenoptera" pageId="0" pageNumber="992" phylum="Arthropoda" rank="species" species="anomala">Zatypota anomala</taxonomicName>
