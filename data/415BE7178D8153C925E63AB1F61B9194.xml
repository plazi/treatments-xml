<taxonomicName id="0EBC9A5F09BD0F8D115B366C0A01A8C3" class="Insecta" family="Ichneumonidae" genus="Sinarachna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinarachna" order="Hymenoptera" pageId="0" pageNumber="992" phylum="Arthropoda" rank="genus">Sinarachna</taxonomicName>
