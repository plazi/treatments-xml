<collectingCountry id="0DAC7664FFC8FF84FE54FB9D5A75FBB0" box="[452,513,1094,1120]" name="United States of America" pageId="20" pageNumber="195">USA</collectingCountry>
. Tehama Co.: 
<quantity id="B2439B11FFC8FF84FD33FB9D5A82FBB0" box="[675,758,1094,1120]" metricMagnitude="3" metricUnit="m" metricValue="4.828032" pageId="20" pageNumber="195" unit="mi" value="3.0">3 miles</quantity>
W of Child’s Meadows, 
<quantity id="B2439B11FFC8FF84FB84FB9D5C1BFBB0" box="[1044,1135,1094,1121]" metricMagnitude="3" metricUnit="m" metricValue="1.58496" pageId="20" pageNumber="195" unit="ft" value="5200.0">5200 ft.</quantity>
, on dead twigs and duff, 
<date id="01051034FFC8FF84FA1FFB9D5974FB54" pageId="20" pageNumber="195" value="1967-05-20">20 May 1967</date>
, 
<emphasis id="47CFEAE6FFC8FF84FE9DFBB059F1FB54" box="[269,389,1130,1156]" italics="true" pageId="20" pageNumber="195">DTK 6237</emphasis>
(as 
<taxonomicName id="B2BB4D77FFC8FF84FE28FBB05A0CFB54" authority=", UC" authorityName="UC" box="[440,632,1130,1157]" class="Myxomycetes" family="Stemonitidaceae" genus="Lamproderma" kingdom="Protozoa" order="Stemonitales" pageId="20" pageNumber="195" phylum="Mycetozoa" rank="species" species="carestiae">
<emphasis id="47CFEAE6FFC8FF84FE28FBB05A4AFB54" box="[440,574,1131,1156]" italics="true" pageId="20" pageNumber="195">L. carestiae</emphasis>
, UC
</taxonomicName>
1408244!); Well’s Cabin Campground, 
<quantity id="B2439B11FFC8FF84FBA9FBB15CE0FB54" box="[1081,1172,1130,1156]" metricMagnitude="3" metricUnit="m" metricValue="1.92024" pageId="20" pageNumber="195" unit="ft" value="6300.0">6300 ft.</quantity>
, on dead twigs, 
<date id="01051034FFC8FF84FAC3FBB158B7FB78" pageId="20" pageNumber="195" value="1967-06-24">24 June 1967</date>
, 
<emphasis id="47CFEAE6FFC8FF84FF5EFB545930FB78" box="[206,324,1166,1192]" italics="true" pageId="20" pageNumber="195">DTK 6361</emphasis>
(as 
