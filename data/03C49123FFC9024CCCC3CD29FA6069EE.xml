<subSubSection id="C37773BEFFC8024CCC93CEDAFAEA6AAE" box="[199,1302,1157,1180]" pageId="1" pageNumber="237" type="etymology">
<paragraph id="8BD22035FFC8024CCC93CEDAFAEA6AAE" blockId="1.[151,1436,152,1527]" box="[199,1302,1157,1180]" pageId="1" pageNumber="237">
<emphasis id="B919FC27FFC8024CCC93CEDAFEBB6AAE" bold="true" box="[199,327,1157,1180]" pageId="1" pageNumber="237">Etymology.</emphasis>
