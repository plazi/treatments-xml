</taxonomicName>
(
<collectingRegion id="49D89DB2FF95FF93184DFA2584F304AB" box="[1103,1209,1508,1534]" country="Japan" name="Ishikawa" pageId="1" pageNumber="258">Ishikawa</collectingRegion>
, 1957) (
<taxonomicName id="4C1C28D3FF95FF931910FA25853304AA" authorityName="Lepeletier de Saint Fargeau" authorityYear="1845" box="[1298,1401,1508,1535]" class="Insecta" family="Pompilidae" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="258" phylum="Arthropoda" rank="subFamily" subFamily="Pepsinae">Pepsinae</taxonomicName>
