, 
<geoCoordinate id="632D044034F359DC88CD397B1FF25B42" degrees="37" direction="east" minutes="45" orientation="longitude" precision="925" value="37.75">37°45'E</geoCoordinate>
, -700 m. Station 106, south of Jeddah, Saudi Arabia, 
<geoCoordinate id="FD9B9B5B67BAA8EBE606A74232BE828D" degrees="21" direction="north" minutes="2" orientation="latitude" precision="925" value="21.033333">21°2'N</geoCoordinate>
, 
<geoCoordinate id="DCEBB8A4D60C5DF9210B6D05C5E70C9B" degrees="38" direction="east" minutes="41.4" orientation="longitude" precision="92" value="38.69">38°41.4'E</geoCoordinate>
