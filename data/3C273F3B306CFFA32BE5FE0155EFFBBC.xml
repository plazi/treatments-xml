<taxonomicName id="738EF5AE306AFFA32826FD9454AEFD90" authorityName="Waldren &amp; Williams &amp; Cambra &amp; Pitts" authorityYear="2020" box="[852,950,547,573]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Invreiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="172" phylum="Arthropoda" rank="species" species="australis">
