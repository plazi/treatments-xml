<taxonomicName id="738EF5AE306AFFA32A0BFDF056FDFDCC" authorityName=", Suarez" authorityYear="1966" box="[377,485,583,609]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Invreiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="172" phylum="Arthropoda" rank="genus">
