= 
<taxonomicName id="E3A7FE88FFF08300FCA95B44FAC0FCC6" authority="M. Bieb., Fl. Taur." authorityName="M. Bieb., Fl. Taur." box="[877,1360,804,829]" class="Magnoliopsida" family="Brassicaceae" genus="Sterigmostemum" kingdom="Plantae" order="Brassicales" pageId="77" pageNumber="77" phylum="Tracheophyta" rank="species" species="incanum">
