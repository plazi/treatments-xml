<typeStatus id="DB556783FFC3FFC7FF79FD90FF06FDE9" box="[151,209,577,601]" pageId="38" pageNumber="39">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="C3EEA2A2FFC3FFC7FED4FD90FDB0FDE9" authorityName="Annenkova" authorityYear="1952" box="[314,615,577,601]" class="Polychaeta" family="Flabelligeridae" genus="Flabelligera" kingdom="Animalia" order="Terebellida" pageId="38" pageNumber="39" phylum="Annelida" rank="species" species="mastigophora">
