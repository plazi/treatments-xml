<subSubSection id="4CF48AAAFFC3FFC7FF79FD90FCE9FDE9" box="[151,830,576,601]" pageId="38" pageNumber="39" type="type_taxon">
<paragraph id="0451D921FFC3FFC7FF79FD90FCE9FDE9" blockId="38.[151,830,576,601]" box="[151,830,576,601]" pageId="38" pageNumber="39">
