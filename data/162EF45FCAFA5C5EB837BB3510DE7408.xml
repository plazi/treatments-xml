<taxonomicName id="86F4AF3200B00AF32E3C9BC9BFDA6DDC" authorityName="Gray" authorityYear="1848" class="Gymnolaemata" family="Bugulidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Cheilostomata" pageId="0" pageNumber="73" phylum="Bryozoa" rank="family">Bugulidae</taxonomicName>
