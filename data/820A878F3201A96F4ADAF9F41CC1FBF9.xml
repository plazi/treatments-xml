<taxonomicName id="CDA34D1A3201A96E4B6DF8571FE0F81A" authority="Forster" authorityName="Forster" box="[288,674,2010,2035]" class="Aves" family="Spheniscidae" genus="Spheniscus" kingdom="Animalia" order="Sphenisciformes" pageId="8" pageNumber="266" phylum="Chordata" rank="species" species="magellanicus">
