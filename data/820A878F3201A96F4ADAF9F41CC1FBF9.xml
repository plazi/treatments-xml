<collectingCountry id="72B476093201A96E4F40F8E3180AF86E" box="[1293,1352,1902,1927]" name="Cuba" pageId="8" pageNumber="266">Cuba</collectingCountry>
[
<taxonomicName id="CDA34D1A3201A96E4F15F8E31D8BF842" class="Aves" family="Ardeidae" genus="Ardea" kingdom="Animalia" order="Pelecaniformes" pageId="8" pageNumber="266" phylum="Chordata" rank="species" species="alba">
<emphasis id="38D7EA8B3201A96E4F15F8E31D8BF842" italics="true" pageId="8" pageNumber="266">Ardea alba</emphasis>
</taxonomicName>
(L.), 
<taxonomicName id="CDA34D1A3201A96E4B40F81F1FA0F842" authority="Boddaert" authorityName="Boddaert" box="[269,738,1938,1963]" class="Aves" family="Ardeidae" genus="Butorides" kingdom="Animalia" order="Pelecaniformes" pageId="8" pageNumber="266" phylum="Chordata" rank="subSpecies" species="virescens" subSpecies="maculata">
<emphasis id="38D7EA8B3201A96E4B40F81F1F1FF842" box="[269,605,1938,1963]" italics="true" pageId="8" pageNumber="266">Butorides virescens maculata</emphasis>
(Boddaert)
</taxonomicName>
, 
<taxonomicName id="CDA34D1A3201A96E48BDF8191EEDF842" box="[752,943,1938,1963]" class="Aves" family="Ardeidae" genus="Egretta" kingdom="Animalia" order="Pelecaniformes" pageId="8" pageNumber="266" phylum="Chordata" rank="species" species="caerulea">
<emphasis id="38D7EA8B3201A96E48BDF8191EEDF842" box="[752,943,1938,1963]" italics="true" pageId="8" pageNumber="266">Egretta caerulea</emphasis>
</taxonomicName>
(L.), 
<taxonomicName id="CDA34D1A3201A96E49B9F8191827F842" authority="Gosse" authorityName="Gosse" box="[1012,1381,1938,1963]" class="Aves" family="Ardeidae" genus="Egretta" kingdom="Animalia" order="Pelecaniformes" pageId="8" pageNumber="266" phylum="Chordata" rank="subSpecies" species="tricolor" subSpecies="ruficollis">
