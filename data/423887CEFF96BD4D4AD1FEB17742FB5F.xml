, 
<collectingRegion id="0855F83AFF96BD4D4E62FB82708CFBBB" box="[1083,1242,1094,1120]" country="Brazil" name="Minas Gerais" pageId="9" pageNumber="238">Minas Gerais</collectingRegion>
