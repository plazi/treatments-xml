<emphasis id="B97FEACE780EFF94FCE0FEEE9F71FE7A" box="[772,837,364,389]" italics="true" pageId="27" pageNumber="28">
<taxonomicName id="4C0B4D5F780EFF94FCE0FEEE9F74FE7A" box="[772,832,364,389]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="catie">catie</taxonomicName>
