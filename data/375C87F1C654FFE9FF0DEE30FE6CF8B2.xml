<taxonomicName id="78F54D64C656FFE9FEBDEF3EFD9DFB8D" authority="Ustjuzhanin" authorityName="Ustjuzhanin" box="[295,603,1061,1083]" class="Insecta" family="Pterophoridae" genus="Stenoptilia" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="324" phylum="Arthropoda" rank="species" species="lasani">
