<paragraph id="A5DAA72E397E2D647643C68393C1C592" pageId="9" pageNumber="148">
<taxonomicName id="94B2D414286B8E43BCC274843C96FC93" class="Insecta" family="Noctuidae" genus="Eriopyga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eriopyga melanopis" order="Lepidoptera" pageId="9" pageNumber="148" phylum="Arthropoda" rank="species" species="melanopis">Eriopyga melanopis</taxonomicName>
