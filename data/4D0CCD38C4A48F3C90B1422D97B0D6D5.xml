<taxonomicName id="2459AA58D49AB341993D08E486CF9544" class="Insecta" family="Noctuidae" genus="Eriopyga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eriopyga melanopis" order="Lepidoptera" pageId="9" pageNumber="148" phylum="Arthropoda" rank="species" species="melanopis">Eriopyga melanopis</taxonomicName>
