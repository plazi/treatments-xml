<paragraph id="557C490A47C96EA73312DFE9E8837701" pageId="0" pageNumber="39">
<taxonomicName id="468A3822F18C5F89F4CED57ACB8AD3CD" LSID="26530819-DF2D-55B9-9EE9-0EF9E232A106" authority="Acev. - Rodr." authorityName="Acevedo-Rodríguez" authorityYear="2011" class="Magnoliopsida" family="Sapindaceae" genus="Allophylastrum" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="Allophylastrum" order="Sapindales" pageId="0" pageNumber="39" phylum="Tracheophyta" rank="genus" status="gen. nov.">Allophylastrum Acev.-Rodr.</taxonomicName>
