<emphasis id="EF84EAC5BC4DFFCC24873533FC060543" box="[929,1008,1843,1864]" italics="true" pageId="2" pageNumber="229">insignis</emphasis>
</taxonomicName>
; 4: 
<taxonomicName id="1AF04D54BC4DFFCC233B3533FB380543" authority="Starý &amp; Oboňa, 2018" authorityName="Starý &amp; Oboňa" authorityYear="2018" box="[1053,1230,1842,1864]" class="Insecta" family="Limoniidae" genus="Thaumastoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="229" phylum="Arthropoda" rank="species" species="spathifera" status="sp. n." subGenus="Thaumastoptera">
