</taxonomicName>
; 3: 
<taxonomicName id="1AF04D54BC4DFFCC247E3533FC060543" authorityName="Lackschewitz" authorityYear="1940" box="[856,1008,1842,1864]" class="Insecta" family="Limoniidae" genus="Thaumastoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="229" phylum="Arthropoda" rank="species" species="insignis" subGenus="Thaumastoptera">
<emphasis id="EF84EAC5BC4DFFCC247E3533FC9D0543" box="[856,875,1843,1864]" italics="true" pageId="2" pageNumber="229">T.</emphasis>
(
<emphasis id="EF84EAC5BC4DFFCC245A3533FC780543" box="[892,910,1843,1864]" italics="true" pageId="2" pageNumber="229">T.</emphasis>
