; anterior genital plates L 
<number id="3DF624E31968FF9100F6496EFE4AF9EF" box="[430,460,1618,1643]" pageId="34" pageNumber="435" value="65.0">65</number>
, W 
<number id="3DF624E31968FF9100A4496FFDAFF9EF" box="[508,553,1619,1643]" pageId="34" pageNumber="435" value="108.0">108</number>
; posterior genital plates L 
<number id="3DF624E31968FF910201496FFCFEF9EF" box="[857,888,1619,1643]" pageId="34" pageNumber="435" value="90.0">90</number>
, W 
<number id="3DF624E31968FF9102FF496FFC40F9EF" box="[935,966,1619,1643]" pageId="34" pageNumber="435" value="90.0">90</number>
; pedipalpal segments (P- 
<number id="3DF624E31968FF9105BE496EFA88F9EF" box="[1254,1294,1618,1643]" pageId="34" pageNumber="435" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
) L: 
<number id="3DF624E31968FF910418496FFA11F9EF" box="[1344,1431,1619,1643]" pageId="34" pageNumber="435" value="30.13">30, 130</number>
, 
<number id="3DF624E31968FF9101CF494AFF69F90B" box="[151,239,1654,1679]" pageId="34" pageNumber="435" value="65.125">65, 125</number>
, 
