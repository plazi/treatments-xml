<taxonomicName id="CFD6FC8BFFB67E70696AEC3CFB99F8DA" authority="Xue &amp; Hong, 2005" authorityName="Xue &amp; Hong" authorityYear="2005" box="[633,1132,1826,1852]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="amygdali">
