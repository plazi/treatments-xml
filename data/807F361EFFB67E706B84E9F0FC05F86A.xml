<taxonomicName id="CFD6FC8BFFB67E706842EC6CFC2AF86A" box="[849,991,1907,1932]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="amygdali">
