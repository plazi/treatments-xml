<taxonomicName id="019F2FC0D020BD0BDD4D5F8DA5B2BFC8" authority="Sharkey, 2017" authorityName="Sharkey" authorityYear="2017" class="Insecta" family="Braconidae" genus="Zosteragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zosteragathis samensis" order="Hymenoptera" pageId="18" pageNumber="125" phylum="Arthropoda" rank="species" species="samensis">Zosteragathis samensis</taxonomicName>
