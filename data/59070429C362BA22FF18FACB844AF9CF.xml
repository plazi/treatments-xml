<taxonomicName id="16AECEBCC362BA22FE41FACB833AFA5E" authorityName="Linnaeus" authorityYear="1758" box="[462,550,1398,1424]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
