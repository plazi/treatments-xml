<taxonomicName id="4C484D5AFFFEFFE7843AF9975E45F9E1" authority="Latreille, 1803" authorityName="Latreille" authorityYear="1803" box="[561,818,1562,1583]" class="Insecta" family="Pamphiliidae" genus="Pamphilius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="315" phylum="Arthropoda" rank="genus">
