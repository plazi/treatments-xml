, 
<collectingCountry id="398BD6D7FF9BFF86FE97FF46FE2BFF46" box="[330,464,188,213]" name="Ivory Coast" pageId="10" pageNumber="461">
Ivory 
<collectingRegion id="835858A5FF9BFF86FE50FF47FE2BFF46" box="[397,464,188,212]" country="Tanzania" name="Pwani" pageId="10" pageNumber="461">Coast</collectingRegion>
</collectingCountry>
, 
<collectingCountry id="398BD6D7FF9BFF86FE06FF46FDECFF47" box="[475,535,189,213]" name="Togo" pageId="10" pageNumber="461">Togo</collectingCountry>
, 
<collectingCountry id="398BD6D7FF9BFF86FDFFFF47FD67FF46" box="[546,668,188,212]" name="Cameroon" pageId="10" pageNumber="461">Cameroon</collectingCountry>
).
