<collectingCountry id="F312DCCD8865FFBAFB21FA93B279446A" box="[1174,1289,1327,1353]" name="Nicaragua" pageId="7" pageNumber="576">Nicaragua</collectingCountry>
(
<collectingRegion id="49C152BF8865FFBAFAADFA93B2E14469" box="[1306,1425,1327,1354]" country="Nicaragua" name="Chontales" pageId="7" pageNumber="576">Chontales</collectingRegion>
