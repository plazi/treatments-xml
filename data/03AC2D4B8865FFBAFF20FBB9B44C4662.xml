<collectingCountry id="F312DCCD8865FFBAFA9FFAEFB2E8444E" box="[1320,1432,1363,1389]" name="Honduras" pageId="7" pageNumber="576">Honduras</collectingCountry>
, 
<collectingCountry id="F312DCCD8865FFBAFF20FACBB67D44B2" box="[151,269,1399,1425]" name="Nicaragua" pageId="7" pageNumber="576">Nicaragua</collectingCountry>
