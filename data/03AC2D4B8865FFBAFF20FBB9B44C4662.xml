(
<bibRefCitation id="EF94E1AC8865FFBAFB0AFA27B2314496" author="Bates, H. W." box="[1213,1345,1434,1461]" pageId="7" pageNumber="576" pagination="249 - 436" refId="ref8053" refString="Bates, H. W. (1885) s. n. Biologia Centrali-Americana, Insecta, Coleoptera, Supplement to Longicornia, 5, 249 - 436." type="journal article" year="1885">Bates 1885</bibRefCitation>
), 
<collectingRegion id="49C152BF8865FFBAFAEFFA27B2EC4496" box="[1368,1436,1435,1461]" country="Panama" name="Colon" pageId="7" pageNumber="576">Colón</collectingRegion>
(
<bibRefCitation id="EF94E1AC8865FFBAFF28FA03B65244FA" author="Bates, H. W." box="[159,290,1470,1497]" pageId="7" pageNumber="576" pagination="249 - 436" refId="ref8053" refString="Bates, H. W. (1885) s. n. Biologia Centrali-Americana, Insecta, Coleoptera, Supplement to Longicornia, 5, 249 - 436." type="journal article" year="1885">Bates 1885</bibRefCitation>
