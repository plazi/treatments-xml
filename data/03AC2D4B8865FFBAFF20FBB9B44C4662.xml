<paragraph id="8BBA9C5D8865FFBAFF20FA93B4C244DE" blockId="7.[151,1437,1327,1857]" pageId="7" pageNumber="576">
<taxonomicName id="4C05E7DE8865FFBAFF20FA93B6C4446B" authorityName="Bates" authorityYear="1872" box="[151,436,1327,1353]" class="Insecta" family="Cerambycidae" genus="Anisopodus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="576" phylum="Arthropoda" rank="species" species="scriptipennis">
