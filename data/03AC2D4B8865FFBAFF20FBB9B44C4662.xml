, 
<collectingRegion id="49C152BF8865FFBAFED3FA5FB6C244DE" box="[356,434,1507,1533]" country="Costa Rica" name="Limon" pageId="7" pageNumber="576">Limón</collectingRegion>
, 
<collectingRegion id="49C152BF8865FFBAFE0BFA5FB54A44DE" box="[444,570,1507,1533]" country="Costa Rica" name="Puntarenas" pageId="7" pageNumber="576">Puntarenas</collectingRegion>
