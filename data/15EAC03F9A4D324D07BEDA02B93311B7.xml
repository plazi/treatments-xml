<taxonomicName id="BEAAB5FF0F5F46226818B9BD10671750" authorityName="Schaus" authorityYear="1910" class="Insecta" family="Arctiidae" genus="Ormetica" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ormetica ataenia" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="ataenia">
