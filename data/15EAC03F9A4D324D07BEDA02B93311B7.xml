<paragraph id="1358C7875E48C5E2B8282E62ABA94223" pageId="0" pageNumber="1">
<taxonomicName id="232B6A98C4C4625E1DAD33F79B8F076E" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Exasticolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exasticolus robertofernandezi" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="robertofernandezi">
