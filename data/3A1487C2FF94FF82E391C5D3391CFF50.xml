<taxonomicName id="75BD4D57FF94FF83E760C4F93EFAF820" box="[1126,1240,1933,1956]" class="Magnoliopsida" family="Ebenaceae" genus="Diospyros" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="2" phylum="Magnoliopsida" rank="genus">
