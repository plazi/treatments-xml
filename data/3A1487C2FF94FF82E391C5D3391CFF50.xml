<taxonomicName id="75BD4D57FF94FF83E614C4F83FAFF821" box="[1298,1421,1932,1957]" class="Magnoliopsida" family="Ebenaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="2" phylum="Magnoliopsida" rank="family">Ebenaceae</taxonomicName>
