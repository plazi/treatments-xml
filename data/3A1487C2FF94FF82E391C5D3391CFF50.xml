</taxonomicName>
) (
<taxonomicName id="75BD4D57FF94FF83E0D4C44B3E64F8D3" authorityName="" authorityYear="1835" box="[978,1094,1855,1879]" class="Magnoliopsida" family="Moraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="2" phylum="Magnoliopsida" rank="family">Moraceae</taxonomicName>
