<taxonomicName id="75BD4D57FF94FF83E0AAC4C53F40F84D" authority=", Sudabe Amini" authorityName="Sudabe Amini" box="[940,1378,1968,1993]" class="Magnoliopsida" family="Juglandaceae" genus="Pterocarya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="2" phylum="Tracheophyta" rank="species" species="fraxinifolia">
