<geoCoordinate id="EE38AC8ADA6459499718EF0AF8DEFEB3" box="[636,744,314,340]" degrees="27" direction="south" minutes="34" orientation="latitude" pageId="52" pageNumber="53" precision="925" value="-27.566668">27°34’ S</geoCoordinate>
