<taxonomicName id="6CCE4D17E87C0B735062FDEE4442E809" authority="(Malloch)" authorityName="Johannsen" authorityYear="1952" baseAuthorityName="Malloch" baseAuthorityYear="1915" box="[830,1131,554,578]" class="Insecta" family="Chironomidae" genus="Hydrobaenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="286" phylum="Arthropoda" rank="species" species="pilipes">
