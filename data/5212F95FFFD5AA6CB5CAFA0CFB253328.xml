<taxonomicName id="1DBB33CAFFD5AA6EB690F85EFBA1358C" box="[973,1087,1960,1985]" class="Actinopterygii" family="Nemacheilidae" genus="Barbatula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="9" pageNumber="10" phylum="Chordata" rank="genus">
