<subSubSection id="C32B6532FFC1A878FF4BFACDFE0BFA80" box="[199,387,1303,1328]" pageId="11" pageNumber="382" type="biology_ecology">
<paragraph id="8B8E36B9FFC1A878FF4BFACDFE0BFA80" blockId="11.[151,1437,151,1365]" box="[199,387,1303,1328]" pageId="11" pageNumber="382">
