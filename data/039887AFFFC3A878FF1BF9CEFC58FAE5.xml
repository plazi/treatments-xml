<subSubSection id="C32B6532FFC1A878FF4BFAE6FC58FAE5" box="[199,976,1340,1365]" pageId="11" pageNumber="382" type="etymology">
<paragraph id="8B8E36B9FFC1A878FF4BFAE6FC58FAE5" blockId="11.[151,1437,151,1365]" box="[199,976,1340,1365]" pageId="11" pageNumber="382">
