<paragraph id="373844A94FEA887A6B175BB7EE8E94CB" pageId="6" pageNumber="327">
<taxonomicName id="A51033AD20158E38B2A67F76C0969FCF" class="Insecta" family="Chrysomelidae" genus="Erynephala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erynephala maritima" order="Coleoptera" pageId="6" pageNumber="327" phylum="Arthropoda" rank="species" species="maritima">Erynephala maritima</taxonomicName>
