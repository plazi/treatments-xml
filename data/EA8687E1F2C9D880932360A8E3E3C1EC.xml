<taxonomicName id="512EF645610E2C3951C402F72325BC12" class="Magnoliopsida" family="Amaranthaceae" genus="Suaeda" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Suaeda" order="Caryophyllales" pageId="6" pageNumber="327" phylum="Tracheophyta" rank="genus">Suaeda</taxonomicName>
