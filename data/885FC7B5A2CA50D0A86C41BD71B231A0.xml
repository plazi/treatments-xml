<emphasis id="8250A2C620FBE5E4D8551781A121CC92" italics="true" pageId="0" pageNumber="1">Michelia septipetala</emphasis>
</taxonomicName>
, 
<taxonomicName id="3D68759300E62CCB241137604300365D" genus="M." lsidName="M. fujianensis" pageId="0" pageNumber="1" rank="species" species="fujianensis">
