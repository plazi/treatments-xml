<taxonomicName id="C3345FFD1A9D44520C8B504647DF2684" class="Insecta" family="Lasiocampidae" genus="Symphyta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Symphyta" order="Hymenoptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
