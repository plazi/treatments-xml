<paragraph id="572836E1FFDAB901B9E2E3F0A94EFE9B" blockId="14.[151,1436,1422,2023]" lastBlockId="16.[151,1436,151,1149]" lastPageId="16" lastPageNumber="17" pageId="14" pageNumber="15">
Structure as in 
<taxonomicName id="90974D62FFDAB91FB853E3F0AE06F819" box="[374,636,1927,1951]" class="Insecta" family="Eumenidae" genus="Polistes" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="reliciniclypeus">
