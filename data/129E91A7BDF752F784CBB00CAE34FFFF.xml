<taxonomicName id="20119251052150950521A8FD2A87A7E7" authority="S.F.Gray, 1821" authorityName="Gray" authorityYear="1821" baseAuthorityName="Sw." class="Lecanoromycetes" family="Icmadophilaceae" genus="Cerania" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Cerania vermicularis" order="Pertusariales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="vermicularis">
<emphasis id="ED2470DE5D2CEF750D2EF3A006F14328" italics="true" pageId="0" pageNumber="17">Cerania vermicularis</emphasis>
</taxonomicName>
, 
<taxonomicName id="BAB4F21F0EA1B9944EA8A1C0FEC471D5" class="Lecanoromycetes" family="Parmeliaceae" genus="Cetraria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Cetraria nivalis" order="Lecanorales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="nivalis">
<emphasis id="4EA6A0F14FB21A90ACF446BA93D52884" italics="true" pageId="0" pageNumber="17">Cetraria nivalis</emphasis>
</taxonomicName>
); over leaves and flowers of 
<taxonomicName id="BB197C251FDC50C7E9F74F356762F405" class="Insecta" family="Nymphalidae" genus="Dryas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dryas" order="Lepidoptera" pageId="0" pageNumber="17" phylum="Arthropoda" rank="genus">
<emphasis id="254B6756C935FFE9ED740250F6D43B62" italics="true" pageId="0" pageNumber="17">Dryas</emphasis>
</taxonomicName>
