<collectingCountry id="138627F33516216EFC3AF8E075A769A8" box="[936,1007,1912,1937]" name="Brazil" pageId="8" pageNumber="438">Brazil</collectingCountry>
, and 
<collectingCountry id="138627F33516216EFBA4F8E072AA69A8" box="[1078,1250,1912,1937]" name="French Guiana" pageId="8" pageNumber="438">French Guiana</collectingCountry>
(
<bibRefCitation id="0F001A923516216EFB64F8E073DF69A8" author="Metcalf" box="[1270,1431,1912,1937]" pageId="8" pageNumber="438" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
