<taxonomicName id="4C69E9C1FF1F7F2EFEB8FE3C19654B71" box="[365,560,511,537]" class="Insecta" family="Saldidae" genus="Zemacrosaldula" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="247" phylum="Arthropoda" rank="genus" status="new genus">
