<taxonomicName id="8FEE4D41FFB7FF9CFDECFEC867AACACE" box="[636,745,367,393]" class="Mammalia" family="Sciuridae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Sciuridae</taxonomicName>
, and 
<taxonomicName id="8FEE4D41FFB7FF9CFCB3FEC866A4CACE" authorityName="Gill" authorityYear="1872" box="[803,999,367,393]" class="Mammalia" kingdom="Animalia" order="Didelphimorphia" pageId="94" pageNumber="95" phylum="Chordata" rank="order">Didelphimorphia</taxonomicName>
