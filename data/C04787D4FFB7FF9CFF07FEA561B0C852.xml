<taxonomicName id="8FEE4D41FFB7FF9CFD8DFE3467BCCAEA" authorityName="Neumann" authorityYear="1907" box="[541,767,403,429]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="94" pageNumber="95" phylum="Arthropoda" rank="species" species="fuscum">
<emphasis id="7A9AEAD0FFB7FF9CFD8DFE3467BCCAEA" box="[541,767,403,429]" italics="true" pageId="94" pageNumber="95">Amblyomma fuscum</emphasis>
</taxonomicName>
