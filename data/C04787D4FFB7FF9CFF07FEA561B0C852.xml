<taxonomicName id="8FEE4D41FFB7FF9CFE50FEC86705CACE" box="[448,582,367,393]" class="Mammalia" family="Echimyidae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Echimyidae</taxonomicName>
