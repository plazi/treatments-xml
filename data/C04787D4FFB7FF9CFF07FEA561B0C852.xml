<taxonomicName id="8FEE4D41FFB7FF9CFB4DFEEC6012CA22" authorityName="Bowdich" authorityYear="1821" box="[1245,1361,331,357]" class="Mammalia" kingdom="Animalia" order="Carnivora" pageId="94" pageNumber="95" phylum="Chordata" rank="order">Carnivora</taxonomicName>
: 
<taxonomicName id="8FEE4D41FFB7FF9CFACEFEEC6582CACE" class="Mammalia" family="Canidae" kingdom="Animalia" order="Carnivora" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Canidae</taxonomicName>
, 
<taxonomicName id="8FEE4D41FFB7FF9CFF5CFEC86475CACE" box="[204,310,367,393]" class="Mammalia" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="order">Rodentia</taxonomicName>
: 
