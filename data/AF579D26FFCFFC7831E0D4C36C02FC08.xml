<quantity id="E00681D5FFCFFC753221D0E969C6F972" box="[328,421,1759,1786]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="22" pageNumber="23" unit="mm" value="5.5">5.5 mm</quantity>
