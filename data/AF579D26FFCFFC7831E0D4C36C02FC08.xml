<taxonomicName id="E0FE57B3FFC2FC78320BD55C69A2FC08" authorityName="Thurston" authorityYear="1980" box="[354,449,874,899]" class="Malacostraca" family="Dexaminidae" genus="Lepechinella" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="grimi">
