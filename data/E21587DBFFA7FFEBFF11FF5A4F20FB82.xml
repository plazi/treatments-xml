<paragraph id="6A0336CDFFA7FFEBFF11FF30488FFEB3" blockId="63.[151,1181,222,275]" box="[151,908,253,275]" pageId="63" pageNumber="64">
<treatmentCitationGroup id="4AAC11E3FFA7FFEBFF11FF30488FFEB3" box="[151,908,253,275]" pageId="63" pageNumber="64">
<taxonomicName id="ADBC4D4EFFA7FFEBFF11FF304A13FEB3" authorityName="Simon" authorityYear="1893" box="[151,272,253,275]" class="Arachnida" family="Pholcidae" genus="Psilochorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="genus">
