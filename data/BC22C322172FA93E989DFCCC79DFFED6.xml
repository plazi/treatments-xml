(
<collectionCode id="529AEAF1172FA9009BDEFBD37C58FBCE" box="[980,1051,1048,1074]" country="Canada" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34778" lsid="urn:lsid:biocol.org:col:34778" name="Royal Ontario Museum" pageId="30" pageNumber="31" type="Museum">ROM</collectionCode>
