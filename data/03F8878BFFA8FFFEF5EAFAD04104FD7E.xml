<taxonomicName id="4C514D1EFFA8FFFDF5EAFAD045B7FA83" authority="Mayr, 1901" authorityName="Mayr" authorityYear="1901" box="[151,493,1327,1354]" class="Insecta" family="Formicidae" genus="Probolomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
