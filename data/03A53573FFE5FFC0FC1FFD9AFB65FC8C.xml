<taxonomicName id="4C0CFFE6FFE5FFC0FBCEFD9AFAD2FD8D" authority="Baird 1843" authorityName="Baird" authorityYear="1843" box="[1049,1296,542,566]" class="Branchiopoda" genus="Pleuroxus" kingdom="Animalia" order="Diplostraca" pageId="5" pageNumber="776" phylum="Arthropoda" rank="genus">
<emphasis id="B9785877FFE5FFC0FBCEFD9AFB48FD8D" bold="true" box="[1049,1162,542,566]" italics="true" pageId="5" pageNumber="776">Pleuroxus</emphasis>
<bibRefCitation id="EF9DF994FFE5FFC0FB46FD9AFAD2FD8D" author="Baird W" box="[1169,1296,542,566]" pageId="5" pageNumber="776" pagination="81 - 95" refId="ref13226" refString="Baird W (1843) The natural history of the British Entomostraca. Annual Magazine of Natural History, 11: 81 - 95." type="journal article" year="1843">Baird 1843</bibRefCitation>
