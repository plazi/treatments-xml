<heading id="D0C781EE3707FF9BE8F4FDC2FD68E77A" bold="true" box="[490,711,602,627]" centered="true" fontSize="8" level="2" pageId="1" pageNumber="254" reason="3">
<taxonomicName id="4C304D013707FF9BE8F4FDC2FD68E77A" authority="Konow, 1897" authorityName="Konow" authorityYear="1897" box="[490,711,602,627]" class="Insecta" family="Tenthredinidae" genus="Dinax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="254" phylum="Arthropoda" rank="genus">
