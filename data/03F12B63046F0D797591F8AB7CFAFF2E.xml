<location id="8E87CCAE046E0D79707BFF1E7DC6FF4A" LSID="urn:lsid:plazi:treatment:03F12B63046F0D797591F8AB7CFAFF2E:8E87CCAE046E0D79707BFF1E7DC6FF4A" box="[1405,1436,151,177]" country="Bolivia" name="As" pageId="23" pageNumber="424" stateProvince="Cochabamba">As</location>
