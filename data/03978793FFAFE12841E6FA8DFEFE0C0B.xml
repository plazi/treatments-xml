<taxonomicName id="4C3E4D06FFAEE12845B8FF71FF040F2B" authority="Cshaped" authorityName="Cshaped" class="Arachnida" family="Thomisidae" genus="Borboropactus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
