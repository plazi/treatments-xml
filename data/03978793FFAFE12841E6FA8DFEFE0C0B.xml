<taxonomicName id="4C3E4D06FFAFE1294202F9C4FBE1085B" box="[883,1024,1620,1644]" class="Arachnida" family="Thomisidae" genus="Cupa" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="zhengi">
