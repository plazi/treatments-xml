<taxonomicName id="4C3E4D06FFAFE12943E1F9A0FCC1087F" box="[656,800,1584,1608]" class="Arachnida" family="Thomisidae" genus="Cupa" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="zhengi">Cupa zhengi</taxonomicName>
