<emphasis id="B9382234DD6CAC2427ED1C10FE98F9BF" bold="true" box="[199,317,1580,1606]" pageId="17" pageNumber="188">
<typeStatus id="54F74084DD6CAC2427ED1C10FE9DF9BF" box="[199,312,1580,1606]" pageId="17" pageNumber="188">Holotype</typeStatus>
:
</emphasis>
<specimenCount id="9D4A35AFDD6CAC24266E1C10FEFCF9BF" box="[324,345,1580,1606]" pageId="17" pageNumber="188" type="male">♂</specimenCount>
(
<collectionCode id="ED5D66E3DD6CAC2426421C10FE67F9BF" box="[360,450,1580,1606]" pageId="17" pageNumber="188">BMNH</collectionCode>
), 
<location id="8E93A8FDDD6CAC2426FF1C10FDA9F9BF" LSID="urn:lsid:plazi:treatment:03E54F30DD6CAC2427BD1F65FEB2F82B:8E93A8FDDD6CAC2426FF1C10FDA9F9BF" box="[469,524,1580,1606]" country="Samoa" municipality="Apia" name="West" pageId="17" pageNumber="188">West</location>
<collectingCountry id="F35BBEB6DD6CAC2425391C10FDC7F9BF" box="[531,610,1580,1606]" name="Samoa" pageId="17" pageNumber="188">Samoa</collectingCountry>
, 
<collectingMunicipality id="6B97645CDD6CAC2425411C10FD00F9BF" box="[619,677,1580,1606]" pageId="17" pageNumber="188">Apia</collectingMunicipality>
