<taxonomicName id="50D280FCFFF0FF9651FBFE4AF73CD0A9" authorityName="Agardh ex Bornet et Flahault" authorityYear="1886" box="[151,267,384,409]" class="Cyanophyceae" family="Rivulariaceae" genus="Calothrix" higherTaxonomySource="CoL" kingdom="Fungi" order="Nostocales" pageId="5" pageNumber="41" phylum="Cyanobacteria" rank="genus">Calothrix</taxonomicName>
