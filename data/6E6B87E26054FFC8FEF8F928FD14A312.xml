<taxonomicName id="21C24D776055FFCBFDE9FD4AFD6AA451" box="[537,684,764,789]" class="Insecta" family="Cerambycidae" genus="Sympagus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="58" phylum="Arthropoda" rank="species" species="laetabilis">
