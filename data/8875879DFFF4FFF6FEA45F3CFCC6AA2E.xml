<treatmentCitation id="817D109AFFF4FFF9FFE25FC4FE77ADB8" author="OBENBERGER J." box="[113,474,1423,1442]" page="141" pageId="7" pageNumber="682" year="1914">
<taxonomicName id="C7DC4D08FFF4FFF9FFE25FC4FE77ADB8" authority="Obenberger, 1914: 141" authorityName="Obenberger" authorityPageNumber="141" authorityYear="1914" box="[113,474,1423,1442]" class="Insecta" family="Curculionidae" genus="Urometopus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="682" phylum="Arthropoda" rank="species" species="malyi">
<emphasis id="32A8EA99FFF4FFF9FFE25FC4FEA3ADB8" box="[113,270,1424,1442]" italics="true" pageId="7" pageNumber="682">Urometopus malyi</emphasis>
<bibRefCitation id="644D4B7AFFF4FFF9FE875FDBFE77ADB8" author="OBENBERGER J." box="[276,474,1423,1442]" pageId="7" pageNumber="682" pagination="129 - 142" refId="ref5070" refString="OBENBERGER J. 1914: Beitrag zur Kenntnis der palaearktischen Kaferfauna (Fortsetzung und Schluss). Coleopterologische Rundschau 1914: 129 - 142." type="journal article" year="1914">Obenberger, 1914: 141</bibRefCitation>
