<taxonomicName id="4C444D2FFFFDFFE08DA9FC38B552FC22" box="[732,833,896,917]" class="Insecta" family="Staphylinidae" genus="Circocerus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="325" phylum="Arthropoda" rank="genus">
