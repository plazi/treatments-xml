<subSubSection id="57C1ECB0FFFCD60BCBF7BDEE0589F5EE" box="[151,485,1743,1769]" pageId="23" pageNumber="24" type="nomenclature">
<paragraph id="1F64BF3BFFFCD60BCBF7BDEE0589F5EE" blockId="23.[151,485,1743,1769]" box="[151,485,1743,1769]" pageId="23" pageNumber="24">
<heading id="442C0857FFFCD60BCBF7BDEE0589F5EE" bold="true" box="[151,485,1743,1769]" fontSize="11" level="1" pageId="23" pageNumber="24" reason="1">
<emphasis id="2DAF6329FFFCD60BCBF7BDEE0589F5EE" bold="true" box="[151,485,1743,1769]" pageId="23" pageNumber="24">
Genus 
<taxonomicName id="D8DBC4B8FFFCD60BCB8EBDEE051DF5EE" authority="Boxshall &amp; O’Reilly &amp; Sikorski &amp; Summerfield, 2019" authorityName="Boxshall &amp; O’Reilly &amp; Sikorski &amp; Summerfield" authorityYear="2019" box="[238,369,1743,1769]" class="Polychaeta" family="Aphroditidae" genus="Pholoicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="23" pageNumber="24" phylum="Annelida" rank="genus" status="gen. nov.">
<emphasis id="2DAF6329FFFCD60BCB8EBDEE051DF5EE" bold="true" box="[238,369,1743,1769]" italics="true" pageId="23" pageNumber="24">Pholoicola</emphasis>
</taxonomicName>
<taxonomicNameLabel id="369CDE52FFFCD60BCA17BDEE0589F5EE" box="[375,485,1743,1769]" pageId="23" pageNumber="24" rank="genus">gen. nov.</taxonomicNameLabel>
