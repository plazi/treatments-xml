<taxonomicName id="90DC4D37FFDAFF93FF70FCA0FD200289" authority="Pitts and Sadler" authorityName="Sadler" authorityYear="2017" box="[151,736,871,898]" class="Insecta" family="Pompilidae" genus="Anoplius" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="544" phylum="Arthropoda" rank="species" species="evansi" status="sp. nov." subGenus="Dicranoplius">
<emphasis id="65A8EAA6FFDAFF93FF70FCA0FEC4028A" bold="true" box="[151,260,871,897]" italics="true" pageId="7" pageNumber="544">Anoplius</emphasis>
(
<emphasis id="65A8EAA6FFDAFF93FEF2FCA0FE73028A" bold="true" box="[277,435,871,897]" italics="true" pageId="7" pageNumber="544">Dicranoplius</emphasis>
) 
<emphasis id="65A8EAA6FFDAFF93FE23FCA0FDD0028A" bold="true" box="[452,528,871,897]" italics="true" pageId="7" pageNumber="544">evansi</emphasis>
Pitts and Sadler
</taxonomicName>
