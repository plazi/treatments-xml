<taxonomicName id="4C004D5DFFCD781B1B01F8323CE989CC" box="[320,427,1980,2002]" class="Insecta" family="Tortricidae" genus="Zebraodes" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="394" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="B974EACCFFCD781B1B01F8323CE989CC" box="[320,427,1980,2002]" italics="true" pageId="2" pageNumber="394">Zebraodes</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A24757B7FFCD781B1BF9F8323CBD89CF" box="[440,511,1980,2001]" pageId="2" pageNumber="394" rank="genus">
<emphasis id="B974EACCFFCD781B1BF9F8323CBD89CF" bold="true" box="[440,511,1980,2001]" pageId="2" pageNumber="394">n. gen.</emphasis>
</taxonomicNameLabel>
, from Vietnam. 1. 
<taxonomicName id="4C004D5DFFCD781B18FFF8333E7289CC" box="[702,816,1980,2002]" class="Insecta" family="Tortricidae" genus="Zebraodes" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="394" phylum="Arthropoda" rank="species" species="lucidalis" status="n. sp.">
