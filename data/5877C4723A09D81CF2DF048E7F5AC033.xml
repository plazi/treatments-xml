[[ Genus 
<taxonomicName id="6D1D3AD064515317549907D275473ADA" LSID-ZBK="urn:lsid:zoobank.org:act:29414993-65FF-4F0D-A669-69490FE216D3" class="Actinopterygii" family="Nothobranchiidae" genus="Episemion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Episemion Epiplatys Radda &amp; Pürzl 1987:18" order="Cyprinodontiformes" pageNumber="2" phylum="Chordata" rank="genus">Episemion Radda &amp; Puerzl</taxonomicName>
