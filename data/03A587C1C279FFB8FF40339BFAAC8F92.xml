<emphasis id="B978EAC5C278FFB8FF4034F8FEFD89EF" bold="true" box="[151,274,997,1021]" pageId="3" pageNumber="431">FIGURE 2.</emphasis>
<taxonomicName id="4C0C4D54C278FFB8FECF34F8FDF589EC" authority="Shi &amp; Zhu, 2021" authorityName="Shi &amp; Zhu" authorityYear="2021" box="[280,538,997,1022]" class="Insecta" family="Gryllacrididae" genus="Ocellarnaca" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="431" phylum="Arthropoda" rank="species" species="nigrofemora" status="sp. nov.">
<emphasis id="B978EAC5C278FFB8FECF34F8FDF589EC" box="[280,538,997,1022]" italics="true" pageId="3" pageNumber="431">Ocellarnaca nigrofemora</emphasis>
</taxonomicName>
