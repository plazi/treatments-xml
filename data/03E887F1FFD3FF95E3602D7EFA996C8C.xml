<taxonomicName id="4C414D64FFD3FF95E3602D7EFB0D6E27" authority="(C. Chr.) M. Kato" authorityName="M. Kato" baseAuthorityName="C. Chr." box="[804,1215,282,306]" class="Polypodiopsida" family="Athyriaceae" genus="Deparia" kingdom="Plantae" order="Polypodiales" pageId="13" pageNumber="370" phylum="Tracheophyta" rank="species" species="parvisora">
<emphasis id="B935EAF5FFD3FF95E3602D7EFC5C6E27" bold="true" box="[804,1006,282,306]" italics="true" pageId="13" pageNumber="370">Deparia parvisora</emphasis>
