; elytral length 
<quantity id="4CFC6393C023144FFC2CA051FBD7677B" box="[904,1078,1037,1063]" metricMagnitude="-3" metricUnit="m" metricValue="1.675" metricValueMax="1.78" metricValueMin="1.57" pageId="1" pageNumber="44" unit="mm" value="1.675" valueMax="1.78" valueMin="1.57">1.57–1.78 mm</quantity>
