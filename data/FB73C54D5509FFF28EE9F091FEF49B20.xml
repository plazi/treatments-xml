(
<figureCitation id="EBE168DE5509FFF08E1EF0B6FBEA9CFD" box="[891,1028,1711,1732]" captionStart="Figures 20–31" captionStartId="16.[164,254,1377,1396]" captionTargetBox="[323,1257,198,1342]" captionTargetId="figure-167@16.[323,1283,196,1347]" captionTargetPageId="16" captionText="Figures 20–31. Adults: 20, male, Biocellata alfarae; 21, female, Biocellata alfarae; 22, male, Biocellata bifida; 23, male, Biocellata davisorum; 24, male, Spinulata acutipennis; 25, female, Spinulata acutipennis; 26, male, Spinulata manes; 27, female, Spinulata manes; 28, male, Spinulata oblongata; 29, male, Spinulata quasivinnea; 30, female, Spinulata quasivinnea; 31, male, Simplicivalva ampliophilobia. Scale bar = 10.0 mm." figureDoi="http://doi.org/10.5281/zenodo.5446389" httpUri="https://zenodo.org/record/5446389/files/figure.png" pageId="33" pageNumber="255">FIGS 29, 30</figureCitation>
, 
<figureCitation id="EBE168DE5509FFF08977F0B6FBB09CFC" box="[1042,1118,1711,1733]" captionStart="Figures 65–76" captionStartId="20.[164,254,1372,1391]" captionTargetBox="[326,1281,198,1340]" captionTargetId="figure-213@20.[323,1283,196,1342]" captionTargetPageId="20" captionText="Figures 65–76. Male genitalia. Spinulata quasivinnea: 65, genital capsule; 66, aedoeagus. Simplicivalva ampliophilobia: 67, genital capsule; 68, aedoeagus. Magulacra niveogrisea: 69, genital capsule; 70, aedoeagus. Cossula arpi: 71, genital capsule; 72, aedoeagus. Cossula buspina: 73, genital capsule; 74, aedoeagus. Cossula coerulescens: 75, genital capsule; 76, aedoeagus." figureDoi="http://doi.org/10.5281/zenodo.5446397" httpUri="https://zenodo.org/record/5446397/files/figure.png" pageId="33" pageNumber="255">65, 66</figureCitation>
