(
<taxonomicName id="4C1B4D23505C623745D7F9CDFA6AA80F" authority="Benth." authorityName="Benth." box="[1180,1445,1583,1605]" class="Magnoliopsida" family="Fabaceae" genus="Cymbosema" kingdom="Plantae" order="Fabales" pageId="7" pageNumber="273" phylum="Tracheophyta" rank="species" species="roseum">
