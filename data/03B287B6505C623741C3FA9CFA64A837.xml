<taxonomicName id="4C1B4D23505C62374552F9CDFB41A80E" box="[1049,1166,1583,1604]" class="Magnoliopsida" family="Fabaceae" genus="Cymbosema" kingdom="Plantae" order="Fabales" pageId="7" pageNumber="273" phylum="Tracheophyta" rank="genus">
