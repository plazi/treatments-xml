<taxonomicName id="59E304AABE10FFA4FEB7F9E8FD5BB300" authority="Schellenberg, 1931" authorityName="Schellenberg" authorityYear="1931" box="[299,668,1589,1613]" class="Malacostraca" family="Paramelitidae" genus="Falklandella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="55" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis id="AC97A33BBE10FFA4FEB7F9E8FE7DB300" box="[299,442,1589,1613]" italics="true" pageId="55" pageNumber="56">Falklandella</emphasis>
Schellenberg, 
<number id="282B6D0EBE10FFA4FDFDF9E8FD5BB300" box="[609,668,1589,1613]" pageId="55" pageNumber="56" value="1931.0">1931</number>
