<materialsCitation id="3B1F3CF28338FF82FEF76B01F2E676D0" ID-GBIF-Occurrence="3118982303" box="[151,939,1662,1689]" country="Germany" location="Germany" pageId="3" pageNumber="178" specimenCount="10" specimenCount-female="5" specimenCount-male="5">
