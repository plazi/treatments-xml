<quantity id="4C8F9B4A833EFF84FCA76A66F2557779" box="[711,792,1816,1840]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="5" pageNumber="180" unit="mm" value="0.2">0.2 mm</quantity>
