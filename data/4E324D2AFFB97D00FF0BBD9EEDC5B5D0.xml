(
<bibRefCitation id="A20A81CDFFB97D1FFE4EBD9EEB08B663" author="Gisin" box="[466,611,1783,1809]" pageId="15" pageNumber="49" refString="Gisin, H. (1954) Description de cinq especes inedites de Collemboles. Bulletin de la Societe Entomologique Suisse, 27, 49 - 52" type="journal article" year="1954">
