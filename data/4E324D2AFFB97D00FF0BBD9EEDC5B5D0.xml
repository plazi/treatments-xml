</taxonomicName>
, 
<taxonomicName id="019B87BFFFB97D1FFDE4BD9EED3EB663" authority="Gisin, 1964" authorityName="Gisin" authorityYear="1964" box="[632,1109,1783,1809]" class="Entognatha" family="Onychiuridae" genus="Deuteraphorura" kingdom="Animalia" order="Collembola" pageId="15" pageNumber="49" phylum="Arthropoda" rank="species" species="bosnaria">
