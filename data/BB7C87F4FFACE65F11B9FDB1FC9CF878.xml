</elevation>
E 
<elevation id="B8F8D1D1FFACE65F1599FC9FFAE9FCBF" box="[1207,1335,823,848]" metricMagnitude="6" metricUnit="m" metricValue="3.161892" pageId="19" pageNumber="210" unit="m" value="3161892.0">
<quantity id="F42D9B07FFACE65F1599FC9FFAE9FCBF" box="[1207,1335,823,848]" metricMagnitude="6" metricUnit="m" metricValue="3.161892" pageId="19" pageNumber="210" unit="m" value="3161892.0">3161892 m</quantity>
</elevation>
N
</materialsCitation>
, 
<quantity id="F42D9B07FFACE65F144CFC90FA42FCBF" box="[1378,1436,824,848]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="210" unit="m" value="10.0">10 m</quantity>
