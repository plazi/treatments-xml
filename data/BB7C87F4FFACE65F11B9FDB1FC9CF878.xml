(
<location id="360A6039FFACE65F131CFC90FD0CFCA0" LSID="urn:lsid:plazi:treatment:BB7C87F4FFACE65F11B9FDB1FC9CF878:360A6039FFACE65F131CFC90FD0CFCA0" box="[562,722,824,848]" municipality="Bajas Amarillas" name="Fuerteventura" pageId="19" pageNumber="210" stateProvince="Canary Islands">Fuerteventura</location>
, 
<collectingRegion id="F111F800FFACE65F13F0FC90FC55FCA0" box="[734,907,823,848]" country="Spain" name="Canarias" pageId="19" pageNumber="210">Canary Islands</collectingRegion>
). UTM 28 R 
<elevation id="B8F8D1D1FFACE65F150AFC9FFB48FCBF" box="[1060,1174,823,848]" metricMagnitude="5" metricUnit="m" metricValue="5.93643" pageId="19" pageNumber="210" unit="m" value="593643.0">
