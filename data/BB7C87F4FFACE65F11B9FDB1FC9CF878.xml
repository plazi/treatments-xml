<quantity id="F42D9B07FFACE65F1428FD2CFAB5FD6C" box="[1286,1387,644,669]" metricMagnitude="-2" metricUnit="m" metricValue="1.06" pageId="19" pageNumber="210" unit="mm" value="10.6">10.6 mm</quantity>
