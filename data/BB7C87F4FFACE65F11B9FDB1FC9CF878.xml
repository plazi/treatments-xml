<materialsCitation id="83BD3CBFFFACE65F11E9FC9FFA88FCBF" ID-GBIF-Occurrence="1845007325" box="[199,1366,823,848]" elevation="593643" location="Fuerteventura" municipality="Bajas Amarillas" pageId="19" pageNumber="210" specimenCount="1" stateProvince="Canary Islands" typeStatus="holotype">
