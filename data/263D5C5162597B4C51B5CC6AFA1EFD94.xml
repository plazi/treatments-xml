<number id="185CFF6062597B4D51B5CC6AFF33F9D7" box="[151,182,1551,1577]" pageId="22" pageNumber="23" value="18.0">18</number>
. 
<taxonomicName id="699496C462597B4D51E1CC6AFE4DF9D7" box="[195,456,1551,1577]" class="Insecta" family="Chrysomelidae" genus="Lema" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="michioi" subGenus="Lema">
