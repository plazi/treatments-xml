<taxonomicName id="5EAC4D264D07481DFED9F91549AB33E2" authority="Lehnert &amp; van Soest, 1996" authorityName="Lehnert &amp; van Soest" authorityYear="1996" box="[285,730,1762,1786]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="12" pageNumber="13" phylum="Porifera" rank="species" species="sventres">
