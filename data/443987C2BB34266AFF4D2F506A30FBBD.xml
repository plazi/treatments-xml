<collectionCode id="AA81AE11BB34266BFF4528876EC1FAF3" box="[159,246,1261,1287]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="14" pageNumber="15">SMNH</collectionCode>
<number id="7A5824F3BB34266BFF2728876F0BFAF3" box="[253,316,1261,1287]" pageId="14" pageNumber="15" value="7498.0">7498</number>
), another one designated 
<typeStatus id="132B8876BB34266BFDBF28876CFDFAF3" box="[613,714,1261,1287]" pageId="14" pageNumber="15">paratype</typeStatus>
(HU no. 
<number id="7A5824F3BB34266BFCE228876D5EFAF3" box="[824,873,1261,1287]" pageId="14" pageNumber="15" value="409.0">409</number>
