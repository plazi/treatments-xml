<paragraph id="8BC936ECFFEC4C7AFF1DEF4C6EFCFAC5" blockId="16.[151,1437,1286,1493]" pageId="16" pageNumber="17">
<emphasis id="B902EAFEFFEC4C7AFF1DEF4C6E24FA8C" bold="true" box="[151,276,1286,1312]" pageId="16" pageNumber="17">Diagnosis:</emphasis>
