<taxonomicName id="4C764D6FFFEC4C7AFD32EF4D6C10FA8D" authorityName="Bosquet" authorityYear="1852" box="[696,800,1287,1313]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
