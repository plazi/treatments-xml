<number id="3DEE24DF01533004FAB7D373FAB8F8B3" box="[1323,1381,1856,1880]" pageId="2" pageNumber="506" value="1879.0">1879</number>
</bibRefCitation>
)
</taxonomicName>
and 
<taxonomicName id="4C264D7B01533004FF0BD356FE66F896" authority="Alluaud, 1930" authorityName="Alluaud" authorityYear="1930" box="[151,443,1892,1917]" class="Insecta" family="Carabidae" genus="Craspedophorus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="506" phylum="Arthropoda" rank="species" species="stanleyi">
<emphasis id="B952EAEA01533004FF0BD356FED2F896" box="[151,271,1892,1917]" italics="true" pageId="2" pageNumber="506">C. stanleyi</emphasis>
<bibRefCitation id="EFB74B0901533004FE8BD357FE66F896" author="Alluaud" box="[279,443,1892,1917]" pageId="2" pageNumber="525" refString="Alluaud, C. (1930) Note sur les Panageides dAfrique tropicale. Afra, Cahiers d'Entomologie, 1, 1 - 9." type="journal article" year="1930">
Alluaud, 
<number id="3DEE24DF01533004FE1CD356FE66F896" box="[384,443,1893,1917]" pageId="2" pageNumber="506" value="1930.0">1930</number>
</bibRefCitation>
</taxonomicName>
, or even entirely black as in 
<taxonomicName id="4C264D7B01533004FC9FD356FB9FF896" authority="Alluaud, 1930" authorityName="Alluaud" authorityYear="1930" box="[771,1090,1892,1917]" class="Insecta" family="Carabidae" genus="Craspedophorus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="506" phylum="Arthropoda" rank="species" species="ethmoides">
<emphasis id="B952EAEA01533004FC9FD356FC4BF896" box="[771,918,1892,1917]" italics="true" pageId="2" pageNumber="506">C. ethmoides</emphasis>
<bibRefCitation id="EFB74B0901533004FC02D357FB9FF896" author="Alluaud" box="[926,1090,1892,1917]" pageId="2" pageNumber="525" refString="Alluaud, C. (1930) Note sur les Panageides dAfrique tropicale. Afra, Cahiers d'Entomologie, 1, 1 - 9." type="journal article" year="1930">
Alluaud, 
<number id="3DEE24DF01533004FB9BD356FB9FF896" box="[1031,1090,1893,1917]" pageId="2" pageNumber="506" value="1930.0">1930</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C264D7B01533004FBD2D356FA41F896" authority="Boheman, 1848" authorityName="Boheman" authorityYear="1848" box="[1102,1436,1892,1917]" class="Insecta" family="Carabidae" genus="Craspedophorus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="506" phylum="Arthropoda" rank="species" species="impictus">
<emphasis id="B952EAEA01533004FBD2D356FB12F897" box="[1102,1231,1893,1916]" italics="true" pageId="2" pageNumber="506">C. impictus</emphasis>
(
<bibRefCitation id="EFB74B0901533004FB43D357FA49F896" author="Boheman" box="[1247,1428,1892,1917]" pageId="2" pageNumber="525" refString="Boheman, C. H. (1848) Insecta Caffraria annis 1838 - 1845 a J. A. Wahlberg collecta amici auxillio suffultus. Pars I, Fasc. 1. (Carabici, Hydrocanthari, Gyrinii et Staphylinii). Fritze &amp; Norstedt, Holmiae, viii + 625 pp. [1851]" type="book chapter" year="1848">
Boheman, 
