, 
<collectingCountry id="8CCB763686690B3B7F847C95FA86FE5F" box="[1188,1298,456,482]" name="Germany" pageId="2" pageNumber="20">Germany</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7E3D7C95FF2AFDB7" name="United Kingdom" pageId="2" pageNumber="20">Great Britain</collectingCountry>
, Hawaii, 
<collectingCountry id="8CCB763686690B3B7A147CADFEEDFDB7" box="[308,377,496,522]" name="Japan" pageId="2" pageNumber="20">Japan</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7AA97CADFDDDFDB7" box="[393,585,496,522]" name="Netherlands" pageId="2" pageNumber="20">the Netherlands</collectingCountry>
, 
