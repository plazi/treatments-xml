Antilles, 
<collectingCountry id="8CCB763686690B3B78AF7CADFBD8FDB7" box="[911,1100,496,522]" name="New Caledonia" pageId="2" pageNumber="20">New Caledonia</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7F7B7CADFADCFDB7" box="[1115,1352,496,522]" name="Papua New Guinea" pageId="2" pageNumber="20">Papua New Guinea</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7E777CADFF71FD8F" name="South Africa" pageId="2" pageNumber="20">South Africa</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7BD07F45FE13FD8F" box="[240,391,536,562]" name="South Korea" pageId="2" pageNumber="20">South Korea</collectingCountry>
, 
<collectingCountry id="8CCB763686690B3B7AB27F45FE7BFD8F" box="[402,495,536,562]" name="Sweden" pageId="2" pageNumber="20">Sweden</collectingCountry>
and the 
<collectingCountry id="8CCB763686690B3B79737F45FD7BFD8F" box="[595,751,536,562]" name="United States of America" pageId="2" pageNumber="20">United States</collectingCountry>
(
<figureCitation id="6CE72A2386690B3B78207F45FCD0FD8F" box="[768,836,536,562]" captionStart="FIGURE 1" captionStartId="2.[151,255,1207,1231]" captionTargetBox="[218,1369,609,1178]" captionTargetId="figure@2.[217,1369,607,1180]" captionTargetPageId="2" captionText="FIGURE 1. Map showing natural occurrences (bullets) and inadvertent introductions (stars) of Orchidophilus species." httpUri="https://zenodo.org/record/182380/files/figure.png" pageId="2" pageNumber="20">
