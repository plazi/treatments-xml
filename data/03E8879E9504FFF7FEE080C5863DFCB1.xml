<taxonomicName id="4C414D0B9507FFF7FED086CD85BCFD3A" box="[312,450,724,749]" class="Insecta" family="Chrysomelidae" genus="Metallactus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
