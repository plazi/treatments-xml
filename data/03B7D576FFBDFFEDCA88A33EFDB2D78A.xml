<taxonomicName id="4C1E1FE3FFBDFFE2C927A21EFC61D400" box="[824,992,861,886]" class="Arachnida" family="Ctenidae" genus="Ohvida" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="brevitarsus">
