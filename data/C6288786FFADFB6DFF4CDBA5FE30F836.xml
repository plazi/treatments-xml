<taxonomicName id="89814D13FFADFB6DFB6FDA03FA63F878" box="[1204,1436,1914,1937]" class="Insecta" family="Crambidae" genus="Notaspis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="114" phylum="Arthropoda" rank="species" species="marginatus">
