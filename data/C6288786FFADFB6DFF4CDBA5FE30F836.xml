<taxonomicName id="89814D13FFADFB6DFCFADAD9FB79F851" authority="De Geer, 1768" authorityName="De Geer" authorityYear="1768" box="[801,1158,1951,1976]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="13" pageNumber="114" phylum="Arthropoda" rank="species" species="vegetans">
