</taxonomicName>
<taxonomicNameLabel id="A23D57D9FFCC8A3E0ACBF8EBFD82FD3A" box="[493,603,1810,1836]" pageId="15" pageNumber="51" rank="genus">gen. nov.</taxonomicNameLabel>
and 
<taxonomicName id="4C7A4D33FFCC8A3E09BFF8EBFD33FD3A" box="[665,746,1810,1836]" class="Insecta" family="Phaneropteridae" genus="Xantia" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="51" phylum="Arthropoda" rank="genus">
<emphasis id="B90EEAA2FFCC8A3E09BFF8EBFD33FD3A" bold="true" box="[665,746,1810,1836]" italics="true" pageId="15" pageNumber="51">Xantia</emphasis>
</taxonomicName>
</emphasis>
