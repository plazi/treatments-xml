<taxonomicName id="4C744CD0D43B9A3E41A0FD75FBD855A2" box="[981,1121,695,719]" class="Insecta" family="Geometridae" genus="Syzeuxis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="seminanis">
