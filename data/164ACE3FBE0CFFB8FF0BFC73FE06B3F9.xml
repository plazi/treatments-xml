<subSubSection id="D6F92CA2BE0CFFB8FF0BFBC9FCD4B305" pageId="43" pageNumber="44" type="materials_examined">
<paragraph id="9E5C7F29BE0CFFB8FF0BFBC9FC8EB161" blockId="43.[151,1437,1013,1716]" box="[151,841,1044,1068]" pageId="43" pageNumber="44">
<emphasis id="AC97A33BBE0CFFB8FF0BFBC9FEE3B161" bold="true" box="[151,292,1044,1068]" pageId="43" pageNumber="44">
<typeStatus id="4158C18BBE0CFFB8FF0BFBC9FF15B161" box="[151,210,1044,1068]" pageId="43" pageNumber="44">Type</typeStatus>
