<paragraph id="A629F7CB51E2EB8BB330B40DA9663CD8" pageId="10" pageNumber="147">
<taxonomicName id="ED904D76686D08854E350CDDA5C96D87" family="Sphingidae" lsidName="" pageId="10" pageNumber="147" rank="family">Sphingidae</taxonomicName>
: 
<taxonomicName id="45913F775F408CC935CBBAB392341CFC" class="Insecta" family="Sphingidae" genus="Erinnyis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erinnyis alope" order="Lepidoptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="species" species="alope">Erinnyis alope</taxonomicName>
, 
<taxonomicName id="75A159AFB0A6145D0C1335BB2C5BF831" class="Insecta" family="Sphingidae" genus="Erinnyis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erinnyis ello" order="Lepidoptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="species" species="ello">Erinnyis ello</taxonomicName>
