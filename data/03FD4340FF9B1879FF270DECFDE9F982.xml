<quantity id="4CAC5FB3FF9B1879FDE30BF5FD3CFB08" box="[595,652,1219,1245]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="33" pageNumber="34" unit="km" value="2.0">2 km</quantity>
<superScript id="7C215F1EFF9B1879FD3B0BF4FD23FB05" attach="left" box="[651,659,1218,1232]" fontSize="6" pageId="33" pageNumber="34">2</superScript>
