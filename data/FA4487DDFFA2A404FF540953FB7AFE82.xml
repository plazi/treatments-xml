</taxonomicName>
, 
<taxonomicName id="B5ED4D48FFA2A403FD130AF4FCE1F8E5" authorityName="Gritzenko" authorityYear="1971" box="[720,864,1766,1792]" class="Secernentea" family="Anguinidae" genus="Ditylenchus" kingdom="Animalia" order="Tylenchida" pageId="11" pageNumber="96" phylum="Nematoda" rank="species" species="tenuidens">
<emphasis id="4099EAD9FFA2A403FD130AF4FCE1F8E5" box="[720,864,1766,1792]" italics="true" pageId="11" pageNumber="96">D. tenuidens</emphasis>
</taxonomicName>
and 
<taxonomicName id="B5ED4D48FFA2A403FC5F0AF4FBA2F8E5" authorityName="Hirschmann &amp; Sasser" authorityYear="1955" box="[924,1059,1766,1792]" class="Secernentea" family="Anguinidae" genus="Ditylenchus" kingdom="Animalia" order="Tylenchida" pageId="11" pageNumber="96" phylum="Nematoda" rank="species" species="triformis">
