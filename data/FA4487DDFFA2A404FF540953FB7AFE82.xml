<collectionCode id="14FCAE0EFFA2A403FEEA09DBFEDDFA07" box="[297,348,1480,1506]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13893" lsid="urn:lsid:biocol.org:col:13893" name="Puslinch House" pageId="11" pageNumber="96" type="Herbarium">PUS</collectionCode>
