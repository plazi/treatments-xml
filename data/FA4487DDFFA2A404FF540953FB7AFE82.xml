<taxonomicName id="B5ED4D48FFA2A403FE970AF4FDA0F8E5" baseAuthorityName="Goodey" baseAuthorityYear="1958" box="[340,545,1766,1792]" class="Secernentea" family="Anguinidae" genus="Ditylenchus" kingdom="Animalia" order="Tylenchida" pageId="11" pageNumber="96" phylum="Nematoda" rank="species" species="myceliophagus">
<emphasis id="4099EAD9FFA2A403FE970AF4FDA0F8E5" box="[340,545,1766,1792]" italics="true" pageId="11" pageNumber="96">D. myceliophagus</emphasis>
</taxonomicName>
, 
<taxonomicName id="B5ED4D48FFA2A403FDF20AF4FD41F8E5" authorityName="Brzeski" authorityYear="1991" box="[561,704,1766,1792]" class="Secernentea" family="Anguinidae" genus="Ditylenchus" kingdom="Animalia" order="Tylenchida" pageId="11" pageNumber="96" phylum="Nematoda" rank="species" species="silvaticus">
