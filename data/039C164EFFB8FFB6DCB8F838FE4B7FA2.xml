<paragraph id="8B8AA758FFB8FFB4DCB8F838FDB77F5A" blockId="57.[138,557,1327,1350]" box="[138,557,1327,1350]" pageId="57" pageNumber="58">
<heading id="D0C21034FFB8FFB4DCB8F838FDB77F5A" box="[138,557,1327,1350]" centered="true" fontSize="9" level="2" pageId="57" pageNumber="58" reason="2">
<taxonomicName id="4C35DCDBFFB8FFB4DCB8F838FE0C7F59" box="[138,406,1327,1350]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="coxatus" status="new species">
<emphasis id="B9417B4AFFB8FFB4DCB8F838FE0C7F59" bold="true" box="[138,406,1327,1350]" italics="true" pageId="57" pageNumber="58">Paramblynotus coxatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFB8FFB4DD94F827FDB77F5A" box="[422,557,1328,1350]" pageId="57" pageNumber="58" rank="species">new species</taxonomicNameLabel>
