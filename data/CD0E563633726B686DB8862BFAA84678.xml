<number id="F36FF50733726B6F6DFD857EFEA74005" box="[210,276,1834,1858]" pageId="9" pageNumber="30" value="0.073">0.073</number>
; AME–AME 
<number id="F36FF50733726B6F6C99857EFE4B4005" box="[438,504,1834,1858]" pageId="9" pageNumber="30" value="0.056">0.056</number>
, AME–ALE 
<number id="F36FF50733726B6F6FBE857EFD604005" box="[657,723,1834,1858]" pageId="9" pageNumber="30" value="0.019">0.019</number>
, PME–PME 
<number id="F36FF50733726B6F6E44857EFC1E4005" box="[875,941,1834,1858]" pageId="9" pageNumber="30" value="0.053">0.053</number>
, PME–PLE 
<number id="F36FF50733726B6F6913857EFBCD4005" box="[1084,1150,1834,1858]" pageId="9" pageNumber="30" value="0.075">0.075</number>
