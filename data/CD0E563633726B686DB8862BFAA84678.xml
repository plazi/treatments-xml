, 
<number id="F36FF50733716B6C690282E8FBD34793" box="[1069,1120,188,212]" pageId="10" pageNumber="31" value="1.01">1.01</number>
, 
<number id="F36FF50733716B6C694082E8FB104793" box="[1135,1187,188,212]" pageId="10" pageNumber="31" value="0.59">0.59</number>
