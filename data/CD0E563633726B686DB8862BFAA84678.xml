<collectionCode id="23B67FE533716B6C69AA8373FB0E4678" box="[1157,1213,295,319]" httpUri="http://grbio.org/cool/9hjz-r0xi" name="Prirodonamen Muzej Skopje" pageId="10" pageNumber="31">PMS</collectionCode>
