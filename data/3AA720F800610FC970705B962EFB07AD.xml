<taxonomicName id="38E7029401AA9466A9E7582B01A00E12" class="Magnoliopsida" family="Compositae" genus="Vernonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vernonia triflora" order="Asterales" pageId="23" pageNumber="72" phylum="Tracheophyta" rank="species" species="triflora">
