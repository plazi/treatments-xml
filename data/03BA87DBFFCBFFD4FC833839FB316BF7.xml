<quantity id="4CEB9B28FFCAFFD4FAB03A38FAD7681B" box="[1305,1366,1338,1362]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="1" pageNumber="542" unit="mm" value="8.0">8 mm</quantity>
