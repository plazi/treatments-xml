<caption id="BBBA51D3FFB2FFD9FF32F874FBAC3691" ID-DOI="http://doi.org/10.5281/zenodo.5127442" ID-Zenodo-Dep="5127442" httpUri="https://zenodo.org/record/5127442/files/figure.png" pageId="32" pageNumber="33" startId="32.[151,255,1974,1998]" targetBox="[252,1360,187,1961]" targetPageId="32">
<paragraph id="EF7A015BFFB2FFD9FF32F874FBAC3691" blockId="32.[151,1436,1974,2036]" pageId="32" pageNumber="33">
