</taxonomicName>
, 
<taxonomicName id="4C2E4D3DFB75FFA0FE91F936FD43FBA4" authority="Kaur &amp; Jonathan, 1979" authorityName="Kaur &amp; Jonathan" authorityYear="1979" box="[359,721,1672,1699]" class="Insecta" family="Ichneumonidae" genus="Netelia" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="578" phylum="Arthropoda" rank="species" species="foersteri">
<emphasis id="B95AEAACFB75FFA0FE91F936FE54FBA4" box="[359,454,1672,1698]" italics="true" pageId="1" pageNumber="578">foersteri</emphasis>
<bibRefCitation id="EFBF4B4FFB75FFA0FE3AF937FD43FBA4" author="Kaur, R. &amp; Jonathan, J. K." box="[460,721,1672,1699]" pageId="1" pageNumber="578" pagination="1 - 276" refId="ref3337" refString="Kaur, R. &amp; Jonathan, J. K. (1979) Ichneumonologica Orientalis, Part VIII. The tribe Phytodietini from India (Hymenoptera: Ichneumonidae). Oriental Insects Monograph, 9, 1 - 276." type="journal article" year="1979">Kaur &amp; Jonathan, 1979</bibRefCitation>
</taxonomicName>
and the 
<taxonomicName id="4C2E4D3DFB75FFA0FCD9F936FC06FBA4" authorityName="Uchida" authorityYear="1953" box="[815,916,1672,1698]" class="Insecta" family="Ichneumonidae" genus="Netelia" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="578" phylum="Arthropoda" rank="species" species="ishiharai">
<emphasis id="B95AEAACFB75FFA0FCD9F936FC06FBA4" box="[815,916,1672,1698]" italics="true" pageId="1" pageNumber="578">ishiharai</emphasis>
</taxonomicName>
group: 
<taxonomicName id="4C2E4D3DFB75FFA0FC1DF936FB62FBA5" authority="Uchida, 1953" authorityName="Uchida" authorityYear="1953" box="[1003,1264,1672,1699]" class="Insecta" family="Ichneumonidae" genus="Netelia" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="578" phylum="Arthropoda" rank="species" species="ishiharai">
<emphasis id="B95AEAACFB75FFA0FC1DF936FBC2FBA4" box="[1003,1104,1672,1698]" italics="true" pageId="1" pageNumber="578">ishiharai</emphasis>
