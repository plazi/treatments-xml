<taxonomicName id="0D036D636A0F9681EDD617023A03AF64" class="Insecta" family="Dytiscidae" genus="Madaglymbus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Madaglymbus kelimaso" order="Coleoptera" pageId="12" pageNumber="101" phylum="Arthropoda" rank="species" species="kelimaso">
<emphasis id="4A736AA443F9DAAB2F6916ED1CAC0B96" italics="true" pageId="12" pageNumber="101">Madaglymbus kelimaso</emphasis>
</taxonomicName>
sp. nov., 
<taxonomicName id="138B162B250F739B70E57A2ED5C5D617" class="Insecta" family="Dytiscidae" genus="Madaglymbus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Madaglymbus semifactus" order="Coleoptera" pageId="12" pageNumber="101" phylum="Arthropoda" rank="species" species="semifactus">
<emphasis id="E25097D1A3837DACF01DC03B99CAAD40" italics="true" pageId="12" pageNumber="101">Madaglymbus semifactus</emphasis>
</taxonomicName>
