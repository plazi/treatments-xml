<taxonomicName id="4C2C89E1FFCDCB46FEA0A04FF4E4265D" authorityName="Servadei" authorityYear="1967" box="[322,476,800,824]" class="Insecta" family="Veliidae" genus="Velia" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="312" phylum="Arthropoda" rank="species" species="gridellii">
