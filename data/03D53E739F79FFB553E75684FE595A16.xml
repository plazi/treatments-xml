<quantity id="4C8422809F79FFB5528C5240FD435B0B" box="[508,644,1048,1072]" metricMagnitude="2" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="1" pageNumber="295" unit="m" value="900.0" valueMax="1000.0" valueMin="800.0">800–1000m</quantity>
