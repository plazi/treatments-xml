, 
<location id="8EA3D9BE9F79FFB552145241FE355B0B" LSID="urn:lsid:plazi:treatment:03D53E739F79FFB553E75684FE595A16:8EA3D9BE9F79FFB552145241FE355B0B" box="[356,498,1047,1072]" name="Mount Emei" pageId="1" pageNumber="295" stateProvince="Sichuan Province">Mount Emei</location>
