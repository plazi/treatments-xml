<specimenCount id="DDA8FD5FA852FF8DFFC8FDBAFF600B9F" box="[93,221,623,645]" pageId="74" pageNumber="74" type="male">nine males</specimenCount>
and 
<specimenCount id="DDA8FD5FA852FF8DFEB6FDBAFE7D0B9F" box="[291,448,623,645]" pageId="74" pageNumber="74" type="female">eight females</specimenCount>
