and t 
<number id="3DFA3253FF8AFFF7FE4ED0C11FA762E8" box="[465,474,726,741]" pageId="7" pageNumber="36" value="1.0">1</number>
, ta 
<number id="3DFA3253FF8AFFF7FE61D0C11C6462E8" box="[510,537,726,741]" pageId="7" pageNumber="36" value="3.0" valueMax="5.0" valueMin="1.0">1–5</number>
; P 
