<taxonomicName id="F24C8547AD25AA4DB60EA8DB02871517" LSID="a3845fcd-e750-5867-ae07-172d4ca2ee61" authority="Erichson, 1832" authorityName="Erichson" authorityYear="1832" class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Copelatus" order="Coleoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="genus">Copelatus Erichson, 1832</taxonomicName>
