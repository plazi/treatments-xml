<geoCoordinate id="D3F46C6462AD32912D1B2D93EB816135" direction="north" orientation="latitude" precision="925" value="29.5">29°30'N</geoCoordinate>
, 
<geoCoordinate id="F8CD39AC5742FDB44F041C3107E18DA9" direction="east" orientation="longitude" precision="925" value="95.38333">95°23'E</geoCoordinate>
