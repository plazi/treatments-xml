<emphasis id="66FBEAA0E594FF0C68CC083D1C675F91" bold="true" box="[151,368,1403,1426]" pageId="168" pageNumber="169">FIGURES 317–319.</emphasis>
<taxonomicName id="938F4D31E594FF0C6920083B1E0B5F92" box="[379,796,1404,1426]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="woohoo" subGenus="Cosmocomoidea">
<emphasis id="66FBEAA0E594FF0C6920083B1CE95F92" box="[379,510,1405,1426]" italics="true" pageId="168" pageNumber="169">Gonatocerus</emphasis>
(
<emphasis id="66FBEAA0E594FF0C6A4B083A1FAE5F92" box="[528,697,1404,1426]" italics="true" pageId="168" pageNumber="169">Cosmocomoidea</emphasis>
) 
<emphasis id="66FBEAA0E594FF0C6A97083A1E0B5F92" box="[716,796,1404,1426]" italics="true" pageId="168" pageNumber="169">woohoo</emphasis>
