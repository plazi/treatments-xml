<subSubSection id="52607396469E5B0DB5B53CBE34EBCB2D" pageId="2" pageNumber="3" type="reference_group">
<paragraph id="28907D1CC5D877EEA104E2115A592A0D" pageId="2" pageNumber="3">
<taxonomicName id="17B191720FE26CEACBEC68B3A6C1ACB0" authority="Takeuchi, 1929" authorityName="Takeuchi" authorityYear="1929" class="Insecta" family="Tenthredinidae" genus="Formosempria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Formosempria" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">Formosempria</taxonomicName>
<bibRefCitation id="20268BE7B82ACBE7AA4CFF966DA7D7AB" author="Takeuchi, K" journalOrPublisher="Transactions of the Natural History Society of Formosa" pageId="9" pageNumber="10" pagination="83 - 91" title="New sawflies from Formosa (3)." volume="19" year="1929">Takeuchi 1929</bibRefCitation>
