<emphasis id="B680EAD2FF8DFFD67BDCF8AEFE3DF888" box="[390,410,1859,1884]" italics="true" pageId="15" pageNumber="112">H</emphasis>
. 
<taxonomicName id="43F44D43FF8DFFD67BF2F8AFFD9FF888" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[424,568,1858,1884]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus" subGenus="Tomomima">
(
<emphasis id="B680EAD2FF8DFFD67BEBF8AEFE66F888" box="[433,449,1859,1884]" italics="true" pageId="15" pageNumber="112">T</emphasis>
.) 
<emphasis id="B680EAD2FF8DFFD67B82F8AEFD9FF888" box="[472,568,1859,1884]" italics="true" pageId="15" pageNumber="112">spinosus</emphasis>
</taxonomicName>
<taxonomicName id="43F44D43FF8DFFD6781AF8AEFD07F888" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[576,672,1859,1884]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus">
