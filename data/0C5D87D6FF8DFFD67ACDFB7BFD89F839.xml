<taxonomicName id="43F44D43FF8DFFD67EFFF93BFA92F924" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[1189,1333,1750,1776]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus" subGenus="Tomomima">
(
<emphasis id="B680EAD2FF8DFFD67EF4F93AFB19F924" box="[1198,1214,1751,1776]" italics="true" pageId="15" pageNumber="112">T</emphasis>
.) 
<emphasis id="B680EAD2FF8DFFD67E8FF93AFA92F924" box="[1237,1333,1751,1776]" italics="true" pageId="15" pageNumber="112">spinosus</emphasis>
</taxonomicName>
<emphasis id="B680EAD2FF8DFFD67F67F93BFA3BF924" box="[1341,1436,1750,1776]" italics="true" pageId="15" pageNumber="112">robustus</emphasis>
<emphasis id="B680EAD2FF8DFFD67ACDF917FEBCF8C0" bold="true" box="[151,283,1786,1812]" pageId="15" pageNumber="112">subsp. nov.</emphasis>
are 7.7–7.9, 35.8–36.2 and 
<quantity id="430C9B25FF8DFFD6780CF917FD56F8C0" box="[598,753,1786,1812]" metricMagnitude="-2" metricUnit="m" metricValue="1.975" metricValueMax="2.06" metricValueMin="1.89" pageId="15" pageNumber="112" unit="mm" value="19.75" valueMax="20.6" valueMin="18.9">18.9–20.6mm</quantity>
respectively, but these are 7.3–8.3, 30.3–34.2 and 
<quantity id="430C9B25FF8DFFD67F76F917FF64F8EC" metricMagnitude="-2" metricUnit="m" metricValue="1.86" metricValueMax="1.96" metricValueMin="1.76" pageId="15" pageNumber="112" unit="mm" value="18.6" valueMax="19.6" valueMin="17.6">17.6–19.6 mm</quantity>
in 
<emphasis id="B680EAD2FF8DFFD67AB1F8F2FF58F8EC" box="[235,255,1823,1848]" italics="true" pageId="15" pageNumber="112">H</emphasis>
. 
<taxonomicName id="43F44D43FF8DFFD67B55F8F3FE05F8EC" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[271,418,1822,1848]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus" subGenus="Tomomima">
(
<emphasis id="B680EAD2FF8DFFD67B43F8F2FE8EF8EC" box="[281,297,1823,1848]" italics="true" pageId="15" pageNumber="112">T</emphasis>
.) 
<emphasis id="B680EAD2FF8DFFD67B18F8F2FE05F8EC" box="[322,418,1823,1848]" italics="true" pageId="15" pageNumber="112">spinosus</emphasis>
</taxonomicName>
<taxonomicName id="43F44D43FF8DFFD67BF1F8F2FDACF8EC" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[427,523,1823,1848]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus">
<emphasis id="B680EAD2FF8DFFD67BF1F8F2FDACF8EC" box="[427,523,1823,1848]" italics="true" pageId="15" pageNumber="112">spinosus</emphasis>
</taxonomicName>
, respectively). 
<emphasis id="B680EAD2FF8DFFD678E7F8F2FD76F8EC" box="[701,721,1823,1848]" italics="true" pageId="15" pageNumber="112">H</emphasis>
. 
<taxonomicName id="43F44D43FF8DFFD678BBF8F3FCD3F8EC" authorityName=": Gorochov &amp; Voltshenkova" authorityYear="1998" box="[737,884,1822,1848]" class="Insecta" family="Tettigoniidae" genus="Hemigyrus" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="112" phylum="Arthropoda" rank="species" species="spinosus" subGenus="Tomomima">
(
<emphasis id="B680EAD2FF8DFFD678B1F8F2FD5CF8EC" box="[747,763,1823,1848]" italics="true" pageId="15" pageNumber="112">T</emphasis>
