<quantity id="4CF79B33FFECFFC5C2CCFE49FF61FE48" box="[181,247,400,420]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="3" pageNumber="158" unit="m" value="1700.0">1700m</quantity>
