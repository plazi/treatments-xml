<taxonomicName id="4C060095A17090173D7B0853B228CCDC" box="[835,1079,880,904]" class="Magnoliopsida" family="Fabaceae" genus="Rhynchosia" kingdom="Plantae" order="Fabales" pageId="12" pageNumber="11" phylum="Tracheophyta" rank="species" species="minima">
