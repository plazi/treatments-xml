<taxonomicName id="AAFCECEF31CA194035C0B725F668E197" authority="Pall. (1766)" authorityName="Pall." authorityYear="1766" class="Hydrozoa" family="Milleporidae" genus="Millepora" higherTaxonomySource="CoL" kingdom="Plantae" order="Anthoathecata" phylum="Cnidaria" rank="species" species="calcarea">
