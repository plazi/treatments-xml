</taxonomicName>
and 
<taxonomicName id="B643A3C83AEA92EA42AE6F4EC2DEAA7F" genus="R." lsidName="R. ovata" pageId="0" pageNumber="19" rank="species" species="ovata">
<emphasis id="6A0A2FDE69F2CB5C317D95EE2BB8A36F" italics="true" pageId="0" pageNumber="19">R. ovata</emphasis>
</taxonomicName>
(= 
<taxonomicName id="A6D7BD0804F0E7A0D08070B1E2EE8178" genus="R." lsidName="R. umbellata" pageId="0" pageNumber="19" rank="species" species="umbellata">
<emphasis id="E4435A2ABB3F365887D561E6540DC66F" italics="true" pageId="0" pageNumber="19">R. umbellata</emphasis>
</taxonomicName>
).
</paragraph>
