<taxonomicName id="E376F4BF8C927155A7EAD860437453BD" class="Magnoliopsida" family="Rosaceae" genus="Rhaphiolepis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rhaphiolepis yui" order="Rosales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="yui">
<emphasis id="934AA51F36401B067862D2143FE887E7" italics="true" pageId="0" pageNumber="19">Rhaphiolepis yui</emphasis>
