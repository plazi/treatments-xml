<taxonomicName id="080726BA4BC7A17B604DB32A22A69028" class="Magnoliopsida" family="Rosaceae" genus="Rhaphiolepis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rhaphiolepis indica" order="Rosales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="indica">
