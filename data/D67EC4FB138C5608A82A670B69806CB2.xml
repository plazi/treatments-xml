<paragraph id="A3F51F0C6B692A9D7DD7652B407BD582" pageId="0" pageNumber="19">
<emphasis id="8328C9659D7CB7DD49AB4C883EC2D642" bold="true" pageId="0" pageNumber="19">Note T.</emphasis>
