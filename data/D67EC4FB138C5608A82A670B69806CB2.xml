≡ 
<taxonomicName id="7DA13F4628511373E6BD1F108E544F8D" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pyrus delacourii" order="Rosales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="delacourii">
