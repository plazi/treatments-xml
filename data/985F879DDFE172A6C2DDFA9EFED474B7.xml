<taxonomicName id="D7F64D08DFE772A6C386FCA8FD9C742B" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[460,636,800,824]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="solea">
