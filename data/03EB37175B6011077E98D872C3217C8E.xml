<typeStatus id="54F938A35B5E11077C38DFD4C26D7FFE" box="[567,615,477,501]" pageId="32" pageNumber="133">type</typeStatus>
locality in Conguillo National Park (
<quantity id="4CBA2BE45B5E11077A09DFD5C46D7FFF" box="[1030,1127,476,501]" metricMagnitude="3" metricUnit="m" metricValue="1.15" pageId="32" pageNumber="133" unit="m" value="1150.0">1,150 m</quantity>
), 
<collectingRegion id="498648E35B5E11077A74DFD5C5437FFE" box="[1147,1353,476,501]" country="Chile" name="Araucania" pageId="32" pageNumber="133">Araucania Region</collectingRegion>
, 
<collectingCountry id="F355C6915B5E11077B5BDFD5C59E7FFE" box="[1364,1428,476,501]" name="Chile" pageId="32" pageNumber="133">Chile</collectingCountry>
