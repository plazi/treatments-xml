(
<bibRefCitation id="EFE64B7A1C1F337C425EC01EA7956DAC" author="Ramil, F. &amp; Vervoort, W." box="[1147,1424,1399,1426]" pageId="14" pageNumber="426" pagination="1 - 262" refId="ref43541" refString="Ramil, F. &amp; Vervoort, W. (1992) Report on the Hydroida collected by the &quot; BALGIM &quot; expedition in and around the Strait of Gibraltar. Zoologische Verhandelingen, Leiden, 277, 1 - 262." type="journal article" year="1992">Ramil &amp; Vervoort 1992</bibRefCitation>
), 
<collectingCountry id="F360761B1C1F337C46B2C0F2A3176D8B" box="[151,274,1435,1461]" name="Mauritania" pageId="14" pageNumber="426">Mauritania</collectingCountry>
(
<bibRefCitation id="EFE64B7A1C1F337C4707C0F2A0056D88" author="Gil, M. &amp; Ramil, F." box="[290,512,1435,1462]" pageId="14" pageNumber="426" pagination="419 - 444" refId="ref39805" refString="Gil, M. &amp; Ramil, F. (2017 a) Hydrozoans from Mauritanian Deep-Waters. In: Ramos, A., Ramil, F. &amp; Sanz, J. L. (Eds.), Deep sea ecosystems off Mauritania: Researching marine biodiversity and habitats in West African Deep-waters. Springer, Dordrecht, pp. 419 - 444. https: // doi. org / 10.1007 / 978 - 94 - 024 - 1023 - 5 _ 11" type="book chapter" year="2017">Gil &amp; Ramil 2017a</bibRefCitation>
