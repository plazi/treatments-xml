<paragraph id="9F04076FD779A3E0B18E6FE89BA4B8AC" pageId="1" pageNumber="44">
<taxonomicName id="FFBE1DB004565B4CE529D2F17307A66F" LSID="http://zoobank.org/1076C4FA-D580-45AC-A918-B28C2D1FC396" class="Malacostraca" family="Bopyridae" genus="Paguristione" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paguristione" order="Isopoda" pageId="1" pageNumber="44" phylum="Arthropoda" rank="genus">Paguristione</taxonomicName>
<taxonomicNameLabel id="094A1041448B8500E4D7DA958C9D46FB" pageId="1" pageNumber="44">gen. n.</taxonomicNameLabel>
