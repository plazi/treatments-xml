<taxonomicName id="4C7FF8D6FFB8FFD0CD89FC16256BA4EC" box="[207,336,980,1005]" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="112" pageNumber="113" phylum="Tracheophyta" rank="genus">
<emphasis id="B90B5F47FFB8FFD0CD89FC16256BA4EC" box="[207,336,980,1005]" italics="true" pageId="112" pageNumber="113">Phragmites</emphasis>
</taxonomicName>
