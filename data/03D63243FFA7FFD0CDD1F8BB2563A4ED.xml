<taxonomicName id="4C7FF8D6FFB8FFD0CC39FC4E25C7A4A4" authority="Benth. &amp; Hook.f., 1865 (L.)" authorityName="Benth. &amp; Hook.f., 1865 (L.)" box="[383,508,908,933]" class="Magnoliopsida" family="Rosaceae" genus="Amygdalus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="112" pageNumber="113" phylum="Tracheophyta" rank="genus">
