<taxonomicName id="4C7FF8D6FFB8FFD0CC56FC6D263DA4C9" box="[272,518,943,968]" class="Insecta" family="Buprestidae" genus="Paracylindromorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="112" pageNumber="113" phylum="Arthropoda" rank="genus">
