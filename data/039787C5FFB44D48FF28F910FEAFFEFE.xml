<paragraph id="8B8136D3FFB44D4BFF28F973FD4BF903" blockId="9.[151,1437,1700,2013]" box="[151,715,1735,1760]" pageId="9" pageNumber="524">
<taxonomicName id="4C3E4D50FFB44D4BFF28F973FEA3F903" authorityName="Kottelat, Britz, Tan &amp; Witte" authorityYear="2006" box="[151,291,1735,1760]" class="Actinopterygii" family="Cyprinidae" genus="Paedocypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="9" pageNumber="524" phylum="Chordata" rank="genus">
