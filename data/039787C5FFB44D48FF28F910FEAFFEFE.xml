<emphasis id="B94AEAC1FFB44D4BFF28F910FECBF95E" bold="true" box="[151,331,1700,1725]" pageId="9" pageNumber="524">
<taxonomicName id="4C3E4D50FFB44D4BFF28F910FEA3F95E" authorityName="Kottelat, Britz, Tan &amp; Witte" authorityYear="2006" box="[151,291,1700,1725]" class="Actinopterygii" family="Cyprinidae" genus="Paedocypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="9" pageNumber="524" phylum="Chordata" rank="genus">
<emphasis id="B94AEAC1FFB44D4BFF28F910FEA3F95E" bold="true" box="[151,291,1700,1725]" italics="true" pageId="9" pageNumber="524">Paedocypris</emphasis>
