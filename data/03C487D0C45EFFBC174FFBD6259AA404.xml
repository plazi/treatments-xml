<heading id="D09A81AAC45EFFBB174FFBD626FDA21C" bold="true" box="[264,741,1084,1110]" fontSize="11" level="1" pageId="3" pageNumber="26" reason="1">
<taxonomicName id="4C6D4D45C45EFFBB174FFBD626FDA21C" authority="Bates, 1887" authorityName="Bates" authorityYear="1887" box="[264,741,1084,1110]" class="Insecta" family="Melolonthidae" genus="Macrodactylus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="sericeicollis">
