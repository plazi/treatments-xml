<quantity id="4CE24F9BB6580236FAEEFE86FF70B22E" metricMagnitude="-2" metricUnit="m" metricValue="1.605" metricValueMax="1.8" metricValueMin="1.41" pageId="10" pageNumber="11" unit="mm" value="16.05" valueMax="18.0" valueMin="14.1">14.1–18 mm</quantity>
