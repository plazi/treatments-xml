<taxonomicName authorityName="Cook" authorityYear="1983" box="[1334,1449,1663,1687]" class="Arachnida" family="Aturidae" genus="Kritaturus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="27" pageNumber="929" phylum="Arthropoda" rank="species" species="uncipalpis">
