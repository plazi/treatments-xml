<emphasis box="[496,667,1712,1735]" italics="true" pageId="26" pageNumber="27">Maevia vittata</emphasis>
(Hentz)
</taxonomicName>
(
<taxonomicName authorityName="Blackwall" authorityYear="1841" box="[789,905,1711,1735]" class="Arachnida" family="Salticidae" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="family">Salticidae</taxonomicName>
), 
<taxonomicName authority="Peckham &amp; Peckham" authorityName="Peckham &amp; Peckham" authorityYear="1909" box="[932,1436,1711,1736]" class="Arachnida" family="Salticidae" genus="Phidippus" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="whitmani">
<emphasis box="[932,1164,1712,1736]" italics="true" pageId="26" pageNumber="27">Phidippus whitmani</emphasis>
Peckham &amp; Peckham
</taxonomicName>
(
<taxonomicName authorityName="Blackwall" authorityYear="1841" box="[158,274,1748,1772]" class="Arachnida" family="Salticidae" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="family">Salticidae</taxonomicName>
), 
<taxonomicName box="[294,447,1748,1772]" class="Arachnida" family="Salticidae" genus="Phidippus" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[294,407,1748,1772]" italics="true" pageId="26" pageNumber="27">Phidippus</emphasis>
sp.
</taxonomicName>
(
<taxonomicName authorityName="Blackwall" authorityYear="1841" box="[462,578,1748,1772]" class="Arachnida" family="Salticidae" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="family">Salticidae</taxonomicName>
