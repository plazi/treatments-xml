<paragraph blockId="45.[151,1436,1015,1221]" pageId="45" pageNumber="46">
<taxonomicName authority="Walsh, 1862" authorityName="Walsh" authorityYear="1862" box="[151,535,1087,1112]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="maligna">
