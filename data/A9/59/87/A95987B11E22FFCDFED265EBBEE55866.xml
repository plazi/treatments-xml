<number box="[490,519,999,1025]" pageId="10" pageNumber="11" value="20.0">20</number>
(
<number box="[534,610,999,1025]" pageId="10" pageNumber="11" value="19.5" valueMax="20.0" valueMin="19.0">19–20</number>
