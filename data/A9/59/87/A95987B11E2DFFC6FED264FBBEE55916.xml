<number box="[1188,1217,1119,1145]" pageId="5" pageNumber="6" value="80.0">80</number>
(
<number box="[1238,1315,1119,1145]" pageId="5" pageNumber="6" value="80.5" valueMax="82.0" valueMin="79.0">79–82</number>
