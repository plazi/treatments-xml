<number box="[765,809,1119,1145]" pageId="5" pageNumber="6" value="265.0">265</number>
(
<number box="[830,938,1119,1145]" pageId="5" pageNumber="6" value="263.5" valueMax="269.0" valueMin="258.0">258–269</number>
