<number box="[566,581,1359,1385]" pageId="20" pageNumber="21" value="7.0">7</number>
(
<number box="[597,645,1359,1385]" pageId="20" pageNumber="21" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
