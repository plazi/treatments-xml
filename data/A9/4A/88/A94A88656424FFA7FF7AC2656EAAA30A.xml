</taxonomicName>
(S. 
<collectingCountry box="[825,896,360,383]" name="China" pageId="2" pageNumber="100">China</collectingCountry>
, 
<collectingRegion box="[909,1036,360,383]" country="China" name="Guangdong" pageId="2" pageNumber="100">Guangdong</collectingRegion>
, Shaoguan, Nanling National Nature Reserve, 
<quantity box="[253,337,394,417]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="2" pageNumber="100" unit="m" value="1100.0">1100 m</quantity>
, 
<date box="[350,515,394,417]" pageId="2" pageNumber="100" value="2003-06-18" valueMax="2003-06-22" valueMin="2003-06-18">18–22.VI.2003</date>
, K.Yazaki leg.) in SCAU and specimens in IZCAS of 
<taxonomicName authorityName="Sato &amp; Wang" authorityYear="2004" box="[1117,1233,394,417]" class="Insecta" family="Geometridae" genus="Arbomia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="100" phylum="Arthropoda" rank="species" species="kishidai">
<emphasis box="[1117,1233,394,417]" italics="true" pageId="2" pageNumber="100">A. kishidai</emphasis>
</taxonomicName>
(
<specimenCount box="[1253,1294,394,417]" pageId="2" pageNumber="100" type="male">2♂</specimenCount>
<specimenCount box="[1293,1320,394,417]" pageId="2" pageNumber="100" type="female">1♀</specimenCount>
, 
<collectingRegion box="[1334,1432,394,417]" country="China" name="Zhejiang" pageId="2" pageNumber="100">Zhejiang</collectingRegion>
, Qingyuan, Fengyangshan, Datianping, 
<date box="[603,771,429,452]" pageId="2" pageNumber="100" value="2003-08-06" valueMax="2003-08-10" valueMin="2003-08-06">
6–10.VIII. 
<quantity box="[714,771,429,452]" metricMagnitude="3" metricUnit="m" metricValue="2.003129" pageId="2" pageNumber="100" unit="m" value="2003.129">2003</quantity>
</date>
, 1290 m, coll. Han Hongxiang; 
<specimenCount box="[1184,1217,429,452]" pageId="2" pageNumber="100" type="female">1♀</specimenCount>
, 
<collectingRegion box="[1243,1316,429,452]" country="China" name="Hunan" pageId="2" pageNumber="100">Hunan</collectingRegion>
, Daoxian, Tiejiangchang, 
<date box="[320,463,464,487]" pageId="2" pageNumber="100" value="1982-08-18">18.VIII.1982</date>
; 
<specimenCount box="[478,516,464,487]" pageId="2" pageNumber="100" type="male">1♂</specimenCount>
<specimenCount box="[516,546,464,487]" pageId="2" pageNumber="100" type="female">1♀</specimenCount>
, 
<collectingRegion box="[562,633,464,487]" country="China" name="Fujian" pageId="2" pageNumber="100">Fujian</collectingRegion>
, Sanming, 
<date box="[761,893,464,487]" pageId="2" pageNumber="100" value="1981-07-16">16.VII.1981</date>
, 
<date box="[909,1034,464,487]" pageId="2" pageNumber="100" value="1981-06-18">18.VI.1981</date>
; 
<specimenCount box="[1049,1082,464,487]" pageId="2" pageNumber="100" type="female">1♀</specimenCount>
, 
<collectingRegion box="[1100,1227,464,487]" country="China" name="Guangdong" pageId="2" pageNumber="100">Guangdong</collectingRegion>
, Ruyuan, Nanling, Baohuzhan, 
<quantity box="[285,368,498,521]" metricMagnitude="3" metricUnit="m" metricValue="1.02" pageId="2" pageNumber="100" unit="m" value="1020.0">1020 m</quantity>
, 
<date box="[380,552,498,521]" pageId="2" pageNumber="100" value="2008-07-16" valueMax="2008-07-20" valueMin="2008-07-16">16–20.VII.2008</date>
, coll. Chen Fuqiang; 
<specimenCount box="[783,818,498,521]" pageId="2" pageNumber="100" type="male">1♂</specimenCount>
, 
<collectingRegion box="[830,925,498,521]" country="China" name="Guangxi" pageId="2" pageNumber="100">Guangxi</collectingRegion>
, Longsheng, Hongtan, 
<quantity box="[1173,1244,498,521]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="2" pageNumber="100" unit="m" value="900.0">900 m</quantity>
