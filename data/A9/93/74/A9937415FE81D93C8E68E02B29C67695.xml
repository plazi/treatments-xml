, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:3F1F6AF2-6F38-42C0-9C82-C1468F19FAF0" class="Actinopterygii" family="Ariidae" genus="Hemiarius" kingdom="Animalia" lsidName-ZBK="Hemiarius Bleeker 1862:7, 29" order="Siluriformes" pageNumber="25" phylum="Chordata" rank="genus">Hemiarius</taxonomicName>
, 
