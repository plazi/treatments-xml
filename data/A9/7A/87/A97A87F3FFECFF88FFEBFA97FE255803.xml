<location LSID="urn:lsid:plazi:treatment:A97A87F3FFECFF88FFEBFA97FE255803:240C603EFFEBFF8FFEB5F86DFEC459E9" box="[303,370,1957,1981]" country="Puerto Rico" municipality="Santiago-Blay" name="Banks" pageId="4" pageNumber="3" stateProvince="San Juan">Banks</location>
