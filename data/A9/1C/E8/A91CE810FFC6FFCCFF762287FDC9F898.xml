<taxonomicName authorityName="Jurine" authorityYear="1807" box="[1357,1436,1555,1581]" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="444" phylum="Arthropoda" rank="genus">
<emphasis box="[1357,1436,1555,1581]" italics="true" pageId="1" pageNumber="444">Anteon</emphasis>
</taxonomicName>
