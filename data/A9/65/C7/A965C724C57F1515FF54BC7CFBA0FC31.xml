<taxonomicName authority="LeConte, 1852: 207" authorityName="LeConte" authorityPageNumber="207" authorityYear="1852" box="[294,731,1656,1678]" class="Insecta" family="Dytiscidae" genus="Hydroporus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="striatellus">
