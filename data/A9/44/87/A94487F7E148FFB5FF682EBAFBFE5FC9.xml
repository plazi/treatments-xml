<location LSID="urn:lsid:plazi:treatment:A94487F7E148FFB5FF682EBAFBFE5FC9:2432603AE145FFB5FE222871FDA65FC9" box="[477,595,620,645]" country="Brazil" county="Hetschka" municipality="Blumenan" name="Blumenau" pageId="8" pageNumber="9" stateProvince="Santa Catarina">Blumenau</location>
, capital of 
<collectingRegion box="[718,883,620,645]" country="Brazil" name="Santa Catarina" pageId="8" pageNumber="9">Santa Catarina</collectingRegion>
State
</materialsCitation>
, Brazil.
