<collectingCountry box="[954,1025,583,608]" name="Brazil" pageId="8" pageNumber="9">Brasil</collectingCountry>
, 
<collectingCounty box="[1038,1145,583,608]" pageId="8" pageNumber="9">Hetschka</collectingCounty>
, 
<collectingMunicipality box="[1156,1276,583,608]" pageId="8" pageNumber="9">Blumenan</collectingMunicipality>
