<taxonomicName class="Insecta" family="Noctuidae" genus="Aseptis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aseptis torreyana" order="Lepidoptera" pageId="17" pageNumber="74" phylum="Arthropoda" rank="species" species="torreyana">
