<taxonomicName class="Insecta" family="Braconidae" genus="Apanteles" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apanteles iphitus" order="Hymenoptera" pageId="6" pageNumber="33" phylum="Arthropoda" rank="species" species="iphitus">Apanteles iphitus</taxonomicName>
