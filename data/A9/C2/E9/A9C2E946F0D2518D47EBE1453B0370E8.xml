<td align="left" pageId="null" pageNumber="919" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Primulaceae" genus="Primula" kingdom="Plantae" order="Ericales" pageId="null" pageNumber="919" phylum="Tracheophyta" rank="species" species="latifolia">
