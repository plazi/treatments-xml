<emphasis bold="true" box="[151,299,283,310]" pageId="2" pageNumber="229">
<typeStatus box="[151,210,284,310]" pageId="2" pageNumber="229">Type</typeStatus>
species
</emphasis>
. 
<emphasis box="[313,542,284,310]" italics="true" pageId="2" pageNumber="229">
<taxonomicName authority="Limeira-De-Oliveira &amp; Marques &amp; Gaimari &amp; Rafael, 2020" authorityName="Limeira-De-Oliveira &amp; Marques &amp; Gaimari &amp; Rafael" authorityYear="2020" box="[313,538,284,310]" class="Insecta" family="Odiniidae" genus="Pauximyia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="229" phylum="Arthropoda" rank="species" species="oliveirai" status="sp. nov.">Pauximyia oliveirai</taxonomicName>
,
