<taxonomicName authority="Villalobos, 1943" authorityName="Villalobos" authorityYear="1943" box="[355,1026,1906,1932]" class="Malacostraca" family="Cambaridae" genus="Cambarellus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="388" phylum="Arthropoda" rank="species" species="zempoalensis" subGenus="Cambarellus">
