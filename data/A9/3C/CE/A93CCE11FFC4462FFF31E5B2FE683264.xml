<emphasis italics="true" pageId="15" pageNumber="316">P. fantasia</emphasis>
Chapman
</taxonomicName>
(
<collectingCountry box="[369,501,943,967]" name="Philippines" pageId="15" pageNumber="316">Philippines</collectingCountry>
), 
<taxonomicName authority="Eguchi" authorityName="Eguchi" authorityYear="2001" box="[519,778,943,968]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="clypeocornis">
<emphasis box="[519,689,944,968]" italics="true" pageId="15" pageNumber="316">P. clypeocornis</emphasis>
Eguchi
