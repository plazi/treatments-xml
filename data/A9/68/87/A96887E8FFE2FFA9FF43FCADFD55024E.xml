<taxonomicName authority="Gibbs" authorityName="Gibbs" box="[308,529,1947,1969]" class="Insecta" family="Bombyliidae" genus="Usia" kingdom="Animalia" order="Diptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="cornigera" status="sp. nov.">
<emphasis box="[308,463,1948,1969]" italics="true" pageId="56" pageNumber="57">Usia cornigera</emphasis>
Gibbs
</taxonomicName>
<taxonomicNameLabel box="[536,616,1947,1969]" pageId="56" pageNumber="57" rank="species">
<emphasis bold="true" box="[536,616,1947,1969]" pageId="56" pageNumber="57">sp. nov.</emphasis>
</taxonomicNameLabel>
; 
<emphasis bold="true" box="[636,649,1947,1969]" pageId="56" pageNumber="57">a</emphasis>
epiphallic complex ventral, 
<emphasis bold="true" box="[957,971,1947,1969]" pageId="56" pageNumber="57">b</emphasis>
epiphallic complex lateral, 
<emphasis bold="true" box="[1273,1284,1947,1969]" pageId="56" pageNumber="57">c</emphasis>
gonocoxite &amp; gonostyli ventral, 
<emphasis bold="true" box="[348,362,1978,2000]" pageId="56" pageNumber="57">d</emphasis>
gonocoxite &amp; gonostyli lateral, 
<emphasis bold="true" box="[718,729,1978,2000]" pageId="56" pageNumber="57">e</emphasis>
epandrium dorsal, 
<emphasis bold="true" box="[938,947,1978,2000]" pageId="56" pageNumber="57">f</emphasis>
epandrium lateral, 
<emphasis bold="true" box="[1157,1170,1978,2000]" pageId="56" pageNumber="57">g</emphasis>
female genitalia ventral, 
<emphasis bold="true" box="[151,165,2009,2031]" pageId="56" pageNumber="57">h</emphasis>
female sternite 
