(
<figureCitation id="26BB2BF9FF5B2E82B8B7FD71FF6AB1BF" box="[159,245,654,678]" captionStart="FIGURES 51–55" captionStartId="237.[151,258,1988,2009]" captionTargetBox="[172,1416,193,1973]" captionTargetId="figure-51@237.[172,1416,193,1973]" captionTargetPageId="237" captionText="FIGURES 51–55. Lateral view. 51, M. euryoikos. 52, M. apiopharkis*. 53, M. lordos. 54, M. teutonicus*. 55, M. unidentatus*." figureDoi="http://doi.org/10.5281/zenodo.5262149" httpUri="https://zenodo.org/record/5262149/files/figure.png" pageId="197" pageNumber="198">Figs 55</figureCitation>
, 
<figureCitation id="26BB2BF9FF5B2E82B929FD71FEB2B1BF" box="[257,301,654,678]" captionStart="FIGURES 189–197" captionStartId="261.[151,257,1919,1940]" captionTargetBox="[151,1436,193,1898]" captionTargetId="figure-77@261.[151,1436,193,1898]" captionTargetPageId="261" captionText="FIGURES 189–197. Propodeum, dorsal view. 189, M. bimaculatus. 190, M. unidentatus*. 191, M. semiaurantigaster. 192, M. occidentalis. 193, M. tibiator. 194, M. dominicanus*. 195, M. jalapa. 196, M. allomeros*. 197, M. mesonotator." figureDoi="http://doi.org/10.5281/zenodo.5262197" httpUri="https://zenodo.org/record/5262197/files/figure.png" pageId="197" pageNumber="198">190</figureCitation>
