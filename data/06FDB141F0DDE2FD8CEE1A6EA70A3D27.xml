<taxonomicName id="E12113E16A22678C3966B9A863E9CBDF" class="Insecta" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. arnaudi" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="arnaudi">
