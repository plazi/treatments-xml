<taxonomicName id="A907BE2C4CC56113D979488D093A65E0" class="Insecta" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. arnaudi subsp. rioensis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="arnaudi" subSpecies="rioensis">
