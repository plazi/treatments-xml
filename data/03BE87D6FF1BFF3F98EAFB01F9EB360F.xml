<quantity id="4CEF9B25FF1BFF3F989BFAACFB63357C" box="[230,290,1374,1401]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="138" pageNumber="139" unit="m" value="15.0">15 m</quantity>
