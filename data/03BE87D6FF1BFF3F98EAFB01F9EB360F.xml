<accessionNumber id="9444AB23FF1BFF3F98BBFA39FB1035E0" box="[198,337,1483,1509]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/UW028618" pageId="138" pageNumber="139">UW 028618</accessionNumber>
and 
<accessionNumber id="9444AB23FF1BFF3F99F7FA39F85635E0" box="[394,535,1482,1509]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/UW158024" pageId="138" pageNumber="139">UW 158024</accessionNumber>
