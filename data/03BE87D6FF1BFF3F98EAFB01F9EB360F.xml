<quantity id="4CEF9B25FF1BFF3F9949FAACFB2E357F" box="[308,367,1374,1402]" metricMagnitude="1" metricUnit="m" metricValue="1.524" pageId="138" pageNumber="139" unit="ft" value="50.0">50 ft</quantity>
