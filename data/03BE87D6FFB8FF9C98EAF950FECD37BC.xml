<quantity id="4CEF9B25FFB8FF9C9BECF8A4F9983774" box="[913,985,1878,1905]" metricMagnitude="2" metricUnit="m" metricValue="5.71" pageId="41" pageNumber="42" unit="m" value="571.0">571 m</quantity>
, 
<quantity id="4CEF9B25FFB8FF9C9B98F8A5FE683774" box="[997,1065,1879,1905]" metricMagnitude="2" metricUnit="m" metricValue="2.660904" pageId="41" pageNumber="42" unit="ft" value="873.0">873 ft</quantity>
, 
<quantity id="4CEF9B25FFB8FF9C9C4EF8A5FE3D3774" box="[1075,1148,1879,1905]" metricMagnitude="2" metricUnit="m" metricValue="6.01" pageId="41" pageNumber="42" unit="m" value="601.0">601 m</quantity>
