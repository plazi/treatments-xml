<quantity id="4CEF9B25FFB8FF9C9CFAF8A5FEA13774" box="[1159,1248,1879,1905]" metricMagnitude="2" metricUnit="m" metricValue="6.038088" pageId="41" pageNumber="42" unit="ft" value="1981.0">1,981 ft</quantity>
