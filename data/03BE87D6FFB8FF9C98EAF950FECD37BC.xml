<quantity id="4CEF9B25FFB8FF9C9C96F8A5FF723774" box="[1259,1331,1879,1905]" metricMagnitude="2" metricUnit="m" metricValue="8.059999999999999" pageId="41" pageNumber="42" unit="m" value="806.0">806 m</quantity>
, 
<quantity id="4CEF9B25FFB8FF9C9D42F8A5FFD93774" box="[1343,1432,1879,1905]" metricMagnitude="2" metricUnit="m" metricValue="8.058912" pageId="41" pageNumber="42" unit="ft" value="2644.0">2,644 ft</quantity>
