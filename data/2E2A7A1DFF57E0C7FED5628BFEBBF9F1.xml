<taxonomicName id="6183B088FF58E0C7FD876486FCB5FA7A" box="[602,892,1444,1469]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="136" pageNumber="137" phylum="Arthropoda" rank="species" species="coturnicis">
