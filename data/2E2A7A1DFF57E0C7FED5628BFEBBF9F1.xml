<taxonomicName id="6183B088FF58E0C7FD6464D6FCE7F9CA" box="[697,814,1524,1549]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="136" pageNumber="137" phylum="Arthropoda" rank="species" species="coturnicis">
