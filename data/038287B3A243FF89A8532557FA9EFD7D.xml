<taxonomicName id="4C2B4D26A242FF89A9D32678FB7CFD7D" authorityName="San Martin &amp; Hutchings" authorityYear="2006" baseAuthorityName="San Martin &amp; Hutchings" baseAuthorityYear="2006" box="[967,1230,633,656]" class="Polychaeta" family="Syllidae" genus="Pionosyllis" kingdom="Animalia" order="Phyllodocida" pageId="67" pageNumber="324" phylum="Annelida" rank="species" species="hartmannschroederae" status="n.comb.">
<emphasis id="B95FEAB7A242FF89A9D32678FB7CFD7D" box="[967,1230,633,656]" italics="true" pageId="67" pageNumber="324">P. hartmannschroederae</emphasis>
