<heading id="3F2381CE2A2F052DFF03F8A596A9F89B" bold="true" box="[151,495,1877,1903]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<emphasis id="56A0EAB02A2F052DFF03F8A596A9F89B" bold="true" box="[151,495,1877,1903]" pageId="7" pageNumber="8">
<taxonomicName id="A3D44D212A2F052DFF03F8A596E0F89B" box="[151,422,1877,1903]" class="Insecta" family="Cecidomyiidae" genus="Zadbimyia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="artborkenti" status="sp. n.">
<emphasis id="56A0EAB02A2F052DFF03F8A596E0F89B" bold="true" box="[151,422,1877,1903]" italics="true" pageId="7" pageNumber="8">Zadbimyia artborkenti</emphasis>
</taxonomicName>
<taxonomicNameLabel id="4D9357CB2A2F052DFE39F8A596A9F89B" box="[429,495,1877,1903]" pageId="7" pageNumber="8" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
