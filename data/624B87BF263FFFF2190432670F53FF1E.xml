<taxonomicName id="2DE24D2A2639FFF0194D313F0F2AF985" box="[317,514,1583,1605]" class="Insecta" family="Ichneumonidae" genus="Pristiboea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="1321" phylum="Arthropoda" rank="species" species="leiomano">
