<taxonomicName id="8ACCB1C7FFE7B368478CF9E7CC590D69" baseAuthorityName="Stimpson" baseAuthorityYear="1857" box="[970,1119,1582,1605]" class="Malacostraca" family="Astacidae" genus="Pacifastacus" kingdom="Animalia" order="Decapoda" pageId="123" pageNumber="124" phylum="Arthropoda" rank="species" species="nigrescens">
