<paragraph id="8BEDA91F907EFFE8EDE440B0FDEDBECA" blockId="0.[151,1437,1739,2017]" box="[151,521,1739,1766]" pageId="0" pageNumber="393">
<heading id="D0A51E73907EFFE8EDE440B0FDEDBECA" bold="true" box="[151,521,1739,1766]" fontSize="11" level="1" pageId="0" pageNumber="393" reason="1">
<taxonomicName id="4C52D29C907EFFE8EDE440B0FDEDBECA" authority="Blanchard, 1852" authorityName="Blanchard" authorityYear="1852" box="[151,521,1739,1766]" class="Insecta" family="Enicocephalidae" genus="Systelloderes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="393" phylum="Arthropoda" rank="genus">
<emphasis id="B926750D907EFFE8EDE440B0FDEDBECA" bold="true" box="[151,521,1739,1766]" pageId="0" pageNumber="393">
<emphasis id="B926750D907EFFE8EDE440B0FED7BEC9" bold="true" box="[151,307,1739,1766]" italics="true" pageId="0" pageNumber="393">Systelloderes</emphasis>
Blanchard, 1852
