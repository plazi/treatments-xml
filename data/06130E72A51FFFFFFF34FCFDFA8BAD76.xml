<taxonomicName id="49BAC4E7A518FFFEFEC2FC72FDE1AFF3" authority="Calvanese &amp; Brescovit &amp; Bonato, 2019" authorityName="Calvanese &amp; Brescovit &amp; Bonato" authorityYear="2019" box="[353,554,942,966]" class="Chilopoda" family="Aphilodontidae" genus="Aphilodon" kingdom="Animalia" order="Geophilomorpha" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="caboclos" status="n. sp.">
