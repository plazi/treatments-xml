<taxonomicName id="4C2AACCF37511B71FD534372FCF5EA3F" authorityName="Lane" authorityYear="1958" box="[765,892,1314,1337]" class="Insecta" family="Cerambycidae" genus="Orteguaza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="genus">
