<taxonomicName id="4C2AACCF37511B71FB694372FAD6EA3C" box="[1223,1375,1314,1338]" class="Insecta" family="Cerambycidae" genus="Orteguaza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="species" species="funeraria">
