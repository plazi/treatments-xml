</taxonomicName>
[
<taxonomicName id="4C2AACCF37511B71FA9D4332FEDCEA9C" authority="Giorgi, 1998" authorityName="Giorgi" authorityYear="1998" class="Insecta" family="Cerambycidae" genus="Clavidesmus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="species" species="chicae">
