<taxonomicName id="4C2AACCF37511B71FC054332FAAAEA7C" authority="Dillon and Dillon" authorityName="Dillon and Dillon" authorityYear="1946" box="[939,1315,1378,1402]" class="Insecta" family="Cerambycidae" genus="Clavidesmus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="genus">
