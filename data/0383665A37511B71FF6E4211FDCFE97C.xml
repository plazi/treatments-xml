<taxonomicName id="4C2AACCF37511B71FBE24312FB6AEA5C" box="[1100,1251,1346,1370]" class="Insecta" family="Cerambycidae" genus="Orteguaza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="17" phylum="Arthropoda" rank="species" species="funeraria">
