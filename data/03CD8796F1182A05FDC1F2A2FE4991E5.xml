<taxonomicName id="4C644D03F1182A03FDC1F2A2FCF593DC" authority="ROCA, 2003" authorityName="ROCA" authorityYear="2003" box="[530,886,899,923]" class="Trematoda" family="Dicrocoeliidae" genus="Pseudoparadistomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="2" pageNumber="1402" phylum="Platyhelminthes" rank="species" species="yaizaensis" status="n.g. and n.sp.">
<emphasis id="B910EA92F1182A03FDC1F2A2FCF593DC" bold="true" box="[530,886,899,923]" italics="true" pageId="2" pageNumber="1402">Pseudoparadistomum yaizaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22357E9F1182A03FCACF2A5FB9893DC" box="[895,1051,899,924]" pageId="2" pageNumber="1402" rank="species">n.g. and n.sp.</taxonomicNameLabel>
