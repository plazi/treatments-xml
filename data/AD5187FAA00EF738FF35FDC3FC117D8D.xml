<taxonomicName id="E2F84D6FA00EF738FE78FC84FDD47D8D" box="[474,576,887,912]" class="Insecta" family="Ichneumonidae" genus="Glypta" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="ichitai">
