<taxonomicName id="E2F84D6FA00EF738FC8FFC8BFCE07D8D" box="[813,884,887,912]" class="Insecta" family="Ichneumonidae" genus="Glypta" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="ichitai">
