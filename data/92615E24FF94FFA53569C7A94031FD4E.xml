<taxonomicName id="DDC894B1FF95FFA53726C63E40BDFD8C" box="[268,492,609,631]" class="Liliopsida" family="Cyperaceae" genus="Schoenus" kingdom="Plantae" order="Poales" pageId="95" pageNumber="96" phylum="Tracheophyta" rank="species" species="nigricans">
<emphasis id="28BC3320FF95FFA53726C63E40BDFD8C" box="[268,492,609,631]" italics="true" pageId="95" pageNumber="96">Schoenus nigricans</emphasis>
</taxonomicName>
, 
<taxonomicName id="DDC894B1FF95FFA537D4C63E432AFD8D" authorityName="C.Linnaeus" authorityYear="1753" box="[510,635,609,630]" class="Sphagnopsida" family="Sphagnaceae" genus="Sphagnum" kingdom="Plantae" order="Sphagnales" pageId="95" pageNumber="96" phylum="Bryophyta" rank="genus">
<emphasis id="28BC3320FF95FFA537D4C63E432AFD8D" box="[510,635,609,630]" italics="true" pageId="95" pageNumber="96">Sphagnum</emphasis>
</taxonomicName>
and 
<taxonomicName id="DDC894B1FF95FFA53496C63E418EFD6D" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="95" pageNumber="96" phylum="Tracheophyta" rank="genus">
<emphasis id="28BC3320FF95FFA53496C63E418EFD6D" italics="true" pageId="95" pageNumber="96">Phragmites</emphasis>
</taxonomicName>
; grazed 
<taxonomicName id="DDC894B1FF95FFA5376DC6DF40F0FD6E" box="[327,417,640,661]" class="Liliopsida" family="Poaceae" genus="Molinia" kingdom="Plantae" order="Poales" pageId="95" pageNumber="96" phylum="Tracheophyta" rank="genus">
<emphasis id="28BC3320FF95FFA5376DC6DF40F0FD6E" box="[327,417,640,661]" italics="true" pageId="95" pageNumber="96">Molinia</emphasis>
