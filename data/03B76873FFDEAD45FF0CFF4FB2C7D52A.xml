<collectingCountry id="F30999F5FFDEAD45FE1CFEFEB783D316" box="[391,472,294,319]" name="Turkey" pageId="3" pageNumber="124">Turkey</collectingCountry>
, Trabzon Province, Trabzon, 
<date id="FFA0FFA5FFDEAD45FCA2FEFEB59FD316" box="[825,964,294,319]" pageId="3" pageNumber="124" value="1971-07-06">
06.VII. 
<number id="3DD6CB42FFDEAD45FC11FEFEB59FD316" box="[906,964,294,319]" pageId="3" pageNumber="124" value="1971.0">1971</number>
</date>
, 
<taxonomicName id="4C1EA2E6FFDEAD45FC4EFEFEB221D317" box="[981,1146,294,318]" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" kingdom="Plantae" order="Ericales" pageId="3" pageNumber="124" phylum="Tracheophyta" rank="genus">
<emphasis id="B96A0577FFDEAD45FC4EFEFEB221D317" box="[981,1146,294,318]" italics="true" pageId="3" pageNumber="124">Rhododendron</emphasis>
</taxonomicName>
sp., leg. Nazife Tuatay; 
<emphasis id="B96A0577FFDEAD45FF0CFE93B752D34B" box="[151,265,331,354]" italics="true" pageId="3" pageNumber="124">
<typeStatus id="54A567C7FFDEAD45FF0CFE93B752D34B" box="[151,265,331,354]" pageId="3" pageNumber="124">Paratypes</typeStatus>
