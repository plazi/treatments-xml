<taxonomicName id="122A16F9EC074F5089BA095A8279A245" LSID="http://zoobank.org/16B28CCC-D6BA-4587-AC30-8E1A02A3899E" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus arawak" order="Hymenoptera" pageId="200" pageNumber="201" phylum="Arthropoda" rank="species" species="arawak">Heterospilus arawak Marsh</taxonomicName>
