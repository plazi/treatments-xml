<number id="2D14596FDB56FFDEBD9DFC24E341FC5E" box="[1238,1315,906,931]" pageId="7" pageNumber="420" value="27.55">
27, 
<number id="2D14596FDB56FFDEBC4EFC25E341FC5E" box="[1285,1315,906,931]" pageId="7" pageNumber="420" value="55.11">55</number>
