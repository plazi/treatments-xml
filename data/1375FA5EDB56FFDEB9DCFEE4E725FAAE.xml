<number id="2D14596FDB56FFDEBB79FC25E43DFC5E" box="[562,607,906,931]" pageId="7" pageNumber="420" value="935.0">935</number>
, median width 
<number id="2D14596FDB56FFDEBA52FC24E527FC5E" box="[793,837,907,931]" pageId="7" pageNumber="420" value="110.0">110</number>
