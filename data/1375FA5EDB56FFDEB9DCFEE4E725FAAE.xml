<number id="2D14596FDB56FFDEB9DCFC25E6A6FC5E" box="[151,196,906,931]" pageId="7" pageNumber="420" value="160.0">160</number>
; width 
<number id="2D14596FDB56FFDEB86BFC25E72CFC5E" box="[288,334,906,931]" pageId="7" pageNumber="420" value="225.0">225</number>
