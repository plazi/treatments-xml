<taxonomicName id="4C349A9ADF7EFF92FAA9FDA4FA3FFD4E" box="[1308,1435,621,642]" class="Insecta" family="Leptophlebiidae" genus="Edmundsula" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="384" phylum="Arthropoda" rank="genus">
