<number id="3D9CAC74FFF8E932FCFDA499AFC18F7A" box="[888,916,511,535]" pageId="5" pageNumber="48" value="30.0">30</number>
(
<number id="3D9CAC74FFF8E932FC20A499AFBA8F7A" box="[933,1007,511,535]" pageId="5" pageNumber="48" value="29.0" valueMax="30.0" valueMin="28.0">28–30</number>
), 
