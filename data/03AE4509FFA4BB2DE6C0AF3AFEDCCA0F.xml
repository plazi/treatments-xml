<paragraph id="8BB8F41FFFA1BB2CE700A837FB92CAFE" blockId="6.[191,1442,214,480]" pageId="6" pageNumber="5">
<taxonomicName id="4C078F9CFFA1BB2CE700A837FDC8CA67" baseAuthorityName="Brunner von Wattenwyl" baseAuthorityYear="1907" box="[256,525,351,377]" class="Insecta" family="Diapheromeridae" genus="Oncotophasma" kingdom="Animalia" order="Phasmida" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="coxatum">
