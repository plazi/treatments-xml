<quantity id="4CE785F84265FFA3FC884091DD88FD72" box="[770,929,708,742]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="7" pageNumber="285" unit="mm" value="0.95">0.95 mm</quantity>
