<taxonomicName id="1FEF4D6EC741FFBFFA74FC11386CFBAA" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="noctivaga">
<emphasis id="EA9BEAFFC741FFBFFA74FC11386CFBAA" italics="true" pageId="28" pageNumber="29">S. noctivaga</emphasis>
</taxonomicName>
species-group that also contains 
<emphasis id="EA9BEAFFC741FFBFFD4BFBFE3C39FBAA" box="[697,1350,1034,1060]" italics="true" pageId="28" pageNumber="29">
<taxonomicName id="1FEF4D6EC741FFBFFD4BFBFE3A09FBAA" box="[697,886,1034,1059]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="brachyptera">S. brachyptera</taxonomicName>
, 
<taxonomicName id="1FEF4D6EC741FFBFFC76FBFE3D5FFBAA" box="[900,1056,1034,1059]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="chandleri" status="sp. nov.">S. chandleri</taxonomicName>
