<emphasis id="EA9BEAFFC741FFBFFBDCFBFE3DEFFBAD" bold="true" box="[1070,1168,1034,1060]" italics="true" pageId="28" pageNumber="29">
<taxonomicNameLabel id="F1A85784C741FFBFFBDCFBFE3DEFFBAD" box="[1070,1168,1034,1060]" pageId="28" pageNumber="29" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="1FEF4D6EC741FFBFFB50FBFE3C3EFBAA" box="[1186,1345,1034,1059]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="noctivaga">S. noctivaga</taxonomicName>
