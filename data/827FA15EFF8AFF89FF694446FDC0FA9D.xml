<collectionCode id="6CC7888DFF8AFF89FF694528FF7DFCD7" box="[168,243,837,861]" country="Brazil" httpUri="http://grbio.org/cool/5xp9-edpx" name="Universidade Federal do Parana, Colecao de Entomologia Pe. Jesus Santiago Moure" pageId="12" pageNumber="137" type="University or college">DZUP</collectionCode>
