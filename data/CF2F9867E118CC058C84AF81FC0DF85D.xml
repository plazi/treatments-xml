(
<collectingRegion id="8542E793E118CC058DF7AA9CFD04F85C" box="[484,765,1972,1998]" country="Sri Lanka" name="Sabaragamuwa" pageId="4" pageNumber="570">Sabaragamuwa Province</collectingRegion>
