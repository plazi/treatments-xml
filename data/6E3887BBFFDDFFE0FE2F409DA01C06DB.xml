<taxonomicName id="21914D2EFFDDFFE0FDDB40A5A0250794" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[524,790,687,706]" class="Insecta" family="Silphidae" genus="Silpha" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="229" phylum="Arthropoda" rank="species" species="depressa">
