<taxonomicName id="4C3C6A9E7035EB5B2820FE0767F166CA" box="[558,672,475,500]" class="Insecta" family="Agromyzidae" genus="Liriomyza" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
