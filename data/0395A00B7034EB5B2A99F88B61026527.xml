<taxonomicName id="4C3C6A9E7035EB5B29DFFE07616466CB" authorityName="Linnaeus" authorityYear="1753" box="[977,1077,475,501]" class="Magnoliopsida" family="Asteraceae" genus="Silphium" kingdom="Plantae" order="Asterales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
