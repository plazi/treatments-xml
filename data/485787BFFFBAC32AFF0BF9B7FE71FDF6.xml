<emphasis id="F28AEABBFFBAC325FF0BF9B7FD31F93C" bold="true" box="[151,694,1659,1686]" pageId="7" pageNumber="44">
<taxonomicName id="07FE4D2AFFBAC325FF0BF9B7FE7EF93C" authority="Tenkatei Lidth" authorityName="Tenkatei Lidth" box="[151,505,1659,1686]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="44" phylum="Chordata" rank="genus">
<emphasis id="F28AEABBFFBAC325FF0BF9B7FE2CF93F" bold="true" box="[151,427,1660,1686]" italics="true" pageId="7" pageNumber="44">Hemidactylus Tenkatei</emphasis>
Lidth
</taxonomicName>
de 
<bibRefCitation id="A46F4B58FFBAC325FDB8F9B0FD31F93C" author="Jeude" box="[548,694,1659,1685]" pageId="7" pageNumber="44" refString="Lidth de Jeude, T. W. V. (1895) Reptiles from Timor and the neighbouring islands. Notes from the Leyden Museum, 16, 119 - 127." type="journal article" year="1895">
Jeude, 
<number id="7636248EFFBAC325FDE0F9B0FD31F93C" box="[636,694,1659,1685]" pageId="7" pageNumber="44" value="1895.0">1895</number>
</bibRefCitation>
