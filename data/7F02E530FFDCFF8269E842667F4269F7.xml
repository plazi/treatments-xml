<emphasis id="C5DF8834FFDCFF8269E842FA7D3068CA" bold="true" box="[151,277,1158,1184]" pageId="46" pageNumber="47">Locations:</emphasis>
<materialsCitation id="47C35E7BFFDCFF82686342FB78D368CB" ID-GBIF-Occurrence="3424965649" box="[284,1270,1158,1185]" collectingDate="24 Jan. 1977, 15 Dec. 1976, 12 Jul 1977 and 5 Jul. 1979" collectionCode="AKMM" pageId="46" pageNumber="47" specimenCount="1">
Martin, 
<collectingDate id="93518B0EFFDCFF82680942FB7FC968CB" box="[374,1004,1158,1185]" isEnumeration="true" pageId="46" pageNumber="47">
<date id="831572E6FFDCFF82680942FB7E2D68CB" box="[374,520,1159,1185]" pageId="46" pageNumber="47" value="1977-01-24">24 Jan. 1977</date>
, 
<date id="831572E6FFDCFF826B6C42FA7E8968CB" box="[531,684,1158,1185]" pageId="46" pageNumber="47" value="1976-12-15">15 Dec. 1976</date>
