<taxonomicName id="E18F68AEBF27FFA3FD5FFC55FC92FC53" authorityName="Matsumura" authorityYear="1940" box="[724,841,938,962]" class="Insecta" family="Cercopidae" genus="Baibarana" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="2077" phylum="Arthropoda" rank="genus">
