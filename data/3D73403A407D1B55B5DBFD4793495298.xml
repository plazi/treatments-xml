<taxonomicName id="72DA8AAF407D1B53B415F9F19560564A" authority="Qin &amp; Zhang" authorityName="Qin &amp; Zhang" box="[345,804,1631,1654]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="species" species="dianella" status="sp. nov." subGenus="Okubasca">
