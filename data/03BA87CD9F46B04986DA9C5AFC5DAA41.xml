(UPLBLG.19.66), 
<geoCoordinate id="EE27501C9F46B04F86DA9FF9FEFDAFB2" box="[151,279,723,749]" degrees="14.39983" direction="north" orientation="latitude" pageId="10" pageNumber="537" precision="1" value="14.39983">N14.39983</geoCoordinate>
, 
<geoCoordinate id="EE27501C9F46B04F876F9FF8FE44AFB2" box="[290,430,722,749]" degrees="121.54133" direction="east" orientation="longitude" pageId="10" pageNumber="537" precision="1" value="121.54133">E121.54133</geoCoordinate>
, 320.5Â± 
