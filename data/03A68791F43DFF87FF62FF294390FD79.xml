<taxonomicName id="4C0F4D04F43DFF87FD0AFD5B4369FD79" box="[767,891,741,765]" class="Insecta" family="Noctuidae" genus="Diascoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
<emphasis id="B97BEA95F43DFF87FD0AFD5B4369FD79" box="[767,891,741,765]" italics="true" pageId="27" pageNumber="28">Diascoides</emphasis>
