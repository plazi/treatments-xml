<taxonomicName id="4C0F4D04F43DFF87FD22FD7F4348FD5D" box="[727,858,705,729]" class="Insecta" family="Noctuidae" genus="Lophoptera" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
