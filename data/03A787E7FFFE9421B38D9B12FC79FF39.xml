<heading id="D0F9819DFFFE9423B38D9B12FDE0F87C" bold="true" box="[151,622,1909,1936]" fontSize="11" level="1" pageId="20" pageNumber="321" reason="1">
<taxonomicName id="4C0E4D72FFFE9423B38D9B12FDE0F87C" authority="Haliday" authorityName="Haliday" authorityYear="1836" box="[151,622,1909,1936]" class="Insecta" family="Vespidae" genus="Polistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="321" phylum="Arthropoda" rank="species" species="actaeon" subGenus="Epicnemius">
