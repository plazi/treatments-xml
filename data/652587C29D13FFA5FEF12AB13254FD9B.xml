<taxonomicName id="2A8C4D579D12FFA2FB98293F3443F8DA" authorityName="Price &amp; Dalgleish" authorityYear="2006" box="[1121,1316,1824,1847]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="cyanocephalae">
<emphasis id="DFF8EAC69D12FFA2FB98293F3443F8DA" box="[1121,1316,1824,1847]" italics="true" pageId="3" pageNumber="4">M. cyanocephalae</emphasis>
