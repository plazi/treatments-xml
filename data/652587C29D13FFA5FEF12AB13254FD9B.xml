<taxonomicName id="2A8C4D579D12FFA2FE0D28E23269F8F9" authorityName="Eichler" authorityYear="1956" box="[500,782,1789,1812]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="laciniaesternata">
