<paragraph id="D72536AFFF946057FF4B2D5128BBF99D" blockId="11.[151,642,1546,1570]" box="[151,642,1546,1570]" pageId="11" pageNumber="12">
<typeStatus id="0821880DFF946057FF4B2D512AF5F99E" box="[151,204,1546,1569]" pageId="11" pageNumber="12">Type</typeStatus>
species 
<taxonomicName id="109A4D2CFF946057FEFA2D512815F99E" box="[294,556,1546,1569]" class="Insecta" family="Eriococcidae" genus="Montanococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="graemei" status="sp.nov.">
