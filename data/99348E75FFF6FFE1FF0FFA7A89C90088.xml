<collectingCountry id="698A7FF3FFF7FFE1FEFFFD9D89C80024" box="[359,470,583,609]" name="Myanmar" pageId="3" pageNumber="524">Myanmar</collectingCountry>
, 
<collectingCountry id="698A7FF3FFF7FFE1FE79FD9D8A030024" box="[481,541,583,609]" name="India" pageId="3" pageNumber="524">India</collectingCountry>
