<taxonomicName id="4C5937FDFFF5FF95FDC1023FFCEEF9DC" authority="(Zetterstedt, 1839)" authorityName="Zetterstedt" authorityYear="1839" baseAuthorityName="Zetterstedt" baseAuthorityYear="1839" box="[534,893,1618,1645]" class="Insecta" family="Nepticulidae" genus="Ectoedemia" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="224" phylum="Arthropoda" rank="species" species="minimella">
