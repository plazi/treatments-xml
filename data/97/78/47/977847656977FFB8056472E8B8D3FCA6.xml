<emphasis box="[164,389,613,634]" italics="true" pageId="70" pageNumber="798">Material examined:</emphasis>
<materialsCitation collectingDate="2008-12-23" collectingDateMax="2009-01-03" collectingDateMin="2008-12-23" collectionCode="CASC" collectorName="Parcelle I &amp; N Tolagnaro &amp; F. Alvarez-Padilla &amp; H. Wood" country="Madagascar" elevation="670" latitude="-24.743055" location="Manangotry" longLatPrecision="1" longitude="46.856472" municipality="Parc National Andohahela" pageId="70" pageNumber="798" specimenCode="CASENT9031164, CASENT9031168, 9031167, 9031165, 9031166, 9031168" specimenCount="13" specimenCount-female="12" specimenCount-male="1" stateProvince="Toliara" typeStatus="holotype">
