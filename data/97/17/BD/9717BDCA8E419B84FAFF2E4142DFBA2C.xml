<subSubSection pageId="null" pageNumber="103" type="vernacular_name">
<paragraph pageId="null" pageNumber="103">
<normalizedToken originalValue="Eisenkrautähnliche" pageId="null" pageNumber="103">Eisenkrautaehnliche</normalizedToken>
Salbei
</paragraph>
