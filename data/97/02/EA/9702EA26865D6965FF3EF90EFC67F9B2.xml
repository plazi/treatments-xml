<typeStatus box="[598,646,1125,1149]" pageId="13" pageNumber="14">type</typeStatus>
localities in Napo Province, 
<collectingCountry box="[972,1067,1124,1148]" name="Ecuador" pageId="13" pageNumber="14">Ecuador</collectingCountry>
