<number box="[1373,1432,1919,1943]" pageId="14" pageNumber="15" value="2006.0">2006</number>
</date>
, adult wasp emerged 
<date box="[395,542,1955,1980]" pageId="14" pageNumber="15" value="2006-05-09">
<number box="[395,409,1955,1979]" pageId="14" pageNumber="15" value="9.0">9</number>
May 
<number box="[484,542,1955,1979]" pageId="14" pageNumber="15" value="2006.0">2006</number>
</date>
, host caterpillar 
<taxonomicName box="[741,967,1955,1979]" class="Insecta" family="Arctiidae" genus="Dysschema" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="palmeri">
