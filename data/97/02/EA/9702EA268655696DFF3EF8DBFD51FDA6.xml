<quantity box="[585,675,1688,1712]" metricMagnitude="3" metricUnit="m" metricValue="1.0379999999999998" pageId="4" pageNumber="5" unit="m" value="1038.0">1038 m</quantity>
, parasitoids pupated 
<date box="[919,1091,1688,1713]" pageId="4" pageNumber="5" value="2006-08-02">
<number box="[919,933,1688,1712]" pageId="4" pageNumber="5" value="2.0">2</number>
August 
<number box="[1033,1091,1688,1712]" pageId="4" pageNumber="5" value="2006.0">2006</number>
</date>
, adult parasitoids emerged 
<date pageId="4" pageNumber="5" value="2006-08-11">
<number box="[1409,1436,1688,1712]" pageId="4" pageNumber="5" value="11.0">11</number>
August 
<number box="[239,299,1724,1748]" pageId="4" pageNumber="5" value="2006.0">2006</number>
</date>
. Deposited in 
