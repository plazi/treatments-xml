<paragraph blockId="8.[151,1436,1157,1254]" pageId="8" pageNumber="65">
<taxonomicName authority="Parrott, 1900" authorityName="Parrott" authorityYear="1900" box="[151,441,1157,1181]" class="Insecta" family="Eriococcidae" genus="Ovaticoccus" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="65" phylum="Arthropoda" rank="species" species="nativus">
