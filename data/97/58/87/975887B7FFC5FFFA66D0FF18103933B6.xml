, 
<taxonomicName authority="(Hayashi, 1981)" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" baseAuthorityName="Hayashi" baseAuthorityYear="1981" box="[932,1273,1859,1883]" class="Insecta" family="Cerambycidae" genus="Komiyandra" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="13" phylum="Arthropoda" rank="species" species="lanyuana">
