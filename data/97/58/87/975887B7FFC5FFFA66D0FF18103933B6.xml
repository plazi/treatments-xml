(
<bibRefCitation author="Hayashi, M." box="[1091,1264,1859,1883]" pageId="16" pageNumber="13" pagination="27 - 32" refId="ref68535" refString="Hayashi, M. 1981. Studies on Asian Cerambycidae, IV (Coleoptera). Entomological Review of Japan 34 (1): 27 - 32." type="journal article" year="1981">Hayashi, 1981</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Miwa and Mitono, 1939)" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" baseAuthorityName="Miwa and Mitono" baseAuthorityYear="1939" class="Insecta" family="Cerambycidae" genus="Komiyandra" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="13" phylum="Arthropoda" rank="species" species="formosana">
