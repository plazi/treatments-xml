<subSubSection box="[199,654,1417,1442]" pageId="2" pageNumber="116" type="distribution">
<paragraph blockId="2.[151,1437,151,1658]" box="[199,654,1417,1442]" pageId="2" pageNumber="116">
<emphasis bold="true" box="[199,352,1417,1442]" pageId="2" pageNumber="116">Distribution.</emphasis>
<collectingCountry box="[359,426,1417,1442]" name="China" pageId="2" pageNumber="116">China</collectingCountry>
(
<collectingRegion box="[441,526,1417,1442]" country="China" name="Hainan" pageId="2" pageNumber="116">Hainan</collectingRegion>
, 
<collectingRegion box="[537,642,1417,1442]" country="China" name="Zhejiang" pageId="2" pageNumber="116">Zhejiang</collectingRegion>
