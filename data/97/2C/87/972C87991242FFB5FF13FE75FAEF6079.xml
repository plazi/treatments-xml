; length of antennae 
<quantity box="[978,1081,942,967]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" pageId="5" pageNumber="581" unit="mm" value="1.45">1.45 mm</quantity>
; with of abdomen across tergite IV 
<number box="[187,239,979,1003]" pageId="5" pageNumber="581" value="1.12">1.12</number>
; 
<typeStatus box="[251,349,979,1003]" pageId="5" pageNumber="581">Paratype</typeStatus>
female: length 
<quantity box="[524,612,978,1003]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="5" pageNumber="581" unit="mm" value="4.5">4.5 mm</quantity>
