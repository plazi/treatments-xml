, B–G = 
<quantity box="[277,357,2000,2022]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="10" pageNumber="388" unit="mm" value="0.5">0.5 mm</quantity>
, H–J = 
<number box="[435,448,2000,2022]" pageId="10" pageNumber="388" value="5.0">5</number>
µm, K = 
