<taxonomicName box="[981,1140,244,266]" class="Insecta" family="Formicidae" genus="Pogonomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">
