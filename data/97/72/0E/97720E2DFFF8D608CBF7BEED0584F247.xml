<subSubSection box="[151,581,1484,1510]" pageId="19" pageNumber="20" type="nomenclature">
<paragraph blockId="19.[151,581,1484,1510]" box="[151,581,1484,1510]" pageId="19" pageNumber="20">
<heading bold="true" box="[151,581,1484,1510]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[151,581,1484,1510]" pageId="19" pageNumber="20">
Genus 
<taxonomicName authority="M. Sars, 1870" authorityName="M. Sars" authorityYear="1870" box="[238,581,1484,1510]" class="Maxillopoda" family="Herpyllobiidae" genus="Eurysilenium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,403,1484,1510]" italics="true" pageId="19" pageNumber="20">Eurysilenium</emphasis>
<bibRefCitation author="Sars, M." box="[410,581,1484,1510]" pageId="19" pageNumber="20" pagination="13 - 226" refId="ref48630" refString="Sars, M. (1870) Bidrag til Kundskab om Christianiafjordens Fauna. II. Nyt Magazin for Naturvidenskaberne, 17, 13 - 226, pls. 8 - 13." type="journal article" year="1870">M. Sars, 1870</bibRefCitation>
</taxonomicName>
