<taxonomicName class="Insecta" family="Asilidae" genus="Anasillomos" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="119" phylum="Arthropoda" rank="genus">Anasillomos</taxonomicName>
and 
<taxonomicName class="Insecta" family="Asilidae" genus="Oratostylum" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="119" phylum="Arthropoda" rank="genus">Oratostylum</taxonomicName>
