<emphasis box="[369,525,1070,1094]" italics="true" pageId="2" pageNumber="445">D. luzonensis</emphasis>
<bibRefCitation author="Otte" box="[536,663,1071,1095]" pageId="2" pageNumber="445" refString="Otte, D. (1988) Bark crickets of the Western Pacific Region (Gryllidae: Pteroplistinae). Proceedings of the Academy of Natural Sciences of Philadelphia, 140 (2), 281 - 334." type="journal article" year="1988">
Otte, 
<number box="[604,663,1071,1095]" pageId="2" pageNumber="445" value="1988.0">1988</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Otte, 1988" authorityName="Otte" authorityYear="1988" box="[678,968,1070,1095]" class="Insecta" family="Haglotettigoniidae" genus="Duolandrevus" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="445" phylum="Arthropoda" rank="species" species="balabacus">
