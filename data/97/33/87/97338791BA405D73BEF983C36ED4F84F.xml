<collectingCountry box="[557,632,1662,1683]" name="Taiwan" pageId="2" pageNumber="445">Taiwan</collectingCountry>
, 
<collectingCountry box="[642,752,1662,1683]" name="Philippines" pageId="2" pageNumber="445">Philippines</collectingCountry>
, New 
<collectingCountry box="[813,885,1662,1683]" name="Guinea-Bissau" pageId="2" pageNumber="445">Guinea</collectingCountry>
