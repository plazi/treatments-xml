<number box="[731,783,1986,2007]" pageId="3" pageNumber="54" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
, male; 
<number box="[855,881,1986,2007]" pageId="3" pageNumber="54" value="12.0">12</number>
