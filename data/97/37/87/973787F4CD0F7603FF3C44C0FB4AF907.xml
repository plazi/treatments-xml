<subSubSection pageId="1" pageNumber="166" type="discussion">
<paragraph blockId="1.[151,1436,1514,1755]" pageId="1" pageNumber="166">
<materialsCitation ID-GBIF-Occurrence="1439650092" box="[199,927,1622,1647]" collectorName="Remarks. A" location="Guangxi" pageId="1" pageNumber="166" specimenCount="1" stateProvince="Guangxi">
