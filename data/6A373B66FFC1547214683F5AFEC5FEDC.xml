<paragraph id="E2218A70FFC1547314683F13FA1AFB6C" blockId="14.[813,1419,1142,1405]" box="[813,1416,1142,1166]" pageId="14" pageNumber="309">
<emphasis id="D0EA5662FFC1547314683F13FA1AFB6C" box="[813,1416,1142,1166]" inLineHeading="true" pageId="14" pageNumber="309" reason="1">
<taxonomicName id="259EF1F3FFC1547314683F13FA90FB6C" authority="(Blanco) Merr., PJS" authorityName="Merr., PJS" baseAuthorityName="Blanco" box="[813,1282,1142,1166]" class="Magnoliopsida" family="Rubiaceae" genus="Canthium" kingdom="Plantae" order="Gentianales" pageId="14" pageNumber="309" phylum="Tracheophyta" rank="species" species="glandulosum">
