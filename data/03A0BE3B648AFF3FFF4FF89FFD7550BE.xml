<taxonomicName id="4C0974AE648AFF3FFF36F89FFDF05049" authority="Duvernoy, 1851" authorityName="Duvernoy" authorityYear="1851" box="[238,561,1801,1827]" class="Mammalia" family="Hyperoodontidae" genus="Berardius" kingdom="Animalia" order="Cetacea" pageId="128" pageNumber="129" phylum="Chordata" rank="genus">
