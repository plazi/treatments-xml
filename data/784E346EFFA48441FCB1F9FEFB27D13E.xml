</taxonomicName>
, 
<taxonomicName id="37E7FEFBFFA48442FF96F87AFBC0D7A3" box="[944,1101,1986,2012]" class="Mammalia" family="Muridae" genus="Allocricetulus" kingdom="Animalia" order="Rodentia" pageId="5" pageNumber="107" phylum="Chordata" rank="genus">
