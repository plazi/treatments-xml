<taxonomicName id="37E7FEFBFFA78441FDE7FF2FFD93D0CF" box="[449,542,151,176]" class="Mammalia" family="Muridae" genus="Lagurus" kingdom="Animalia" order="Rodentia" pageId="6" pageNumber="108" phylum="Chordata" rank="genus">
