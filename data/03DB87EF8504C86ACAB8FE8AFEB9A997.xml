<taxonomicName id="4C724D7A8504C86AC8EBFDADFC36A91C" box="[708,982,530,555]" class="Polychaeta" family="Aeolosomatidae" genus="Aeolosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sabellida" pageId="35" pageNumber="36" phylum="Annelida" rank="species" species="tenebrarum">
