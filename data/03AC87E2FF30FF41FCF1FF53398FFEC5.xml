of 
<taxonomicName id="4C054D77FF30FF46FCAEFD7D3AC6FCB5" baseAuthorityName="Rothschild" baseAuthorityYear="1893" box="[849,929,762,785]" class="Aves" family="Mohoidae" genus="Moho" kingdom="Animalia" order="Passeriformes" pageId="139" pageNumber="139" phylum="Chordata" rank="species" species="bishopi">
