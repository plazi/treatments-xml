<materialsCitation id="39A1235DAC7F3B18FE18DA415F02FDB9" box="[457,833,516,536]" collectingDate="1957-03-30" collectingDateMax="1957-05-19" collectingDateMin="1957-03-30" collectionCode="V" collectorName="Ziela, U." location="Nimba" pageId="51" pageNumber="444" specimenCount="1" stateProvince="Nimba">
<collectingRegion id="4B0DE7E2AC7F3B18FE18DA415E44FDB9" box="[457,519,516,536]" country="Liberia" name="Nimba" pageId="51" pageNumber="444">Nimba</collectingRegion>
: 
<collectorName id="243C4CD6AC7F3B18FDC8DA415E2BFDB9" box="[537,616,516,536]" pageId="51" pageNumber="444">Ziéla, U.</collectorName>
<collectionCode id="EFD8B1C5AC7F3B18FDB6DA415E39FDB9" box="[615,634,516,536]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="51" pageNumber="444" type="Museum">V</collectionCode>
., 
<date id="FD770FC0AC7F3B18FD56DA415F02FDB9" box="[647,833,516,536]" pageId="51" pageNumber="444" value="1957-03-30" valueMax="1957-05-19" valueMin="1957-03-30">
