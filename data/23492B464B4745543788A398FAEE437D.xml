<quantity id="6C1837B54B47454A3327A5D9FB69421E" box="[1080,1174,1516,1540]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="46" pageNumber="147" unit="mm" value="3.9">3.9 mm</quantity>
