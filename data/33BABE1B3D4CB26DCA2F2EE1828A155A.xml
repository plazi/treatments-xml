<subSubSection id="DA1EA102D48229803FD728D95422FE85" type="etymology">
<paragraph id="6F04F535307FF838E66B44550BB16836" pageNumber="662">
etymology NL 
<taxonomicName id="022B2944F4B21F3A026FF494C159FAF2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33607" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole bambusarum Forel" lsidName-HNS="Pheidole bambusarum Forel" order="Hymenoptera" pageNumber="662" phylum="Arthropoda" rank="species" species="bambusarum">bambusarum</taxonomicName>
