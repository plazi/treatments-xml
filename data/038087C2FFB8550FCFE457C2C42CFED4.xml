<paragraph id="8B9636D4FFB8550DCFE45405C37DF9D2" blockId="28.[151,1436,1633,2020]" box="[151,825,1633,1660]" pageId="28" pageNumber="29">
<emphasis id="B95DEAC6FFB8550DCFE45405C1C9F9D5" bold="true" box="[151,397,1633,1659]" pageId="28" pageNumber="29">Material examined:</emphasis>
<specimenCount id="9D2FFD5DFFB8550DCEE75406C245F9D2" box="[404,513,1634,1660]" pageId="28" pageNumber="29" type="larva">10 larvae</specimenCount>
