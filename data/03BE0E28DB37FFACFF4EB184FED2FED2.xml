<quantity id="4CEF12DBDB30FFADFD83B451FD00FE82" box="[602,709,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="3.08" pageId="60" pageNumber="61" unit="mm" value="3.08">3.08 mm</quantity>
; PW 
<quantity id="4CEF12DBDB30FFADFCDCB451FCB4FE82" box="[773,881,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="1.31" pageId="60" pageNumber="61" unit="mm" value="1.31">1.31 mm</quantity>
; PL 
<quantity id="4CEF12DBDB30FFADFC7FB451FBD7FE82" box="[934,1042,312,338]" metricMagnitude="-4" metricUnit="m" metricValue="5.3" pageId="60" pageNumber="61" unit="mm" value="0.53">0.53 mm</quantity>
; EL 
<quantity id="4CEF12DBDB30FFADFB90B451FB71FE82" box="[1097,1204,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="2.55" pageId="60" pageNumber="61" unit="mm" value="2.55">2.55 mm</quantity>
; EW 
<quantity id="4CEF12DBDB30FFADFB2FB451FAA4FE82" box="[1270,1377,312,338]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="60" pageNumber="61" unit="mm" value="1.8">1.80 mm</quantity>
