<number id="A51324E42110FF87FB5BFC483D9AFC27" box="[1207,1239,958,984]" pageId="2" pageNumber="55" value="57.0">57</number>
(
<number id="A51324E42110FF87FB06FC483C72FC27" box="[1258,1343,958,984]" pageId="2" pageNumber="55" value="56.0" valueMax="64.0" valueMin="48.0">48–64</number>
) thick. 
<emphasis id="21AFEAD12110FF87FF7BFC13381AFC00" bold="true" box="[151,343,997,1023]" pageId="2" pageNumber="55">Gnathosoma—</emphasis>
<number id="A51324E42110FF87FEBBFC133838FC00" box="[343,373,997,1023]" pageId="2" pageNumber="55" value="47.0">47</number>
(
<number id="A51324E42110FF87FE6AFC13389AFC00" box="[390,471,997,1023]" pageId="2" pageNumber="55" value="47.0" valueMax="48.0" valueMin="46.0">46–48</number>
