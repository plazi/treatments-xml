<emphasis id="21AFEAD12110FF87FB47FC133DF7FC01" box="[1195,1210,997,1022]" italics="true" pageId="2" pageNumber="55">d</emphasis>
) 
<number id="A51324E42110FF87FB20FC133D96FC00" box="[1228,1243,997,1023]" pageId="2" pageNumber="55" value="8.0">8</number>
(
<number id="A51324E42110FF87FB01FC133C52FC00" box="[1261,1311,997,1023]" pageId="2" pageNumber="55" value="8.0" valueMax="9.0" valueMin="7.0">7–9</number>
), pedipalp coxal setae (
<emphasis id="21AFEAD12110FF87FED9FBFA381FFBDA" box="[309,338,1036,1061]" italics="true" pageId="2" pageNumber="55">ep</emphasis>
) 
<number id="A51324E42110FF87FE8AFBFA383BFBD9" box="[358,374,1036,1062]" pageId="2" pageNumber="55" value="3.0">3</number>
(
<number id="A51324E42110FF87FE64FBFA38F6FBD9" box="[392,443,1036,1062]" pageId="2" pageNumber="55" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
), palp tarsal ventral setae (
<emphasis id="21AFEAD12110FF87FCFEFBFA3A52FBDA" box="[786,799,1036,1061]" italics="true" pageId="2" pageNumber="55">v</emphasis>
) 
<number id="A51324E42110FF87FCDFFBFA3A0FFBD9" box="[819,834,1036,1062]" pageId="2" pageNumber="55" value="5.0">5</number>
(
<number id="A51324E42110FF87FCBAFBFA3AC7FBD9" box="[854,906,1036,1062]" pageId="2" pageNumber="55" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
