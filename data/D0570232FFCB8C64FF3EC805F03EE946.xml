<collectingCountry id="20E9F3B4FFCA8C64FEF6C904F3C6E946" box="[351,416,1578,1601]" name="Brazil" pageId="1" pageNumber="288">Brazil</collectingCountry>
(
<collectingRegion id="9A3A7DC6FFCA8C64FE07C904F02AE946" box="[430,588,1578,1601]" country="Brazil" name="Espirito Santo" pageId="1" pageNumber="288">Espírito Santo</collectingRegion>
).
</paragraph>
