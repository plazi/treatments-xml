<taxonomicName id="E2D54D0E1C0DFFAE5D80FC40FD52FBD3" authority="Smith 1852" authorityName="Smith" authorityYear="1852" box="[463,760,1014,1040]" class="Insecta" family="Vespidae" genus="Vespa" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="54" phylum="Arthropoda" rank="species" species="basalis">
