<taxonomicName id="4C4ABED7FF891866E338F93CFB3BF972" authority="Moulton, 1968: 97" authorityName="Moulton" authorityPageNumber="97" authorityYear="1968" box="[769,1258,1691,1714]" class="Insecta" family="Phlaeothripidae" genus="Rhynchothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="13" pageNumber="54" phylum="Arthropoda" rank="species" species="fuscipennis">
