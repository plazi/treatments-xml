<taxonomicName id="A4E862C21F52FFA9FC4BFAD5FB1F364C" box="[977,1205,1332,1355]" class="Globothalamea" family="Lituolidae" genus="Lamina" kingdom="Chromista" order="Lituolida" pageId="25" pageNumber="304" phylum="Foraminifera" rank="species" species="pergamentacea">Lamina pergamentacea</taxonomicName>
