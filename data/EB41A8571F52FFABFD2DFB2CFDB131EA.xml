<quantity id="A410B4A41F52FFA9FC2AF9C0FC4F353E" box="[944,997,1569,1593]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="25" pageNumber="304" unit="mm" value="2.0">2 mm</quantity>
