<quantity id="A410B4A41F52FFA9FBF4F990FB13358F" box="[1134,1209,1649,1673]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="25" pageNumber="304" unit="mm" value="7.5" valueMax="8.0" valueMin="7.0">7-8 mm</quantity>
