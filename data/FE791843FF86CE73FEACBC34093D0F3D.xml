<number id="C018BB72FF87CE73FE02BD150B980F20" box="[422,429,838,851]" pageId="5" pageNumber="56" value="1.0">1</number>
and C 
<number id="C018BB72FF87CE73FE57BD150BCF0F20" box="[499,506,838,851]" pageId="5" pageNumber="56" value="2.0">2</number>
