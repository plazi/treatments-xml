<caption id="DF1F6655990CFFA1EFA4FBAAFD33E7AD" ID-DOI="http://doi.org/10.5281/zenodo.5174342" ID-Zenodo-Dep="5174342" httpUri="https://zenodo.org/record/5174342/files/figure.png" pageId="10" pageNumber="9" startId="10.[192,276,1107,1129]" targetBox="[192,1440,192,1083]" targetPageId="10">
<paragraph id="8BDF36DD990CFFA1EFA4FBAAFD33E7AD" blockId="10.[192,1440,1107,1158]" pageId="10" pageNumber="9">
