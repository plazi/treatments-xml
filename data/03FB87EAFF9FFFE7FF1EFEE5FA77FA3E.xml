<date id="FFEC103CFF9FFFE7FE33FE57FDD2FDCA" box="[442,562,510,535]" pageId="8" pageNumber="497" value="1961-04-20">
<number id="3D9A24DBFF9FFFE7FE33FE57FE39FDCA" box="[442,473,510,535]" pageId="8" pageNumber="497" value="20.0">20</number>
.iv. 
<number id="3D9A24DBFF9FFFE7FE71FE57FDD2FDCA" box="[504,562,510,535]" pageId="8" pageNumber="497" value="1961.0">1961</number>
