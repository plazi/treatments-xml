<taxonomicName id="4C364D306758FF834FC4FC38FE6FFBEE" authority="Herbert, 2017" authorityName="Herbert" authorityYear="2017" box="[189,461,1021,1047]" class="Gastropoda" family="Urocyclidae" genus="Selatodryas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="31" pageNumber="32" phylum="Mollusca" rank="species" species="roseosoma" status="gen. et sp. nov.">
