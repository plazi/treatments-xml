<taxonomicName id="7E904D16FF88707BFE0AA959A11DF9F1" baseAuthorityName="Jenkins" baseAuthorityYear="1903" box="[397,544,1568,1592]" class="Actinopterygii" family="Lutjanidae" genus="Etelis" kingdom="Animalia" order="Perciformes" pageId="93" pageNumber="754" phylum="Chordata" rank="species" species="marshi">
