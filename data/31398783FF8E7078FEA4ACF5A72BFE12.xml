(5 
<taxonomicName id="7E904D16FF88707BFDD7A959A027F9F1" authorityName="Cuvier" authorityYear="1828" box="[592,794,1568,1592]" class="Actinopterygii" family="Lutjanidae" genus="Etelis" kingdom="Animalia" order="Perciformes" pageId="93" pageNumber="754" phylum="Chordata" rank="species" species="carbunculus">
