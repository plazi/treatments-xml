<normalizedToken id="39A4E2284E47E8BA85E5FC6EC3EA3104" originalValue="Monné">Monne</normalizedToken>
(1995b: 1, as 
<taxonomicName id="FA9021B0C94F1A3A2ABE858DFEE6D774" lsidName="" pageId="202" pageNumber="203" rank="tribe" tribe="Desmocerini">Desmocerini</taxonomicName>
