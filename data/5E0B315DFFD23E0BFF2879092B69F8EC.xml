<taxonomicName id="11A2FBC8FFD43E08FBFC793528F8F88D" authority="Cherbonnier, 1953" authorityName="Cherbonnier" authorityYear="1953" class="Holothuroidea" family="Cucumariidae" genus="Ocnus" kingdom="Animalia" order="Dendrochirotida" pageId="5" pageNumber="6" phylum="Echinodermata" rank="species" species="corbulus">
<emphasis id="E4D65C59FFD43E08FBFC79352CD1F8A7" box="[1091,1276,1854,1879]" italics="true" pageId="5" pageNumber="6">Ocnus corbulus</emphasis>
Cherbonnier, 
<number id="606A926CFFD43E08FF28796828F8F88D" box="[151,213,1891,1917]" pageId="5" pageNumber="6" value="1953.0">1953</number>
</taxonomicName>
