<collectingCountry id="F367189FFFF2FF8422C4EF16FCBCFEEC" box="[725,804,262,288]" name="France" pageId="32">France</collectingCountry>
, northern 
<collectingCountry id="F367189FFFF2FF842387EF15FC52FED3" box="[918,970,261,287]" name="Italy" pageId="32">Italy</collectingCountry>
, 
<collectingCountry id="F367189FFFF2FF8423C7EF16FBA1FEEC" box="[982,1081,262,288]" name="Hungary" pageId="32">Hungary</collectingCountry>
, 
<collectingCountry id="F367189FFFF2FF842454EF15FB32FED3" box="[1093,1194,261,287]" name="Slovakia" pageId="32">Slovakia</collectingCountry>
, 
<collectingCountry id="F367189FFFF2FF8424A4EF15FA8AFED3" box="[1205,1298,261,287]" name="Ukraine" pageId="32">Ukraine</collectingCountry>
, 
<collectingCountry id="F367189FFFF2FF84250FEF15FAF3FED3" box="[1310,1387,261,287]" name="Russia" pageId="32">Russia</collectingCountry>
, Africa south to southern 
<collectingCountry id="F367189FFFF2FF8421D1EF3AFD9BFE88" box="[448,515,298,324]" name="Egypt" pageId="32">Egypt</collectingCountry>
to 
<collectingCountry id="F367189FFFF2FF842236EF39FDC7FE8F" box="[551,607,297,323]" name="Serbia" pageId="32">Mali</collectingCountry>
, Asia north to 
<collectingCountry id="F367189FFFF2FF842310EF39FCCAFE8F" box="[769,850,297,323]" name="Turkey" pageId="32">Turkey</collectingCountry>
