<collectingCountry id="A0A376376728A4292260FDDC5798BD1B" box="[359,481,548,573]" name="Costa Rica" pageId="21" pageNumber="522">Costa Rica</collectingCountry>
and 
<collectingRegion id="1A70F8456728A429211FFDDD5409BD1A" box="[536,624,549,572]" country="Panama" name="Panama" pageId="21" pageNumber="522">
<collectingCountry id="A0A376376728A429211FFDDD5409BD1A" box="[536,624,549,572]" name="Panama" pageId="21" pageNumber="522">Panama</collectingCountry>
</collectingRegion>
(
<figureCitation id="408F2A226728A4292178FDDC5491BD1A" box="[639,744,548,573]" captionStart="FIGURE 17" captionStartId="27.[151,250,1766,1788]" captionTargetBox="[151,1435,193,1736]" captionTargetId="figure@27.[151,1436,193,1744]" captionTargetPageId="27" captionText="FIGURE 17. Data of distribution records of Epicadus species compiled from Machado et al. (2015, 2017); Silva-Moreira and Machado (2016); present study." httpUri="https://zenodo.org/record/1183164/files/figure.png" pageId="21" pageNumber="522">Fig. 17D</figureCitation>
).
</paragraph>
