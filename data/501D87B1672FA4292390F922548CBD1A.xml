<emphasis id="EAC0EAB5672EA42F2221F91B57A0B9DF" box="[294,473,1763,1785]" italics="true" pageId="19" pageNumber="520">
<taxonomicName id="1FB44D24672EA42F2221F91B57FDB9DF" box="[294,388,1763,1785]" class="Arachnida" family="Thomisidae" genus="Epicadus" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="520" phylum="Arthropoda" rank="genus">Epicadus</taxonomicName>
<taxonomicName id="1FB44D24672EA42F228DF91C57A0B9DF" box="[394,473,1764,1785]" pageId="19" pageNumber="520" rank="species" species="tigrinus" status="sp. nov.">tigrinus</taxonomicName>
