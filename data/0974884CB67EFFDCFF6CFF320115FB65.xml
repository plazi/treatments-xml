<taxonomicName id="46DD42D9B67EFFDCFDD6FD4E0080FCD5" authorityName="" authorityYear="1789" box="[557,715,747,771]" class="Magnoliopsida" family="Apocynaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="13" pageNumber="14" phylum="Magnoliopsida" rank="family">Apocynaceae</taxonomicName>
