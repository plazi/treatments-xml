</taxonomicName>
(
<taxonomicName id="46DD42D9B67EFFDCFDD5FD6300A8FD08" authorityName="" authorityYear="1789" box="[558,739,710,734]" class="Magnoliopsida" family="Amaranthaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="13" pageNumber="14" phylum="Magnoliopsida" rank="family">Amaranthaceae</taxonomicName>
