<taxonomicName id="4C014D765646FFEFFF0CEFA26CA443F4" authority="Gutu, 1997" authorityName="Gutu" authorityYear="1997" box="[151,405,430,455]" class="Malacostraca" family="Pagurapseudidae" genus="Macrolabrum" kingdom="Animalia" order="Tanaidacea" pageId="2" pageNumber="151" phylum="Arthropoda" rank="species" species="trifidus">
