<collectingRegion id="C04CF800FFEDA7786B99098563AFA8E8" box="[481,682,440,463]" country="Australia" name="New South Wales" pageId="14" pageNumber="431">New South Wales</collectingRegion>
, 
<collectingCountry id="7A9F7672FFEDA77868CF0985621FA8E8" box="[695,794,440,463]" name="Australia" pageId="14" pageNumber="431">Australia</collectingCountry>
(Haswell, 1880), Banda Sea, 
<collectingCountry id="7A9F7672FFEDA778687C09E86373A8CB" box="[516,630,469,492]" name="Indonesia" pageId="14" pageNumber="431">Indonesia</collectingCountry>
, 
<collectingCountry id="7A9F7672FFEDA77868FE09E8621CA8CB" box="[646,793,469,492]" name="South Africa" pageId="14" pageNumber="431">South Africa</collectingCountry>
(McCain &amp; Steinberg, 1970), 
<collectingCountry id="7A9F7672FFEDA778685D09CF63E7AB2E" box="[549,738,498,521]" name="New Caledonia" pageId="14" pageNumber="431">New Caledonia</collectingCountry>
and 
<collectingCountry id="7A9F7672FFEDA7786ADE0A2D6027AB00" box="[166,290,528,551]" name="Philippines" pageId="14" pageNumber="431">Philippines</collectingCountry>
(Laubitz, 1991; Guerra-García, 2002a), and 
<collectingCountry id="7A9F7672FFEDA7786ADE0A106073AB63" box="[166,374,557,580]" name="Papua New Guinea" pageId="14" pageNumber="431">Papua New Guinea</collectingCountry>
