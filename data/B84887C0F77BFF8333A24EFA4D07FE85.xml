<taxonomicName id="F7E14D55F77AFF80370D4FEF4AC7F7A0" authorityName="Schummel" authorityYear="1832" box="[1063,1220,2029,2051]" class="Insecta" family="Raphidiidae" genus="Raphidia" kingdom="Animalia" order="Raphidioptera" pageId="1" pageNumber="110" phylum="Arthropoda" rank="species" species="xanthostigma">
