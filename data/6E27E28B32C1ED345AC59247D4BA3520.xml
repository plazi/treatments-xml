<taxonomicName id="76B42D2A78FFC5B4D85202A49F12FC6A" authority="Hulcr &amp; Cognato, 2020" authorityName="Hulcr &amp; Cognato" authorityYear="2020" baseAuthorityName="Motschulsky" baseAuthorityYear="1863" class="Insecta" family="Curculionidae" genus="Debus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Debus adusticollis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="adusticollis">
