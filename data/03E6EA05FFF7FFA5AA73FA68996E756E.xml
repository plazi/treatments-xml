<taxonomicName id="4C4F2090FFF0FFA4A9D3F98D99B37738" authority="Dodge" authorityName="Dodge" box="[680,760,1601,1624]" pageId="12" pageNumber="13" rank="species" species="currani">currani</taxonomicName>
