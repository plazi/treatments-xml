r, 
<number id="C24FAD5F6B1CFFB95C81E2B19DA5FF6B" box="[919,933,221,244]" pageId="3" pageNumber="92" value="2.0">2</number>
- 
<date id="003999B86B1CFFB95CBBE2B19DCBFF6B" box="[941,971,221,244]" pageId="3" pageNumber="92">
<number id="C24FAD5F6B1CFFB95CBBE2B19DBBFF6B" box="[941,955,221,244]" pageId="3" pageNumber="92" value="2.0">2</number>
v
</date>
; metatarsus 
