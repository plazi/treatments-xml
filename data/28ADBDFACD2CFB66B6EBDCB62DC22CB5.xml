<taxonomicName id="F40656062563F25650D4B1719CF5A354" class="Gastropoda" family="Coryphellidae" genus="Gulenia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gulenia orjani" order="Nudibranchia" pageId="29" pageNumber="30" phylum="Mollusca" rank="species" species="orjani">Gulenia orjani</taxonomicName>
