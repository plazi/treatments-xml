<date id="FFF210607726DB149164FC29FD6DFBF3" box="[605,734,1013,1039]" pageId="1" pageNumber="60" value="1936" valueMax="1937">
<number id="3D8424877726DB149164FC29FD6DFBF3" box="[605,734,1013,1039]" pageId="1" pageNumber="60" value="1936.5" valueMax="1937.0" valueMin="1936.0">1936-1937</number>
