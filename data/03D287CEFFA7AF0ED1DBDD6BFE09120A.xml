<taxonomicName id="4C7B4D5BFFA7AF09D1F8DD6BFECC1161" authority="Vaz-De-Mello, 2008" authorityName="Vaz-De-Mello" authorityYear="2008" box="[180,362,543,569]" class="Insecta" family="Scarabaeidae" genus="Degallieridium" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B90FEACAFFA7AF09D1F8DD6BFECC1161" bold="true" box="[180,362,543,569]" italics="true" pageId="19" pageNumber="20">Degallieridium</emphasis>
