<bibRefCitation id="EFFCB19DDB195B7DCD7BF9838997F9A8" author="Bauer" box="[812,1215,1605,1630]" pageId="3" pageNumber="319" refString="Bauer, A. M., Sumontha, M. &amp; Pauwels, O. S. G. (2008) A new red-eyed Gekko (Reptilia: Gekkonidae) from Kanchanaburi Province, Thailand. Zootaxa, 1750, 32 - 42." type="journal article" year="2008">Bauer, Sumontha &amp; Pauwels, 2008</bibRefCitation>
</taxonomicName>
(
<taxonomicName id="4C6DB7EFDB195B7DCA86F9808877F9A8" authorityName="Gray" authorityYear="1825" box="[1233,1375,1605,1630]" class="Reptilia" family="Gekkonidae" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="319" phylum="Chordata" rank="family">Gekkonidae</taxonomicName>
), and 
<taxonomicName id="4C6DB7EFDB195B7DCEC0F9AF8F0DF974" authority="Smith" authorityName="Smith" authorityYear="1943" box="[151,549,1641,1666]" class="Reptilia" family="Viperidae" genus="Trimeresurus" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="319" phylum="Chordata" rank="species" species="kanburiensis">
