</taxonomicName>
, 
<taxonomicName id="4C6DB7EFDB195B7DCCE7F9E78C0BF9A8" authority="Bauer, Sumontha, Grossmann, Pauwels &amp; Vogel, 2004" authorityName="Bauer, Sumontha, Grossmann, Pauwels &amp; Vogel" authorityYear="2004" class="Reptilia" family="Gekkonidae" genus="Dixonius" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="319" phylum="Chordata" rank="species" species="hangseesom">
