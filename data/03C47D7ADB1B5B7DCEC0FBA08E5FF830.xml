<taxonomicName id="4C6DB7EFDB195B7DCAADF94888BDF950" box="[1274,1429,1677,1702]" class="Amphibia" family="Microhylidae" kingdom="Animalia" order="Anura" pageId="3" pageNumber="319" phylum="Chordata" rank="family">Microhylidae</taxonomicName>
) and 
<taxonomicName id="4C6DB7EFDB195B7DCE99F9748FAAF93C" authority="Hallowell" authorityName="Hallowell" authorityYear="1861" box="[206,642,1713,1738]" class="Amphibia" family="Rhacophoridae" genus="Polypedates" kingdom="Animalia" order="Anura" pageId="3" pageNumber="319" phylum="Chordata" rank="species" species="megacephalus">
