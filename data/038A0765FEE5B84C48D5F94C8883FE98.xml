<taxonomicName id="4C23CDF0FEE2B84448A2FF148B76FF28" authorityName="Mathot Fauna" authorityYear="1966" box="[224,364,189,215]" class="Insecta" family="Mymaridae" genus="Eofoersteria" kingdom="Animalia" order="Hymenoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
<emphasis id="B9576A61FEE2B84448A2FF148B76FF28" box="[224,364,189,215]" italics="true" pageId="308" pageNumber="309">Eofoersteria</emphasis>
