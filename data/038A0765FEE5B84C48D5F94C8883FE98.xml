<emphasis id="B9576A61FEE2B8444938FF17884BFF28" box="[378,593,190,215]" italics="true" pageId="308" pageNumber="309">Macrocamptoptera</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C23CDF0FEE2B8444ACCFF148953FF28" authorityName="Mathot" authorityYear="1966" box="[654,841,189,215]" class="Insecta" family="Mymaridae" genus="Stephanocampta" kingdom="Animalia" order="Hymenoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
<emphasis id="B9576A61FEE2B8444ACCFF148953FF28" box="[654,841,189,215]" italics="true" pageId="308" pageNumber="309">Stephanocampta</emphasis>
</taxonomicName>
. Tarsomere 
<quantity id="4CDB1B96FEE2B8444B91FF1489E7FF28" box="[979,1021,189,215]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="308" pageNumber="309" unit="in" value="4.0">4 in</quantity>
both 
<taxonomicName id="4C23CDF0FEE2B8444C7FFF148ED3FF28" authorityName="Mathot Fauna" authorityYear="1966" box="[1085,1225,189,215]" class="Insecta" family="Mymaridae" genus="Eofoersteria" kingdom="Animalia" order="Hymenoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
<emphasis id="B9576A61FEE2B8444C7FFF148ED3FF28" box="[1085,1225,189,215]" italics="true" pageId="308" pageNumber="309">Eofoersteria</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C23CDF0FEE2B8444CBDFF148F99FF28" authorityName="Annecke &amp; Doutt Fauna" authorityYear="1961" box="[1279,1411,189,215]" class="Insecta" family="Mymaridae" genus="Ptilomymar" kingdom="Animalia" order="Hymenoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
<emphasis id="B9576A61FEE2B8444CBDFF148F99FF28" box="[1279,1411,189,215]" italics="true" pageId="308" pageNumber="309">Ptilomymar</emphasis>
</taxonomicName>
