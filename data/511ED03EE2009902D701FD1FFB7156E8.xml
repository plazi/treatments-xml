<paragraph id="D9086128E2009901D731FB92FE6954BF" blockId="1.[264,1324,777,1689]" pageId="1" pageNumber="2">
Total length 
<quantity id="1E4FCCCDE2009901D7DFFB92FD06546F" box="[470,650,1097,1123]" metricMagnitude="-3" metricUnit="m" metricValue="2.08" metricValueMax="2.14" metricValueMin="2.02" pageId="1" pageNumber="2" unit="mm" value="2.08" valueMax="2.14" valueMin="2.02">2.02–2.14 mm</quantity>
. Wing length 
<quantity id="1E4FCCCDE2009901D537FB92FC7E546F" box="[830,1010,1097,1123]" metricMagnitude="-3" metricUnit="m" metricValue="1.22" metricValueMax="1.36" metricValueMin="1.08" pageId="1" pageNumber="2" unit="mm" value="1.22" valueMax="1.36" valueMin="1.08">1.08–1.36 mm</quantity>
