<number id="6F7F730FE2009901D701FBAAFE0D5487" box="[264,385,1137,1163]" pageId="1" pageNumber="2" value="1.7200000000000002" valueMax="1.87" valueMin="1.57">1.57–1.87</number>
. Wing length/profemur length 
<number id="6F7F730FE2009901D4E7FBAAFCEB5487" box="[750,871,1137,1163]" pageId="1" pageNumber="2" value="2.3049999999999997" valueMax="2.36" valueMin="2.25">2.25–2.36</number>
