<taxonomicName id="655E98650CFD3D98DDB4BCF90EA22178" LSID="http://zoobank.org/2FA441A2-4634-4A7F-B694-FD1A9414B43A" authority="Yoder" class="Insecta" family="Scelionidae" genus="Scelio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scelio clypeatus" order="Hymenoptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="clypeatus">Scelio clypeatus Yoder</taxonomicName>
<taxonomicNameLabel id="5E2B3AEF726735126548FA8F7ED4A975" pageId="152" pageNumber="153">sp. n.</taxonomicNameLabel>
