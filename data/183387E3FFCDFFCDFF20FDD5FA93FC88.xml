valid 
<taxonomicName id="579A4D76FFCDFFCDFD23FCF4FC96FC88" box="[660,867,868,893]" class="Adenophorea" family="Choniolaimidae" genus="Halichoanolaimus" kingdom="Animalia" order="Desmodorida" pageId="7" pageNumber="66" phylum="Nematoda" rank="genus">
