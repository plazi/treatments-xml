<taxonomicName id="4C7A4D1FB738FF8D64F87947DE28FB14" box="[428,600,1141,1167]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Chitonomyces" kingdom="Fungi" order="Laboulbeniales" pageId="105" pageNumber="106" phylum="Ascomycota" rank="species" species="melanurus">
