<taxonomicName id="812AA4C56B798BF09C4DA4F8FC446CE9" family="Hemirhamphidae" lsidName="" pageId="3" pageNumber="4" rank="family">Hemirhamphidae</taxonomicName>
buccal-cavity- 
<taxonomicName id="D894C77C8701BBC1772EA4B6463F3227" authority="Voigt, 1832" authorityName="Voigt" authorityYear="1832" class="Actinopterygii" family="Hemiramphidae" genus="Hemirhamphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hemirhamphus far" order="Beloniformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="far">Hemirhamphus far</taxonomicName>
(
<normalizedToken id="7923B35EB91EE0075069871816D0211B" originalValue="Forsskål">Forsskal</normalizedToken>
, 1775) and 
<taxonomicName id="39F6C5D5B7ABC83C907928D542345C68" authority="Voigt, 1832" authorityName="Voigt" authorityYear="1832" class="Actinopterygii" family="Hemiramphidae" genus="Hemirhamphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hemirhamphus robustus" order="Beloniformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="robustus">Hemirhamphus robustus</taxonomicName>
