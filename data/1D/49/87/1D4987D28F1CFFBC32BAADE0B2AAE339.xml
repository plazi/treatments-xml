<subSubSection box="[151,889,152,218]" pageId="15" pageNumber="16" type="etymology">
<paragraph blockId="15.[151,889,152,218]" box="[151,276,152,177]" pageId="15" pageNumber="16">
<emphasis box="[151,276,152,177]" italics="true" pageId="15" pageNumber="16">Etymology</emphasis>
</paragraph>
