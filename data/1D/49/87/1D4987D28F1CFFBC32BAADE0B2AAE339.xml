<paragraph blockId="14.[151,1436,1729,2035]" box="[198,1287,1769,1795]" pageId="14" pageNumber="15">
<taxonomicName box="[198,476,1769,1794]" class="Rhabditophora" family="Chromoplanidae" genus="Chromoplana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="14" pageNumber="15" phylum="Platyhelminthes" rank="species" species="kaikouris">
<emphasis box="[198,476,1769,1794]" italics="true" pageId="14" pageNumber="15">Chromoplana kaikouris</emphasis>
