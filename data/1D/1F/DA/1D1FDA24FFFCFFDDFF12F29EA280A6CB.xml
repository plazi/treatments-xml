<paragraph blockId="11.[151,1437,1337,2009]" box="[151,641,1337,1361]" pageId="11" pageNumber="38">
<emphasis bold="true" box="[151,292,1337,1361]" inLineHeading="true" pageId="11" pageNumber="38" reason="1">
<typeStatus box="[151,210,1337,1361]" pageId="11" pageNumber="38">Type</typeStatus>
