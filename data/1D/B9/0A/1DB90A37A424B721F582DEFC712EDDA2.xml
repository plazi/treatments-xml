<subSubSection type="nomenclature">
<paragraph pageNumber="338">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180296" class="Insecta" family="Formicidae" genus="Leptogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptogenys incisa suarensis Emery" lsidName-HNS="Leptogenys incisa suarensis Emery" order="Hymenoptera" pageNumber="338" phylum="Arthropoda" rank="subSpecies" species="incisa" subSpecies="suarensis">Leptogenys incisa Forel, subsp. suarensis</taxonomicName>
