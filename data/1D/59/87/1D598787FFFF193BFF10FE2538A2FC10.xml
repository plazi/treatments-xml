<number box="[228,315,871,895]" pageId="2" pageNumber="66" value="5.949999999999999" valueMax="7.1" valueMin="4.8">4.8–7.1</number>
; prosoma length 
<number box="[507,595,871,895]" pageId="2" pageNumber="66" value="2.9" valueMax="3.4" valueMin="2.4">2.4–3.4</number>
