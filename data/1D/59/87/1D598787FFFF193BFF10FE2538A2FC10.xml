, 
<number box="[1121,1172,620,644]" pageId="2" pageNumber="66" value="0.08">0.08</number>
, 
<number box="[1186,1238,620,644]" pageId="2" pageNumber="66" value="0.4">0.40</number>
, 
<number box="[1250,1302,620,644]" pageId="2" pageNumber="66" value="0.44">0.44</number>
, 
<number box="[1315,1367,620,644]" pageId="2" pageNumber="66" value="0.42">0.42</number>
, 
<number box="[1380,1433,620,644]" pageId="2" pageNumber="66" value="0.2">0.20</number>
. Legs: I: 
<number box="[244,293,655,679]" pageId="2" pageNumber="66" value="12.1">12.1</number>
(
<number box="[309,347,655,679]" pageId="2" pageNumber="66" value="3.4">3.4</number>
, 
<number box="[359,396,655,679]" pageId="2" pageNumber="66" value="1.3">1.3</number>
, 
<number box="[409,446,655,679]" pageId="2" pageNumber="66" value="3.5">3.5</number>
, 
<number box="[458,495,655,679]" pageId="2" pageNumber="66" value="2.8">2.8</number>
, 
<number box="[507,545,655,679]" pageId="2" pageNumber="66" value="1.1">1.1</number>
); II: 
<number box="[600,649,655,679]" pageId="2" pageNumber="66" value="13.4">13.4</number>
(
<number box="[665,704,655,679]" pageId="2" pageNumber="66" value="3.9">3.9</number>
, 
<number box="[715,752,655,679]" pageId="2" pageNumber="66" value="1.4">1.4</number>
, 
<number box="[764,801,655,679]" pageId="2" pageNumber="66" value="3.8">3.8</number>
, 
<number box="[813,850,655,679]" pageId="2" pageNumber="66" value="3.1">3.1</number>
