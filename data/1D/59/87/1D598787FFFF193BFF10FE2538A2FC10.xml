long, 
<number box="[335,370,620,644]" pageId="2" pageNumber="66" value="2.5">2.5</number>
wide. Eye diameters: 
<number box="[627,678,620,644]" pageId="2" pageNumber="66" value="0.15">0.15</number>
, 
<number box="[692,744,620,644]" pageId="2" pageNumber="66" value="0.22">0.22</number>
, 
