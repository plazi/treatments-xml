<materialsCitation ID-GBIF-Occurrence="3071291312" collectingDate="2020-04" collectionCode="PMCH" country="Vietnam" elevation="1700" location="Mu Cang Chai" pageId="3" pageNumber="195" specimenCount="3" specimenCount-female="2" specimenCount-male="1" stateProvince="Yen Bai">
