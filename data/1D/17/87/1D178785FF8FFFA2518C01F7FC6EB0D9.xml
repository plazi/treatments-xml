<subSubSection pageId="25" pageNumber="26" type="discussion">
<paragraph blockId="25.[794,1422,256,397]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[794,898,256,280]" pageId="25" pageNumber="26">Remarks</emphasis>
. 
<taxonomicName authority="Petrescu, 2018" authorityName="Petrescu" authorityYear="2018" box="[914,1120,256,280]" class="Malacostraca" family="Nannastacidae" genus="Campylaspis" kingdom="Animalia" order="Cumacea" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="radui" status="sp. nov.">
<emphasis box="[914,1120,256,280]" italics="true" pageId="25" pageNumber="26">Campylaspis radui</emphasis>
</taxonomicName>
