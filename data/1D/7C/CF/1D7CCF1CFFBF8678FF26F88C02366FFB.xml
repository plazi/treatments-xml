<subSubSection box="[199,773,1412,1437]" pageId="11" pageNumber="366" type="etymology">
<paragraph blockId="11.[151,1437,151,1437]" box="[199,773,1412,1437]" pageId="11" pageNumber="366">
<emphasis bold="true" box="[199,334,1412,1436]" pageId="11" pageNumber="366">Etymology.</emphasis>
