. — Mont-des-Récollets, ‘ 
<collectorName box="[513,702,661,685]" pageId="36" pageNumber="345">Grande Carrière’, N</collectorName>
<collectingCountry box="[706,772,662,685]" name="France" pageId="36" pageNumber="345">France</collectingCountry>
; 
<geoCoordinate box="[130,283,688,712]" degrees="50" direction="north" minutes="48" orientation="latitude" pageId="36" pageNumber="345" precision="1" seconds="02.74" value="50.800762">50°48’02.74”N</geoCoordinate>
, 
<geoCoordinate box="[293,425,688,712]" degrees="2" direction="east" minutes="30" orientation="longitude" pageId="36" pageNumber="345" precision="1" seconds="23.06" value="2.5064056">2°30’23.06”E</geoCoordinate>
