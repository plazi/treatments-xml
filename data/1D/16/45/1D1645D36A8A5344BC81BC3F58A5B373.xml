<subSubSection pageId="0" pageNumber="141" type="etymology">
<paragraph pageId="0" pageNumber="141">Etymology.</paragraph>
<paragraph pageId="0" pageNumber="141">The species epithet refers to the type locality, Medog County.</paragraph>
</subSubSection>
<subSubSection pageId="0" pageNumber="141" type="vernacular name">
<paragraph pageId="0" pageNumber="141">Vernacular name.</paragraph>
