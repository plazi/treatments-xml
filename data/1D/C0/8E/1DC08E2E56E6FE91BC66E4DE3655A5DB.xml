<taxonomicName class="Insecta" family="Charipidae" genus="Phaenoglyphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phaenoglyphis" order="Hymenoptera" pageId="0" pageNumber="8049" phylum="Arthropoda" rank="genus">Phaenoglyphis</taxonomicName>
