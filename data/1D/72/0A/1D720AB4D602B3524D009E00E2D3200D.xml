and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25263" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" lsidName-HNS="Anochetus grandidieri Forel" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="grandidieri">grandidieri</taxonomicName>
A; 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:235700" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" lsidName-HNS="Anochetus boltoni Fisher" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="boltoni">boltoni</taxonomicName>
