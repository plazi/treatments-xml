<quantity box="[813,919,564,590]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="15" pageNumber="16" unit="mm" value="0.55">0.55 mm</quantity>
<emphasis box="[923,1238,564,589]" italics="true" pageId="15" pageNumber="16">
.......................... 
