<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Trifolium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Trifolium arvense" order="Fabales" pageId="0" pageNumber="207" phylum="Tracheophyta" rank="species" species="arvense">
