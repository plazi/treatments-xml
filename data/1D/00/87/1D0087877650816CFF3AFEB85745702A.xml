<paragraph blockId="12.[151,291,332,358]" box="[151,291,332,358]" pageId="12" pageNumber="313">
<heading bold="true" box="[151,291,332,358]" fontSize="11" level="1" pageId="12" pageNumber="313" reason="1">
<taxonomicName box="[151,291,332,358]" class="Malacostraca" family="Diastylidae" kingdom="Animalia" order="Cumacea" pageId="12" pageNumber="313" phylum="Arthropoda" rank="family">
