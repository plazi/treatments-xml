<quantity box="[672,841,1478,1503]" metricMagnitude="3" metricUnit="m" metricValue="5.231" metricValueMax="5.415" metricValueMin="5.047" pageId="19" pageNumber="320" unit="m" value="5231.0" valueMax="5415.0" valueMin="5047.0">5047 – 5415 m</quantity>
