</taxonomicName>
and 
<taxonomicName id="7FFB4D60FF84FFD5FE7AFA27FDEE7442" authority="Monro, 1936" authorityName="Monro" authorityYear="1936" box="[420,516,284,309]" class="Polychaeta" family="Pilargidae" genus="Loandalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="7" pageNumber="66" phylum="Annelida" rank="species" species="riojai">
