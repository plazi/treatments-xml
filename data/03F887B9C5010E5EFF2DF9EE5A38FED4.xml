<taxonomicName id="4C514D2CC5000E5EFB76FF755A6BFF0C" authorityName="Zanclea Cocco" authorityYear="1832" box="[1228,1345,213,238]" class="Malacostraca" family="Portunidae" genus="Charybdis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="204" phylum="Arthropoda" rank="genus">
<emphasis id="B925EABDC5000E5EFB76FF755A6BFF0C" box="[1228,1345,213,238]" italics="true" pageId="3" pageNumber="204">Charybdis</emphasis>
</taxonomicName>
