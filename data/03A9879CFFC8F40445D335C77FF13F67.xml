<quantity id="4CF89B6FFFC8F40A476333207F9739A2" box="[551,654,1902,1926]" metricMagnitude="-2" metricUnit="m" metricValue="1.3199999999999998" pageId="54" pageNumber="55" unit="mm" value="13.2">13.2 mm</quantity>
