<taxonomicName id="4C3ABDD84019B23A67781CB6FAC8FBB2" authority="Rahayu &amp; Ng, 2019" authorityName="Rahayu &amp; Ng" authorityYear="2019" box="[800,1344,1012,1038]" class="Malacostraca" family="Hymenosomatidae" genus="Elamenopsis" kingdom="Animalia" order="Decapoda" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="gracilipes">
