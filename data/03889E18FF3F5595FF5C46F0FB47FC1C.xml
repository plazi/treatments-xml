<quantity id="4CD982EBFF3D5594FC6A43E1FBCFFD45" box="[929,1097,639,664]" metricMagnitude="-3" metricUnit="m" metricValue="1.62" metricValueMax="1.68" metricValueMin="1.56" pageId="130" pageNumber="131" unit="mm" value="1.62" valueMax="1.68" valueMin="1.56">1.56–1.68 mm</quantity>
, (
<specimenCount id="9D27E487FF3D5594FBAA43E1FBF5FD4A" box="[1121,1139,640,664]" pageId="130" pageNumber="131" type="female">♀</specimenCount>
