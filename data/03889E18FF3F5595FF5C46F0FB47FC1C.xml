<quantity id="4CD982EBFF3D5594FB4343E1FB77FD45" box="[1160,1265,640,664]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="130" pageNumber="131" unit="mm" value="1.8">1.80 mm</quantity>
