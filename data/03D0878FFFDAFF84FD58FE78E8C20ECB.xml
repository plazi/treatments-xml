<taxonomicName id="4C794D1AFFDBFF84FE91FD79EBB90E5E" box="[285,496,1601,1624]" class="Insecta" family="Apidae" genus="Pasites" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="maculatus">
