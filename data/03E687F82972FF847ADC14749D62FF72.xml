<collectingCountry id="F358767E2971FF8479AB17E19821FF55" box="[992,1086,190,216]" name="Panama" pageId="54" pageNumber="55">Panamá</collectingCountry>
(
<bibRefCitation id="EFDE4B1F2971FF847E0417E19917FF55" author="Child, C. A." box="[1103,1288,190,216]" pageId="54" pageNumber="55" pagination="1 - 86" refId="ref76281" refString="Child, C. A. (1979) Shallow-water Pycnogonida of the Isthmus of Panama and the coasts of Middle America. Smithsonian Contributions to Zoology, 293, 1 - 86." type="journal article" year="1979">Child 1979: 34</bibRefCitation>
) and 
<collectingCountry id="F358767E2971FF847F0517E19983FF55" box="[1358,1436,190,216]" name="Kenya" pageId="54" pageNumber="55">Kenya</collectingCountry>
(
<bibRefCitation id="EFDE4B1F2971FF847AD417BA9D6FFF72" author="Muller, H. - G." box="[159,368,229,255]" pageId="54" pageNumber="55" pagination="63 - 79" refId="ref77146" refString="Muller, H. - G. (1990 c) Shallow-water Pycnogonida from Kenya and Sri Lanka, with descriptions of three new species. Bonner zoologische Beitrage 41 (1), 63 - 79; Bonn." type="journal article" year="1990">Müller 1990c: 67</bibRefCitation>
