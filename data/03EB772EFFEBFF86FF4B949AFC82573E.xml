<number id="3D8AD41FFFE9FF86FE7F92C8FE3655CE" box="[419,474,189,215]" pageId="31" pageNumber="32" value="0.35">0.35</number>
/ 
<number id="3D8AD41FFFE9FF86FE3892C8FDF755CE" box="[484,539,189,215]" pageId="31" pageNumber="32" value="0.25">0.25</number>
