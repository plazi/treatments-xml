<number id="3D8AD41FFFE9FF86FB1B92EDFB1155AB" box="[1223,1277,152,178]" pageId="31" pageNumber="32" value="1.35">1.35</number>
/ 
<number id="3D8AD41FFFE9FF86FADA92EDFAC555AB" box="[1286,1321,152,178]" pageId="31" pageNumber="32" value="1.1">1.1</number>
