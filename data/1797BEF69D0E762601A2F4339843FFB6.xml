<normalizedToken id="6056FF4356C508238D63397DBBE2CBDB" originalValue="Pará">Para</normalizedToken>
and 
<normalizedToken id="F88ED5FDAC5558E3E38310606651313D" originalValue="Maranhão">Maranhao</normalizedToken>
