<taxonomicName id="9FCC4D051951FF8BFC6A2295FC35AEF6" box="[918,1017,408,431]" class="Arachnida" family="Stigmaeidae" genus="Prostigmaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="integrius">
