<taxonomicName id="4C714D292160F55FFF19B4D4FE45FE79" box="[238,394,413,439]" class="Maxillopoda" family="Austrobalanidae" genus="Protelminius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="7" pageNumber="46" phylum="Arthropoda" rank="genus">
