<quantity id="1669F2E2FF80C23F5697E91C6CF5991A" box="[417,510,306,332]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="16" pageNumber="17" unit="mm" value="4.1">4.1 mm</quantity>
