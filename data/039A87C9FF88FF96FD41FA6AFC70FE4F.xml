<materialsCitation id="3B5B3C82FF89FF96FFCBFF78FE9CF9C4" ID-GBIF-Occurrence="3313181335" collectionCode="PIN" county="Occiput" location="Scape" municipality="Eyes" pageId="9" pageNumber="10" specimenCode="PIN 3130" specimenCount="3" stateProvince="West Coast" typeStatus="paratype">
