<location id="8EEC6004FF89FF96FEA4FAEFFEC3FAA9" LSID="urn:lsid:plazi:treatment:039A87C9FF88FF96FD41FA6AFC70FE4F:8EEC6004FF89FF96FEA4FAEFFEC3FAA9" box="[276,354,1366,1389]" county="Occiput" municipality="Eyes" name="Eighth" pageId="9" pageNumber="10" stateProvince="West Coast">Eighth</location>
