<typeStatus id="5488887DFF89FF96FE02FF78FD93FF1C" box="[434,562,193,216]" pageId="9" pageNumber="10">paratypes</typeStatus>
); wing length = 
<quantity id="4CCB9B3AFF89FF96FF58FF67FEEEFF31" box="[232,335,222,245]" metricMagnitude="-3" metricUnit="m" metricValue="1.46" pageId="9" pageNumber="10" unit="mm" value="1.46">1.46 mm</quantity>
(
