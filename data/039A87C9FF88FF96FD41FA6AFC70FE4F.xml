<quantity id="4CCB9B3AFF89FF96FF6EFF78FE38FF1C" box="[222,409,193,216]" metricMagnitude="-3" metricUnit="m" metricValue="1.845" metricValueMax="1.85" metricValueMin="1.84" pageId="9" pageNumber="10" unit="mm" value="1.845" valueMax="1.85" valueMin="1.84">1.84 –1.85 mm</quantity>
