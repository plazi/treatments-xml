<taxonomicName id="87E9E13148832DFDEC0D3E152F8AD5DF" authorityName="Balat" authorityYear="1955" class="insecta" family="Philopteridae" genus="Brueelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brueelia kluzi" order="Phthiraptera" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="kluzi">
