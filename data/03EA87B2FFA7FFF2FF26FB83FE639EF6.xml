<emphasis id="B937EAB6FFA7FFF2FF26FB17FEAB9ED2" bold="true" box="[151,256,1153,1178]" pageId="25" pageNumber="326">Remark.</emphasis>
Probably 
<taxonomicName id="4C434D27FFA7FFF2FECFFB14FDC49ED2" authority="Bagnall" authorityName="Bagnall" box="[382,623,1153,1178]" class="Entognatha" family="Isotomidae" genus="Hemisotoma" kingdom="Animalia" order="Collembola" pageId="25" pageNumber="326" phylum="Arthropoda" rank="genus">
