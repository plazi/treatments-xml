<taxonomicName id="4ECA2BEB497CA2B1C2C8767708C842B3" authority="Cameron, 1881" authorityName="Cameron" authorityYear="1881" class="Insecta" family="Bethylidae" genus="Sierola" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sierola testaceipes" order="Hymenoptera" pageId="0" pageNumber="405" phylum="Arthropoda" rank="species" species="testaceipes">Sierola testaceipes</taxonomicName>
