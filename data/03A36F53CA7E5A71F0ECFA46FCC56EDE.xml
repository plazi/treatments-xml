<subSubSection id="C3108DCECA7E5A71F0ECFA36FB976F2E" box="[151,1147,1535,1562]" pageId="31" pageNumber="32" type="materials_examined">
<paragraph id="8BB5DE45CA7E5A71F0ECFA36FB976F2E" blockId="31.[151,1436,1535,2026]" box="[151,1147,1535,1562]" pageId="31" pageNumber="32">
<emphasis id="B97E0257CA7E5A71F0ECFA36FEE16F2D" bold="true" box="[151,269,1535,1561]" pageId="31" pageNumber="32">Material:</emphasis>
<collectionCode id="ED1B4680CA7E5A71F16EF9C9FE816F2E" LSID="urn:lsid:biocol.org:col:34992" box="[277,365,1536,1562]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="31" pageNumber="32">RMNH</collectionCode>
Coel. 
<number id="3DC2CC62CA7E5A71F1C1F9C9FDEA6F2E" box="[442,518,1536,1562]" pageId="31" pageNumber="32" value="38782.0">38782</number>
