<taxonomicName id="AEA44D52247FFFA4FFE8E943FD8AF9BA" authority="(Conrad, 1857)" authorityName="Conrad" authorityYear="1857" baseAuthorityName="Conrad" baseAuthorityYear="1857" box="[108,543,1585,1609]" class="Cephalopoda" family="Sphenodiscidae" genus="Sphenodiscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ammonitida" pageId="22" pageNumber="23" phylum="Mollusca" rank="species" species="pleurisepta">
<emphasis id="5BD0EAC3247FFFA4FFE8E943FEE5F9BA" box="[108,368,1585,1609]" italics="true" pageId="22" pageNumber="23">Sphenodiscus pleurisepta</emphasis>
(
<bibRefCitation id="0D354B20247FFFA4FEFBE943FD82F9BA" author="Conrad, T. A." box="[383,535,1585,1609]" pageId="22" pageNumber="23" pagination="141 - 174" refId="ref20265" refString="Conrad, T. A. 1857. Descriptions of Cretaceous and Tertiary fossils. In W. H. Emory (editor), Report on the United States and Mexican Boundary Survey. United States 34 th Congress, 1 st Session, Senate Ex Document 108 and House Ex Document 135 1 (2): 141 - 174." type="journal article" year="1857">Conrad, 1857</bibRefCitation>
)
</taxonomicName>
. 
<treatmentCitation id="E80510C0247FFFA4FDADE943FC11F9BA" author="Landman, N. H. &amp; W. A. Cobban" box="[553,900,1585,1609]" page="17" pageId="22" pageNumber="23" year="2003">
