<taxonomicName id="4C0A11DCC04EB821801FC3A6FD517BD5" box="[623,752,1920,1945]" class="Elasmobranchii" family="Scyliorhinidae" genus="Apristurus" kingdom="Animalia" order="Carcharhiniformes" pageId="11" pageNumber="267" phylum="Chordata" rank="species" species="australis">
