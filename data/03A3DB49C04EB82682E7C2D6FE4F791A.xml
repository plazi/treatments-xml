<taxonomicName id="4C0A11DCC04EB8218047C37BFD187B39" box="[567,697,1884,1909]" class="Elasmobranchii" family="Scyliorhinidae" genus="Apristurus" kingdom="Animalia" order="Carcharhiniformes" pageId="11" pageNumber="267" phylum="Chordata" rank="species" species="herklotsi">
