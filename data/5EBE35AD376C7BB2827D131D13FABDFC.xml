</paragraph>
<paragraph id="1D51B8D0DB148A827317F381A126E91D" pageId="818" pageNumber="819">
<taxonomicName id="8652069ECBD253AD77DC9BD59C49CFAC" authorityName="Casey" authorityYear="1920" class="Hexapoda" family="Carabidae" genus="Lebia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="818" pageNumber="819" phylum="Arthropoda" rank="species" species="pacifica">Lebia pacifica</taxonomicName>
