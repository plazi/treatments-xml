<taxonomicName id="629BED659A55EE0AE7F905F1F2EB6CBE" authorityName="LeConte" authorityYear="1851" class="Hexapoda" family="Carabidae" genus="Lebia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="818" pageNumber="819" phylum="Arthropoda" rank="species" species="guttula">Lebia guttula</taxonomicName>
