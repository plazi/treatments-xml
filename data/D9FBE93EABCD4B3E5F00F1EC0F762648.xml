[[ 
<taxonomicName id="48F813A1CD15B49C56AF7999B83B5FA0" LSID-ZBK="urn:lsid:zoobank.org:act:3211E178-02DC-46D1-8ACE-964198B34650" class="Actinopterygii" family="Cichlidae" genus="Geophagus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Geophagus Heckel 1840:383" order="Perciformes" pageNumber="2" phylum="Chordata" rank="genus">Geophagus Heckel</taxonomicName>
