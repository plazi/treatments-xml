<taxonomicName id="AB8764B1FFC6FFE2FDFFF93DFD7BF8BE" box="[603,744,1776,1793]" class="Insecta" family="Gryllidae" genus="Matuanus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="280" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="5EF3C320FFC6FFE2FDFFF93DFD38F8BE" box="[603,683,1776,1793]" italics="true" pageId="7" pageNumber="280">Matuanus</emphasis>
