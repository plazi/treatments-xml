<taxonomicName id="4C0E95D9FFB0C05787B6F9D7BD68F9BB" box="[429,519,1584,1605]" class="Insecta" family="Cicadellidae" genus="Nicolaus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="529" phylum="Arthropoda" rank="genus">
