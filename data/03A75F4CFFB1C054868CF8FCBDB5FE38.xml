<taxonomicName id="4C0E95D9FFB0C0578443F9D6BDA2F9BB" box="[600,717,1584,1606]" class="Insecta" family="Cicadellidae" genus="Nicolaus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="529" phylum="Arthropoda" rank="species" species="abuensis" status="sp. nov.">
