<taxonomicName id="4C0E95D9FFB0C057833DF9D6BAF3F9B8" box="[1318,1436,1585,1606]" class="Insecta" family="Cicadellidae" genus="Nicolaus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="529" phylum="Arthropoda" rank="species" species="cornutus" status="sp. nov.">
