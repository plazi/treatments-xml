<emphasis id="B906EAE8FF94A574FC70F804B296E3D3" box="[931,985,1931,1956]" italics="true" pageId="9" pageNumber="25">neffi</emphasis>
Urban
</taxonomicName>
(
<collectingCountry id="F365766AFF94A574FBEEF805B5F2E3D3" box="[1085,1213,1930,1956]" name="Argentina" pageId="9" pageNumber="25">Argentina</collectingCountry>
), 
<taxonomicName id="4C724D79FF94A574FB00F804B4D3E3D3" authority="Friese" authorityName="Friese" box="[1235,1436,1930,1956]" class="Insecta" family="Apidae" genus="Svastrides" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="25" phylum="Arthropoda" rank="species" species="zebra">
