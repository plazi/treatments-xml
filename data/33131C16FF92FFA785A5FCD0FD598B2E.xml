<taxonomicName id="7CBAD683FF92FFA78474FAB2FDFE889E" box="[326,621,1405,1430]" class="Malacostraca" family="Lysianassidae" genus="Waldeckia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="312" phylum="Arthropoda" rank="species" species="brachycephala">
