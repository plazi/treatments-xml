and 
<taxonomicName id="7CBAD683FF92FFA780B2FAB1FEB388B2" class="Malacostraca" family="Lysianassidae" genus="Waldeckia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="312" phylum="Arthropoda" rank="species" species="selayarensis" status="sp. nov.">
