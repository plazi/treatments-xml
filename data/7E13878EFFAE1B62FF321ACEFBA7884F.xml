<subSubSection id="BEA06513FFAF1B62FF071E47FBBD8C2F" box="[185,1108,272,299]" pageId="25" pageNumber="26" type="etymology">
<paragraph id="F6053698FFAF1B62FF071E47FBBD8C2F" blockId="25.[140,1108,167,1355]" box="[185,1108,272,299]" pageId="25" pageNumber="26">
<emphasis id="C4CEEA8AFFAF1B62FF071E47FED78C2F" bold="true" box="[185,318,272,298]" pageId="25" pageNumber="26">
<emphasis id="C4CEEA8AFFAF1B62FF071E47FED08C2F" box="[185,313,272,298]" inLineHeading="true" pageId="25" pageNumber="26" reason="1">Etymology</emphasis>
