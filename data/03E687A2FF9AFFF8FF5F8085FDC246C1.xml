<quantity id="4CB79B51FF9AFFFAFC7C8355FB8143F8" box="[948,1038,1909,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="36" pageNumber="487" unit="mm" value="1.5">1.5 mm</quantity>
; AL= 
<quantity id="4CB79B51FF9AFFFAFB9D8355FB2143F8" box="[1109,1198,1909,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="36" pageNumber="487" unit="mm" value="1.1">1.1 mm</quantity>
; SW= 
<quantity id="4CB79B51FF9AFFFAFB348355FADA43F8" box="[1276,1365,1909,1936]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="36" pageNumber="487" unit="mm" value="1.2">1.2 mm</quantity>
; AW= 
<quantity id="4CB79B51FF9AFFFAFF5F83B9FF7F43DC" box="[151,240,1945,1972]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="36" pageNumber="487" unit="mm" value="0.7">0.7 mm</quantity>
