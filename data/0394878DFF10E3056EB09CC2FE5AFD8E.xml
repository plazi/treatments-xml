<quantity id="4CC59B7EFF10E3096DC79F29FB9CF91F" box="[992,1094,1649,1674]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="20" pageNumber="21" unit="mm" value="1.05">1.05 mm</quantity>
, PL = 
<quantity id="4CC59B7EFF10E3096AB69F29FB30F91F" box="[1169,1258,1649,1673]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="20" pageNumber="21" unit="mm" value="1.7">1.7 mm</quantity>
