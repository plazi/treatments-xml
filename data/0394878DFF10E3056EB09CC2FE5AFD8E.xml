, PYL = 
<quantity id="4CC59B7EFF10E3096FAA9FCDFE3FF93B" box="[397,485,1685,1709]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="20" pageNumber="21" unit="mm" value="1.1">1.1 mm</quantity>
, PYW = 
<quantity id="4CC59B7EFF10E3096C6A9FCDFD7FF93B" box="[589,677,1685,1709]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="20" pageNumber="21" unit="mm" value="1.2">1.2 mm</quantity>
