<taxonomicName id="0C8A4D52410AFF8BBFB22F01FDA79A78" authority="Claassen, 1923" authorityName="Claassen" authorityYear="1923" box="[189,631,1359,1383]" class="Insecta" family="Leuctridae" genus="Leuctra" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="37" phylum="Arthropoda" rank="species" species="sibleyi">
