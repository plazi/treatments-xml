<taxonomicName id="7AC7AE709F383B6A1EB86AC7A1C20168" LSID-ZBK="urn:lsid:zoobank.org:act:AA1B525C-6B62-477E-A45B-87C44FE5987A" class="Actinopterygii" family="Bagridae" genus="Rita" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Rita Bleeker 1854:122" order="Siluriformes" pageNumber="9" phylum="Chordata" rank="genus">Rita Bleeker</taxonomicName>
