<taxonomicName id="45B44D3DFF87FFECFE22FBCAFD80FB94" box="[484,557,1046,1071]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" kingdom="Plantae" order="Fagales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
