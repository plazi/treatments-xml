<collectingRegion id="4070F85CFF87FFECFE44FBE7FE7AFBEE" box="[386,471,1083,1109]" country="Germany" name="Hessen" pageId="59" pageNumber="60">Hessen</collectingRegion>
(
<collectingCountry id="FAA3762EFF87FFECFE23FBE7FDFBFBEE" box="[485,598,1083,1109]" name="Germany" pageId="59" pageNumber="60">Germany</collectingCountry>
) one of the few millipedes in 
<taxonomicName id="45B44D3DFF87FFECFC69FBE7FC5FFBEF" box="[943,1010,1083,1108]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFC69FBE7FC5FFBEF" box="[943,1010,1083,1108]" italics="true" pageId="59" pageNumber="60">Picea</emphasis>
</taxonomicName>
monocultures, raised bog, shrub, heathland, grassland, cave. Found from 
<quantity id="454C9B5BFF87FFECFD6CFBBCFC99FBC1" box="[682,820,1120,1146]" metricMagnitude="2" metricUnit="m" metricValue="7.0" metricValueMax="7.8" metricValueMin="6.2" pageId="59" pageNumber="60" unit="m" value="700.0" valueMax="780.0" valueMin="620.0">620â€“780 m</quantity>
a.s.l. in 
<collectingCountry id="FAA3762EFF87FFECFC60FBBCFB9BFBC1" box="[934,1078,1120,1146]" name="Switzerland" pageId="59" pageNumber="60">Switzerland</collectingCountry>
