</taxonomicName>
, 
<taxonomicName id="45B44D3DFF87FFECFDFAFBCAFD0BFB94" box="[572,678,1046,1071]" class="Magnoliopsida" family="Oleaceae" genus="Fraxinus" kingdom="Plantae" order="Lamiales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFDFAFBCAFD0BFB94" box="[572,678,1046,1071]" italics="true" pageId="59" pageNumber="60">Fraxinus</emphasis>
</taxonomicName>
, 
<taxonomicName id="45B44D3DFF87FFECFD75FBC9FD47FB94" box="[691,746,1045,1071]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFD75FBC9FD47FB94" box="[691,746,1045,1071]" italics="true" pageId="59" pageNumber="60">Acer</emphasis>
</taxonomicName>
, 
<taxonomicName id="45B44D3DFF87FFECFD3FFBC9FCF6FB94" box="[761,859,1045,1071]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFD3FFBC9FCF6FB94" box="[761,859,1045,1071]" italics="true" pageId="59" pageNumber="60">Quercus</emphasis>
</taxonomicName>
, montane 
<emphasis id="B0C0EAACFF87FFECFC12FBC9FBCFFB94" box="[980,1122,1045,1071]" italics="true" pageId="59" pageNumber="60">Abies-Picea</emphasis>
