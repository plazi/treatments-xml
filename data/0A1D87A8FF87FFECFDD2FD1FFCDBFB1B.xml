<taxonomicName id="45B44D3DFF87FFECFB7FFBCAFB51FB94" box="[1209,1276,1046,1071]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFB7FFBCAFB51FB94" box="[1209,1276,1046,1071]" italics="true" pageId="59" pageNumber="60">Picea</emphasis>
</taxonomicName>
and 
<taxonomicName id="45B44D3DFF87FFECFAF5FBCAFADBFB94" box="[1331,1398,1046,1071]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="59" pageNumber="60" phylum="Tracheophyta" rank="genus">
<emphasis id="B0C0EAACFF87FFECFAF5FBCAFADBFB94" box="[1331,1398,1046,1071]" italics="true" pageId="59" pageNumber="60">Pinus</emphasis>
</taxonomicName>
