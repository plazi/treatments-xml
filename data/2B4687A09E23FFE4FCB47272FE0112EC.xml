<taxonomicName id="64EF4D359E2CFFE4FF047090FED912EC" baseAuthorityName="Lafresnaye" baseAuthorityYear="1844" box="[183,303,1162,1185]" class="Aves" family="Vireonidae" genus="Vireo" kingdom="Animalia" order="Passeriformes" pageId="40" pageNumber="40" phylum="Chordata" rank="species" species="leucophrys">
