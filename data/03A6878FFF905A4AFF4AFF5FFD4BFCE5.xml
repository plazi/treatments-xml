, 
<specimenCount id="9D09FD10FF905A4AFD55FCF2FD4BFCE5" box="[648,715,826,852]" pageId="17" pageNumber="18" type="generic">
<quantity id="4CF79B7CFF905A4AFD55FCF2FD47FCE5" box="[648,711,826,852]" metricMagnitude="1" metricUnit="m" metricValue="1.7" pageId="17" pageNumber="18" unit="m" value="17.0">17 m</quantity>
