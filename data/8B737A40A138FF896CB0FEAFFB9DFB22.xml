<quantity id="C42266B3A13BFF89694AFE2DFF18FDEF" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="2" pageNumber="65" unit="mm" value="0.28">0.28 mm</quantity>
, t 
