<subSubSection id="4BC098DDA13BFF896CE0FBB8FC34FBBA" box="[199,1007,1093,1116]" pageId="2" pageNumber="65" type="etymology">
<paragraph id="0365CB56A13BFF896CE0FBB8FC34FBBA" blockId="2.[151,1436,152,1220]" box="[199,1007,1093,1116]" pageId="2" pageNumber="65">
<emphasis id="31AE1744A13BFF896CE0FBB8FEECFBBA" box="[199,311,1093,1116]" italics="true" pageId="2" pageNumber="65">Etymology</emphasis>
