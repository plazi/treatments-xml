: 
<quantity id="C42266B3A13BFF896CD7FE0FFE8BFDEF" box="[240,336,498,521]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="2" pageNumber="65" unit="mm" value="0.13">0.13 mm</quantity>
, t 
<number id="B512D971A13BFF896D42FE0FFEA8FDEF" box="[357,371,498,521]" pageId="2" pageNumber="65" value="3.0">3</number>
: 
<quantity id="C42266B3A13BFF896D5AFE0FFE05FDEF" box="[381,478,498,521]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="2" pageNumber="65" unit="mm" value="0.16">0.16 mm</quantity>
