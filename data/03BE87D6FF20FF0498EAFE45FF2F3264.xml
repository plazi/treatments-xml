<quantity id="4CEF9B25FF20FF049D54FE45FF2E31D4" box="[1321,1391,439,465]" metricMagnitude="-1" metricUnit="m" metricValue="7.1" pageId="177" pageNumber="178" unit="cm" value="71.0">71 cm</quantity>
