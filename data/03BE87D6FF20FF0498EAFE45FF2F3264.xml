<quantity id="4CEF9B25FF20FF049AADFDB4F9703267" box="[720,817,582,610]" metricMagnitude="1" metricUnit="m" metricValue="4.84632" metricValueMax="9.6012" metricValueMin="0.09144" pageId="177" pageNumber="178" unit="ft" value="159.0" valueMax="315.0" valueMin="3.0">3–315 ft</quantity>
