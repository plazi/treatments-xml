</taxonomicName>
(
<taxonomicName id="4C0F4D457B5A0324ECA4FD18FCEAFD31" authorityName="" authorityYear="1789" box="[667,812,692,716]" class="Magnoliopsida" family="Annonaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Magnoliales" pageId="34" pageNumber="35" phylum="Magnoliopsida" rank="family">Annonaceae</taxonomicName>
); 
<taxonomicName id="4C0F4D457B5A0324ED00FD18FB67FD30" authorityName="Wall. ex Nees" authorityYear="1833" box="[831,1185,692,717]" class="Magnoliopsida" family="Lauraceae" genus="Lepidadenia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Laurales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="wightiana" subGenus="Litsea">
