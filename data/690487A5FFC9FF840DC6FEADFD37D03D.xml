<collectingCountry id="99BA7623FFC9FF840F2BFE0EFDBFD28E" box="[532,634,483,509]" name="Vietnam" pageId="10" pageNumber="202">Vietnam</collectingCountry>
, 
<collectingCountry id="99BA7623FFC9FF840FB4FE0EFCC1D28E" box="[651,772,483,509]" name="Cambodia" pageId="10" pageNumber="202">Cambodia</collectingCountry>
and Lao PDR; 
<collectingCountry id="99BA7623FFC9FF840C64FDE8FE08D16C" box="[347,461,517,543]" name="Indonesia" pageId="10" pageNumber="202">Indonesia</collectingCountry>
