<taxonomicName id="18CC4D4CFFB39753FF36FC467642D8C3" authority="Li &amp; Yang" authorityName="Li &amp; Yang" box="[151,549,902,928]" class="Insecta" family="Leuctridae" genus="Rhopalopsole" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="18" phylum="Arthropoda" rank="species" species="wolong" status="sp. nov.">
