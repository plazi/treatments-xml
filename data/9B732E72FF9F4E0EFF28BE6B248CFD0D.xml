<taxonomicName id="D4DAE4E7FF9F4E0EFBFDB8B12498FDAD" authority="Pace, 2014" authorityName="Pace" authorityYear="2014" box="[1078,1158,489,513]" class="Insecta" family="Staphylinidae" genus="Parademosoma" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="subGenus" status="subg. n." subGenus="Elixusa">
