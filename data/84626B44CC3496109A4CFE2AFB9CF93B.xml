<taxonomicName id="CBCBA1D1CC3496109A3EFB3EFD44FB1B" authority="Risso, 1826" authorityName="Risso" authorityYear="1826" box="[358,683,1162,1189]" class="Gastropoda" family="Ovulidae" genus="Simnia" kingdom="Animalia" order="Littorinimorpha" pageId="12" pageNumber="341" phylum="Mollusca" rank="species" species="nicaeensis">
