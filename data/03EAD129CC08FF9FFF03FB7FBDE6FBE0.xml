<taxonomicName id="4C431BBCCC08FF9CFD23F8D5BBA3F8C9" authority="Forster, 1869" authorityName="Forster" authorityYear="1869" box="[695,985,1892,1918]" class="Insecta" family="Ichneumonidae" genus="Syzeuctus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="260" phylum="Arthropoda" rank="genus">
