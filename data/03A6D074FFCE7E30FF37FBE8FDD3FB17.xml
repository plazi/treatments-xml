<taxonomicName id="4C0F1AE1FFCC7E30FEB7FC95FD10FC15" authority="Waschurova, 1964" authorityName="Waschurova" authorityYear="1964" class="Stenolaemata" family="Fenestellidae" genus="Isotrypa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Fenestrida" pageId="35" pageNumber="728" phylum="Bryozoa" rank="subSpecies" species="sibirica" subGenus="Tectulipora" subSpecies="communis">
