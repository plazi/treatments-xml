<geoCoordinate id="EE595028FFB0FFF7A3D4151AFBB7FB09" box="[971,1125,1147,1173]" direction="north" orientation="latitude" pageId="8" pageNumber="51" precision="92" value="58.885">
<number id="3DA524C8FFB0FFF7A3D4151AFC3EFB09" box="[971,1004,1147,1173]" pageId="8" pageNumber="51" value="58.0">58</number>
° 
<number id="3DA524C8FFB0FFF7A41F151AFBEEFB09" box="[1024,1084,1147,1173]" pageId="8" pageNumber="51" value="53.1">53.1</number>
’ N
</geoCoordinate>
; 
<geoCoordinate id="EE595028FFB0FFF7A46C151AFADAFB09" box="[1139,1288,1147,1173]" direction="east" orientation="longitude" pageId="8" pageNumber="51" precision="92" value="11.106667">
