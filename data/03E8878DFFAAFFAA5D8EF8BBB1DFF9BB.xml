<bibRefCitation id="EFD04B6AFFA5FFAA5A74FB39B033FB2F" author="Horvath G" box="[1145,1312,1174,1196]" pageId="72" pageNumber="630" pagination="501 - 505" refId="ref45036" refString="Horvath G. 1898. Capsidae novae aut minus cognitae. Termeszetrajzi Fuzetek 21: 501 - 505." type="journal article" year="1898">Horváth, 1898</bibRefCitation>
</taxonomicName>
<collectingCountry id="F356760BFFA5FFAA5B35FB39B069FB2F" box="[1336,1402,1174,1196]" name="Spain" pageId="72" pageNumber="630">Spain</collectingCountry>
</paragraph>
<paragraph id="8BFE369BFFA5FFAA5D5EFB19B10BFB69" blockId="72.[826,1441,1113,1258]" pageId="72" pageNumber="630">
<taxonomicName id="4C414D18FFA5FFAA5D5EFB19B00BFB49" authority="Horvath, 1877" authorityName="Horvath" authorityYear="1877" box="[851,1304,1205,1227]" class="Insecta" family="Miridae" genus="Piezocranum" kingdom="Animalia" order="Hemiptera" pageId="72" pageNumber="630" phylum="Arthropoda" rank="species" species="simulans">
