], 
<date id="FFF0105FDF1B6C00FB7758E3FA8EF908" box="[1191,1304,1688,1712]" pageId="10" pageNumber="586" value="1934-05-24">
<number id="3D8624B8DF1B6C00FB7758E3FB53F908" box="[1191,1221,1688,1712]" pageId="10" pageNumber="586" value="24.0">24</number>
.v. 
<number id="3D8624B8DF1B6C00FB0C58E3FA8EF908" box="[1244,1304,1688,1712]" pageId="10" pageNumber="586" value="1934.0">1934</number>
