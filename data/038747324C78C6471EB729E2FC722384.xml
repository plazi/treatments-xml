<taxonomicName id="4C2E8DA74C79C6581CAA2A9BFCC326BA" authority="Mabille, 1877" authorityName="Mabille" authorityYear="1877" box="[650,784,1973,1998]" class="Insecta" family="Nymphalidae" genus="Strabena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="mabillei">
