. It (“sp. 
<number id="3DE6E4034C66C6471FB62C0AFE652034" box="[406,438,295,320]" pageId="16" pageNumber="17" value="56.0">56</number>
”; 
<bibRefCitation id="EFBF8BD54C66C6471FF22C05FD862034" author="Lees" box="[466,597,296,320]" pageId="16" pageNumber="17" refString="Lees, D. C. (1997) Systematics and biogeography of Madagascan mycalesine butterflies (Lepidoptera: Satyrinae). Unpubl. PhD thesis, University of London, London, 411 pp." type="book" year="1997">
