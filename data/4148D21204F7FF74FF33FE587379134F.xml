<taxonomicName id="0EE1188704F7FF74FAD4FCF776C8134F" authorityName=", Koehler &amp; Vaney" authorityYear="1910" class="Gastropoda" family="Bursidae" genus="Talisman" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="167" pageNumber="168" phylum="Mollusca" rank="genus">
<emphasis id="FB95BF1604F7FF74FAD4FCF776C8134F" italics="true" pageId="167" pageNumber="168">Talisman</emphasis>
</taxonomicName>
(
<bibRefCitation id="AD701EF504F7FF74FF4CFC93776B134F" author="Perrier" box="[232,377,862,888]" firstAuthor="Perrier" pageId="167" pageNumber="168" refId="ref197270" refString="Perrier, M. E. (1894) Stellerides. Expeditions Scientifiques du Travailleur et du Talisman. Masson, Paris, 431 pp." type="book" year="1894">Perrier 1894</bibRefCitation>
, as 
<taxonomicName id="0EE1188704F7FF74FE06FC9374BE134F" authority="Perrier, 1885" authorityName="Perrier" authorityYear="1894" box="[418,684,862,888]" class="Asteroidea" family="Astropectinidae" genus="Crenaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Paxillosida" pageId="167" pageNumber="168" phylum="Echinodermata" rank="species" species="semispinosus">
<emphasis id="FB95BF1604F7FF74FE06FC9374BE134F" box="[418,684,862,888]" italics="true" pageId="167" pageNumber="168">Crenaster semispinosus</emphasis>
</taxonomicName>
) and by 
<emphasis id="FB95BF1604F7FF74FCAAFC9275AE134F" box="[782,956,862,888]" italics="true" pageId="167" pageNumber="168">Princesse Alice</emphasis>
(
