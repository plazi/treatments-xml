. 
<collectingCountry id="F36BC58CCC49FA388246F89ACF9AF891" box="[359,426,1891,1917]" name="China" pageId="3" pageNumber="404">China</collectingCountry>
(
<collectingRegion id="49B84BFECC49FA388298F89ACC12F892" box="[441,546,1891,1918]" country="China" name="Xinjiang Uygur" pageId="3" pageNumber="404">Xinjiang</collectingRegion>
).
</paragraph>
