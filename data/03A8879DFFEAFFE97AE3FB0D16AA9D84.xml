<subSubSection id="C31B6500FFEAFFE97A9BFACA12F49A7B" box="[136,413,1339,1364]" pageId="27" pageNumber="731" type="materials_examined">
<paragraph id="8BBE368BFFEAFFE97A9BFACA12F49A7B" blockId="27.[136,413,1339,1364]" box="[136,413,1339,1364]" pageId="27" pageNumber="731">
