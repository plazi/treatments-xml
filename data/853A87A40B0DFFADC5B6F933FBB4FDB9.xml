''E
</geoCoordinate>
), 
<quantity id="CA6B9B570B0DFFACC478F881FE65F870" box="[345,510,1899,1925]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.7" metricValueMin="1.55" pageId="32" pageNumber="33" unit="m" value="1625.0" valueMax="1700.0" valueMin="1550.0">1550–1700 m</quantity>
