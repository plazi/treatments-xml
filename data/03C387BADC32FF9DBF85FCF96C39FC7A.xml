<taxonomicName id="4C6A4D2FDC3DFF9DBD95FD2E6C54FD5D" authority="Cassis &amp; Symonds, 2008" authorityName="Cassis &amp; Symonds" authorityYear="2008" box="[963,1034,699,723]" class="Insecta" family="Tingidae" genus="Inoma" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="441" phylum="Arthropoda" rank="species" species="stysi" status="sp. nov.">
