<number id="A22A24BEFFA8E20B8E1FFCE7FC70DD63" box="[889,920,836,861]" pageId="10" pageNumber="383" value="16.0">16</number>
”, from 
<bibRefCitation id="70734B68FFA8E20B8E8BFCE7FB7DDD63" author="Martins" box="[1005,1173,836,861]" pageId="10" pageNumber="383" refString="Martins, U. R. (1996) Tribo Oemini. In: MARTINS, U. R. (Org.), Cerambycidae sul-americanos (Coleoptera). Taxonomia. Vol. 1. Sociedade Brasileira de Entomologia, Sao Paulo, pp. 3 - 155." type="book" year="1996">
Martins (
<number id="A22A24BEFFA8E20B8934FCE7FB64DD63" box="[1106,1164,836,861]" pageId="10" pageNumber="383" value="1996.0">1996</number>
)
</bibRefCitation>
