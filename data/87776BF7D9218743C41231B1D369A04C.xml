<normalizedToken id="FB6BA9A339973199761C7F02484F95F8" originalValue="«états">&quot;etats</normalizedToken>
<normalizedToken id="2B561D2292CD538635CED8087C98777F" originalValue="méridionaux">meridionaux</normalizedToken>
de 
