<taxonomicName id="4C6CB499A777ED2FB2A8FD5AFBA9A796" authority="Baker, 1979" authorityName="Baker" authorityYear="1979" box="[754,1055,644,669]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophiomusium" kingdom="Animalia" order="Ophiurida" pageId="11" pageNumber="12" phylum="Echinodermata" rank="species" species="incertum">
