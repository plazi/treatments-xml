<taxonomicName id="4C7B4D37EE0EFF9BFAB7FA32246639FE" authority="Lei &amp; Chou, 1997" authorityName="Lei &amp; Chou" authorityYear="1997" box="[997,1385,1439,1461]" class="Arachnida" family="Salticidae" genus="Tanna" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="499" phylum="Arthropoda" rank="species" species="pseudocalis" status="syn. nov.">
