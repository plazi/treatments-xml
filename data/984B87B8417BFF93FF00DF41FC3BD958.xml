<taxonomicName id="D7E24D2D417BFF93FB8FDC34FBD5D867" box="[1048,1148,1592,1616]" class="Arachnida" family="Ctenidae" genus="Celaetycheus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="abara" status="sp. nov.">
