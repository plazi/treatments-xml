<taxonomicName id="D7E24D2D417BFF93FE13DC34FD04D867" box="[388,685,1592,1616]" class="Arachnida" family="Ctenidae" genus="Celaetycheus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="flavostriatus">
