<taxonomicName id="D7E24D2D417BFF93FE3EDC8CFDFFD8AF" box="[425,598,1664,1688]" class="Arachnida" family="Ctenidae" genus="Celaetycheus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="flavostriatus">
<emphasis id="2296EABC417BFF93FE3EDC8CFDFFD8AF" box="[425,598,1664,1688]" italics="true" pageId="1" pageNumber="140">C. flavostriatus</emphasis>
</taxonomicName>
(
<figureCitation id="88D92A2B417BFF93FDF3DC73FD49D8A0" box="[612,736,1663,1687]" captionStart="FIGURE 1. A – D" captionStartId="2.[151,250,1093,1116]" captionTargetBox="[163,1435,210,1051]" captionTargetId="figure@2.[151,1436,193,1072]" captionTargetPageId="2" captionText="FIGURE 1. A – D Celaetycheus flavostriatus: A – B, Male palp (scale bar 1 mm); A, ventral view; B, retrolateral view. C – D, epigynum (scale bar 0.5 mm); C, ventral view; D, dorsal view." httpUri="https://zenodo.org/record/217255/files/figure.png" pageId="1" pageNumber="140">
Figs 
<number id="A62A2489417BFF93FD0ADC73FD0FD8A0" box="[669,678,1663,1687]" pageId="1" pageNumber="140" value="1.0">1</number>
C–D
</figureCitation>
) resemble those of 
<taxonomicName id="D7E24D2D417BFF93FC2CDC8CFBB7D8AF" box="[955,1054,1664,1688]" class="Arachnida" family="Ctenidae" genus="Celaetycheus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="abara" status="sp. nov.">
<emphasis id="2296EABC417BFF93FC2CDC8CFBB7D8AF" box="[955,1054,1664,1688]" italics="true" pageId="1" pageNumber="140">C. abara</emphasis>
</taxonomicName>
<taxonomicNameLabel id="39A557C7417BFF93FBB2DC8CFBD7D8AF" box="[1061,1150,1664,1688]" pageId="1" pageNumber="140" rank="species">
<emphasis id="2296EABC417BFF93FBB2DC8CFBD7D8AF" bold="true" box="[1061,1150,1664,1688]" pageId="1" pageNumber="140">sp. nov.</emphasis>
</taxonomicNameLabel>
(
<figureCitation id="88D92A2B417BFF93FB1ADC73FB4BD8A0" box="[1165,1250,1663,1687]" captionStart="FIGURE 5. A – D" captionStartId="6.[151,250,1393,1416]" captionTargetBox="[160,1420,208,1340]" captionTargetId="figure@6.[151,1436,193,1372]" captionTargetPageId="6" captionText="FIGURE 5. A – D Celaetycheus abara sp. nov.: A – B, Male palp (scale bar 1 mm); A, ventral view; B, retrolateral view. C – D, epigynum (scale bar 0.5 mm); C, ventral view; D, dorsal view." httpUri="https://zenodo.org/record/217259/files/figure.png" pageId="1" pageNumber="140">
Fig. 
<number id="A62A2489417BFF93FB56DC73FB67D8A0" box="[1217,1230,1663,1687]" pageId="1" pageNumber="140" value="5.0">5</number>
C
</figureCitation>
