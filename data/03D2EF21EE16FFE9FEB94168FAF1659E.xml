<taxonomicName id="4C7B25B4EE16FFE9FE3D4795FDF36491" box="[396,542,1236,1262]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
