<paragraph id="8BAB36B4C66AA218FC2EF5B3FB464862" blockId="28.[981,1777,2337,2905]" pageId="28" pageNumber="255">
<taxonomicName id="4C144D37C66AA218FC2EF5B3FA644916" authorityName="Kock &amp; Bhat" authorityYear="1994" box="[982,1419,2601,2640]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="28" pageNumber="255" phylum="Chordata" rank="species" species="hypophyllus">
