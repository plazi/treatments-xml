(a, b) and 
<taxonomicName id="06BF4D01FFF35656348AF8B2266D3199" box="[944,1094,1917,1939]" class="Insecta" family="Mycetophilidae" genus="Neoempheria" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="157" phylum="Arthropoda" rank="species" species="latisternata" status="sp. n.">
