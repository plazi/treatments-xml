<taxonomicName id="E3EBE29254A4F3BA47410FF75FB2DE67" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="532" phylum="Tracheophyta" rank="genus">
