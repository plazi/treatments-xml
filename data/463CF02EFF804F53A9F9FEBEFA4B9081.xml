<number id="785D531FFF804F53A883FE5AFE2993FC" box="[493,505,431,452]" pageId="2" pageNumber="27" value="2.0">2</number>
A–D, 
<number id="785D531FFF804F53AB5DFE5AFDEE93FC" box="[563,574,431,452]" pageId="2" pageNumber="27" value="3.0">3</number>
A–B, 
<number id="785D531FFF804F53AB16FE5AFD5393FC" box="[632,643,431,452]" pageId="2" pageNumber="27" value="8.0">8</number>
A–B, E)......................................................... 
<number id="785D531FFF804F53ACFEFE5AFA4C93FC" box="[1424,1436,431,452]" pageId="2" pageNumber="27" value="2.0">2</number>
</keyLead>
