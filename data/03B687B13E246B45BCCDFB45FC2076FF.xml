<taxonomicName id="4C1F4D243E276B45BD89FE50FDEB75E3" box="[467,605,440,464]" class="Insecta" family="Geometridae" genus="Thalassodes" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="28" phylum="Arthropoda" rank="genus">
