<taxonomicName id="4C1F4D243E276B45B862FE7CFB51759F" box="[1080,1255,404,428]" class="Insecta" family="Geometridae" genus="Orothalassodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="28" phylum="Arthropoda" rank="genus">
