[10] 
<taxonomicName id="9F7DE859D2528D99407A734AE51A3E59" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25244" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus brevis Brown" order="Hymenoptera" pageId="32" pageNumber="581" phylum="Arthropoda" rank="species" species="brevis">Anochetus brevis</taxonomicName>
