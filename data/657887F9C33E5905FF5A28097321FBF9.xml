<taxonomicName id="2AD14D6CC33D5904FE922E1D7537F8F4" authority="Linzmeier &amp; Konstantinov" authorityName="Linzmeier &amp; Konstantinov" authorityYear="2020" box="[351,824,1881,1906]" class="Insecta" family="Chrysomelidae" genus="Menudos" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="maricao" status="new species">
<emphasis id="DFA5EAFDC33D5904FE922E1D77B3F8F7" box="[351,444,1881,1905]" italics="true" pageId="10" pageNumber="11">Menudos</emphasis>
<taxonomicName id="2AD14D6CC33D5904FE092E1E7537F8F4" authority="Linzmeier &amp; Konstantinov" authorityName="Linzmeier &amp; Konstantinov" box="[452,824,1881,1906]" pageId="10" pageNumber="11" rank="species" species="maricao" status="new species">
<taxonomicName id="2AD14D6CC33D5904FE092E1E7486F8F4" authority="Linzmeier" authorityName="Linzmeier" box="[452,649,1881,1906]" pageId="10" pageNumber="11" rank="species" species="maricao">
<taxonomicName id="2AD14D6CC33D5904FE092E1E7417F8F7" box="[452,536,1882,1905]" pageId="10" pageNumber="11" rank="species" species="maricao">
<emphasis id="DFA5EAFDC33D5904FE092E1E7417F8F7" box="[452,536,1882,1905]" italics="true" pageId="10" pageNumber="11">maricao</emphasis>
