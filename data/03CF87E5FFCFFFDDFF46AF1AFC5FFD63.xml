<taxonomicName id="4C664D70FFC8FFDCFE22AFCBFD86F944" authority="Yang" authorityName="Yang" box="[499,619,1670,1693]" pageId="28" pageNumber="29" rank="species" species="hunanensis">hunanensis</taxonomicName>
