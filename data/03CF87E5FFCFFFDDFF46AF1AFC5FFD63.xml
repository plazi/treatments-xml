<taxonomicName id="4C664D70FFCFFFDBFF46AF87FD60F938" authority="Yang &amp; Yang, 1992" authorityName="Yang &amp; Yang" authorityYear="1992" box="[151,653,1738,1761]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="hunanensis">
