<location id="514F6002A53F89128FD24FBB8BD537BA" LSID="urn:lsid:plazi:treatment:DC3987CFA53F89128F4F4A028CB336DA:514F6002A53F89128FD24FBB8BD537BA" box="[930,985,1674,1697]" country="Turkey" county="Anatolian region" municipality="eastern Anatolian" name="Here" pageId="33" pageNumber="306" stateProvince="Hakkari">Here</location>
