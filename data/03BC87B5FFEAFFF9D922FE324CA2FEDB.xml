<taxonomicName id="4C154D20FFE8FFF9D976FEC74C31FE9B" authorityName="Vandel" authorityYear="1968" box="[264,559,291,315]" class="Malacostraca" family="Philosciidae" genus="Colombophiloscia" kingdom="Animalia" order="Isopoda" pageId="18" pageNumber="513" phylum="Arthropoda" rank="species" species="alticola">
