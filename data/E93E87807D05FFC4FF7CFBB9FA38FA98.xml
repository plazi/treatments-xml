<quantity id="A66F9B737D05FFC4FE5FFADDFDE6FADD" box="[436,578,1289,1311]" metricMagnitude="-3" metricUnit="m" metricValue="2.675" metricValueMax="3.2" metricValueMin="2.15" pageId="17" pageNumber="18" unit="mm" value="2.675" valueMax="3.2" valueMin="2.15">2.15–3.20 mm</quantity>
