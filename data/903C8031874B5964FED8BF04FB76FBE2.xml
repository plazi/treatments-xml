<taxonomicName id="DF954AA4874B5964FAE4BF9AFA1BFE7E" box="[1301,1421,458,481]" class="Anthozoa" family="Turbinoliidae" genus="Idiotrochus" kingdom="Animalia" order="Scleractinia" pageId="37" pageNumber="296" phylum="Cnidaria" rank="genus">
