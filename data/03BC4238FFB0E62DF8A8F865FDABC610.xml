<materialsCitation id="3B7DF973FFB0E62AF935F786FCB0CC3F" accessionNumber="MZB 10981" box="[361,776,2081,2102]" collectionCode="MZB" pageId="2" pageNumber="417" specimenCount="1" typeStatus="holotype">
