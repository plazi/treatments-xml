<heading id="D0AA81EDFFEAFF924C37F9ADFBCDF984" box="[824,1072,1563,1587]" centered="true" fontSize="9" level="2" pageId="29" pageNumber="29" reason="2">
<taxonomicName id="4C5D4D02FFEAFF924C37F9ADFC14F984" authorityName="Cope" authorityYear="1877" box="[824,1001,1563,1587]" class="Reptilia" family="Camarasauridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Saurischia" pageId="29" pageNumber="29" phylum="Chordata" rank="family">Camarasauridae</taxonomicName>
