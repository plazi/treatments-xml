<taxonomicName id="4C5A1A8F73350E12FF137C53FE58FCB3" box="[191,300,772,796]" class="Malacostraca" family="Calappidae" genus="Calappa" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="534" phylum="Arthropoda" rank="species" species="lophos">
