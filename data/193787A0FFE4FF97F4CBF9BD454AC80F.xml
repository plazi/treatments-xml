<taxonomicName id="569E4D35FFEAFF97F590FF7C460ECAC2" authorityName="Fisher" authorityYear="1913" box="[460,618,188,213]" class="Asteroidea" family="Goniasteridae" genus="Anthenoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Valvatida" pageId="8" pageNumber="9" phylum="Echinodermata" rank="species" species="granulosus">
