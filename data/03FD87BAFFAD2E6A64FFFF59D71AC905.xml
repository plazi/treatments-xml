<emphasis id="B920EABEFFAD2E6A64AFFD14D316C8F1" bold="true" box="[199,345,685,710]" pageId="65" pageNumber="66">Distribution</emphasis>
. Weddell Sea, 
<collectingCountry id="F343763CFFAD2E6A6669FD14D036C8F1" box="[513,633,685,710]" name="Antarctica" pageId="65" pageNumber="66">Antarctica</collectingCountry>
, 70â€“ 
<geoCoordinate id="EE60506BFFAD2E6A66DAFD14D0A8C8F1" box="[690,743,685,710]" degrees="76" direction="south" orientation="latitude" pageId="65" pageNumber="66" precision="55555" value="-76.0">76Â°S</geoCoordinate>
, 11â€“ 
<geoCoordinate id="EE60506BFFAD2E6A6773FD14D111C8F1" box="[795,862,685,710]" degrees="56" direction="west" orientation="longitude" pageId="65" pageNumber="66" precision="55555" value="-56.0">56Â°W</geoCoordinate>
