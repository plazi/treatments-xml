<taxonomicName id="4C297C91841A774A53DEFF14FC21FF15" authority="Borovec &amp; Skuhrovec" authorityName="Borovec &amp; Skuhrovec" authorityYear="2019" box="[465,962,152,179]" class="Insecta" family="Curculionidae" genus="Barclayanthus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="457" phylum="Arthropoda" rank="species" species="micros" status="sp. nov.">
