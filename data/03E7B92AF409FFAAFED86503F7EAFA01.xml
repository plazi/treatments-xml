<caption id="DF3158B4F40BFFAAFF726656F7E0FAAB" ID-DOI="http://doi.org/10.5281/zenodo.5510014" ID-Zenodo-Dep="5510014" httpUri="https://zenodo.org/record/5510014/files/figure.png" pageId="6" pageNumber="7" startId="6.[192,267,1342,1366]" targetBox="[199,1436,199,1301]" targetPageId="6">
