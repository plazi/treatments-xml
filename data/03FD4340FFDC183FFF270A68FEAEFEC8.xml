and ten localities (or subpopulations); EOO = 
<quantity id="4CAC5FB3FFDD183FFF270E34FEBFFEC8" box="[151,271,258,285]" metricMagnitude="4" metricUnit="m" metricValue="5.1705" pageId="103" pageNumber="104" unit="km" value="51.705">51,705 km</quantity>
<superScript id="7C215F1EFFDD183FFEBF0E34FEA7FEC5" attach="left" box="[271,279,258,272]" fontSize="6" pageId="103" pageNumber="104">2</superScript>
