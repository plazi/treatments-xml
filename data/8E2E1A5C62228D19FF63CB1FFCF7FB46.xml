<collectionCode id="6096338F62228D19FDE1CBEDFDD7FC36" box="[533,628,949,975]" httpUri="http://grbio.org/cool/6t80-4rpg" name="Natuurhistorische Museum Maastricht" pageId="6" pageNumber="39">NHMM</collectionCode>
<number id="B04FB96D62228D19FD8FCBEDFD16FC36" box="[635,693,949,975]" pageId="6" pageNumber="39" value="2003.0">2003</number>
