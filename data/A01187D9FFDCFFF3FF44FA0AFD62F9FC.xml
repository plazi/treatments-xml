<taxonomicName id="EFB84D4CFFDCFFF2FF44FA0AFCE6FD9B" authority="(Renaud-Debyser, 1959)" authorityName="Renaud-Debyser" authorityYear="1959" baseAuthorityName="Renaud-Debyser" baseAuthorityYear="1959" box="[151,771,1493,1519]" class="Heterotardigrada" family="Halechiniscidae" genus="Dipodarctus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Arthrotardigrada" pageId="12" pageNumber="55" phylum="Tardigrada" rank="species" species="subterraneus">
<emphasis id="1ACCEADDFFDCFFF2FF44FA0AFCE6FD9B" bold="true" box="[151,771,1493,1519]" pageId="12" pageNumber="55">
<emphasis id="1ACCEADDFFDCFFF2FF44FA0AFE35FD9B" bold="true" box="[151,464,1493,1519]" italics="true" pageId="12" pageNumber="55">Dipodarctus subterraneus</emphasis>
(
<bibRefCitation id="4C294B3EFFDCFFF2FE0CFA0AFD1EFD9B" box="[479,763,1493,1519]" pageId="12" pageNumber="55" refId="ref16934" refString="Renaud-Debyser, J. (1959) Etudes sur la faune interstitielle des Iles Bahamas. III. Tardigrades. Vie et Milieu, 10, 296 - 302." type="journal article">Renaud-Debyser, 1959</bibRefCitation>
