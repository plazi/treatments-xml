<subSubSection id="C363651FFF34C6E875B0585240EDF814" lastPageId="153" lastPageNumber="153" pageId="152" pageNumber="152" type="materials_examined">
<paragraph id="8BC63694FF34C6E975B0585244B3FF9C" blockId="152.[667,1193,354,1759]" pageId="152" pageNumber="152">
<materialsCitation id="3B113CC9FF34C6E975B0585244B3FF9C" collectingDate="1938-09-03" collectionCode="BPBM" collectorName="E. C. Zimmerman" country="Fiji" elevation="914" location="Levu" municipality="Nandarivatu" pageId="152" pageNumber="152" specimenCount="1" typeStatus="holotype">
