<number id="F84924B7FFA4FB64FE3ADDE6FDE2FF5E" box="[481,541,159,183]" pageId="4" pageNumber="105" value="1981.0">1981</number>
a
</bibRefCitation>
: 
<number id="F84924B7FFA4FB64FDEEDDE7FD9DFF5E" box="[565,610,158,183]" pageId="4" pageNumber="105" value="169.0">169</number>
</treatmentCitation>
