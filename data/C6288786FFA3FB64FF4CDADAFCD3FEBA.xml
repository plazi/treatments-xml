<number id="F84924B7FFA4FB64FE0ADC6AFDF1FEC2" box="[465,526,275,299]" pageId="4" pageNumber="105" value="1989.0">1989</number>
: 
<number id="F84924B7FFA4FB64FDC3DC6AFDBCFEC5" box="[536,579,275,300]" pageId="4" pageNumber="105" value="164.0">164</number>
)
</bibRefCitation>
</treatmentCitation>
