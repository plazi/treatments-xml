<taxonomicName id="929A4D79FFD2665874F7347CFB4CF3A8" authority="anak" box="[941,1234,3157,3193]" class="Mammalia" family="Muridae" genus="Uromys" kingdom="Animalia" order="Rodentia" pageId="11" pageNumber="156" phylum="Chordata" rank="subSpecies" species="anak" subSpecies="anak">
