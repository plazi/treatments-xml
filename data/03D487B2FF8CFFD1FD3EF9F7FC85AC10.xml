<taxonomicName id="4C7D4D27FF8CFFD3FD00F9F7FC1AAF99" box="[675,890,1662,1697]" class="Insecta" family="Synlestidae" genus="Phylolestes" kingdom="Animalia" order="Odonata" pageId="0" pageNumber="256" phylum="Arthropoda" rank="genus" status="new genus">Phylolestes</taxonomicName>
