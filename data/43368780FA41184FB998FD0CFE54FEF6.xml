<taxonomicName id="0C9F4D15FA41184CB98FFB3BFBD4FB1A" authorityName="Schell" authorityYear="1943" box="[989,1098,1218,1241]" class="Insecta" family="Ochteridae" genus="Ochterus" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="44" phylum="Arthropoda" rank="species" species="parvus">
