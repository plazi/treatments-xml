[[ 
<taxonomicName id="6F571E1178D1B47CF91A9019AB6B72F0" LSID-ZBK="urn:lsid:zoobank.org:act:82206F20-179E-4058-83D9-ABD1190748A8" class="Elasmobranchii" family="Etmopteridae" genus="Etmopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Etmopterus Rafinesque 1810:14" order="Squaliformes" pageNumber="62" phylum="Chordata" rank="genus">Etmopterus</taxonomicName>
