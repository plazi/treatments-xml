<date id="3B6E105A9A4B2225FE8FCC7BCDEEFCFD" box="[329,427,804,830]" pageId="14" pageNumber="15" value="1971-01-05">
<number id="F91824BD9A4B2225FE8FCC7BCD1DFCFD" box="[329,344,804,830]" pageId="14" pageNumber="15" value="5.0">5</number>
