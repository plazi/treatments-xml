<taxonomicName id="35E655287A50083CD7092E691BCE5C54" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="villosum">
Hieracium 
<normalizedToken id="1A06C6F58E66C138B823E11DFAA86F8B" originalValue="villósum" pageId="null" pageNumber="590">villosum</normalizedToken>
<authorityName id="90D9E46777A5DFC146CFFD890F676B33" pageId="null" pageNumber="590">L.</authorityName>
