<taxonomicName id="4C744D0ECF06FFE4FF6AEFC0FE10FF50" authority="González-Obando &amp; Carrejo-Gironza &amp; García, 2017" authorityName="González-Obando &amp; Carrejo-Gironza &amp; García" authorityYear="2017" box="[151,439,152,178]" class="Insecta" family="Ptiloneuridae" genus="Triplocania" kingdom="Animalia" order="Psocodea" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="mocoaensis" status="n. sp.">
<emphasis id="B900EA9FCF06FFE4FF6AEFC0FE10FF50" bold="true" box="[151,439,152,178]" italics="true" pageId="69" pageNumber="70">Triplocania mocoaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23357E4CF06FFE4FE43EFC0FDA7FF50" box="[446,512,152,178]" pageId="69" pageNumber="70" rank="species">n. sp.</taxonomicNameLabel>
