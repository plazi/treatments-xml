<subSubSection id="C30C92B7951EFFD9FDB5F94073AEF955" pageId="27" pageNumber="28" type="discussion">
<paragraph id="8BA9C13C951EFFD9FDB5F94073AEF955" blockId="27.[151,1437,1588,1830]" pageId="27" pageNumber="28">
<emphasis id="B9621D2E951EFFD9FDB5F9407259F8AD" bold="true" box="[199,308,1732,1758]" pageId="27" pageNumber="28">Remarks</emphasis>
. The 
<typeStatus id="54AD7F9E951EFFD9FC08F94172C7F8AC" box="[378,426,1733,1759]" pageId="27" pageNumber="28">type</typeStatus>
