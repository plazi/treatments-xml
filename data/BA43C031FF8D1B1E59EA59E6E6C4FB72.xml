<taxonomicName id="F5EA0AA4FF8A1B1E5A3F5FC5E089FF52" box="[834,977,181,202]" class="Insecta" family="Hydrophilidae" genus="Oocyclus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="68" phylum="Arthropoda" rank="species" species="sumatrensis">
