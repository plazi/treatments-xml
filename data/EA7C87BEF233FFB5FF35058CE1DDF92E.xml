<taxonomicName id="A5D54D2BF233FFB5FEB205C3E4A4F8EE" authorityName="Iwasa" authorityYear="1999" box="[272,402,216,239]" class="Insecta" family="Sepsidae" genus="Meroplius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="199" phylum="Arthropoda" rank="species" species="wallacei">
