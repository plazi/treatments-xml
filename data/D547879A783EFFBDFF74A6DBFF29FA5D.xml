<collectingRegion id="9F2AF86E7839FFBDFC86A241FBF7FC9C" box="[890,1082,842,865]" country="Iran" name="Hamadan" pageId="4" pageNumber="121">Hamadan Province</collectingRegion>
], Amadan (= 
<collectingRegion id="9F2AF86E7839FFBDFB3DA241FAD0FC9D" box="[1217,1309,843,865]" country="Iran" name="Hamadan" pageId="4" pageNumber="121">Hamadan</collectingRegion>
or 
<collectingRegion id="9F2AF86E7839FFBDFABDA241FA6CFC9D" box="[1345,1441,843,865]" country="Iran" name="Hamadan" pageId="4" pageNumber="121">Hamedan</collectingRegion>
),
</paragraph>
