<collectingCountry id="15E693E2FFD21D14FBD1C998FB3FFCD6" box="[1126,1209,757,783]" name="Mexico" pageId="8" pageNumber="7">Mexico</collectingCountry>
(
<collectingRegion id="AF351D90FFD21D14FB70C998FAA3FCD6" box="[1223,1317,757,783]" country="Mexico" name="Chiapas" pageId="8" pageNumber="7">Chiapas</collectingRegion>
, 
<collectingRegion id="AF351D90FFD21D14FA87C99BFA03FCC9" box="[1328,1413,758,784]" country="Mexico" name="Oaxaca" pageId="8" pageNumber="7">Oaxaca</collectingRegion>
).............................................................. 
