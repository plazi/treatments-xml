<quantity id="4CEF9B25FFC4FFE09B71FEF1F9143118" box="[780,853,259,285]" metricMagnitude="2" metricUnit="m" metricValue="1.26" pageId="85" pageNumber="86" unit="m" value="126.0">126 m</quantity>
(
