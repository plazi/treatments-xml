<collectionCode id="ED0DAE0AFF98381E91DBFD67F125FD51" LSID="urn:lsid:biocol.org:col:34988" box="[484,576,730,756]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="13" pageNumber="44">MNHN</collectionCode>
<number id="3DD424E8FF98381E9278FD67F1A6FD51" box="[583,707,730,756]" pageId="13" pageNumber="44" value="1966.1248">1966.1248</number>
– Ngaoundéré, 
<collectingCountry id="F30B765FFF98381E9340FD67F09EFD51" box="[895,1019,730,756]" name="Cameroon" pageId="13" pageNumber="44">Cameroon</collectingCountry>
) 
<quantity id="4CE49B2AFF98381E9435FD67F70EFD51" box="[1034,1131,730,756]" metricMagnitude="-1" metricUnit="m" metricValue="5.05" pageId="13" pageNumber="44" unit="mm" value="505.0">505 mm</quantity>
