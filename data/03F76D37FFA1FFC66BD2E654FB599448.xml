<taxonomicName id="4C5EA7A2FFA1FFC66A28E12CFD869728" authority="Westrop &amp; Eoff, 2020" authorityName="Westrop &amp; Eoff" authorityYear="2020" box="[365,520,1300,1326]" class="Trilobita" family="Kingstoniidae" genus="Blountia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Ptychopariida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="nevadensis" status="n. sp.">
