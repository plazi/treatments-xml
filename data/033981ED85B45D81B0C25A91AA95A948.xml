<taxonomicName id="53D4197F6D655928EB09368FF781D4EF" authority="Agassiz, 1832" authorityName="Heckel" authorityYear="1843" class="Actinopterygii" family="Cyprinidae" genus="Aspius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aspius vorax" order="Cypriniformes" pageId="0" pageNumber="747" phylum="Chordata" rank="species" species="vorax">
