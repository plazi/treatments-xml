<taxonomicName id="D0927FB7FB1950AB54865605845754F4" genus="A." lsidName="A. thisbeae" pageId="0" pageNumber="747" rank="species" species="thisbeae">
<emphasis id="DCE4A735F07602539A4622A125AE24E3" italics="true" pageId="0" pageNumber="747">A. thisbeae</emphasis>
