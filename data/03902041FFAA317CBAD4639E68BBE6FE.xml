<td id="76D8FA69FFAACE88B97066A66E97E0F0" box="[819,955,1252,1277]" gridcol="1" gridrow="10" pageId="7" pageNumber="162">3.53</td>
<td id="76D8FA69FFAACE88BE3366A669D1E0F0" box="[1136,1277,1252,1277]" gridcol="2" gridrow="10" pageId="7" pageNumber="162">3.9</td>
