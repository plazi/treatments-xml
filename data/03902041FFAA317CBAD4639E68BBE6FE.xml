<tr id="35099315FFAACE88BAE566FC69D1E0DA" box="[166,1277,1214,1239]" gridrow="9" pageId="7" pageNumber="162">
<th id="76D8FA69FFAACE88BAE566FC6C82E0DA" box="[166,430,1214,1239]" gridcol="0" gridrow="9" pageId="7" pageNumber="162">Head width</th>
<td id="76D8FA69FFAACE88B97066FC6E97E0DA" box="[819,955,1214,1239]" gridcol="1" gridrow="9" pageId="7" pageNumber="162">6.67</td>
<td id="76D8FA69FFAACE88BE3366FC69D1E0DA" box="[1136,1277,1214,1239]" gridcol="2" gridrow="9" pageId="7" pageNumber="162">8,1</td>
</tr>
<tr id="35099315FFAACE88BAE566A669D1E0F0" box="[166,1277,1252,1277]" gridrow="10" pageId="7" pageNumber="162">
