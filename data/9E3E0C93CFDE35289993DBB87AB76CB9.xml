<subSubSection id="A39D9D42044E5FFFC47179007A1B52E8" pageId="null" pageNumber="795" type="nomenclature">
<paragraph id="1CB1C68A8E107FA89D0F38BB70C1E7C0" pageId="null" pageNumber="795">
<taxonomicName id="28BEA67ACADA59A905F83650B7EF84AA" authority="L." class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="795" phylum="Tracheophyta" rank="species" species="carthusianorum">
Dianthus 
<normalizedToken id="2BAF66E682DBC1AE341025555670A2A1" originalValue="Carthusianórum" pageId="null" pageNumber="795">Carthusianorum</normalizedToken>
<authorityName id="9AB06ACA9599C7EF97D4C5E0D4283887" pageId="null" pageNumber="795">L.</authorityName>
