<taxonomicName id="4C4A9AB999258557E8D1FDFFD71EF275" authority="Haliday, 1838" authorityName="Haliday" authorityYear="1838" box="[193,591,512,539]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="signifer">
