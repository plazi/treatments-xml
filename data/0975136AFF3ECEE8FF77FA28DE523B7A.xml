</paragraph>
<paragraph id="8163A27CFF39CEE8FCD5F910D9403403" blockId="180.[810,1452,1729,2092]" lastBlockId="181.[136,778,1816,2093]" lastPageId="181" lastPageNumber="182" pageId="180" pageNumber="181">
<materialsCitation id="31B4A821FF39CEE9FCD5F910DEFB3402" ID-GBIF-Occurrence="3399910310" collectionCode="MALES, PME, PLE, ALE, AME-AME, ALE-AME, AME-PME" county="Chelicerae" location="Ocular area" municipality="Labium" pageId="180" pageNumber="181" specimenCode="PME-PME 1.2, 1.5" specimenCount="1">
<collectionCode id="E7CD3AB9FF39CEE9FCD5F910D8DA3AF5" box="[810,905,1729,1755]" pageId="180" pageNumber="181">MALES</collectionCode>
