<taxonomicName id="EB964D7BDE15FFE785DFFB627CECFB52" authorityName="Cavichioli" authorityYear="1986" box="[160,418,1166,1190]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="1529" phylum="Arthropoda" rank="species" species="altmanni">
