(reported as 
<taxonomicName id="4C484F9EFFF8FF92D79EF9C66463F9B0" authorityName="Blyth" authorityYear="1844" box="[993,1119,1563,1586]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="1" pageNumber="660" phylum="Chordata" rank="species" species="macrotis">
