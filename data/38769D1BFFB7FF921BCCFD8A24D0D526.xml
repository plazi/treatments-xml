<taxonomicName id="77DF578EFFB7FF921884FD072423D3BB" authority="Scopoli, 1763" authorityName="Scopoli" authorityYear="1763" box="[991,1387,686,713]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="390" phylum="Arthropoda" rank="species" species="palliatella">
