<subSubSection id="4EE865746D72F46341C2FEC4FAD3FEB7" box="[199,1381,332,357]" pageId="22" pageNumber="464" type="etymology">
<paragraph id="064D36FF6D72F46341C2FEC4FAD3FEB7" blockId="22.[151,1436,151,357]" box="[199,1381,332,357]" pageId="22" pageNumber="464">
<emphasis id="3486EAED6D72F46341C2FEC4FEF8FEB7" bold="true" box="[199,334,332,357]" pageId="22" pageNumber="464">Etymology.</emphasis>
