<emphasis id="B94C3B7E6675FFC5FE1F01C6698E9EE0" bold="true" box="[387,464,1750,1777]" pageId="19" pageNumber="300">
<collectionCode id="ED297FA96675FFC5FE1F01C669959EE0" box="[387,459,1750,1777]" pageId="19" pageNumber="300">GOM</collectionCode>
.
</emphasis>
<location id="8EE7B1B76675FFC5FE4B01C76A5D9EE0" LSID="urn:lsid:plazi:treatment:0391567A6675FFC5FF0B02566ADC9FFC:8EE7B1B76675FFC5FE4B01C76A5D9EE0" box="[471,515,1751,1777]" name="The" pageId="19" pageNumber="300">The</location>
first record of this species from the northern
</materialsCitation>
<materialsCitation id="3B50ED316675FFC5FC6601C769209F28" collectingDate="1885-05-21" collectionCode="GOM, USNM" location="Cedar Keys" pageId="19" pageNumber="300" specimenCode="USNM 37196" specimenCount="1" stateProvince="Florida">
<collectionCode id="ED297FA96675FFC5FC6601C76C659EE0" box="[1018,1083,1751,1777]" pageId="19" pageNumber="300">GOM</collectionCode>
