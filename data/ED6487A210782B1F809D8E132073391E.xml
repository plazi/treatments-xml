<paragraph id="657236B410782B1E80CC8E8E20B53EBE" blockId="8.[151,1005,1592,1727]" box="[198,1005,1700,1727]" pageId="8" pageNumber="179">
<treatmentCitationGroup id="45DD119A10782B1E80CC8E8E20B53EBE" box="[198,1005,1700,1727]" pageId="8" pageNumber="179">
<taxonomicName id="A2CD4D3710782B1E80CC8E8E21D33EBE" authority="Schedl, 1951: 152" authorityName="Schedl" authorityPageNumber="152" authorityYear="1951" box="[198,651,1700,1727]" class="Insecta" family="Curculionidae" genus="Xyleborus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="179" phylum="Arthropoda" rank="species" species="upoluensis">
<emphasis id="57B9EAA610782B1E80CC8E8E22EE3EBE" box="[198,438,1701,1727]" italics="true" pageId="8" pageNumber="179">Xyleborus upoluensis</emphasis>
Schedl, 1951: 152
</taxonomicName>
. Synonymy: 
<treatmentCitation id="E46C10A510782B1E83148E8E20B13EBE" author="Beaver, R. A." box="[798,1001,1700,1727]" page="542" pageId="8" pageNumber="179" year="1976">
