<taxonomicName id="C5ADFBD9FF9DFFA6FF61FA1AFCB4FA51" authority="(Sprengel 1825: 297) Triana (1871: 118)" authorityName="Triana" authorityPageNumber="118" authorityYear="1871" baseAuthorityName="Sprengel" baseAuthorityPageNumber="297" baseAuthorityYear="1825" box="[185,884,1479,1507]" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" kingdom="Plantae" order="Myrtales" pageId="12" pageNumber="73" phylum="Tracheophyta" rank="species" species="brasiliensis">
<emphasis id="30D95C48FF9DFFA6FF61FA1AFE64FA51" bold="true" box="[185,420,1479,1506]" italics="true" pageId="12" pageNumber="73">Miconia brasiliensis</emphasis>
(
<bibRefCitation id="663CFDABFF9DFFA6FE6AFA1AFD50FA51" author="Sprengel, K. P. J." box="[434,656,1480,1507]" pageId="12" pageNumber="73" pagination="295 - 305" refId="ref27755" refString="Sprengel, K. P. J. (1825) Melastoma. Caroli Linnaei Systema vegetabilium, vol. 2. Gottingae, pp. 295 - 305." type="journal article" year="1825">Sprengel 1825: 297</bibRefCitation>
