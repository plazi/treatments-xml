<taxonomicName id="4C544D092C4A2E01FE575A0AFD782425" authorityName="Fannes" authorityYear="2013" box="[409,653,591,611]" class="Arachnida" family="Oonopidae" genus="Zyngoonops" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="19" phylum="Arthropoda" rank="species" species="goedaerti">Zyngoonops goedaerti</taxonomicName>
