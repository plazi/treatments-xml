<taxonomicName id="4C544D092C4B2E00FF1A5D21FE5923CA" authority="Fannes, 2013" authorityName="Fannes" authorityYear="2013" box="[212,428,1401,1420]" class="Arachnida" family="Oonopidae" genus="Zyngoonops" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="18" phylum="Arthropoda" rank="species" species="moffetti" status="new species">
