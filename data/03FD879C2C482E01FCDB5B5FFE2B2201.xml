<taxonomicName id="4C544D092C4A2E01FF335A30FE5A2438" authorityName="Fannes" authorityYear="2013" box="[253,431,618,638]" class="Arachnida" family="Oonopidae" genus="Zyngoonops" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="19" phylum="Arthropoda" rank="species" species="swammerdami">
