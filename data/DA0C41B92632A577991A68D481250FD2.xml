<taxonomicName id="B011FD9AB0DE71877F02C5F5971AFCFF" class="Insecta" family="Tingidae" genus="Ulmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ulmus" order="Hemiptera" pageId="10" pageNumber="57" phylum="Arthropoda" rank="genus">Ulmus</taxonomicName>
