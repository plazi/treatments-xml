<taxonomicName id="9A954D43F965FF92FE25FDB5FDFB1980" box="[489,580,627,648]" class="Actinopterygii" family="Serranidae" genus="Serrano" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="38" pageNumber="39" phylum="Chordata" rank="genus">
