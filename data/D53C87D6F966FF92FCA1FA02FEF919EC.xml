<date id="292B1000F965FF92FD81FD92FD001962" box="[589,703,596,618]" pageId="38" pageNumber="39" value="1987-07-09">9.vii.1987</date>
, ‘#10 
<emphasis id="6FE1EAD2F965FF92FF5DFDB5FED0198F" box="[145,367,626,648]" italics="true" pageId="38" pageNumber="39">
ex 
<taxonomicName id="9A954D43F965FF92FF62FDB4FED0198F" baseAuthorityName="Doubleday" baseAuthorityYear="1844" box="[174,367,626,647]" class="Insecta" family="Papilionidae" genus="Parides" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="photinus">Parides photinus</taxonomicName>
