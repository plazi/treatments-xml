<taxonomicName id="4C13648EFFABFFBDFD76FCBF50C8FCEA" baseAuthorityName="Studer" baseAuthorityYear="1883" box="[652,919,793,820]" class="Malacostraca" family="Parapaguridae" genus="Sympagurus" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="588" phylum="Arthropoda" rank="species" species="dimorphus">
