<taxonomicName id="4C2B4D46F825FFF8FB9BF913EEE3F975" box="[1048,1172,1721,1746]" class="Insecta" family="Sesiidae" genus="Scoliokona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="194" phylum="Arthropoda" rank="genus">
<emphasis id="B95FEAD7F825FFF8FB9BF913EEE3F975" box="[1048,1172,1721,1746]" italics="true" pageId="9" pageNumber="194">Scoliokona</emphasis>
</taxonomicName>
