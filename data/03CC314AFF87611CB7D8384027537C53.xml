<taxonomicName id="4C65FBDFFF84611CB3843F9C204479B8" box="[1227,1381,423,448]" class="Mammalia" family="Indriidae" genus="Avahi" kingdom="Animalia" order="Primates" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="meridionalis">
