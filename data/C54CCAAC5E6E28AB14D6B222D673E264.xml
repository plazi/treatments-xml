<subSubSection id="C19EF393E11BFD2D30C384C5E44839DF" pageId="26" pageNumber="27" type="reference_group">
<paragraph id="F8BC61A5D2842045C1744033A9A780FF" pageId="26" pageNumber="27">
<taxonomicName id="A64C1DAF86275DE986B31E67EE7119D9" class="Mammalia" family="Procyonidae" genus="Bassaricyon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bassaricyon richardsoni" order="Carnivora" pageId="26" pageNumber="27" phylum="Chordata" rank="species" species="richardsoni">Bassaricyon richardsoni</taxonomicName>
