<quantity id="8512ADC7FF83FF82AFA642D9FC287DDA" box="[925,1009,418,440]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" metricValueMax="6.0" metricValueMin="5.0" pageId="11" pageNumber="1274" unit="mm" value="5.5" valueMax="6.0" valueMin="5.0">5-6 mm</quantity>
, 
<quantity id="8512ADC7FF83FF82A82042D9FF057DB7" metricMagnitude="-3" metricUnit="m" metricValue="6.0" metricValueMax="7.0" metricValueMin="5.0" pageId="11" pageNumber="1274" unit="mm" value="6.0" valueMax="7.0" valueMin="5.0">5-7 mm</quantity>
