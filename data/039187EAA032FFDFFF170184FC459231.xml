<quantity id="4CC09B19A032FFDFFF74029EFE33958E" box="[239,387,730,757]" metricMagnitude="-3" metricUnit="m" metricValue="8.7" metricValueMax="9.9" metricValueMin="7.5" pageId="5" pageNumber="70" unit="mm" value="8.7" valueMax="9.9" valueMin="7.5">7.5–9.9 mm</quantity>
