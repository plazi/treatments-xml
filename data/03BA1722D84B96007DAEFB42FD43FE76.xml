<taxonomicName id="4C13DDB7D83496007FE9FE05FC94FE56" authorityName="Nielson" authorityYear="2015" box="[647,789,451,475]" class="Insecta" family="Cicadellidae" genus="Taharana" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="63" phylum="Arthropoda" rank="species" species="biavicula">
<emphasis id="B9677A26D83496007FE9FE05FC94FE56" box="[647,789,451,475]" italics="true" pageId="64" pageNumber="63">T. biavicula</emphasis>
