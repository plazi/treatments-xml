</taxonomicName>
<emphasis id="B97BEAA68701FFC65B80C601F836FBB3" bold="true" box="[976,1065,1123,1149]" pageId="85" pageNumber="86">
<taxonomicNameLabel id="A24857DD8701FFC65B80C601F836FBB3" box="[976,1065,1123,1149]" pageId="85" pageNumber="86" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C0F4D378701FFC65C66C601F8B2FBB3" authority="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz, 2020" authorityName="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz" authorityYear="2020" box="[1078,1197,1123,1149]" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="clavata" status="sp. nov.">
