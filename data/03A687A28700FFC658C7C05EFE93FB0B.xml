and 
<taxonomicName id="4C0F4D378701FFC65D13C601FD54FB6D" authority="(Sulc)" baseAuthorityName="Sulc" class="Insecta" family="Psyllidae" genus="Psylla" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="surinamensis">
