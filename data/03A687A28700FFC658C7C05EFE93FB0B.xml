</taxonomicName>
<emphasis id="B97BEAA68701FFC65A0DC601FEA9FBB3" bold="true" box="[605,694,1123,1149]" pageId="85" pageNumber="86">
<taxonomicNameLabel id="A24857DD8701FFC65A0DC601FEA9FBB3" box="[605,694,1123,1149]" pageId="85" pageNumber="86" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
resembles 
<taxonomicName id="4C0F4D378701FFC65B63C601FFD5FBB3" authority="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz, 2020" authorityName="Rendón-Mera &amp; Burckhardt &amp; Cavichioli &amp; Queiroz" authorityYear="2020" box="[819,970,1123,1149]" class="Insecta" family="Psyllidae" genus="Mitrapsylla" kingdom="Animalia" order="Hemiptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="machaerii" status="sp. nov.">
