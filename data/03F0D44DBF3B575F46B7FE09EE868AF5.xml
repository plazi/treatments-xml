<collectingRegion id="499DABB9BF3B575F42B8FDB1E96E8A65" box="[1176,1355,583,608]" country="South Africa" name="KwaZulu-Natal" pageId="2" pageNumber="223">KwaZulu-Natal</collectingRegion>
