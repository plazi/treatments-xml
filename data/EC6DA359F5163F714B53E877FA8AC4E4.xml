<specimenCount id="72C2D9C6F5103F714875EDE3FD8EC3DB" box="[533,618,651,691]" pageId="28" pageNumber="39" type="male">male</specimenCount>
, and one male paratype, 
<collectingMunicipality id="841F8835F5103F714E20EDE3FB12C3DB" box="[1088,1270,651,691]" pageId="28" pageNumber="39">Machanao</collectingMunicipality>
