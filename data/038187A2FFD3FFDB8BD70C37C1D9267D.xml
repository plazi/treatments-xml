<emphasis id="B95CEAA6FFD0FFDA8BF10F82C26722C0" box="[177,331,1930,1953]" italics="true" pageId="2" pageNumber="33">P. longiuncata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26F57DDFFD0FFDA8A120F81C28822C1" box="[338,420,1929,1952]" pageId="2" pageNumber="33" rank="species">sp. nov.</taxonomicNameLabel>
5, 
<taxonomicName id="4C284D37FFD0FFDA8A850F82C10322C0" box="[453,559,1930,1953]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="biovata" status="sp. nov.">
<emphasis id="B95CEAA6FFD0FFDA8A850F82C10322C0" box="[453,559,1930,1953]" italics="true" pageId="2" pageNumber="33">P. biovata</emphasis>
