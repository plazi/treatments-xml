<quantity id="8B5427A9FFE5C92DFD678B438251722D" box="[750,818,1312,1336]" metricMagnitude="2" metricUnit="m" metricValue="3.2" pageId="32" pageNumber="271" unit="m" value="320.0">320 m</quantity>
