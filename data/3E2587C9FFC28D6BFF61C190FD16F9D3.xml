<taxonomicName id="718C4D5CFFC28D6BFBF0C325FBA1F96A" authorityName="Moore &amp; Dieguez" authorityYear="2019" box="[1030,1108,547,572]" class="Insecta" family="Buprestidae" genus="Lasionota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="468" phylum="Arthropoda" rank="species" species="inca">
