<taxonomicName id="718C4D5CFFC28D6BFDD1C36DFD80F9D2" authorityName="Moore &amp; Dieguez" authorityYear="2019" box="[551,629,619,644]" class="Insecta" family="Buprestidae" genus="Lasionota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="468" phylum="Arthropoda" rank="species" species="inca" status="syn. nov.">
