—penis, connective, and style, right side, lateral view; 
<number id="AD765E59FFB3FFA97E196255FEC9A612" box="[291,330,1628,1651]" pageId="31" pageNumber="32" value="100.0">100</number>
—penis, ventral view; 
<number id="AD765E59FFB3FFA97D046255FDE1A612" box="[574,610,1628,1651]" pageId="31" pageNumber="32" value="101.0">101</number>
—pygofer, lateral view; 
<number id="AD765E59FFB3FFA97C566255FC11A612" box="[876,914,1628,1651]" pageId="31" pageNumber="32" value="102.0">102</number>
