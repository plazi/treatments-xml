<geoCoordinate id="EE6BEDADFC53FFDAF124FE2BFC64FE14" box="[822,897,477,497]" degrees="85" direction="east" minutes="39" orientation="longitude" pageId="18" pageNumber="1027" precision="925" value="85.65">85°39‘E</geoCoordinate>
