<quantity id="4CA7268FFC53FFDAF19CFE2BFC28FE14" box="[910,973,477,497]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="18" pageNumber="1027" unit="m" value="1300.0">1300m</quantity>
