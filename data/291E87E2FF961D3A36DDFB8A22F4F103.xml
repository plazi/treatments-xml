<quantity id="664F9B11FF961D3E3774FA3C21B5F0BA" box="[325,392,1501,1525]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="65" pageNumber="1466" unit="mm" value="8.0">8 mm</quantity>
, and of fore wing 
<quantity id="664F9B11FF961D3E3460FA3C2294F0BA" box="[593,681,1501,1525]" metricMagnitude="-3" metricUnit="m" metricValue="7.9" pageId="65" pageNumber="1466" unit="mm" value="7.9">7.9 mm</quantity>
