<collectingCountry id="F31674EE5650FFE0414FFAE253A6A08A" name="Austria" pageId="14" pageNumber="519">Austria</collectingCountry>
, 
<collectingCountry id="F31674EE5650FFE045B3FA02526AA08A" box="[200,369,1419,1443]" name="Czech Republic" pageId="14" pageNumber="519">Czech Republic</collectingCountry>
