<taxonomicName id="4C63DCFDF27F474A10B4F985FAB6FD7E" authority="Lacepede, 1802" authorityName="Lacepede" authorityYear="1802" box="[1018,1327,668,693]" class="Actinopterygii" family="Haemulidae" genus="Pomadasys" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="296" phylum="Chordata" rank="genus">
