<collectionCode id="C79F09FF4F4E821EFCC1F8F7FE144F24" box="[826,874,1842,1866]" country="Portugal" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="7" pageNumber="443">COI</collectionCode>
