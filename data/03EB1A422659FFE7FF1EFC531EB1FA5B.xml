<taxonomicName id="4C42D0D72659FFE7FC05FACA1821FA7F" authority="(Grote 1913: 126)" baseAuthorityName="Grote" baseAuthorityPageNumber="126" baseAuthorityYear="1913" box="[908,1309,1389,1416]" class="Magnoliopsida" family="Bataceae" genus="Batis" kingdom="Plantae" order="Brassicales" pageId="1" pageNumber="250" phylum="Tracheophyta" rank="species" species="reichenowi">
