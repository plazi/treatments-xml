<number id="AD2E24EA397A4E60AF1881D62998FA4D" box="[856,999,1473,1497]" pageId="12" pageNumber="47" value="0.868" valueMax="0.923" valueMin="0.813">0.813–0.923</number>
, HL 
<number id="AD2E24EA397A4E60A85E81D62ED2FA4D" box="[1054,1197,1473,1497]" pageId="12" pageNumber="47" value="0.7929999999999999" valueMax="0.836" valueMin="0.75">0.750–0.836</number>
