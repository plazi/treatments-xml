<taxonomicName id="A7975E8715459513400A3E77037613A6" authorityName="Rehder" authorityYear="1919" baseAuthorityName="Hook. f. &amp; Thomson ex Miq." class="Magnoliopsida" family="Fagaceae" genus="Lithocarpus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lithocarpus dealbatus" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="dealbatus">
