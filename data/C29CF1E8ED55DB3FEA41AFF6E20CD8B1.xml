<taxonomicName id="A11E251645BE92D4E78CD65E00F21262" genus="C." lsidName="C. amputatum" pageId="0" pageNumber="1" rank="species" species="amputatum">
<emphasis id="61993857997D02AF3ADC59F7432282BB" italics="true" pageId="0" pageNumber="1">C. amputatum</emphasis>
</taxonomicName>
