<taxonomicName id="BF0546061D7B053AECE795F84C1A69F8" authorityName="Smith &amp; Beaver &amp; Cognato" authorityYear="2020" class="Insecta" family="Curculionidae" genus="Cyclorhipidion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclorhipidion amasoides" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="amasoides">
