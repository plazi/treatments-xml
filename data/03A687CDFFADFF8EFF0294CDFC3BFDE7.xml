<caption id="DF706653FFA9FF8DFF0294B9FC28F8C8" httpUri="https://zenodo.org/record/262867/files/figure.png" pageId="5" pageNumber="192" targetBox="[255,1320,179,1676]" targetPageId="5">
<paragraph id="8BB036DBFFA9FF8DFF0294B9FC28F8C8" blockId="5.[151,1436,1715,1795]" pageId="5" pageNumber="192">
<emphasis id="B97BEAC9FFA9FF8DFF0294B9FEA7F902" bold="true" box="[151,329,1715,1737]" pageId="5" pageNumber="192">FIGURES 26–34.</emphasis>
<emphasis id="B97BEAC9FFA9FF8DFEC594B9FDD9F903" box="[336,567,1715,1736]" italics="true" pageId="5" pageNumber="192">
<taxonomicName id="4C0F4D58FFA9FF8DFEC594B9FDDCF903" box="[336,562,1715,1736]" class="Insecta" family="Cicadellidae" genus="Varicopsella" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="192" phylum="Arthropoda" rank="species" species="odontoida">
<taxonomicName id="4C0F4D58FFA9FF8DFEC594B9FE26F903" box="[336,456,1715,1736]" class="Insecta" family="Cicadellidae" genus="Varicopsella" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="192" phylum="Arthropoda" rank="genus">Varicopsella</taxonomicName>
<taxonomicName id="4C0F4D58FFA9FF8DFE5B94B9FDDCF903" box="[462,562,1715,1736]" pageId="5" pageNumber="192" rank="species" species="odontoida">odontoida</taxonomicName>
</taxonomicName>
