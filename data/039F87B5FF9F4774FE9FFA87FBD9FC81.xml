<paragraph id="8B8936A3FF9F4773FE9FFA87FDEDFADC" blockId="91.[242,551,1375,1434]" box="[272,551,1375,1399]" pageId="91" pageNumber="190">
<heading id="D0C181CFFF9F4773FE9FFA87FDEDFADC" box="[272,551,1375,1399]" fontSize="10" level="2" pageId="91" pageNumber="190" reason="2">
<taxonomicName id="4C364D20FF9F4773FE9FFA87FE2DFADD" authority="Kuschel, 2008" authorityName="Kuschel" authorityYear="2008" box="[272,487,1375,1398]" class="Insecta" family="Curculionidae" genus="Strabonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="91" pageNumber="190" phylum="Arthropoda" rank="species" species="unionis" status="n. sp.">
<emphasis id="B942EAB1FF9F4773FE9FFA87FE2DFADD" bold="true" box="[272,487,1375,1398]" italics="true" pageId="91" pageNumber="190">Strabonus unionis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157CAFF9F4773FE61FAB8FDEDFADC" box="[494,551,1376,1399]" pageId="91" pageNumber="190" rank="species">n. sp.</taxonomicNameLabel>
