<taxonomicName id="1D77DA38A47E2642C59FF0C7F0057B6D" authority="" authorityName="" class="Centricae" family="Chironomidae" genus="T." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="T. maritima" order="Diptera" pageId="0" pageNumber="8013" phylum="Diatomeae" rank="species" species="maritima">T. maritima</taxonomicName>
