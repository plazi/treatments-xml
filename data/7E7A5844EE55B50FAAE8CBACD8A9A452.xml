<taxonomicName id="F20F8F4D0823AED5C1E26AA740FB5410" authority="" authorityName="" class="Centricae" family="Chironomidae" genus="T." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="T. hyalipennis" order="Diptera" pageId="0" pageNumber="8013" phylum="Diatomeae" rank="species" species="hyalipennis">T. hyalipennis</taxonomicName>
