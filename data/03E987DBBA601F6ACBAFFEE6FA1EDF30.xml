<taxonomicName id="4C404D4EBA601F6ACBAFFEE6FA33DD5B" box="[151,307,345,371]" class="Insecta" family="Hybotidae" genus="Tachydromia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="genus">
<emphasis id="B934EADFBA601F6ACBAFFEE6FA33DD5B" bold="true" box="[151,307,345,371]" italics="true" pageId="6" pageNumber="39">Tachydromia</emphasis>
