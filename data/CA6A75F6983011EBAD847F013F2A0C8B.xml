<paragraph id="38BD1A304234389C13269F6057B95BF1" pageId="1" pageNumber="140">Type species.</paragraph>
<paragraph id="254597B5C2934E05898B6C2B3FA06CDC" pageId="1" pageNumber="140">
<taxonomicName id="46E7F8157FF5323083E68B4A215BB78E" authority="Guenée, 1839" authorityName="Guenée" authorityYear="1839" class="Insecta" family="Noctuidae" genus="Taeniocampa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Taeniocampa curtica" order="Lepidoptera" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="curtica">Taeniocampa curtica</taxonomicName>
