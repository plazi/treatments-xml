<taxonomicName id="4C7E3589FF45FF63FEF4F86A3273FCEB" authority="Soula, 2006" authorityName="Soula" authorityYear="2006" box="[380,783,771,795]" class="Insecta" family="Rutelidae" genus="Macraspis" kingdom="Animalia" order="Coleoptera" pageId="142" pageNumber="141" phylum="Arthropoda" rank="subSpecies" species="chrysis" subSpecies="zumbadoi">
