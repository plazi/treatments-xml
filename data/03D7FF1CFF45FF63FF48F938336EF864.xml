<taxonomicName id="4C7E3589FF45FF63FD60F9AA3524FD2B" authority="Soula, 2006" authorityName="Soula" authorityYear="2006" box="[744,1112,707,731]" class="Insecta" family="Rutelidae" genus="Macraspis" kingdom="Animalia" order="Coleoptera" pageId="142" pageNumber="141" phylum="Arthropoda" rank="subSpecies" species="chrysis" subSpecies="isabelae">
