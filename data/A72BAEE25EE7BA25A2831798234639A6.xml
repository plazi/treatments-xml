<taxonomicName id="1E011B5F900F34ACCB580841865B7739" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34021" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole spininodis Mayr" lsidName-HNS="Pheidole spininodis Mayr" order="Hymenoptera" pageNumber="758" phylum="Arthropoda" rank="species" species="spininodis">spininodis</taxonomicName>
