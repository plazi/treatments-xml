<taxonomicName id="4C35ECC8036B044DFEB9F82D61B6F814" authority="Cobb, 1893" authorityName="Cobb" authorityYear="1893" box="[284,458,1927,1949]" class="Adenophorea" family="Plectidae" genus="Plectus" kingdom="Animalia" order="Araeolaimida" pageId="13" pageNumber="14" phylum="Nematoda" rank="species" species="minimus">
<emphasis id="B9414B59036B044DFEB9F82D61B6F814" bold="true" box="[284,458,1927,1949]" italics="true" pageId="13" pageNumber="14">Plectus minimus</emphasis>
