<quantity id="67199B7C3E11C818662E7CDDFD85FF24" box="[545,635,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="5.2" pageId="2" pageNumber="586" unit="mm" value="5.2">5.2 mm</quantity>
