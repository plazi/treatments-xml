<paragraph id="8BD036DEFFCE181697849967B210C23A" blockId="42.[189,1399,1070,1347]" pageId="42" pageNumber="43">
MEASUREMENTS. 
<specimenCount id="9D69FD57FFCE181696479967B6C6C330" box="[382,404,1071,1097]" pageId="42" pageNumber="43" type="male">♂</specimenCount>
<typeStatus id="54D4887CFFCE181696A49966B555C331" box="[413,519,1070,1096]" pageId="42" pageNumber="43">holotype</typeStatus>
