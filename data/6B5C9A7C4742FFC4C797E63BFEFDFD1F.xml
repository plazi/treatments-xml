<taxonomicName id="24F550E94742FFC4C4F6E51CFBB6FDC4" box="[1014,1123,512,535]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="19" pageNumber="420" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
