<taxonomicName id="24F550E94742FFC4C797E571FF14FD57" box="[151,193,621,644]" class="Insecta" family="Curculionidae" genus="Scolytus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="species" species="spp">spp</taxonomicName>
