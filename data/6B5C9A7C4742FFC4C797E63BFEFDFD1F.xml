<taxonomicName id="24F550E94742FFC4C253E55BFA49FDB3" box="[1363,1436,583,608]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="genus">
