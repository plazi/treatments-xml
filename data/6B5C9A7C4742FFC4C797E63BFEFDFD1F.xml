</taxonomicName>
sp. (
<taxonomicName id="24F550E94742FFC4C47AE554FC3FFDB3" box="[890,1002,584,608]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="19" pageNumber="420" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
