<taxonomicName id="24F550E94742FFC4C3A4E55BFA95FDB3" box="[1188,1344,583,608]" class="Magnoliopsida" family="Juglandaceae" kingdom="Plantae" order="Fagales" pageId="19" pageNumber="420" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
