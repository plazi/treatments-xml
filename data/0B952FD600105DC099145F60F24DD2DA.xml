</taxonomicName>
sp. nov. 
<emphasis id="E07284EDB8B721E07328B88BEFEE6CDC" bold="true" pageId="0" pageNumber="77">6-8</emphasis>
<taxonomicName id="B69F2A9B1125839D79E91710152B72DF" class="Insecta" genus="L." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="L. zhamensis" order="Coleoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="zhamensis">
<emphasis id="C3D9434C13F5A527758A2F4E38BAAB56" italics="true" pageId="0" pageNumber="77">L. zhamensis</emphasis>
