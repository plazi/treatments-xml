<taxonomicName id="FDE0848EFF8EB27CF005FAB6FD0BA0B9" authorityName="Hartman" authorityYear="1958" class="Calcarea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosoleniida" pageId="53" pageNumber="256" phylum="Porifera" rank="order">Leucosoleniida</taxonomicName>
