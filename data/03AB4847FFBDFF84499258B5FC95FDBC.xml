<taxonomicName id="4C0282D2FFBEFF8448525C34FDBEFDBC" box="[343,577,583,608]" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="6" pageNumber="171" phylum="Tracheophyta" rank="species" species="australis">
