<collectingCountry id="F3244E9CFF83FFBD8572728AFD260737" box="[592,642,251,271]" name="Poland" pageId="3" pageNumber="1298">Polen</collectingCountry>
(WAR), 
<collectingCountry id="F3244E9CFF83FFBD85F8728AFCE30737" box="[730,839,251,271]" name="Germany" pageId="3" pageNumber="1298">Deutschland</collectingCountry>
(BER, EDI, MÜN, ZSM), 
<collectingCountry id="F3244E9CFF83FFBD87FD7362FEED071F" box="[223,329,275,295]" name="Netherlands" pageId="3" pageNumber="1298">Niederlande</collectingCountry>
(ZWA), 
<collectingCountry id="F3244E9CFF83FFBD868A7362FE48071F" box="[424,492,275,295]" name="Belgium" pageId="3" pageNumber="1298">Belgien</collectingCountry>
(LIB), Grossbritannien (
<collectingCountry id="F3244E9CFF83FFBD85F07362FCBA071F" box="[722,798,275,295]" name="United Kingdom" pageId="3" pageNumber="1298">England</collectingCountry>
, 
<collectingCountry id="F3244E9CFF83FFBD840C7362FC28071F" box="[814,908,275,295]" name="United Kingdom" pageId="3" pageNumber="1298">Schottland</collectingCountry>
) (EDI), 
<collectingCountry id="F3244E9CFF83FFBD84C17362FB99071F" box="[995,1085,275,295]" name="Austria" pageId="3" pageNumber="1298">Österreich</collectingCountry>
