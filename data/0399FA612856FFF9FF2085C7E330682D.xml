<taxonomicName id="4C3030F42856FFFBFE6D86AFE35F6C3F" box="[474,634,1772,1797]" class="Insecta" family="Nolidae" genus="Wittonola" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="279" phylum="Arthropoda" rank="species" species="latifasciata">
