<taxonomicName id="43E002A91A1AFFA43988FA859382441C" authorityName="Hobbs &amp; Walton" authorityYear="1976" box="[409,758,1375,1400]" class="Ostracoda" family="Entocytheridae" genus="Dactylocythere" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="brachydactylus">
