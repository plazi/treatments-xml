<taxonomicName id="B73701E239C2F4C8DA945C93DEC9E926" class="Magnoliopsida" family="Fabaceae" genus="Hymenaea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fabales" pageId="10" pageNumber="111" phylum="Tracheophyta" rank="species" species="altissima">
<emphasis id="FC02487BEDE3AD388A7D2371D2936DD4" italics="true" pageId="10" pageNumber="111">Hymenaea altissima</emphasis>
