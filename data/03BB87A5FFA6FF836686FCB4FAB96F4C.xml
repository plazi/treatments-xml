<subSubSection id="C3086538FFA9FF836686FF61FE306D9D" box="[151,431,150,177]" pageId="8" pageNumber="567" type="diagnosis">
<paragraph id="8BAD36B3FFA9FF836686FF61FE306D9D" blockId="8.[151,1436,150,609]" box="[151,431,150,177]" pageId="8" pageNumber="567">
