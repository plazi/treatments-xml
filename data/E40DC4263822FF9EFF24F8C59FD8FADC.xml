<taxonomicName id="ABA40EB33822FF9FFE95F88A9ED0F8F8" authority="Redtenbacher, 1891" authorityName="Redtenbacher" authorityYear="1891" box="[294,701,1882,1905]" class="Insecta" family="Tettigoniidae" genus="Agraecia" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="species" species="sansibara">
