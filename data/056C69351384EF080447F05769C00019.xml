<taxonomicName id="4CDFF52E6F18B5DA5992F1A76DF5544B" class="Magnoliopsida" family="Asteraceae" genus="Packera" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Packera paupercula" order="Asterales" pageId="0" pageNumber="1099" phylum="Tracheophyta" rank="species" species="paupercula">Packera paupercula</taxonomicName>
