<taxonomicName id="16EF2FDF0E168297625E91935A9C483D" class="Magnoliopsida" family="Asteraceae" genus="Packera" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Packera crawfordii" order="Asterales" pageId="0" pageNumber="1099" phylum="Tracheophyta" rank="species" species="crawfordii">Packera crawfordii</taxonomicName>
