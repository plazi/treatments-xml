<taxonomicName id="967023FD1C2051FEF1F1D4F905BE2D15" class="Magnoliopsida" family="Asteraceae" genus="Senecio" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Senecio pauperculus" order="Asterales" pageId="0" pageNumber="1099" phylum="Tracheophyta" rank="species" species="pauperculus">Senecio pauperculus</taxonomicName>
Michx. sensu RAB; = FNA;&gt; 
<taxonomicName id="992C35AEEDF002EBC45D4605D0190421" class="Magnoliopsida" family="Asteraceae" genus="Packera" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Packera crawfordii" order="Asterales" pageId="0" pageNumber="1099" phylum="Tracheophyta" rank="species" species="crawfordii">Packera crawfordii</taxonomicName>
(Britton) A.M. Mahoney &amp; R.R. Kowal, 
<taxonomicName id="57602BFB8089075BDA6E571001909882" basionym-authority="(Michx.) A. Loeve &amp; D. Loeve" class="Magnoliopsida" family="Asteraceae" genus="Packera" higherTaxonomySource="CoL" infraspecific-rank="var." kingdom="Plantae" lsidName="Packera paupercula var. paupercula" order="Asterales" pageId="0" pageNumber="1099" phylum="Tracheophyta" rank="variety" species="paupercula" variety="paupercula">
