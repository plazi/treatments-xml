<taxonomicName id="0B8351F63B00832CFEB6F8F8CAE4F8E3" authority="Zibrowius, 1973" authorityName="Zibrowius" authorityYear="1973" box="[300,739,1841,1863]" class="Polychaeta" family="Serpulidae" genus="Bathyvermilia" kingdom="Animalia" order="Sabellida" pageId="3" pageNumber="46" phylum="Annelida" rank="species" species="challengeri">
