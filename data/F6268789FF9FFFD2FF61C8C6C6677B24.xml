<collectingRegion id="BC4BF87DFF9FFFD0FB1CCE63C60C7B65" box="[1258,1372,1882,1907]" country="Turkey" name="Gaziantep" pageId="4" pageNumber="423">Gaziantep</collectingRegion>
Zoo, 
<collectingRegion id="BC4BF87DFF9FFFD0FF61CE47C25C7B81" box="[151,268,1918,1943]" country="Turkey" name="Gaziantep" pageId="4" pageNumber="423">Gaziantep</collectingRegion>
, 
<collectingCountry id="0698760FFF9FFFD0FEE0CE47C2387B81" box="[278,360,1918,1943]" name="Turkey" pageId="4" pageNumber="423">Turkey</collectingCountry>
. 
<emphasis id="4CFBEA8DFF9FFFD0FF31CE9BC21E7BAD" bold="true" box="[199,334,1954,1979]" pageId="4" pageNumber="423">Etymology.</emphasis>
The name 
<taxonomicName id="B98F4D1CFF9FFFD0FE3DCE9AC1497BAC" authorityName="Gürelli" authorityYear="2019" box="[459,537,1955,1978]" class="Prostomatea" family="Buetschliidae" genus="Buetschlia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Prostomatida" pageId="4" pageNumber="423" phylum="Ciliophora" rank="species" species="cirrata">
