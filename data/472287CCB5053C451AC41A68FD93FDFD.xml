<taxonomicName id="088B4D59B5023C451BD91FF6FD84FF0C" authorityName="Assing" authorityYear="2018" box="[431,529,227,249]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="1053" phylum="Arthropoda" rank="species" species="manca">
