<taxonomicName id="0DBCB680852E2BBD5A856B4A5D6C53B5" class="Insecta" family="Aphididae" genus="Essigella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Essigella gagnonae" order="Hemiptera" pageId="3" pageNumber="106" phylum="Arthropoda" rank="species" species="gagnonae">Essigella gagnonae</taxonomicName>
