<taxonomicName id="84AC4D44FFF0FFA7FD0744BCFCBBFE1D" authorityName="Fabricius" authorityYear="1775" box="[681,793,453,476]" class="Insecta" family="Leucospidae" genus="Leucospis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">
<emphasis id="71D8EAD5FFF0FFA7FD0744BCFCBBFE1D" box="[681,793,453,476]" italics="true" pageId="39" pageNumber="40">Leucospis</emphasis>
