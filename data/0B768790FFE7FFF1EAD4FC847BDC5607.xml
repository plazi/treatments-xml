<taxonomicName id="44DF4D05FFE4FFF0EBDEFBC878645676" authority="Hippa &amp; Ševčík, 2013" authorityName="Hippa &amp; Ševčík" authorityYear="2013" box="[212,332,1104,1122]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="769" phylum="Arthropoda" rank="species" species="licina" status="sp. nov.">
