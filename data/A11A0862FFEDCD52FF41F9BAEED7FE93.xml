<taxonomicName id="EEB3C2F7FFEBCD53FEA6F823EE5CF841" authority="Liu &amp; Li &amp; Pham, 2010" authorityName="Liu &amp; Li &amp; Pham" authorityYear="2010" box="[368,592,1977,2000]" class="Arachnida" family="Agelenidae" genus="Coelotes" kingdom="Animalia" order="Araneae" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="polyedricus" status="sp. nov.">
