<paragraph id="8BF7935CFFD7FF87FF5CFC835A926B33" blockId="1.[151,1437,785,2034]" box="[199,1247,821,846]" pageId="1" pageNumber="487">
<emphasis id="B93C4F4EFFD7FF87FF5CFC835F356B33" bold="true" box="[199,376,821,846]" pageId="1" pageNumber="487">
<typeStatus id="54F32DFEFFD7FF87FF5CFC835F4F6B33" box="[199,258,821,846]" pageId="1" pageNumber="487">Type</typeStatus>
