<subSubSection id="6CE8AA3A790B4BB01012A4BABDD8E414" pageId="13" pageNumber="14" type="reference_group">
<paragraph id="124DC07BE0B589D146C0563F7F02AF6D" pageId="13" pageNumber="14">
<taxonomicName id="5357D52F9D75B5F0FD8E6B5A53BFF808" class="Insecta" family="Hydroptilidae" genus="Leucotrichia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucotrichia chiriquiensis" order="Trichoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="chiriquiensis">chiriquiensis</taxonomicName>
