<taxonomicName id="4C244D19FFA2725115AA1C793C94FC28" authorityName="Latreille" authorityYear="1802" box="[502,696,878,904]" class="Insecta" family="Chrysomelidae" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="747" phylum="Arthropoda" rank="family">Chrysomelidae</taxonomicName>
