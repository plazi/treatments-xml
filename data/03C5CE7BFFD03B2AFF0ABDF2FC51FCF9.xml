: 
<taxonomicName id="4C6C04EEFFD03B2AFF7FBC4DFD16FEA9" authority="Cabanis, 1874" authorityName="Cabanis" authorityYear="1874" box="[226,732,296,322]" class="Aves" family="Furnariidae" genus="Upucerthia" kingdom="Animalia" order="Passeriformes" pageId="9" pageNumber="38" phylum="Chordata" rank="subSpecies" species="jelskii" subSpecies="jelskii">
<emphasis id="B918A37FFFD03B2AFF7FBC4DFDC4FEAA" box="[226,526,296,321]" italics="true" pageId="9" pageNumber="38">Upucerthia jelskii jelskii</emphasis>
(
<bibRefCitation id="EFFD029CFFD03B2AFD83BC4DFD18FEA9" author="Cabanis" box="[542,722,296,322]" pageId="9" pageNumber="45" refString="Cabanis, J. (1874) [Neue peruanische Vogel des Herrn Jelski]. Journal fur Ornithologie, 22, 96 - 100." type="journal article" year="1874">
Cabanis, 
<number id="3DA46D4AFFD03B2AFD0CBC4DFD18FEA9" box="[657,722,296,322]" pageId="9" pageNumber="38" value="1874.0">1874</number>
</bibRefCitation>
)
</taxonomicName>
. See 
<bibRefCitation id="EFFD029CFFD03B2AFC81BC4DFBB7FEA9" author="Cory" box="[796,1149,296,322]" pageId="9" pageNumber="46" refString="Cory, C. B. &amp; Hellmayr, C. E. (1925) Catalogue of birds of the Americas and the adjacent islands. Field Museum of Natural History, Zoological Series, vol. 13, pt. 4." type="journal article" year="1925">
