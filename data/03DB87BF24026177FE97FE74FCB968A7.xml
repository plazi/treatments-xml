<taxonomicName id="4C724D2A24016176FE04F94DFDD36C95" box="[411,549,1757,1780]" genus="Diptilostatus" pageId="18" pageNumber="19" rank="genus">Diptilostatus</taxonomicName>
<taxonomicName id="4C724D2A24016176FDAFF94DFD846C95" box="[560,626,1757,1780]" pageId="18" pageNumber="19" rank="species" species="mesae" status="n.sp.">mesae</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23557C024016176FDE2F94CFD456C95" box="[637,691,1756,1780]" pageId="18" pageNumber="19" rank="species">
