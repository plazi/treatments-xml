<subSubSection id="13CA0DCAFF81FFFCFAB5FF64FE70318D" box="[151,506,152,178]" pageId="17" pageNumber="278" type="nomenclature">
<paragraph id="5B6F5E41FF81FFFCFAB5FF64FE70318D" blockId="17.[151,506,152,178]" box="[151,506,152,178]" pageId="17" pageNumber="278">
<heading id="0027E92DFF81FFFCFAB5FF64FE70318D" bold="true" box="[151,506,152,178]" fontSize="11" level="1" pageId="17" pageNumber="278" reason="1">
<emphasis id="69A48253FF81FFFCFAB5FF64FE70318D" bold="true" box="[151,506,152,178]" pageId="17" pageNumber="278">
Genus 
<taxonomicName id="9CD025C2FF81FFFCFACCFF64FE28318D" box="[238,418,152,178]" class="Gymnolaemata" family="Cribrilinidae" genus="Spiniflabellum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Cheilostomatida" pageId="17" pageNumber="278" phylum="Bryozoa" rank="genus" status="n. gen.">
<emphasis id="69A48253FF81FFFCFACCFF64FE28318D" bold="true" box="[238,418,152,178]" italics="true" pageId="17" pageNumber="278">Spiniflabellum</emphasis>
</taxonomicName>
