</taxonomicName>
, 
<taxonomicName id="4C7319CA6934EF1AFCD4A8B6FC3722C2" baseAuthorityName="O. F. Müller" baseAuthorityYear="1774" box="[863,1023,1928,1954]" class="Gastropoda" family="Helicidae" genus="Theba" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="pisana">
