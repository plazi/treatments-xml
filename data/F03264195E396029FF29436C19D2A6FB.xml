<taxonomicName id="BF9BAE8C5E366029FB3645E619D2A221" box="[1160,1435,239,260]" class="Reptilia" family="Agamidae" genus="Phrynocephalus" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="257" phylum="Chordata" rank="species" species="helioscopus">
