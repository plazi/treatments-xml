<quantity id="4CDE9B7DE7798D24263DFE8DFBA2D84E" box="[1030,1114,329,353]" metricMagnitude="-3" metricUnit="m" metricValue="9.9" pageId="70" pageNumber="123" unit="mm" value="9.9">9.9 mm</quantity>
; length of forebody 
<quantity id="4CDE9B7DE7798D242700FE8DFA68D84E" box="[1339,1424,329,353]" metricMagnitude="-3" metricUnit="m" metricValue="4.7" pageId="70" pageNumber="123" unit="mm" value="4.7">4.7 mm</quantity>
