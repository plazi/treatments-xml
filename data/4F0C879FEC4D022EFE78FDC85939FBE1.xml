</emphasis>
<taxonomicName id="00A54D0AEC4D0229FE91FDB45F0AFDCF" authority="Stal, 1865: 184" authorityName="Stal" authorityPageNumber="184" authorityYear="1865" box="[268,612,612,638]" class="Insecta" family="Coreidae" genus="Phthia" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="70" phylum="Arthropoda" rank="species" species="decorata">
