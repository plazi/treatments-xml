<specimenCount id="09CCFD425878FFD2E54AFA40FD88FA08" box="[552,575,1485,1508]" pageId="10" pageNumber="11" type="male">♂</specimenCount>
, 
<typeStatus id="C07188695878FFD2E52EFA41FD19FA09" box="[588,686,1484,1509]" pageId="10" pageNumber="11">Allotype</typeStatus>
<specimenCount id="09CCFD425878FFD2E5D4FA40FD7CFA08" box="[694,715,1485,1508]" pageId="10" pageNumber="11" type="female">♀</specimenCount>
