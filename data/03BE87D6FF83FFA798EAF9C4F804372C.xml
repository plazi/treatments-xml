<taxonomicName id="4C174D43FF83FFA798EAF9C4F8213654" baseAuthorityName="Griffith &amp; Smith" baseAuthorityYear="1834" box="[151,608,1590,1617]" class="Elasmobranchii" family="Sphyrnidae" genus="Sphyrna" kingdom="Animalia" order="Carcharhiniformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="lewini">
<emphasis id="B963EAD2FF83FFA798EAF9C4FB043654" bold="true" box="[151,325,1590,1617]" italics="true" pageId="18" pageNumber="19">Sphyrna lewini</emphasis>
