<taxonomicName id="29712D2AC6587E0DF3EB065FDCE5055C" authority="Dejean, 1821" authorityName="Dejean" authorityYear="1821" class="Insecta" family="Tenebrionidae" genus="Elenophorus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Elenophorus" order="Coleoptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="genus">Elenophorus</taxonomicName>
