<paragraph id="5B7B520C1F37912FE97CD4D92B9C65AC" pageId="57" pageNumber="58">
<taxonomicName id="DA03D34C9F295D2A429DA921BCD59092" class="Gastropoda" family="Samlidae" genus="Samla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Samla annuligera" order="Nudibranchia" pageId="57" pageNumber="58" phylum="Mollusca" rank="species" species="annuligera">Samla annuligera</taxonomicName>
