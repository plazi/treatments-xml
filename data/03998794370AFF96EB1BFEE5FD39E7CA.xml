<taxonomicName id="4C304D01370AFF96EB1BFEE5FD57E49B" authority="Hartig, 1837" authorityName="Hartig" authorityYear="1837" box="[517,760,379,403]" class="Insecta" family="Tenthredinidae" genus="Taxonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="265" phylum="Arthropoda" rank="genus">
