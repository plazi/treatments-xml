<taxonomicName id="4C174D3FFF9EFFA3B9ADB364FC0CFB00" authority="R. Br." authorityName="R. Br." box="[701,926,1148,1171]" class="Magnoliopsida" family="Asteraceae" genus="Ozothamnus" kingdom="Plantae" order="Asterales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="genus">
<emphasis id="B963EAAEFF9EFFA3B9ADB364FCD9FB01" box="[701,843,1148,1171]" italics="true" pageId="48" pageNumber="49">Ozothamnus</emphasis>
