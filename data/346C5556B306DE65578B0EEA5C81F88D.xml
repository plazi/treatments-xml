<taxonomicName id="7BC59FC3B306DE6553360B555DA0FAA0" box="[1066,1213,1320,1343]" class="Insecta" family="Meloidae" genus="Epicauta" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="27" phylum="Arthropoda" rank="species" species="grammica">
