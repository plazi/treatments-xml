<subSubSection id="19D2828CB17EFFDFFF1CF99D6B72F953" pageId="2" pageNumber="590" type="etymology">
<paragraph id="5177D107B17EFFDFFF1CF99D6B72F953" blockId="2.[151,1435,1638,1696]" pageId="2" pageNumber="590">
<emphasis id="63BC0D15B17EFFDFFF1CF99D691CF98E" bold="true" box="[199,327,1638,1661]" pageId="2" pageNumber="590">Etymology.</emphasis>
