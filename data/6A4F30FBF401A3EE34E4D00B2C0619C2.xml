<taxonomicName id="C08E8C7C2D5250FBBBA46CCBAE8C2608" authorityName="Barr" authorityYear="1982" class="Insecta" family="Carabidae" genus="Miquihuana" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="728" pageNumber="729" phylum="Arthropoda" rank="species" species="rhadiniformis">
