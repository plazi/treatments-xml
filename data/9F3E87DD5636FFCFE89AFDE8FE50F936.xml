<heading id="4C6081A75636FFCEE89AFDE8FDE8FD5C" bold="true" box="[151,611,619,646]" fontSize="11" level="1" pageId="24" pageNumber="25" reason="1">
<emphasis id="25E3EAD95636FFCEE89AFDE8FDE8FD5C" bold="true" box="[151,611,619,646]" pageId="24" pageNumber="25">
<taxonomicName id="D0974D485636FFCEE89AFDE8FE4EFD5F" box="[151,453,620,646]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="karoyanitensis" status="new species">
<emphasis id="25E3EAD95636FFCEE89AFDE8FE4EFD5F" bold="true" box="[151,453,620,646]" italics="true" pageId="24" pageNumber="25">Chimarra karoyanitensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="3ED057A25636FFCEE9D9FDEFFDE8FD5C" box="[468,611,619,645]" pageId="24" pageNumber="25" rank="species">new species</taxonomicNameLabel>
</emphasis>
