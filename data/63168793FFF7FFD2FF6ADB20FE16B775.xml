<caption id="BFC0660DFFF5FFD2FF6AD978FE1FB4B9" ID-DOI="http://doi.org/10.5281/zenodo.4410639" ID-Zenodo-Dep="4410639" httpUri="https://zenodo.org/record/4410639/files/figure.png" pageId="15" pageNumber="250" startId="15.[151,264,1392,1416]" targetBox="[151,1436,181,1368]" targetPageId="15">
<paragraph id="EB003685FFF5FFD2FF6AD978FE1FB4B9" blockId="15.[151,1436,1392,1452]" pageId="15" pageNumber="250">
