<quantity id="4CC89B356549FF977111C0BAFB06F8B7" box="[1064,1173,1785,1811]" metricMagnitude="-4" metricUnit="m" metricValue="9.3" pageId="14" pageNumber="15" unit="mm" value="0.93">0.93 mm</quantity>
