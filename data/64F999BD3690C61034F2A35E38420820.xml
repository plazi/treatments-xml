<paragraph id="B3B81DF388772BE8ACF15B63B20C4EE1" pageId="0" pageNumber="1">
<taxonomicName id="B2093A11888397E42D49E93A870E5A0E" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon josejaramilloi" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="josejaramilloi">
