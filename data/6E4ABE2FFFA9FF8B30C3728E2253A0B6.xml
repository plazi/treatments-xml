<emphasis id="D497D32BFFA9FF8B30C3728E2616A3BD" bold="true" box="[151,324,1507,1533]" pageId="7" pageNumber="208">
<taxonomicName id="21E374BAFFA9FF8B30C3728E27B8A3BD" box="[151,234,1507,1533]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="219" phylum="Arthropoda" rank="species" species="grisea">Grisea</taxonomicName>
