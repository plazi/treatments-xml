46. - 
<taxonomicName id="C9037A9F3AF76653CCF3776821B484ED" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:150378" class="Insecta" family="Formicidae" genus="Messor" kingdom="Animalia" lsidName="Messor rugosus crawleyi Santschi" lsidName-HNS="Messor rugosus crawleyi Santschi" order="Hymenoptera" pageNumber="203" phylum="Arthropoda" rank="subSpecies" species="rugosus" subSpecies="crawleyi">Messor rugosus Andre st. crawleyi</taxonomicName>
