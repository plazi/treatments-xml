<taxonomicName id="E6411EA30BF1B3BE8D4D2B5D51E3C142" class="Gastropoda" family="Hygromiidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helix pantanellii" order="Stylommatophora" pageId="0" pageNumber="17" phylum="Mollusca" rank="species" species="pantanellii">Helix pantanellii</taxonomicName>
