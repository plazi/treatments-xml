<taxonomicName id="4C734D5A0119DA79FB02FD1D3C812899" authority="Kellogg" authorityName="Kellogg" authorityYear="1896" class="Insecta" family="Ricinidae" genus="Physostomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="diffusum">
