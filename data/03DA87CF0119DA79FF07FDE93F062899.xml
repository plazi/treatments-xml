<taxonomicName id="4C734D5A0119DA79FDD3FD793F2A289A" box="[579,860,725,750]" class="Aves" family="Emberizidae" genus="Ammodramus" kingdom="Animalia" order="Passeriformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="sanctorum">
