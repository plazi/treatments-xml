<taxonomicName id="4C734D5A0119DA79FE59FD1E3E3328BD" baseAuthorityName="Kellogg" baseAuthorityYear="1896" box="[457,581,689,714]" class="Magnoliopsida" family="Euphorbiaceae" genus="Ricinus" kingdom="Animalia" order="Malpighiales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="diffusus">
