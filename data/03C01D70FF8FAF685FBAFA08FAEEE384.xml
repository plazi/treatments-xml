<quantity id="4C910183FF8FAF6A5A09F99CFA1EE05E" box="[1316,1431,1538,1563]" metricMagnitude="2" metricUnit="m" metricValue="1.0" metricValueMax="1.5" metricValueMin="0.5" pageId="4" pageNumber="329" unit="m" value="100.0" valueMax="150.0" valueMin="50.0">50–150 m</quantity>
, 
<date id="FFD78AA6FF8FAF6A5FBAF9B8FE96E07B" box="[151,287,1574,1599]" pageId="4" pageNumber="329" value="2013-07-06">
<number id="3DA1BE41FF8FAF6A5FBAF9B8FF2CE07B" box="[151,165,1574,1599]" pageId="4" pageNumber="329" value="6.0">6</number>
July 
<number id="3DA1BE41FF8FAF6A5FC9F9B9FE96E07B" box="[228,287,1575,1599]" pageId="4" pageNumber="329" value="2013.0">2013</number>
