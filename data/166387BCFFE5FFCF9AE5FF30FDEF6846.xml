<taxonomicName id="59CA4D29FFE1FFCF9835FED6FC6F6B41" authority="Bezděk, 2013" authorityName="Bezděk" authorityYear="2013" box="[867,992,317,340]" class="Insecta" family="Chrysomelidae" genus="Hesperopenna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="738" phylum="Arthropoda" rank="species" species="bonifaci" status="sp. nov.">
