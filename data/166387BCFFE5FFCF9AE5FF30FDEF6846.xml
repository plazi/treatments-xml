<collectingCountry id="E6DD763AFFE1FFCF9A5CFDD7FEFD6841" box="[266,370,572,596]" name="Myanmar" pageId="23" pageNumber="738">Myanmar</collectingCountry>
(
<collectingRegion id="5C0EF848FFE1FFCF9A29FDD0FDDB6846" box="[383,596,571,595]" country="Myanmar" name="Taninthayi" pageId="23" pageNumber="738">Tanintharyi Region</collectingRegion>
).
</paragraph>
