<taxonomicName id="4C7A98F2FF998B63DA39750CADF6C636" authority="Kaur &amp; Jonathan" authorityName="Kaur &amp; Jonathan" authorityYear="1976" box="[605,979,1807,1833]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Longiterebates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="531" phylum="Arthropoda" rank="genus">
