<collectingCountry id="F317766CFFF2FF83FE17FEDAFDD73E4A" box="[435,545,290,314]" name="France" pageId="10" pageNumber="259">Frankreich</collectingCountry>
und 
<collectingCountry id="F317766CFFF2FF83FDC7FEDAFD1C3E4A" box="[611,746,290,314]" name="Germany" pageId="10" pageNumber="259">Deutschland</collectingCountry>
