<taxonomicName id="4C59959E4B04FF865EB00BE3AA3EF90A" box="[738,854,1690,1716]" class="Insecta" family="Muscidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="rustica">
