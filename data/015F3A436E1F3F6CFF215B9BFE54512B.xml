<taxonomicName id="4EF6F0D66E1F3F6CFE1A5A65FC4052D4" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[428,995,361,385]" class="Insecta" family="Tenthredinidae" genus="Tenthredo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="218" phylum="Arthropoda" rank="species" species="mesomela" subGenus="Eurogaster">
