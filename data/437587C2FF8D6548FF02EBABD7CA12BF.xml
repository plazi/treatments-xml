<taxonomicName id="0CDC4D57FF8D6548FE13EC98D6691087" authority="Pfr." authorityName="Brook &amp; Ablett" authorityYear="2019" box="[390,579,1223,1249]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="poecilosticta" status="var N.">poecilosticta Pfr.</taxonomicName>
