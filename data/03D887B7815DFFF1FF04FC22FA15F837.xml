<paragraph id="8BCE36A1815DFFF1FF31FA62FD5CFA3E" blockId="114.[136,1452,1454,1696]" box="[189,751,1490,1517]" pageId="114" pageNumber="115">
<emphasis id="B905EAB3815DFFF1FF31FA62FEC6FA3E" bold="true" box="[189,373,1490,1516]" pageId="114" pageNumber="115">Distribution:—</emphasis>
Southern 
<collectingCountry id="F3667631815DFFF1FE6DFA63FDFDFA3F" box="[481,590,1491,1517]" name="Myanmar" pageId="114" pageNumber="115">Myanmar</collectingCountry>
