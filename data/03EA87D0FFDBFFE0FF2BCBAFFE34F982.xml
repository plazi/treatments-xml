<number id="3D8B24E1FFDFFFE0FC04CA98FBF1FC67" box="[952,1035,944,968]" pageId="5" pageNumber="220" value="1.75" valueMax="1.8" valueMin="1.7">1.7–1.8</number>
× on P 
<number id="3D8B24E1FFDFFFE0FBE1CA98FB90FC67" box="[1117,1130,944,968]" pageId="5" pageNumber="220" value="3.0">3</number>
, 
<number id="3D8B24E1FFDFFFE0FBC9CA87FB62FC67" box="[1141,1176,943,968]" pageId="5" pageNumber="220" value="1.6">1.6</number>
