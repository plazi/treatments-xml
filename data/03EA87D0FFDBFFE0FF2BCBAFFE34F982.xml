<number id="3D8B24E1FFDFFFE0FA35CAA4FA62FC0B" box="[1417,1432,908,932]" pageId="5" pageNumber="220" value="2.0">2</number>
, 
<number id="3D8B24E1FFDFFFE0FF2BCA98FF11FC67" box="[151,235,944,968]" pageId="5" pageNumber="220" value="0.75" valueMax="0.8" valueMin="0.7">0.7–0.8</number>
and P 
<number id="3D8B24E1FFDFFFE0FE8FCA98FEBAFC67" box="[307,320,944,968]" pageId="5" pageNumber="220" value="3.0">3</number>
, 
<number id="3D8B24E1FFDFFFE0FEF7CA98FE94FC67" box="[331,366,944,968]" pageId="5" pageNumber="220" value="0.7">0.7</number>
on P 
<number id="3D8B24E1FFDFFFE0FE15CA98FE42FC67" box="[425,440,944,968]" pageId="5" pageNumber="220" value="4.0">4</number>
. Propodi 
<number id="3D8B24E1FFDFFFE0FD9ECA87FD8CFC67" box="[546,630,943,968]" pageId="5" pageNumber="220" value="1.8" valueMax="2.1" valueMin="1.5">1.5–2.1</number>
