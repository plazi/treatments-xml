<taxonomicName id="2CEFF26AF9C647F8ED822110F5C71F83" class="Insecta" family="Dytiscidae" genus="Hydroporus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydroporus fairmairei" order="Coleoptera" pageId="7" pageNumber="144" phylum="Arthropoda" rank="species" species="fairmairei">Hydroporus fairmairei</taxonomicName>
