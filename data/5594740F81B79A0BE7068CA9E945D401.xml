<taxonomicName id="E88D5141C7C0E204B05FB96B6E12FF90" class="Insecta" family="Dytiscidae" genus="Hydroporus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydroporus dorsoplagiatus" order="Coleoptera" pageId="7" pageNumber="144" phylum="Arthropoda" rank="species" species="dorsoplagiatus">Hydroporus dorsoplagiatus</taxonomicName>
