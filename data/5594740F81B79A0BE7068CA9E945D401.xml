<taxonomicName id="BC33C0363B18F1FF1439C67D0FAEA9DC" class="Insecta" family="Dytiscidae" genus="Deronectes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Deronectes" order="Coleoptera" pageId="7" pageNumber="144" phylum="Arthropoda" rank="genus">Deronectes</taxonomicName>
