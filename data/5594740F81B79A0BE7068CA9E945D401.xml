<taxonomicName id="08E985BAC6D3F608D2879ACC7CE3332F" class="Insecta" family="Dytiscidae" genus="Limbodessus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Limbodessus curviplicatus" order="Coleoptera" pageId="7" pageNumber="144" phylum="Arthropoda" rank="species" species="curviplicatus">Limbodessus curviplicatus</taxonomicName>
