<paragraph id="D2D2B190280F090F0EC51FA6B2837004" pageId="11" pageNumber="150">
<taxonomicName id="ADA0CCA496D59967DEE2AC44A289C23D" LSID="http://zoobank.org/C80FBEEF-2BA2-4C96-966C-7517C3CD38EA" authority="Lafontaine" class="Insecta" family="Noctuidae" genus="Protorthodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Protorthodes mexicana" order="Lepidoptera" pageId="11" pageNumber="150" phylum="Arthropoda" rank="species" species="mexicana">Protorthodes mexicana Lafontaine</taxonomicName>
<taxonomicNameLabel id="FD5FE9E4B72ECFA40A711A9B3DB4B59F" pageId="11" pageNumber="150">sp. n.</taxonomicNameLabel>
