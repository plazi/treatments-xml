<taxonomicName id="0AE3F842051CFBDDC1BE91297D2D08C0" LSID="2EDDE782-C26F-58F0-8152-6068207DC94E" authority="sp." authorityName="sp." class="Actinopterygii" family="Loricariidae" genus="Pareiorhaphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pareiorhaphis" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="genus">Pareiorhaphis sp.</taxonomicName>
