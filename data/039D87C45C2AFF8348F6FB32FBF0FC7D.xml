<taxonomicName id="4C344D515C28FF8C4B88F848FAAEF851" authority="Santos &amp; Azevedo, 2000" authorityName="Santos &amp; Azevedo" authorityYear="2000" box="[1001,1376,1942,1964]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="jandirae">
