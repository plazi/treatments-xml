: 
<taxonomicName id="4C344D515C28FF8C49EEF847FDC8F850" baseAuthorityName="Westwood" baseAuthorityYear="1874" box="[399,518,1944,1965]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="amazonicus">
<emphasis id="B940EAC05C28FF8C49EEF847FDC8F850" box="[399,518,1944,1965]" italics="true" pageId="55" pageNumber="56">amazonicus</emphasis>
</taxonomicName>
