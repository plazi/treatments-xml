<number id="3D9024945347376CFF22FEDBFF74FE83" box="[151,193,256,279]" pageId="1" pageNumber="247" value="734.0">734</number>
(
<number id="3D9024945347376CFF7DFEDBFF47FE83" box="[200,242,256,279]" pageId="1" pageNumber="247" value="730.0">730</number>
