<typeStatus id="54E388115347376CFC39FF06FC47FF60" box="[908,1010,221,244]" pageId="1" pageNumber="247">paratypes</typeStatus>
given as mean, n = 
<number id="3D9024945347376CFB62FF06FB52FF60" box="[1239,1255,221,244]" pageId="1" pageNumber="247" value="5.0">5</number>
