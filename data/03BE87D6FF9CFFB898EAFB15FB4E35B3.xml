<quantity id="4CEF9B25FF9CFFB89B7EFAA6F918356B" box="[771,857,1364,1390]" metricMagnitude="5" metricUnit="m" metricValue="9.6" pageId="13" pageNumber="14" unit="km" value="960.0">960 km</quantity>
