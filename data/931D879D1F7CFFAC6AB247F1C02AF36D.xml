<taxonomicName id="DCB44D081F7FFFAC695A44B6C1E2F32D" authorityName="Geiser" authorityYear="2017" box="[579,698,508,531]" class="Insecta" family="Cantharidae" genus="Socotrasilis" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="99" phylum="Arthropoda" rank="species" species="enigmatica">
