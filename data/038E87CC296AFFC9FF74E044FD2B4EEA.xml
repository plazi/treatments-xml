<taxonomicName id="4C274D59296BFFC9FE33E43AFDDC4F36" authority="Barrion, Barrion-Dupo &amp; Heong" authorityName="Barrion, Barrion-Dupo &amp; Heong" class="Arachnida" family="Thomisidae" genus="Thomisus" kingdom="Animalia" order="Araneae" pageId="43" pageNumber="42" phylum="Arthropoda" rank="species" species="magaspangus" status="new species">
