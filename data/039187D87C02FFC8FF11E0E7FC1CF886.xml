<quantity id="4CC09B2B7C04FFC8FAE1E149FF5AFAFF" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="13" pageNumber="521" unit="mm" value="0.33">0.33 mm</quantity>
; maximum pronotal width: 0.55¯ 
<quantity id="4CC09B2B7C04FFC8FDB8E164FD36FAFF" box="[574,682,1320,1345]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="13" pageNumber="521" unit="mm" value="0.6">0.60 mm</quantity>
; elytral length: 1.12¯ 
