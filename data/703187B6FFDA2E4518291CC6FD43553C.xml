<emphasis id="CAECEAB2FFDB2E421B321A47FDB65468" bold="true" box="[530,551,818,835]" pageId="11" pageNumber="400">31</emphasis>
); forebody (
<emphasis id="CAECEAB2FFDB2E421BBE1A47FD245468" bold="true" box="[670,693,818,835]" pageId="11" pageNumber="400">32</emphasis>
); aedeagus in lateral view (
<emphasis id="CAECEAB2FFDB2E421A911A47FC565468" bold="true" box="[945,967,818,835]" pageId="11" pageNumber="400">33</emphasis>
