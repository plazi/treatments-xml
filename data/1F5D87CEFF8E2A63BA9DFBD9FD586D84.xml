<taxonomicName id="50F44D5BFF8E2A63B994FBB1FF496D87" baseAuthorityName="FALLEN" baseAuthorityYear="1819" class="Insecta" family="Lauxaniidae" genus="Scathophaga" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="1271" phylum="Arthropoda" rank="species" species="litorea">
