<taxonomicName id="D6FF4D55FF97CA10CC76233DFDEBCFB1" authority="Yu &amp; Li, 2019" authorityName="Yu &amp; Li" authorityYear="2019" box="[297,521,1900,1924]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="225" phylum="Arthropoda" rank="species" species="subasrevida" status="sp. nov.">
<emphasis id="238BEAC4FF97CA10CC76233DFDEBCFB1" box="[297,521,1900,1924]" italics="true" pageId="24" pageNumber="225">Clubiona subasrevida</emphasis>
</taxonomicName>
