<typeStatus id="54A88823FFBFFFA5FF2BF8A3FF6EC50C" box="[151,201,1859,1880]" pageId="12" pageNumber="357">Type</typeStatus>
species: 
<taxonomicName id="4C134D02FFBFFFA5FE9AF8A2FD14C50C" authority="Baird, 1870: 358" authorityName="Baird" authorityPageNumber="358" authorityYear="1870" box="[294,691,1858,1880]" class="Polychaeta" family="Onuphidae" genus="Hyalinoecia" kingdom="Animalia" order="Eunicida" pageId="12" pageNumber="357" phylum="Annelida" rank="species" species="bilineata">
<emphasis id="B967EA93FFBFFFA5FE9AF8A2FDA5C50C" box="[294,514,1858,1880]" italics="true" pageId="12" pageNumber="357">Hyalinoecia bilineata</emphasis>
<bibRefCitation id="EF824B70FFBFFFA5FDB4F8A2FD24C50C" author="Baird" box="[520,643,1858,1880]" pageId="12" pageNumber="357" refString="Baird, W. (1870) Remarks on several genera of annelids belonging to the group Eunicea with a notice of such species as are contained in the collection of the British Museum and a description of some others hitherto undescribed. Journal of the Linnean Society of London, 10, 341 - 361. http: // dx. doi. org / 10.1111 / j. 1096 - 3642.1869. tb 00665. x" type="journal article" year="1870">
Baird, 
<number id="3DDB24A6FFBFFFA5FDF1F8A2FD24C50C" box="[589,643,1858,1880]" pageId="12" pageNumber="357" value="1870.0">1870</number>
</bibRefCitation>
: 
