<taxonomicName id="4C5B4D40FFFB146A011DF9D3CDACFA26" box="[601,727,1468,1488]" class="Insecta" family="Staphylinidae" genus="Oedichirus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="37" phylum="Arthropoda" rank="species" species="terminatus">
<emphasis id="B92FEAD1FFFB146A011DF9D3CDACFA26" box="[601,727,1468,1488]" italics="true" pageId="4" pageNumber="37">O. terminatus</emphasis>
</taxonomicName>
, 
<typeStatus id="54E08861FFFB146A01A0F9D5CC4CFA26" box="[740,823,1466,1488]" pageId="4" pageNumber="37">lectotype</typeStatus>
(
<emphasis id="B92FEAD1FFFB146A0001F9D5CC01FA39" bold="true" box="[837,890,1466,1487]" pageId="4" pageNumber="37">12-16</emphasis>
): (
<emphasis id="B92FEAD1FFFB146A00D2F9D5CCBDFA39" bold="true" box="[918,966,1466,1487]" pageId="4" pageNumber="37">8, 12</emphasis>
