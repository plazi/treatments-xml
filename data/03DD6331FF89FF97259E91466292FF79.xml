<typeStatus id="54CF6C85FF89FF9425CC93E16600F823" box="[197,256,1941,1965]" pageId="1" pageNumber="2">Type</typeStatus>
and only species
</emphasis>
. 
<taxonomicName id="4C74A9A4FF89FF9424DA93E164D9F823" authority="Wells &amp; Rao, 1987" authorityName="Wells &amp; Rao" authorityYear="1987" box="[467,985,1941,1965]" class="Maxillopoda" family="Laophontidae" genus="Fiersiphontina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="sensillata">
