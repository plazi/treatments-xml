<quantity id="D5719B32FFFE95553850FE4C55E0FE58" box="[872,950,405,431]" metricMagnitude="3" metricUnit="m" metricValue="2.121" pageId="42" pageNumber="43" unit="m" value="2121.0">2121m</quantity>
