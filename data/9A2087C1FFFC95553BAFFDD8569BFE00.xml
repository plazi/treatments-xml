<taxonomicName id="D5894D54FFFE95553E0AFE6053CAFE24" authorityName="Karsch" authorityYear="1879" box="[1330,1436,441,467]" class="Arachnida" family="Dipluridae" genus="Linothele" kingdom="Animalia" order="Araneae" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
