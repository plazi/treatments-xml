<taxonomicName id="4C7A4D08FFD8FFB9FCFAA3CF4D37FD1F" authority="Izadizadeh" authorityName="Izadizadeh" box="[864,1105,689,710]" class="Insecta" family="Heloridae" genus="Helorus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="578" phylum="Arthropoda" rank="species" species="alborzicus">
