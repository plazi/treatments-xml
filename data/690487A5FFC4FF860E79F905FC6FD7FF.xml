<taxonomicName id="26AD4D30FFCBFF8609F7FC07FCB5D755" baseAuthorityName="Temminck" baseAuthorityYear="1837" class="Mammalia" family="Pteropodidae" genus="Megaerops" kingdom="Animalia" order="Chiroptera" pageId="8" pageNumber="200" phylum="Chordata" rank="species" species="ecaudatus">
