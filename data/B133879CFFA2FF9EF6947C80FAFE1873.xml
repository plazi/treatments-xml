<taxonomicName id="FE9A4D09FFA2FF9DF20B7F60FAF11B34" authority="Sanborn &amp; Villet, 2020" authorityName="Sanborn &amp; Villet" authorityYear="2020" box="[1032,1404,576,602]" class="Insecta" family="Cicadidae" genus="Ingcainyenzane" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="species" species="nolukhanyoensis" status="n. gen., n. sp.">
