<taxonomicName id="FE9A4D09FFA2FF9DF7207F44FDDD1B10" authority="Sanborn &amp; Villet, 2020" authorityName="Sanborn &amp; Villet" authorityYear="2020" box="[291,592,612,638]" class="Insecta" family="Cicadidae" genus="Ingcainyenzane" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="species" species="irhiniensis" status="n. gen., n. sp.">
