and 
<taxonomicName id="FE9A4D09FFA2FF9DF52A7F44FBE41B10" authority="Sanborn &amp; Villet, 2020" authorityName="Sanborn &amp; Villet" authorityYear="2020" box="[809,1129,613,638]" class="Insecta" family="Cicadidae" genus="Ingcainyenzane" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="species" species="umgeniensis" status="n. gen., n. sp.">
