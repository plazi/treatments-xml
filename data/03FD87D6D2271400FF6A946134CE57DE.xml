<quantity id="4CAC9B25D2241401FB74900934565456" box="[1161,1305,836,860]" metricMagnitude="3" metricUnit="m" metricValue="1.054" metricValueMax="1.6469999999999998" metricValueMin="0.461" pageId="22" unit="m" value="1054.0" valueMax="1647.0" valueMin="461.0">461–1647 m</quantity>
