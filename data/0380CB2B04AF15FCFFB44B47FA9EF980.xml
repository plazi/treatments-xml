<taxonomicName id="4C2901BE04AF15FCFB514873FA86F987" authorityName="Laseron" authorityYear="1958" box="[1138,1389,1625,1651]" class="Gastropoda" family="Triphoridae" genus="Notosinister" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="166" pageNumber="167" phylum="Mollusca" rank="species" species="subulatus">
