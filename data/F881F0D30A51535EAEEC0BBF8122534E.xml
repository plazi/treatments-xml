<taxonomicName id="09EA78F35BC403891DD3188CB991CA35" class="Demospongiae" family="Spongiidae" genus="Spongia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Spongia officinalis" order="Dictyoceratida" pageId="33" pageNumber="34" phylum="Porifera" rank="species" species="officinalis">Spongia officinalis</taxonomicName>
