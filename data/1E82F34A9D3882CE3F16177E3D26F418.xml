<taxonomicName id="6C331E6FFDDF55C30108B4C8D7504A23" LSID-HNS="urn:lsid:biosci.ohio-state.edu:27826" class="Insecta" family="Formicidae" genus="Cataulacus" kingdom="Animalia" lsidName-HNS="Cataulacus pullus Santschi" order="Hymenoptera" pageNumber="252" phylum="Arthropoda" rank="species" species="pullus">C. pullus Sant.</taxonomicName>
