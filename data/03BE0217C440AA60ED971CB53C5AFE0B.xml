<taxonomicName id="4C17C882C440AA61EC2D1D7A3E0CF8AF" authority="Marenzeller, 1885" authorityName="Marenzeller" authorityYear="1885" box="[301,776,1850,1873]" class="Polychaeta" family="Serpulidae" genus="Omphalopoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sabellida" pageId="4" pageNumber="53" phylum="Annelida" rank="species" species="langerhansii">
