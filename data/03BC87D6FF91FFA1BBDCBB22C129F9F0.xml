<number id="3DDD24E7FF97FFA1BA5DB882C52FFB6F" box="[278,329,1201,1226]" pageId="4" pageNumber="263" value="0.52">0.52</number>
/ 
<number id="3DDD24E7FF97FFA1BA19B882C5E2FB6F" box="[338,388,1201,1226]" pageId="4" pageNumber="263" value="0.93">0.93</number>
