<date id="FFAB1000FF97FFA1B871BE17C7C2FD99" box="[826,932,548,572]" pageId="4" pageNumber="263" value="1929-12-18">
<number id="3DDD24E7FF97FFA1B871BE17C73FFD99" box="[826,857,548,572]" pageId="4" pageNumber="263" value="29.0">29</number>
/ 
<number id="3DDD24E7FF97FFA1B829BE17C718FD99" box="[866,894,548,572]" pageId="4" pageNumber="263" value="18.0">18</number>
