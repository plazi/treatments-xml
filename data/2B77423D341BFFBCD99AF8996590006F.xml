<subSubSection id="EBC4A0A0341BFFBCD99AF822652401DD" lastPageId="2" lastPageNumber="51" pageId="1" pageNumber="50" type="reference_group">
<paragraph id="A361F32B341BFFBFD99AF82265E50493" blockId="1.[151,913,1973,2026]" box="[151,913,2003,2026]" pageId="1" pageNumber="50">
<treatmentCitationGroup id="83CED405341BFFBFD99AF82265E50493" box="[151,913,2003,2026]" pageId="1" pageNumber="50">
<taxonomicName id="64DE88A8341BFFBFD99AF82264E20493" authority="Borchsenius 1948: 955" authorityName="Borchsenius" authorityPageNumber="955" authorityYear="1948" box="[151,662,2003,2026]" class="Insecta" family="Pseudococcidae" genus="Heterococcopsis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="50" phylum="Arthropoda" rank="species" species="lonicerae">
<emphasis id="91AA2F39341BFFBFD99AF82267D10490" box="[151,421,2004,2026]" italics="true" pageId="1" pageNumber="50">Heterococcopsis lonicerae</emphasis>
<treatmentCitation id="227FD53A341BFFBFD8A6F82464E20493" author="Borchsenius" box="[427,662,2003,2025]" page="955" pageId="1" pageNumber="50" year="1948">
<bibRefCitation id="C74F8EDA341BFFBFD8A6F82464100493" author="Borchsenius" box="[427,612,2003,2025]" pageId="1" pageNumber="50" refString="Borchsenius, N. S. (1948) New mealybugs of the family Pseudococcidae from Central Asia. (In Russian). Doklady Akademii Nauk SSSR. Moscow (n. s.), 63, 581 - 584." type="journal article" year="1948">
Borchsenius 
<number id="1516E10C341BFFBFDB23F82364100493" box="[558,612,2004,2025]" pageId="1" pageNumber="50" value="1948.0">1948</number>
</bibRefCitation>
: 
<number id="1516E10C341BFFBFDB63F82464E20493" box="[622,662,2003,2025]" pageId="1" pageNumber="50" value="955.0">955</number>
</treatmentCitation>
</taxonomicName>
