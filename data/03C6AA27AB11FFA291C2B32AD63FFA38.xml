<collectionCode id="ED7E83F4AB11FFA29218B3E5D4E6FB44" box="[845,926,1192,1216]" httpUri="http://grbio.org/cool/p469-157j" name="Zoological Collection of the Biology Department, University of the Azores" pageId="13" pageNumber="14">DBUA</collectionCode>
