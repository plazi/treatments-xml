<taxonomicName id="4C364D1DFFC2FFEAAD77F85F1429479D" box="[1123,1308,1963,1989]" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="sylvestris">
