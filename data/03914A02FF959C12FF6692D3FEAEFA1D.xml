<taxonomicName id="4C388097FF959C12FD9294C6FCEFFA74" authorityName="Hoyle" authorityYear="1885" box="[611,831,1420,1445]" class="Cephalopoda" family="Octopodidae" genus="Benthoctopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Octopoda" pageId="7" pageNumber="449" phylum="Mollusca" rank="species" species="levis">
