<taxonomicName id="4C388097FF959C12FEE694F9FEBDFA1D" authorityName="Hoyle" authorityYear="1885" box="[279,365,1459,1484]" class="Cephalopoda" family="Octopodidae" genus="Benthoctopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Octopoda" pageId="7" pageNumber="449" phylum="Mollusca" rank="species" species="levis">
<emphasis id="B94C2706FF959C12FEE694F9FEBDFA1D" box="[279,365,1459,1484]" italics="true" pageId="7" pageNumber="449">B. levis</emphasis>
