<emphasis id="4FE9EAE2FFC2D81FFEA3F9E7FE745F76" bold="true" box="[330,393,1584,1609]" pageId="1" pageNumber="492">sp. n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="BA9D4D73FFC2D81FFE7EF9E6FDE05F77" box="[407,541,1584,1608]" class="Diplopoda" family="Paradoxosomatidae" genus="Enghoffosoma" kingdom="Animalia" order="Polydesmida" pageId="1" pageNumber="492" phylum="Arthropoda" rank="species" species="bispinum" status="sp. n.">
<emphasis id="4FE9EAE2FFC2D81FFE7EF9E6FDE05F77" box="[407,541,1584,1608]" italics="true" pageId="1" pageNumber="492">E. bispinum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="54DA5799FFC2D81FFDCDF9E7FD9E5F76" box="[548,611,1584,1609]" pageId="1" pageNumber="492" rank="species">
<emphasis id="4FE9EAE2FFC2D81FFDCDF9E7FD9E5F76" bold="true" box="[548,611,1584,1609]" pageId="1" pageNumber="492">sp. n.</emphasis>
