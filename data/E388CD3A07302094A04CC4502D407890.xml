<taxonomicName id="1A4B7232D9D34C6AF5C696E5BF713A72" class="Insecta" family="Miridae" genus="Cyrtopeltis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyrtopeltis kahakai" order="Hemiptera" pageId="5" pageNumber="102" phylum="Arthropoda" rank="species" species="kahakai">Cyrtopeltis kahakai</taxonomicName>
