<subSubSection id="7088650A6921FFF2FF71F92459E0F88E" pageId="102" pageNumber="103" type="etymology">
<paragraph id="382D36816921FFF2FF71F92459E0F88E" blockId="102.[151,1436,1458,2004]" pageId="102" pageNumber="103">
<emphasis id="0AE6EA936921FFF2FF71F9245A92F8B6" bold="true" box="[199,437,1778,1804]" pageId="102" pageNumber="103">Derivatio nominis:</emphasis>
<emphasis id="0AE6EA936921FFF2FE77F924590CF8B1" box="[449,555,1778,1803]" italics="true" pageId="102" pageNumber="103">Palustris</emphasis>
