<taxonomicName id="67FE4D3CFFF66069F94DFA09625795E5" authority="Ehrend. &amp; Schonb." authorityName="Ehrend. &amp; Schonb." box="[605,1113,1423,1450]" class="Magnoliopsida" family="Rubiaceae" genus="Asperula" kingdom="Plantae" order="Gentianales" pageId="106" pageNumber="107" phylum="Tracheophyta" rank="species" species="malevonensis">
