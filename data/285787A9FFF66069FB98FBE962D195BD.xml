<taxonomicName id="67FE4D3CFFF66069F9A1FA35629D9581" authority="Halacsy" authorityName="Halacsy" box="[689,1171,1459,1486]" class="Magnoliopsida" family="Rubiaceae" genus="Asperula" kingdom="Plantae" order="Gentianales" pageId="106" pageNumber="107" phylum="Tracheophyta" rank="variety" species="boissieri" variety="transiens">
