<taxonomicName id="4C364D51FB3FFEF2CF7BBF6AFB58F92D" box="[1091,1200,1649,1671]" class="Gastropoda" family="Drilliidae" genus="Syntomodrillia" kingdom="Animalia" order="Neogastropoda" pageId="346" pageNumber="352" phylum="Mollusca" rank="species" species="triangulos" status="new species">Triángulos</taxonomicName>
