<taxonomicName id="76C70A3229B3EF3C08D1196B298F754E" authorityName="Chiu" authorityYear="1954" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus flatus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="flatus">
