<collectionCode id="ED7F6FA0386BFF94FF7FFE312AE8FE5E" box="[159,222,478,502]" country="Russia" httpUri="http://grbio.org/cool/vmfx-g997" name="Zoological Institute, Russian Academy of Sciences" pageId="21" pageNumber="518">ZISP</collectionCode>
). (
<collectorName id="269B92B3386BFF94FF18FE322B44FE5E" box="[248,370,477,502]" pageId="21" pageNumber="518">Altogether</collectorName>
