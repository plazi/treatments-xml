<taxonomicName id="DBC662A1FFF7EA1F9075FC66FE6548B7" box="[264,404,982,1008]" class="Insecta" family="Philopteridae" genus="Docophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="cancellosus">
<emphasis id="2EB2C530FFF7EA1F9075FC66FE6548B7" bold="true" box="[264,404,982,1008]" italics="true" pageId="2" pageNumber="3">cancellosus</emphasis>
