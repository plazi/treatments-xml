(
<taxonomicName id="E362AED52FA034172C119358D70EFFA8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136968" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica platygnatha Roger" lsidName-HNS="Formica platygnatha Roger" order="Hymenoptera" pageNumber="323" phylum="Arthropoda" rank="species" species="platygnatha">Formica platygnatha Hog.</taxonomicName>
