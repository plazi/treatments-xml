1. 
<normalizedToken id="A8E8C8C649CCCEEE29312410B81B051C" originalValue="Blütenstiele" pageId="null" pageNumber="700">Bluetenstiele</normalizedToken>
<normalizedToken id="B71C6754B021664A7FB830DD519996D3" originalValue="kürzer" pageId="null" pageNumber="700">kuerzer</normalizedToken>
