<taxonomicName id="4C714D3FB205FF843C57F9F6FDE0AC0F" box="[286,522,1583,1609]" class="Insecta" family="Miridae" genus="Pachymeroceroides" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="37" phylum="Arthropoda" rank="genus">
