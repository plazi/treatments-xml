<taxonomicName id="4C1A4D37FFE6F861FF0DD5B8EB54D848" authority="Mann, 1922: 43" authorityName="Mann" authorityPageNumber="43" authorityYear="1922" box="[199,599,1519,1541]" class="Insecta" family="Formicidae" genus="Rhopalothrix" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="321" phylum="Arthropoda" rank="species" species="wheeleri">
