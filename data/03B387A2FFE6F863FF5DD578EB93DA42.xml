<date id="FFA41074FFE6F861FA49D5C3E833DB8A" pageId="20" pageNumber="321" value="1933-07-24">
<number id="3DD22493FFE6F861FA49D5C3EC9FDBE4" box="[1411,1436,1428,1449]" pageId="20" pageNumber="321" value="24.0">24</number>
