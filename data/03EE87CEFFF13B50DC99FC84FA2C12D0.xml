<taxonomicName id="4C474D5BFFF13B52DCEFF83DFE1B1B5E" authority="Lin &amp; Chang &amp; Li, 2020" authorityName="Lin &amp; Chang &amp; Li" authorityYear="2020" box="[254,432,2028,2049]" class="Arachnida" family="Dysderidae" genus="Dysdera" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="98" phylum="Arthropoda" rank="species" species="dushengi" status="sp. nov.">
