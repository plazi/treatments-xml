<caption id="DF386650FFF13B52DC8AF83DFC8C1B7E" ID-DOI="http://doi.org/10.5281/zenodo.4617222" ID-Zenodo-Dep="4617222" httpUri="https://zenodo.org/record/4617222/files/figure.png" pageId="1" pageNumber="98" startId="1.[155,218,2028,2050]" targetBox="[189,1399,1019,1979]" targetPageId="1">
<paragraph id="8BF836D8FFF13B52DC8AF83DFC8C1B7E" blockId="1.[155,1433,2028,2082]" pageId="1" pageNumber="98">
