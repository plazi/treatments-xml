[
<taxonomicName id="4C319EBF195EFFB26AE491B11D57FE50" authority="Mathews" authorityName="Mathews" box="[152,630,447,470]" class="Aves" family="Paradisaeidae" genus="Craspedophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="97" pageNumber="97" phylum="Chordata" rank="subSpecies" species="magnifica" subSpecies="yorki">
