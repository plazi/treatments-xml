<number id="3D967D58B048FFE9FDFBFEC7FD8A81F3" box="[517,597,364,390]" pageId="3" pageNumber="38" value="106.34">106.34</number>
/ 
<number id="3D967D58B048FFE9FDA1FEC7FD6D81F3" box="[607,690,364,390]" pageId="3" pageNumber="38" value="464.77">464.77</number>
, 0 
<number id="3D967D58B048FFE9FD31FEC7FCDA81F3" box="[719,773,364,390]" pageId="3" pageNumber="38" value="3.05">3.05</number>
. 
<number id="3D967D58B048FFE9FCF7FEC7FC9A81F3" box="[777,837,364,390]" pageId="3" pageNumber="38" value="1983.0">1983</number>
coll. H. Smit; female, 
<collectingCountry id="F3492FEFB048FFE9FB9EFEC7FAF781F3" box="[1120,1320,364,390]" name="Netherlands" pageId="3" pageNumber="38">The Netherlands</collectingCountry>
, Zuid­Holland, polder Nieuwkoop, 
<number id="3D967D58B048FFE9FD5EFE3FFD3081DB" box="[672,751,404,430]" pageId="3" pageNumber="38" value="114.27">114.27</number>
/ 
<number id="3D967D58B048FFE9FD07FE3FFC9381DB" box="[761,844,404,430]" pageId="3" pageNumber="38" value="463.56">463.56</number>
, 
<date id="FFE049BFB048FFE9FCA6FE3FFC0481DB" box="[856,987,404,430]" pageId="3" pageNumber="38" value="1983-04-26">
<number id="3D967D58B048FFE9FCA6FE3FFC4181DB" box="[856,926,404,430]" pageId="3" pageNumber="38" value="26.04">26.04</number>
. 
<number id="3D967D58B048FFE9FC5FFE3FFC0481DB" box="[929,987,404,430]" pageId="3" pageNumber="38" value="1983.0">1983</number>
