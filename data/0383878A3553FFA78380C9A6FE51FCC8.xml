<date id="FF94105C3553FFA087EECE87FAA2FA81" box="[1198,1295,1274,1298]" pageId="2" pageNumber="1" value="2007-01-09">9.i.2007</date>
<taxonomicName id="4C2A4D1F3553FFA08654CE87FA0DFA82" box="[1300,1440,1274,1297]" class="Magnoliopsida" family="Aextoxicaceae" genus="Aextoxicum" kingdom="Plantae" order="Berberidopsidales" pageId="2" pageNumber="1" phylum="Tracheophyta" rank="genus">
