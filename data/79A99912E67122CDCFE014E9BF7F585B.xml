<taxonomicName id="5DAD1692DE8882220D336DFA79C41CDE" authorityName="Motschoulsky" authorityYear="1850" class="Insecta" family="Carabidae" genus="Deltomerus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="462" pageNumber="463" phylum="Arthropoda" rank="genus">
