<taxonomicName id="4C6A4D67FF81FFB4FF034A3B57FBFA83" authorityName="Anderson &amp; Leviton" authorityYear="1966" box="[207,387,1304,1328]" class="Reptilia" family="Eublepharidae" genus="Eublepharis" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="angramainyu">
<emphasis id="B91EEAF6FF81FFB4FF034A3B57FBFA83" box="[207,387,1304,1328]" italics="true" pageId="13" pageNumber="14">E. angramainyu</emphasis>
</taxonomicName>
