<specimenCount id="008F3ED2FF821228FD25FB95C5B4F81C" box="[676,705,1074,1095]" pageId="1" pageNumber="462" type="male">1♂</specimenCount>
, 
<collectingRegion id="D44D3BB9FF821228FD4AFB95C473F813" box="[715,774,1074,1096]" country="Iran" name="Alborz" pageId="1" pageNumber="462">Elburs</collectingRegion>
, Damavend-Gebiet, Polur, 
<quantity id="D17158BEFF821228FC77FB95C7A2F804" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="462" unit="m" value="2000.0">2000 m</quantity>
, 
<date id="6237D39BFF821228FF60FBEDC633F83B" box="[225,326,1098,1120]" pageId="1" pageNumber="462" value="1977-07-22">22.vii.1977</date>
, leg. J. Gusenleitner / 
<taxonomicName id="D1898ED8FF821228FD8AFBEDC5C5F804" authority="AB." authorityName="AB." box="[523,688,1098,1119]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="462" phylum="Arthropoda" rank="genus">
<emphasis id="24FD2949FF821228FD8AFBEDC5FDF804" box="[523,648,1098,1119]" italics="true" pageId="1" pageNumber="462">Hedychridium</emphasis>
AB.
</taxonomicName>
<emphasis id="24FD2949FF821228FD37FBEDC470F804" box="[694,773,1098,1119]" italics="true" pageId="1" pageNumber="462">aheneum</emphasis>
DHLB. 
<specimenCount id="008F3ED2FF821228FCCAFBEDC429F804" box="[843,860,1098,1119]" pageId="1" pageNumber="462" type="male">♂</specimenCount>
