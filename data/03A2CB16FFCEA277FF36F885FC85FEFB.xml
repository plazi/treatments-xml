Ross Sea, 
<collectingCountry id="F31C3A90FFC8A277FE7AFE99FDD6FE84" box="[475,595,295,320]" name="Antarctica" pageId="61" pageNumber="62">Antarctica</collectingCountry>
, 
<quantity id="4CF3D7E5FFC8A277FDFFFE99FD79FEFB" box="[606,764,295,320]" metricMagnitude="3" metricUnit="m" metricValue="1.6195" metricValueMax="1.674" metricValueMin="1.565" pageId="61" pageNumber="62" unit="m" value="1619.5" valueMax="1674.0" valueMin="1565.0">1565–1674 m</quantity>
