<quantity id="4CB8D2BBFFBCFF86FCF5F93C7CE501A7" box="[787,913,1684,1708]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" metricValueMax="1.8" metricValueMin="1.6" pageId="2" pageNumber="67" unit="mm" value="17.0" valueMax="18.0" valueMin="16.0">16–18 mm</quantity>
