<taxonomicName id="07FE4D2AFFB5C32AFF0BFB61FCCAFB6D" authority="Deraniyagala, 1953" authorityName="Deraniyagala" authorityYear="1953" box="[151,845,1194,1220]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="45" phylum="Chordata" rank="subSpecies" species="brooki" subSpecies="parvimaculatus">
