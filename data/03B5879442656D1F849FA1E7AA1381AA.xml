<subSubSection id="C306650942676D1F84CFA2E1AAE8818E" pageId="51" pageNumber="252" type="reference_group">
<paragraph id="8BA3368242676D1F84CFA2E1AAE8818E" blockId="51.[151,1437,151,501]" pageId="51" pageNumber="252">
<taxonomicName id="4C1C4D0142676D1F84CFA2E1AFDE80EE" box="[199,440,152,176]" class="Insecta" family="Formicidae" genus="Paratrechina" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="252" phylum="Arthropoda" rank="species" species="umbra">
