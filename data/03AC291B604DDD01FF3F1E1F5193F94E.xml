<quantity id="4CFD35E8604DDD02FCAD1AE95136F950" box="[773,814,1666,1696]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="1" pageNumber="35" unit="in" value="2.0">2 in</quantity>
