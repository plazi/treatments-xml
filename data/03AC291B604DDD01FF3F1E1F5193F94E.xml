<quantity id="4CFD35E8604DDD02FC751AF95620F950" box="[989,1080,1666,1692]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="1" pageNumber="35" unit="mm" value="1.5">1.5 mm</quantity>
