<number id="102024CA5A308F00FCFDF9FEFCE5B7CE" box="[794,808,1652,1676]" pageId="18" pageNumber="19" value="1.0">1</number>
and 
<number id="102024CA5A308F00FC84F9FEFC4BB7CE" box="[867,902,1652,1676]" pageId="18" pageNumber="19" value="1.8">1.8</number>
: 
<number id="102024CA5A308F00FC47F9FEFC63B7CE" box="[928,942,1652,1676]" pageId="18" pageNumber="19" value="1.0">1</number>
