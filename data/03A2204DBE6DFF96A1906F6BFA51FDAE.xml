, 
<taxonomicName id="4C0BEAD8BE6CFF96A5BC6806F831FECE" authority="(Schaeffer, 1908)" authorityName="Schaeffer" authorityYear="1908" baseAuthorityName="Schaeffer" baseAuthorityYear="1908" class="Insecta" family="Cerambycidae" genus="Megachoriolaus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="ignitus">
