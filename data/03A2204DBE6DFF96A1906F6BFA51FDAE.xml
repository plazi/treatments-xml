<taxonomicName id="4C0BEAD8BE6CFF96A25068E6FDA5FE46" authority="(Breme, 1844)" authorityName="Monné &amp; Monné" authorityYear="2008" baseAuthorityName="Breme" baseAuthorityYear="1844" box="[855,1181,472,498]" class="Insecta" family="Cerambycidae" genus="Megachoriolaus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="venustus" status="new combination">
