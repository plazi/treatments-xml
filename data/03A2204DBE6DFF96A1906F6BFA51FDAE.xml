<taxonomicName id="4C0BEAD8BE6CFF96A1F768E6FB7AFE46" authority="(Gounelle, 1911)" authorityName="Monné &amp; Monné" authorityYear="2008" baseAuthorityName="Gounelle" baseAuthorityYear="1911" box="[240,578,472,498]" class="Insecta" family="Cerambycidae" genus="Megachoriolaus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="bicolor" status="new combination">
