<taxonomicName id="4C0BEAD8BE6CFF96A26A6806FD88FEE6" authority="(Martin, 1930)" authorityName="Martin" authorityYear="1930" baseAuthorityName="Martin" baseAuthorityYear="1930" box="[877,1200,312,338]" class="Insecta" family="Cerambycidae" genus="Megachoriolaus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="cruentus">
