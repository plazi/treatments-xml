<taxonomicName id="1C01DD55B01751699E3ED2C8BC81D391" authority="Schulz, 1906" authorityName="Schulz" authorityYear="1906" class="Insecta" family="Trigonalyidae" genus="Taeniogonalos" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Taeniogonalos similis" order="Hymenoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="similis">Taeniogonalos similis</taxonomicName>
