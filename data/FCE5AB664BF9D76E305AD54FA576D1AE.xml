<taxonomicName id="F379894D04A018529A89BB6DF391C61A" authority="Tsuneki, 1991" authorityName="Tsuneki" authorityYear="1991" class="Insecta" family="Trigonalyidae" genus="Taiwanogonalos" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Taiwanogonalos minima" order="Hymenoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="minima">Taiwanogonalos minima</taxonomicName>
