, as 
<taxonomicName id="8D974D11FFE7FF975B3D9A032AA04A18" authority="Crawford, 1911" authorityName="Crawford" authorityYear="1911" class="Insecta" family="Psyllidae" genus="Psylla" kingdom="Animalia" order="Hemiptera" pageId="54" pageNumber="53" phylum="Arthropoda" rank="species" species="trimaculata">
