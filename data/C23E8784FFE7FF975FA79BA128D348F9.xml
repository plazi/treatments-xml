<taxonomicName id="8D974D11FFE7FF975DD09BF72E674B85" authority="Crawford, 1911" authorityName="Crawford" authorityYear="1911" box="[695,1085,1247,1274]" class="Insecta" family="Psyllidae" genus="Psylla" kingdom="Animalia" order="Hemiptera" pageId="54" pageNumber="53" phylum="Arthropoda" rank="species" species="trimaculata">
