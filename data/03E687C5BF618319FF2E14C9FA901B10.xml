<quantity id="4CB79B36BF618318FCA0102EFC241E03" box="[793,896,1844,1868]" metricMagnitude="-3" metricUnit="m" metricValue="1.28" pageId="22" pageNumber="23" unit="mm" value="1.28">1.28 mm</quantity>
, body whorl height 
<quantity id="4CB79B36BF618318FBD8102EFB6D1E03" box="[1121,1225,1844,1868]" metricMagnitude="-3" metricUnit="m" metricValue="1.62" pageId="22" pageNumber="23" unit="mm" value="1.62">1.62 mm</quantity>
