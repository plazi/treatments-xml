<quantity id="4CCA9B708754FFFEFC5EF34A1709FC19" box="[983,1063,880,907]" metricMagnitude="-2" metricUnit="m" metricValue="3.2" pageId="14" pageNumber="415" unit="mm" value="32.0">32 mm</quantity>
