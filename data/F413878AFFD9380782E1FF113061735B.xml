<quantity id="BB429B79FFD938078094FC18310774C9" box="[738,826,912,939]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="21" pageNumber="22" unit="m" value="2300.0">2300 m</quantity>
