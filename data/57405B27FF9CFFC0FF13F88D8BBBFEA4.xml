<collectingCountry id="A7FEAAA1FF99FFC0FD3FFEDE8854FEDC" box="[699,827,277,303]" name="Zimbabwe" pageId="44" pageNumber="45">Zimbabwe</collectingCountry>
, 
<collectingCountry id="A7FEAAA1FF99FFC0FCCDFEDE88DAFEDC" box="[841,949,277,303]" name="Tanzania" pageId="44" pageNumber="45">Tanzania</collectingCountry>
