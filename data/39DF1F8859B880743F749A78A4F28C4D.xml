<taxonomicName id="91713C9A1E0C65096A3B844740AB3DA0" LSID-ZBK="urn:lsid:zoobank.org:act:449542DD-FEE6-44E8-AFB4-6DC639C50D32" class="Actinopterygii" family="Pimelodidae" genus="Pimelodus" kingdom="Animalia" lsidName-ZBK="Pimelodus bagarius Hamilton 1822:186, 378" order="Siluriformes" pageNumber="26" phylum="Chordata" rank="species" species="bagarius">Pimelodus bagarius Hamilton, 1822</taxonomicName>
