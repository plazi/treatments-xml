<taxonomicName id="4C6E4D727522527CFDBFF741FCB2FD86" box="[565,768,530,555]" class="Insecta" family="Cydnidae" genus="Amnestus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="25" phylum="Arthropoda" rank="species" species="uhleri">
