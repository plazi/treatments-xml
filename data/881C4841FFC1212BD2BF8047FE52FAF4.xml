<taxonomicName id="C7B582D4FFC1212BD3438624FF24FBC6" authority="Durette-Desset, 1970" authorityName="Durette-Desset" authorityYear="1970" class="Chromadorea" family="Heligmosomidae" genus="Longistriata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="6" pageNumber="485" phylum="Nematoda" rank="species" species="lemniscomysi">
