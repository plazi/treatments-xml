<collectionCode id="ED56AE76FFFD3B7AFF06F85E778A9FF1" box="[197,218,1920,1943]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="29" pageNumber="62">R</collectionCode>
., 
