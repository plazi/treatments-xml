<taxonomicName id="26AD4D30FFEBFFA60C11F82AFE5CD492" authorityName="Furey, Thong, Bates and Csorba" authorityYear="2009" box="[302,409,1991,2017]" class="Mammalia" family="Vespertilionidae" genus="Murina" kingdom="Animalia" order="Chiroptera" pageId="40" pageNumber="232" phylum="Chordata" rank="species" species="eleryi">
<emphasis id="D3D9EAA1FFEBFFA60C11F82AFE5CD492" box="[302,409,1991,2017]" italics="true" pageId="40" pageNumber="232">M. eleryi</emphasis>
