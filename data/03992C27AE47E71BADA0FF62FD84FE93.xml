<taxonomicName id="4C30E6B2AE47E719AF65F83EFEA8F85D" box="[594,719,1995,2020]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="4" pageNumber="53" phylum="Arthropoda" rank="species" species="spectabilis">
