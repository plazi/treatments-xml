, 
<taxonomicName id="4C795DCAFFF858418ADCD4F1BB5EFB89" authority="(Duchassaing &amp; Michelotti)" authorityName="Duchassaing &amp; Michelotti" baseAuthorityName="Duchassaing &amp; Michelotti" baseAuthorityYear="1864" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="10" pageNumber="61" phylum="Porifera" rank="species" species="laxissima" subGenus="Arenochalina">
<emphasis id="B90DFA5BFFF858418ADCD4F1BE28FBA2" box="[954,1036,441,467]" italics="true" pageId="10" pageNumber="61">Mycale</emphasis>
