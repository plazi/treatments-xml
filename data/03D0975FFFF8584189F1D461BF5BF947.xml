<taxonomicName id="4C795DCAFFF858418B7ED4F2B8A4FBA2" baseAuthorityName="Hyatt" baseAuthorityYear="1875" box="[536,640,442,468]" class="Demospongiae" family="Aplysinidae" genus="Aiolochroia" kingdom="Animalia" order="Verongida" pageId="10" pageNumber="61" phylum="Porifera" rank="species" species="crassa">
