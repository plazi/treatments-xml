<quantity id="4C818BACFFF8584188A0D7B5B810F969" box="[454,564,765,792]" metricMagnitude="1" metricUnit="m" metricValue="1.55" metricValueMax="3.0" metricValueMin="0.1" pageId="10" pageNumber="61" unit="m" value="15.5" valueMax="30.0" valueMin="1.0">
