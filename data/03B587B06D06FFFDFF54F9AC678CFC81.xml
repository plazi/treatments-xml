and 
<quantity id="4CE49B436D07FFFAFB0AF8AF6634F8EE" box="[1225,1329,1857,1883]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="8.0" metricValueMin="6.0" pageId="11" pageNumber="12" unit="mm" value="7.0" valueMax="8.0" valueMin="6.0">6–8 mm</quantity>
