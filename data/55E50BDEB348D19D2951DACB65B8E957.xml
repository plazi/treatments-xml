<taxonomicName id="88DF0E6C1A5CB9744006B32EA0173803" authority="Sharp, 1883" authorityName="Sharp" authorityYear="1883" class="Insecta" family="Staphylinidae" genus="Tinotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tinotus caviceps" order="Coleoptera" pageId="5" pageNumber="90" phylum="Arthropoda" rank="species" species="caviceps">Tinotus caviceps</taxonomicName>
and 
<taxonomicName id="4957B208D8ED87975C967269EAB4E718" authority="Sharp, 1883" authorityName="Sharp" authorityYear="1883" class="Insecta" family="Staphylinidae" genus="Tinotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tinotus trisecus" order="Coleoptera" pageId="5" pageNumber="90" phylum="Arthropoda" rank="species" species="trisecus">Tinotus trisecus</taxonomicName>
