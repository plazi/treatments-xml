<taxonomicName id="F08A4D08D109FFE0FBC0A8084C50D69A" authorityName="Gilmore" authorityYear="1943" class="Reptilia" family="Reptilia (awaiting allocation)" genus="Isodontosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Reptilia (awaiting allocation)" pageId="90" pageNumber="91" phylum="Chordata" rank="genus">
