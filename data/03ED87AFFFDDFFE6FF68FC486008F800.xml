<taxonomicName id="4C444D3AFFDDFFE6FF68FBE76612FBDF" authority="Hood, 1931: 153" authorityName="Hood" authorityPageNumber="153" authorityYear="1931" box="[151,540,1117,1139]" class="Insecta" family="Thripidae" genus="Leucothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="407" phylum="Arthropoda" rank="species" species="furcatus">
