<collectingRegion id="4992F8081656481B38E84991FBC495E7" box="[985,1104,1187,1214]" country="United States of America" name="New York" pageId="3" pageNumber="340">New York</collectingRegion>
, south to 
<collectingRegion id="4992F8081656481B3BC249F3FED995BA" box="[243,333,1222,1248]" country="United States of America" name="Georgia" pageId="3" pageNumber="340">Georgia</collectingRegion>
and west to 
<collectingRegion id="4992F8081656481B3AD149F2FDB595BB" box="[480,545,1223,1249]" country="United States of America" name="Texas" pageId="3" pageNumber="340">Texas</collectingRegion>
, 
<collectingRegion id="4992F8081656481B391C49F3FD0095BA" box="[557,660,1222,1248]" country="United States of America" name="Arkansas" pageId="3" pageNumber="340">Arkansas</collectingRegion>
, 
<collectingRegion id="4992F8081656481B399149F3FC9395BA" box="[672,775,1222,1248]" country="United States of America" name="Missouri" pageId="3" pageNumber="340">Missouri</collectingRegion>
, 
<collectingRegion id="4992F8081656481B382549F3FC1595BA" box="[788,897,1222,1248]" country="United States of America" name="Nebraska" pageId="3" pageNumber="340">Nebraska</collectingRegion>
, 
<collectingRegion id="4992F8081656481B38BC49F2FC4A95BB" box="[909,990,1223,1249]" country="United States of America" name="Kansas" pageId="3" pageNumber="340">Kansas</collectingRegion>
, and 
<collectingRegion id="4992F8081656481B3F2C49F2FBC095BB" box="[1053,1108,1223,1249]" country="United States of America" name="Iowa" pageId="3" pageNumber="340">Iowa</collectingRegion>
(
<bibRefCitation id="EFC74B1B1656481B3BA249DFFEA5945E" author="Parsons" box="[147,305,1258,1284]" firstAuthor="Parsons" pageId="3" pageNumber="340" pagination="121 - 278" refId="ref11921" refString="Parsons CT (1943) A revision of Nearctic Nitidulidae (Coleoptera). Bulletin of the Museum of Comparative Zoology 92: 121 - 278." type="journal article" year="1943">Parsons 1943</bibRefCitation>
; 
<bibRefCitation id="EFC74B1B1656481B3A7149DFFDFC9459" author="Downie and Arnett" box="[320,616,1257,1284]" firstAuthor="Downie" pageId="3" pageNumber="340" refId="ref10845" refString="Downie NM, Arnett RH, Jr (1996) The beetles of northeastern North America. Sandhill Crane Press, Gainesville, 1721 pp." type="book" year="1996">Downie and Arnett 1996</bibRefCitation>
; 
<bibRefCitation id="EFC74B1B1656481B394649DFFCBE945E" author="Chandler" box="[631,810,1258,1284]" firstAuthor="Chandler" pageId="3" pageNumber="340" refId="ref10669" refString="Chandler DS (2001) University of New Hampshire Insect and Arachnid Collections. http: // colsa 1. unh. edu: 591 / unhinsects. htm [accessed 18 August 2008]" type="url" year="2001">Chandler 2001</bibRefCitation>
). Adults have been found on the flowers of wild plum (
<taxonomicName id="4C564D691656481B3AE9483BFD6E9472" authority="Marsh." authorityName="Marsh." box="[472,762,1293,1320]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="340" phylum="Tracheophyta" rank="species" species="americana">
