<taxonomicName id="4C564D691656481B3A944805FD199410" authorityName="Linnaeus" authorityYear="1753" box="[421,653,1328,1354]" class="Magnoliopsida" family="Adoxaceae" genus="Sambucus" kingdom="Plantae" order="Dipsacales" pageId="3" pageNumber="340" phylum="Tracheophyta" rank="species" species="canadensis">
