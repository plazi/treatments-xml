<taxonomicName id="4C324D7AD124FFCABC051405FE58F529" authority="(Kohl, 1889)" authorityName="Kohl" authorityYear="1889" baseAuthorityName="Kohl" baseAuthorityYear="1889" class="Insecta" family="Sphecidae" genus="Prionyx" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="subSpecies" species="macula" subSpecies="lugens">
