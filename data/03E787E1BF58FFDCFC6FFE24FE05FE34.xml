<taxonomicName id="4C4E4D74BF5BFFD2FE92FAA1FDF7FAAF" authority="Audureau, 2014" authorityName="Audureau" authorityYear="2014" box="[294,635,1307,1326]" class="Insecta" family="Cerambycidae" genus="Cupecuara" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="646" phylum="Arthropoda" rank="species" species="santossilvai">
