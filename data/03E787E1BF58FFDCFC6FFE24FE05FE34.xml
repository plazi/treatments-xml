<caption id="DF31667FBF5BFFD2FF22FAA1FD11FAC9" ID-DOI="http://doi.org/10.5281/zenodo.5177916" ID-Zenodo-Dep="5177916" httpUri="https://zenodo.org/record/5177916/files/figure.png" pageId="7" pageNumber="646" startId="7.[150,187,1307,1326]" targetBox="[160,1425,193,1293]" targetPageId="7">
<paragraph id="8BF136F7BF5BFFD2FF22FAA1FD11FAC9" blockId="7.[150,1435,1307,1352]" pageId="7" pageNumber="646">
