<taxonomicName id="DFC04D41FFAB6F668E2A37E6FA9CFF18" authority="(Hafeezullah, 1981)" authorityName="Blend &amp; Karar &amp; Dronen" authorityYear="2017" baseAuthorityName="Hafeezullah" baseAuthorityYear="1981" box="[773,1401,151,176]" class="Trematoda" family="Apocreadiidae" genus="Paraschistorchis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Plagiorchiida" pageId="13" pageNumber="14" phylum="Platyhelminthes" rank="species" species="longivesiculurus" status="n. comb.">
