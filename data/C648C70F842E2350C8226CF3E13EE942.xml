<taxonomicName id="CBC21FC3AE546C921AA42F3F70D6D937" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. jonellus" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="jonellus" subSpecies="monapiae">B. jonellus monapiae</taxonomicName>
