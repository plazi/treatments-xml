<taxonomicName id="98572415508D5B90473102B0441EF2DC" class="Insecta" family="Choreutidae" genus="Anthophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophila vogtii" order="Lepidoptera" pageId="0" pageNumber="8050" phylum="Arthropoda" rank="species" species="vogtii">vogtii</taxonomicName>
