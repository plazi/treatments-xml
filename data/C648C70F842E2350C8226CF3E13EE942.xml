<taxonomicName id="D6B5866EBE4BA62ADC9A560C9652D2BF" class="Insecta" family="Choreutidae" genus="Anthophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophila vogtianus" order="Lepidoptera" pageId="0" pageNumber="8050" phylum="Arthropoda" rank="species" species="vogtianus">vogtianus</taxonomicName>
