<taxonomicName id="925CC626CBFD67203C6EE58A02CE60DB" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. jonellus" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="jonellus" subSpecies="vogtianus">B. jonellus vogtianus</taxonomicName>
