species: 
<taxonomicName id="8DEE4D50BF40FFE4FF24F0986BD96369" authority="McAtee &amp; Malloch, 1924" authorityName="McAtee &amp; Malloch" authorityYear="1924" box="[199,679,250,274]" class="Insecta" family="Miridae" genus="Alcecoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="periscopus">
