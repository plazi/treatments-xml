<taxonomicName id="4C66CFACAA71FFAEFCD8FA98FB50FA16" authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" box="[804,1174,1388,1414]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="subdeformis">
<emphasis id="B912683DAA71FFAEFCD8FA98FC1FFA15" box="[804,985,1388,1413]" italics="true" pageId="59" pageNumber="60">D. subdeformis</emphasis>
Azevedo, 1999
</taxonomicName>
and 
<taxonomicName id="4C66CFACAA71FFAEFB29FA98FDE1FA3E" authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="lanceolatus">
