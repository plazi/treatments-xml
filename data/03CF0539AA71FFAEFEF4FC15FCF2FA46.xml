<taxonomicName id="4C66CFACAA71FFAEFE50FA98FCD3FA16" authority="Evans, 1979" authorityName="Evans" authorityYear="1979" box="[428,789,1388,1414]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="megomphalus">
