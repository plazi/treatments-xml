<taxonomicName id="4C1B20F10A4A46372D3FF985FDF4D162" box="[340,575,1547,1573]" class="Insecta" family="Eriococcidae" kingdom="Animalia" order="Hemiptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="family">ERIOCOCCIDAE</taxonomicName>
