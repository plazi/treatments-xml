<taxonomicName id="6DC61BCE970DE312B813EEDEA4886AE8" authority="Linnavuori, 1965" authorityName="Linnavuori" authorityYear="1965" box="[1134,1435,969,991]" class="Insecta" family="Miridae" genus="Phytocoris" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="360" phylum="Arthropoda" rank="species" species="strigilifer">
