<keyLead id="1137C378970DE312BCEAEEBBA4886AE8" pageId="6" pageNumber="360">
8 Length 
<quantity id="6D3ECDA8970DE312BD53EEBBA06E6AF6" box="[302,381,940,961]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="6" pageNumber="360" unit="mm" value="6.5">6.5 mm</quantity>
