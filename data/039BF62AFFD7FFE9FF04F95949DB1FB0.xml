<quantity id="4CCAEAD9FFD4FFE9FB7AFF5A4F201AA5" box="[1257,1314,224,248]" metricMagnitude="1" metricUnit="m" metricValue="5.10286" pageId="22" pageNumber="23" unit="in" value="2009.0">2009</quantity>
</taxonomicName>
