<caption id="BCA9F99A682D5F3079D0FBF4FA57FBEE" box="[151,1420,1107,1133]" httpUri="https://zenodo.org/record/188187/files/figure.png" pageId="2" pageNumber="40" targetBox="[159,1424,213,1077]" targetPageId="2">
<paragraph id="E869A912682D5F3079D0FBF4FA57FBEE" blockId="2.[151,1420,1107,1133]" box="[151,1420,1107,1133]" pageId="2" pageNumber="40">
