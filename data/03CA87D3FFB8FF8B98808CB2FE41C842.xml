<bibRefCitation id="EFF24B34FFB8FF8899378D53FE1ECF5E" author="Arrow" box="[288,419,1382,1404]" pageId="1" pageNumber="522" refString="Arrow, G. J. (1948) Further notes on the beetle genus Lachnosterna (Col.: Melolonthinae), with descriptions of three new genera. Proceedings of the Royal Entomological Society of London (B), 17, 49 - 54. http: // dx. doi. org / 10.1111 / j. 1365 - 3113.1948. tb 00885. x" type="journal article" year="1948">
Arrow, 
<number id="3DAB24E2FFB8FF88997A8D52FE1ECF5E" box="[365,419,1382,1404]" pageId="1" pageNumber="522" value="1948.0">1948</number>
</bibRefCitation>
: 
<number id="3DAB24E2FFB8FF8899BB8D52FE75CF5E" box="[428,456,1382,1404]" pageId="1" pageNumber="522" value="51.0">51</number>
</treatmentCitation>
</taxonomicName>
. 
<typeStatus id="54D88867FFB8FF8899C48D53FDB8CF5E" box="[467,517,1383,1404]" pageId="1" pageNumber="522">Type</typeStatus>
species: 
<taxonomicName id="4C634D46FFB8FF889A758D52FC7BCF5E" authority="Arrow, 1948" authorityName="Arrow" authorityYear="1948" box="[610,966,1382,1404]" genus="Hippotrichia" kingdom="Animalia" pageId="1" pageNumber="522" rank="species" species="hainana">
<emphasis id="B917EAD7FFB8FF889A758D52FC83CF5E" box="[610,830,1382,1404]" italics="true" pageId="1" pageNumber="522">Hippotrichia hainana</emphasis>
<bibRefCitation id="EFF24B34FFB8FF889B538D53FC7BCF5E" author="Arrow" box="[836,966,1382,1404]" pageId="1" pageNumber="522" refString="Arrow, G. J. (1948) Further notes on the beetle genus Lachnosterna (Col.: Melolonthinae), with descriptions of three new genera. Proceedings of the Royal Entomological Society of London (B), 17, 49 - 54. http: // dx. doi. org / 10.1111 / j. 1365 - 3113.1948. tb 00885. x" type="journal article" year="1948">
Arrow, 
<number id="3DAB24E2FFB8FF889B868D52FC7BCF5E" box="[913,966,1382,1404]" pageId="1" pageNumber="522" value="1948.0">1948</number>
