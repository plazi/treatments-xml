<taxonomicName id="037930000BB79FD1CCB46E289F851364" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Odontostomidae" genus="Bahiensis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="12" pageNumber="13" phylum="Mollusca" rank="species" species="guarani">
