<taxonomicName id="8ECE4D24FFC8FFC8FE23F5C4FD032DC6" authorityName="Green" authorityYear="1935" box="[509,710,663,686]" class="Insecta" family="Coccidae" genus="Ceroplastes" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="355" phylum="Arthropoda" rank="species" species="pseudoceriferus">
