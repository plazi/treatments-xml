<number id="38510E32C109FFE152E6FF48FD294CBB" box="[654,666,209,231]" pageId="2" pageNumber="563" value="9.0">9</number>
acute...................................... 
<taxonomicName id="49996796C109FFE154CEFF4BFA284CB4" authority="Grazia &amp; Greve" authorityName="Grazia &amp; Greve" box="[1190,1435,210,232]" class="Insecta" family="Pentatomidae" genus="Elanela" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="563" phylum="Arthropoda" rank="species" species="jordi">
