<taxonomicName id="49996796C10AFFE25151FD1FFD344EC3" authority="Rolston, 1980" authorityName="Rolston" authorityYear="1980" box="[313,647,646,671]" class="Insecta" family="Pentatomidae" genus="Elanela" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="562" phylum="Arthropoda" rank="species" species="hevera">
