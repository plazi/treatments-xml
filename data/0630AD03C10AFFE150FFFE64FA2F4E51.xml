<number id="38510E32C10AFFE25325F833FCEA4B9C" box="[845,857,1962,1984]" pageId="1" pageNumber="562" value="8.0">8</number>
.............................................. 
<number id="38510E32C10AFFE255E7F833FA284B9C" box="[1423,1435,1962,1984]" pageId="1" pageNumber="562" value="4.0">4</number>
</keyLead>
