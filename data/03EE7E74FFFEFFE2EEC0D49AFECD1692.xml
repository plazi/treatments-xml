</taxonomicName>
(♀♂, from 
<collectingCountry id="F3508FF2FFF9FFE2ED35D149FC211608" box="[866,925,1124,1149]" name="India" pageId="4" pageNumber="235">India</collectingCountry>
) and 
<taxonomicName id="4C47B4E1FFF9FFE2ED89D148FA201609" authority="Gajbe &amp; Gajbe, 1999" authorityName="Gajbe &amp; Gajbe" authorityYear="1999" box="[990,1436,1124,1149]" class="Arachnida" family="Lycosidae" genus="Hippasa" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="235" phylum="Arthropoda" rank="species" species="wigglesworthi">
<emphasis id="B9331370FFF9FFE2ED89D148FB1D1609" box="[990,1185,1124,1148]" italics="true" pageId="4" pageNumber="235">H. wigglesworthi</emphasis>
<bibRefCitation id="EFD6B293FFF9FFE2EAFED149FA201609" author="Gajbe" box="[1193,1436,1124,1149]" pageId="4" pageNumber="235" refString="Gajbe, U. A. &amp; Gajbe, P. (1999) On three new species of spiders of the genus Hippasa Simon (Araneae: Lycosidae) from Jabalpur, Madhya Pradesh, India. Records of the zoological survey of India, 97 (4), 23 - 28." type="journal article" year="1999">
Gajbe &amp; Gajbe, 
<number id="3D8FDD45FFF9FFE2EB33D149FA201609" box="[1380,1436,1124,1148]" pageId="4" pageNumber="235" value="1999.0">1999</number>
</bibRefCitation>
</taxonomicName>
(♀, from 
<collectingCountry id="F3508FF2FFF9FFE2EF53D1AAFE8316D5" box="[260,319,1159,1184]" name="India" pageId="4" pageNumber="235">India</collectingCountry>
) are not true 
<taxonomicName id="4C47B4E1FFF9FFE2EF8CD1A5FD8616EA" box="[475,570,1160,1183]" class="Arachnida" family="Lycosidae" genus="Hippasa" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="235" phylum="Arthropoda" rank="genus">
<emphasis id="B9331370FFF9FFE2EF8CD1A5FD8616EA" box="[475,570,1160,1183]" italics="true" pageId="4" pageNumber="235">Hippasa</emphasis>
</taxonomicName>
