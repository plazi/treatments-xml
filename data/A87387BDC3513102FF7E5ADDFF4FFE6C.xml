<collectingRegion id="E21EF849C3503102FE8E585AFE1BFEFE" box="[359,443,296,319]" country="American Samoa" name="Eastern" pageId="31" pageNumber="72">Eastern</collectingRegion>
<collectingCountry id="58CD763BC3503102FE2B585AFD9DFE81" box="[450,573,295,320]" name="Costa Rica" pageId="31" pageNumber="72">Costa Rica</collectingCountry>
(
<figureCitation id="B8E12A2EC3503102FDA55855FD0EFE81" box="[588,686,295,320]" captionStart="FIGURE 101" captionStartId="40.[151,250,1770,1792]" captionTargetBox="[151,1414,635,1749]" captionTargetId="figure@40.[151,1436,625,1749]" captionTargetPageId="40" captionText="FIGURE 101. Distribution of nominal Protoconnus species." httpUri="https://zenodo.org/record/1195000/files/figure.png" pageId="31" pageNumber="72">Fig. 101</figureCitation>
