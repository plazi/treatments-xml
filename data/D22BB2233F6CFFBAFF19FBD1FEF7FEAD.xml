<taxonomicName id="9D8278B63F6DFFBAFEC5FF2EFE54FF1F" box="[331,484,230,255]" class="Maxillopoda" family="Caligidae" genus="Caligus" kingdom="Animalia" order="Siphonostomatoida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="schelegeli">
