<taxonomicName id="2EBBF6C7FD63FFA3FC3794215B2CFDF9" authority="Clamp, 1991" authorityName="Clamp" authorityYear="1991" box="[997,1308,550,574]" class="Oligohymenophorea" family="Operculariidae" genus="Operculigera" kingdom="Protozoa" order="Peritrichida" pageId="2" pageNumber="155" phylum="Ciliophora" rank="genus">
