<taxonomicName id="3FF2F9AE33781B5FFBC4A3ADFF10F914" authority="Grant, 1912" authorityName="Grant" authorityYear="1912" class="Aves" family="Charadriidae" genus="Vanellus" kingdom="Animalia" order="Charadriiformes" pageId="17" pageNumber="62" phylum="Chordata" rank="species" species="grisescens">
