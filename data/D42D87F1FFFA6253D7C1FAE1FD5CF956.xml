</taxonomicName>
[partim] (see 
<taxonomicName id="9B844D64FFFA6253D79DFA76FEC7FA5A" baseAuthorityName="Fieber" baseAuthorityYear="1853" box="[203,332,1409,1434]" class="Insecta" family="Tettigoniidae" genus="Eupholidoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="schmidti">
