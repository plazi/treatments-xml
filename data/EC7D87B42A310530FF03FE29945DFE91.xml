<taxonomicName id="A3D44D212A310533FF03FE2996CDFE07" box="[151,395,473,499]" class="Insecta" family="Cecidomyiidae" genus="Zadbimyia" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="aberrans" status="sp. n.">
<emphasis id="56A0EAB02A310533FF03FE2996CDFE07" bold="true" box="[151,395,473,499]" italics="true" pageId="25" pageNumber="26">Zadbimyia aberrans</emphasis>
</taxonomicName>
