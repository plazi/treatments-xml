<subSubSection id="C31E43ACF3196C58FF0E3C9FFCC6FF2C" box="[199,793,188,212]" pageId="18" pageNumber="44" type="etymology">
<paragraph id="8BBB1027F3196C58FF0E3C9FFCC6FF2C" blockId="18.[151,1436,151,464]" box="[199,793,188,212]" pageId="18" pageNumber="44">
<emphasis id="B970CC35F3196C58FF0E3C9FFE91FF2C" bold="true" box="[199,334,188,212]" pageId="18" pageNumber="44">Etymology.</emphasis>
