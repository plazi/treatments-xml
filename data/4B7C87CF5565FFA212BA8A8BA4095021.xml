<quantity id="042D9B3C5566FFA211B28C92A225525B" box="[493,572,1523,1544]" metricMagnitude="3" metricUnit="m" metricValue="1.209" pageId="6" pageNumber="334" unit="m" value="1209.0">1,209 m</quantity>
