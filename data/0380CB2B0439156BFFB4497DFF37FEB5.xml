<taxonomicName id="4C2901BE0438156BFB524F2AFF3EFEB4" authorityName="Hinds" authorityYear="1843" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="49" pageNumber="50" phylum="Mollusca" rank="species" species="concinna" subGenus="Mastonia">
