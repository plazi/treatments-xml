<subSubSection id="C354654AFFEC222F999AFADAFDD8F80A" pageId="15" pageNumber="245" type="distribution">
<paragraph id="8BF136C1FFEC222F999AFADAFB24F8ED" blockId="15.[170,1437,813,1979]" pageId="15" pageNumber="245">
<emphasis id="B93AEAD3FFEC222F999AFADAFEC4FAB9" box="[170,308,1292,1317]" italics="true" pageId="15" pageNumber="245">Verbreitung</emphasis>
