<taxonomicName id="4C1594CE2965FF9AFEE9FF6BEE24FF04" authority="Guéorguiev &amp; Liang, 2020" authorityName="Guéorguiev &amp; Liang" authorityYear="2020" box="[309,522,223,249]" class="Insecta" family="Carabidae" genus="Oodes" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="hainanensis" status="sp. n.">
