<taxonomicName id="CDA34D1A3203A96C4B6FFCAC1CEDFCD1" box="[290,431,799,824]" class="Aves" family="Accipitridae" genus="Rostrhamus" kingdom="Animalia" order="Accipitriformes" pageId="10" pageNumber="268" phylum="Chordata" rank="species" species="sociabilis">
<emphasis id="38D7EA8B3203A96C4B6FFCAC1C71FCD1" box="[290,307,801,824]" italics="true" pageId="10" pageNumber="268">R</emphasis>
