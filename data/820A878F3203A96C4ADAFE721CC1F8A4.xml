<taxonomicName id="CDA34D1A3203A96C48A4FD701E36FCFC" box="[745,884,764,789]" class="Trematoda" family="Microphallidae" genus="Megalophallus" kingdom="Animalia" order="Plagiorchiida" pageId="10" pageNumber="268" phylum="Platyhelminthes" rank="species" species="deblocki">
