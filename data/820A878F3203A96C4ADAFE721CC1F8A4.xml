(
<taxonomicName id="CDA34D1A3203A96C4942FCC91EC7FCB4" box="[783,901,836,861]" class="Malacostraca" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="268" phylum="Arthropoda" rank="order">Decapoda</taxonomicName>
