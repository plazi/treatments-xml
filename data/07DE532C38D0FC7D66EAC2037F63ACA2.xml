<taxonomicName id="D2FEB2636E042FC334AD87299B650436" class="Insecta" family="Chrysopidae" genus="Santocellus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Santocellus" order="Neuroptera" pageId="1" pageNumber="94" phylum="Arthropoda" rank="genus">Santocellus</taxonomicName>
