, 
<taxonomicName id="76F4B2B4128BD1502C9F73BDF2E05C98" class="Insecta" family="Crambidae" genus="Loxostege" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loxostege" order="Lepidoptera" pageId="0" pageNumber="113" phylum="Arthropoda" rank="genus">Loxostege</taxonomicName>
