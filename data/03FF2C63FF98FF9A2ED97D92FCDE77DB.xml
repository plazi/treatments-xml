<taxonomicName id="4C56E6F6FF98FF9A2F187C5EFE43747E" authority="Leach, 1815" authorityName="Leach" authorityYear="1815" box="[318,501,1287,1312]" class="Malacostraca" family="Axiidae" genus="Axius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="446" phylum="Arthropoda" rank="genus">
