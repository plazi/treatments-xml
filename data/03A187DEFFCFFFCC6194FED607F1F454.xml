</taxonomicName>
and 
<taxonomicName id="4C084D4BFFCFFFCC6242FCE507E1F429" box="[833,958,788,814]" class="Insecta" family="Hydrochidae" genus="Hydrochus" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="pupillus">
