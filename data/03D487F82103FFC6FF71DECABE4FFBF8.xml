<taxonomicName id="4C7D4D6D2103FFC6FBDADDEBB995FD25" authorityName="Crosnier (NR" authorityYear="2002" box="[1084,1214,691,716]" class="Malacostraca" family="Carcinidae" genus="Parathranites" kingdom="Animalia" order="Decapoda" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="granosus">
