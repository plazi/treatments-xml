<taxonomicName id="EFF47EE9FFFBFFCFFB5F94E1FAAC6CFD" authorityName="Banks" authorityYear="1945" box="[1214,1321,1498,1516]" class="Insecta" family="Pompilidae" genus="Ageniella" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="95" phylum="Arthropoda" rank="subGenus" subGenus="Ameragenia">
