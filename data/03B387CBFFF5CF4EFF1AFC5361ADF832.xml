<taxonomicName id="4C1A4D5EFFF5CF4EFC57F9AC6769F9AA" box="[986,1105,1536,1560]" class="Magnoliopsida" family="Anacardiaceae" genus="Mangifera" kingdom="Plantae" order="Sapindales" pageId="10" pageNumber="311" phylum="Tracheophyta" rank="genus">
