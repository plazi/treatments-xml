</taxonomicName>
[
<taxonomicName id="7EDB4D17F419FFCCFAB5F874FF28EDD1" authorityName="de Jussieu" authorityYear="1789" class="Liliopsida" family="Cyperaceae" kingdom="Plantae" order="Poales" pageId="1" pageNumber="426" phylum="Tracheophyta" rank="family">Cyperaceae</taxonomicName>
