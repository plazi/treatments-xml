<taxonomicName id="D094D8E0FFF1642EDF15FB250F794AB3" authorityName="Kristan-Tollmann" authorityYear="1970" box="[377,529,1215,1241]" class="Ostracoda" family="Bairdiidae" genus="Hiatobairdia" kingdom="Animalia" order="Podocopida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
