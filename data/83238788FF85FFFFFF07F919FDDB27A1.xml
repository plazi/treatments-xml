<collectionCode id="6D9BAE5BFF83FFFFFE63FE3EFD9E2499" box="[499,574,413,439]" pageId="13" pageNumber="14">FGZC</collectionCode>
<number id="BD4224B9FF83FFFFFDD5FE3EFD212499" box="[581,641,413,439]" pageId="13" pageNumber="14" value="2226.0">2226</number>
