<collectionCode id="6D9BAE5BFF83FFFFFD7FFEDBFC9C24BC" box="[751,828,376,402]" pageId="13" pageNumber="14">FGZC</collectionCode>
<number id="BD4224B9FF83FFFFFCD3FEDBFC2224BC" box="[835,898,376,402]" pageId="13" pageNumber="14" value="2233.0">2233</number>
