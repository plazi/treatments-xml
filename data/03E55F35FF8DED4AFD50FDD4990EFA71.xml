<subSubSection id="C356BDA8FF8DED4AFD6EFD709EFBFCC1" pageId="49" type="materials_examined">
<paragraph id="8BF3EE23FF8DED4AFD6EFD709EFBFCC1" blockId="49.[696,1226,734,1513]" pageId="49">
