<collectingCountry id="F35BAEB3FF8DED4AFB87FD709E3FFC8F" name="Trinidad and Tobago" pageId="49">Trinidad and Tobago</collectingCountry>
: 
<collectingCountry id="F35BAEB3FF8DED4AFC94FD519EB9FC8F" box="[802,904,767,791]" name="Trinidad and Tobago" pageId="49">Trinidad</collectingCountry>
: 
<collectingRegion id="498820C1FF8DED4AFC2AFD519ED9FC8F" box="[924,1000,767,791]" country="Trinidad and Tobago" name="Arima" pageId="49">Arima</collectingRegion>
