<title id="C3CA22ADA0638C8A3D994715ECE07C2F" pageId="null" pageNumber="45">
<normalizedToken id="6BDAFF498E21A8C39184386A3A69B59C" originalValue="Schlüssel" pageId="null" pageNumber="45">Schluessel</normalizedToken>
zur Artengruppe des 
<taxonomicName id="E1C281F2C9C6EF0169FF40151049A645" class="Magnoliopsida" family="Ranunculaceae" genus="Thalictrum" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="45" phylum="Tracheophyta" rank="species" species="flavum">Thalictrum flavum</taxonomicName>
