<paragraph id="6DB442D2FDB378FC2DB9B5062FDDBC49" pageId="null" pageNumber="45">
<pageBreakToken id="3E091CA047F4F63A3A3459D3284DC1D5" pageId="null" pageNumber="45" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="AE45EB04692F2C4D73902F461ABFE9AF" authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Thalictrum" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="45" phylum="Tracheophyta" rank="species" species="flavum">
Thalictrum 
<normalizedToken id="E2F3CADAF52750B06207F6C5969D16F8" originalValue="flávum" pageId="null" pageNumber="45">flavum</normalizedToken>
<authorityName id="D2B2B24B09557BE0B4BBFBAF9EE0899B" pageId="null" pageNumber="45">L.</authorityName>
