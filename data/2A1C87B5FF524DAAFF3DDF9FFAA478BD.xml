<taxonomicNameLabel id="8BF257CAFF514DAAFA78DDC2FF2779ED" pageId="199" pageNumber="200" rank="species">n. sp.</taxonomicNameLabel>
(see above) and 
<taxonomicName id="65B54D20FF514DAAFE63DDA7FE2D79ED" authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[402,482,723,747]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="exilis" status="n. sp.">
<emphasis id="90C1EAB1FF514DAAFE63DDA7FE2D79ED" box="[402,482,723,747]" italics="true" pageId="199" pageNumber="200">U. exilis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="8BF257CAFF514DAAFE1BDDA7FDEF79ED" box="[490,544,724,747]" pageId="199" pageNumber="200" rank="species">n. sp.</taxonomicNameLabel>
(see below). 
<taxonomicName id="65B54D20FF514DAAFD55DDA0FCB479ED" authorityName="Baba" authorityYear="2018" box="[676,891,723,747]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="elongatus">
<emphasis id="90C1EAB1FF514DAAFD55DDA0FCB479ED" box="[676,891,723,747]" italics="true" pageId="199" pageNumber="200">Uroptychus elongatus</emphasis>
</taxonomicName>
