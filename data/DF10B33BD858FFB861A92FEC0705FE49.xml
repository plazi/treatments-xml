(
<bibRefCitation id="33287FDCD85AFFB864332F530399FED1" author="Matsumura" pageId="10" pageNumber="67" refString="Matsumura, S. (1907) Die Cicadinen Japans. [Tokyo]. Annotationes Zoologicae Japonenses, 6, 83 - 116." type="journal article" year="1907">
Matsumura, 
<number id="E171100AD85AFFB861A92F7B0399FED1" box="[151,214,312,338]" pageId="10" pageNumber="67" value="1907.0">1907</number>
</bibRefCitation>
)
