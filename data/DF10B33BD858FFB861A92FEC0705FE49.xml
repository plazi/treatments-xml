<emphasis id="65CDDE3FD859FFBB60212A4A017BFBA3" box="[287,564,1033,1056]" italics="true" pageId="9" pageNumber="66">
<taxonomicName id="90B979AED859FFBB60212A4A02C2FBA3" box="[287,397,1033,1056]" class="Insecta" family="Aphrophoridae" genus="Ariptyelus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="66" phylum="Arthropoda" rank="genus">Ariptyelus</taxonomicName>
<taxonomicName id="90B979AED859FFBB60AC2A4A017BFBA3" authority="Shih and Yang" authorityName="Shih and Yang" box="[402,564,1033,1056]" pageId="9" pageNumber="66" rank="species" species="subauropilosus" status="sp. nov.">subauropilosus</taxonomicName>
</emphasis>
Shih
</taxonomicName>
and Yang
</taxonomicName>
, 
<taxonomicNameLabel id="7EFE6344D859FFBB63DC2A4B0078FBA3" box="[738,823,1032,1056]" pageId="9" pageNumber="66" rank="species">
