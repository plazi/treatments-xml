<paragraph id="8B8936EEFFF0FFFDFF2CFB9AFBF6E62C" blockId="8.[151,1122,1037,1061]" box="[151,1122,1037,1061]" pageId="8" pageNumber="517">
<taxonomicName id="4C364D6DFFF0FFFDFF2CFB9AFD26E62C" authority="U. Aspock &amp; H. Aspock, [1986]" authorityName="U. Aspock &amp; H. Aspock" authorityYear="1986" box="[151,690,1037,1061]" class="Insecta" family="Berothidae" genus="Lekrugeria" kingdom="Animalia" order="Neuroptera" pageId="8" pageNumber="517" phylum="Arthropoda" rank="species" species="nepalica">
