<quantity id="5F7A9B38FFC82528FCF7FE53FC5DFE0D" box="[840,954,442,466]" metricMagnitude="2" metricUnit="m" metricValue="4.65" metricValueMax="5.0" metricValueMin="4.3" pageId="74" pageNumber="683" unit="m" value="465.0" valueMax="500.0" valueMin="430.0">
430- 
<specimenCount id="8E84FD54FFC82528FCC9FE53FC5DFE0D" box="[886,954,442,466]" pageId="74" pageNumber="683" type="generic">500 m</specimenCount>
