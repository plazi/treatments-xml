Comments on 
<emphasis id="B903791CFFEC4302FECBFDCC7208BBF5" bold="true" box="[323,610,616,641]" italics="true" pageId="4" pageNumber="65">
<taxonomicName id="4C77DE8DFFEC4302FECBFDCC71BFBBF5" box="[323,469,616,641]" class="Insecta" family="Kermesidae" genus="Kermes" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="echinatus">K. echinatus</taxonomicName>
, 
<taxonomicName id="4C77DE8DFFEC4302FE6AFDCC7208BBF5" box="[482,610,616,641]" class="Insecta" family="Kermesidae" genus="Kermes" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="vermilio">K. vermilio</taxonomicName>
</emphasis>
and 
<taxonomicName id="4C77DE8DFFEC4302FD15FDCC733BBBF5" box="[669,849,616,641]" class="Insecta" family="Kermesidae" genus="Kermes" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="hermonensis">
