<taxonomicName id="4C2014CFB343D747FA92D4DDDD750AA2" authorityName="Ird &amp; Inra" authorityYear="2017" box="[999,1134,464,490]" class="Insecta" family="Philopotamidae" genus="Chimarra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="lehibemavo">
