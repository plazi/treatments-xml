<collectingRegion id="498DF860FFD8FFE5FE1579B8FDA1FA14" box="[473,576,1464,1490]" country="Mexico" name="Veracruz de Ignacio de la Llave" pageId="25" pageNumber="220">Veracruz</collectingRegion>
, 
<collectingCountry id="F35E7612FFD8FFE5FD8679B7FD43FA17" box="[586,674,1463,1489]" name="Mexico" pageId="25" pageNumber="220">Mexico</collectingCountry>
