<taxonomicName id="4C494D01FFD8FFE5FF7A79B8FED5FA17" authorityName="Brown" authorityYear="2013" box="[182,308,1464,1489]" class="Insecta" family="Tortricidae" genus="Eupinivora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="25" pageNumber="220" phylum="Arthropoda" rank="genus">
