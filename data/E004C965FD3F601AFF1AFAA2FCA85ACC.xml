<collectingCountry id="10BA38E3FD3F601AFC70F82EFBAB5AE8" box="[1021,1124,1977,2001]" name="Thailand" pageId="8" pageNumber="9">Thailand</collectingCountry>
. Despite 
<taxonomicName id="AFAD03F0FD3F601AFB40F82DFA4D5AE8" authorityName="Terayama" authorityYear="1995" box="[1229,1410,1977,2001]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="khaoyaiensis">
<emphasis id="5AD9A461FD3F601AFB40F82DFA4D5AE8" box="[1229,1410,1977,2001]" italics="true" pageId="8" pageNumber="9">D. khaoyaiensis</emphasis>
