</taxonomicName>
, 
<taxonomicName id="AFAD03F0FD3F601AFD43F801FC8F5A94" authorityName="Terayama" authorityYear="2001" box="[718,832,1941,1965]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="browni">
<emphasis id="5AD9A461FD3F601AFD43F801FC8F5A94" box="[718,832,1941,1965]" italics="true" pageId="8" pageNumber="9">D. browni</emphasis>
</taxonomicName>
, 
<taxonomicName id="AFAD03F0FD3F601AFCDDF801FBED5A94" authorityName="Terayama" authorityYear="2001" box="[848,1058,1941,1965]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="chiangmaiensis">
<emphasis id="5AD9A461FD3F601AFCDDF801FBED5A94" box="[848,1058,1941,1965]" italics="true" pageId="8" pageNumber="9">D. chiangmaiensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="AFAD03F0FD3F601AFBBCF801FB795A94" authorityName="Terayama" authorityYear="2001" box="[1073,1206,1941,1965]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="thaianus">
<emphasis id="5AD9A461FD3F601AFBBCF801FB795A94" box="[1073,1206,1941,1965]" italics="true" pageId="8" pageNumber="9">D. thaianus</emphasis>
</taxonomicName>
, 
<taxonomicName id="AFAD03F0FD3F601AFB48F801FAAB5A94" authorityName="Terayama" authorityYear="2001" box="[1221,1380,1941,1965]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="wusheanus">
<emphasis id="5AD9A461FD3F601AFB48F801FAAB5A94" box="[1221,1380,1941,1965]" italics="true" pageId="8" pageNumber="9">D. wusheanus</emphasis>
</taxonomicName>
, and 24 were considered to be new. This is the first record of 
<taxonomicName id="AFAD03F0FD3F601AFC94F82DFC785AE8" authorityName="Terayama" authorityYear="2001" box="[793,951,1977,2001]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="wusheanus">
<emphasis id="5AD9A461FD3F601AFC94F82DFC785AE8" box="[793,951,1977,2001]" italics="true" pageId="8" pageNumber="9">D. wusheanus</emphasis>
