</taxonomicName>
and 
<taxonomicName id="7CAF4D5CFFA1FFE234BE88AFA6D3FC2E" box="[721,899,944,968]" class="Insecta" family="Braconidae" genus="Centistes" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="auricephalus">
