<taxonomicName id="4C37BAB697197C18FD839F8C34BCFF74" box="[610,745,184,208]" class="Lobosa" family="Hyalospheniidae" genus="Gibbocarina" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Arcellinida" pageId="16" pageNumber="621" phylum="Protozoa" rank="genus" status="gen. nov.">
