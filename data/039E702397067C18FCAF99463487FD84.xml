<taxonomicName id="4C37BAB697197C18FE799E273694FEEE" authority="(Archer) Leidy, 1879" authorityName="(Archer) Leidy" authorityYear="1879" class="Lobosa" family="Nebelidae" genus="Nebela" kingdom="Protozoa" order="Arcellinida" pageId="16" pageNumber="622" phylum="Protozoa" rank="species" species="carinata">
