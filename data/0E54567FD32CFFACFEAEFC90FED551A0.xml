<taxonomicName id="41FD9CEAD32CFFACFBF9FC19FEDD51A0" authorityName="Peringuey" authorityYear="1888" class="Insecta" family="Histeridae" genus="Saprinus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="73" phylum="Arthropoda" rank="subSpecies" species="cruciatus" subGenus="Saprinus" subSpecies="flavipennis">
