<taxonomicName id="B3804D2DA255FFF90F9BFF1DD75665E4" authority="Dana, 1852" authorityName="Dana" authorityYear="1852" box="[151,588,221,247]" class="Malacostraca" family="Hippolytidae" genus="Hippolyte" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="obliquimanus">
