<taxonomicName id="03D49CE60121FFA8D8CCA70D9F94FED0" authority="Nosek, 1973" authorityName="Nosek" authorityYear="1973" box="[424,700,283,310]" class="Entognatha" family="Acerentomidae" genus="Brasilidia" kingdom="Animalia" order="Protura" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
