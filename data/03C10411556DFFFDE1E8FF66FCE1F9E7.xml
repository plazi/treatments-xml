<taxonomicName id="4C68CE84556CFFFDE519FF68FB5CFF22" authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[1126,1242,151,176]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="tunstilli">
