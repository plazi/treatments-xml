</taxonomicName>
, 
<taxonomicName id="4C68CE84556CFFFDE198FF10FEB0FF6A" authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[231,310,223,248]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="olea">
<emphasis id="B91C6915556CFFFDE198FF10FEB0FF6A" box="[231,310,223,248]" italics="true" pageId="59" pageNumber="60">B. olea</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C68CE84556CFFFDE03AFF10FE21FF6A" authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[325,423,225,248]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="tariae">
