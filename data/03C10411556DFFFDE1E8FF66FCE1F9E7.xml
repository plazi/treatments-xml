<taxonomicName id="4C68CE84556CFFFDE3FDFF10FD6FFF6A" authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[642,745,225,248]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="raveni">
