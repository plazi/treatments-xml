<taxonomicName id="4C68CE84556CFFFDE0C9FF10FDCDFF6A" authorityName="Harrison &amp; Rix &amp; Harvey &amp; Austin" authorityYear="2018" box="[438,587,225,248]" class="Arachnida" family="Idiopidae" genus="Blakistonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="carnarvon">
<emphasis id="B91C6915556CFFFDE0C9FF10FDCDFF6A" box="[438,587,225,248]" italics="true" pageId="59" pageNumber="60">B. carnarvon</emphasis>
</taxonomicName>
