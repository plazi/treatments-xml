<number id="543B2499FFF555425057FAB3FBB2FABD" box="[1056,1137,1337,1362]" pageId="2" pageNumber="399" value="14.153">14.153</number>
S
</geoCoordinate>
, 
<geoCoordinate id="87C75079FFF5554250FBFAB3FADCFABD" box="[1164,1311,1337,1362]" direction="east" orientation="longitude" pageId="2" pageNumber="399" precision="1" value="166.56924">
