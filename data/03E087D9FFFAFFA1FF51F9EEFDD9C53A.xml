<collectingRegion id="498DF82DFFE5FFA1FDD6FF6BFD30C4DE" box="[528,685,223,249]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="16" pageNumber="217">Buenos Aires</collectingRegion>
