<taxonomicName id="264DDA8BA818BEB79FF0C103C2D49928" class="Insecta" family="Saldidae" genus="Brevrimatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brevrimatus pulchalifer" order="Hemiptera" pageId="2" pageNumber="187" phylum="Arthropoda" rank="species" species="pulchalifer">Brevrimatus pulchalifer</taxonomicName>
