<subSubSection id="C3176560FF8EFFE01FC8FA1AFAF74FD9" box="[151,1358,1498,1523]" pageId="24" pageNumber="25" type="etymology">
<paragraph id="8BB236EBFF8EFFE01FC8FA1AFAF74FD9" blockId="24.[151,1436,258,1523]" box="[151,1358,1498,1523]" pageId="24" pageNumber="25">
<emphasis id="B979EAF9FF8EFFE01FC8FA1AFE914FD8" box="[151,296,1499,1523]" italics="true" pageId="24" pageNumber="25">Etymology—</emphasis>
