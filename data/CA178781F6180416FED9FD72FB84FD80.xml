<taxonomicName id="85BE4D14F6180416FED9FD72FD8EFD76" authorityName="Jeannel" authorityYear="1962" box="[309,524,671,695]" class="Insecta" family="Staphylinidae" genus="Achilia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="123" phylum="Arthropoda" rank="species" species="tumidifrons">
<emphasis id="70CAEA85F6180416FED9FD72FD8EFD76" bold="true" box="[309,524,671,695]" italics="true" pageId="4" pageNumber="123">Achilia tumidifrons</emphasis>
