<taxonomicName id="E3E18191FF83FF8A1ED7F97BFDC0F89A" authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[416,585,1831,1855]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="105" pageNumber="106" phylum="Arthropoda" rank="species" species="micheneri" subGenus="Chaetalictus">
(
<emphasis id="16952600FF83FF8A1EDEF974FE41F89A" box="[425,456,1832,1855]" italics="true" pageId="105" pageNumber="106">Ch</emphasis>
.) 
<emphasis id="16952600FF83FF8A1EA9F974FDC0F89A" box="[478,585,1832,1855]" italics="true" pageId="105" pageNumber="106">micheneri</emphasis>
