<taxonomicName id="09BB4D2DFFA7FFADFEA71A37FD616C3D" authority="Burckhardt &amp; Cho" authorityName="Burckhardt &amp; Cho &amp; Lee" authorityYear="2018" box="[294,654,548,570]" class="Magnoliopsida" family="Moraceae" genus="Moriphila" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="300" phylum="Magnoliopsida" rank="species" species="furva" status="sp. nov.">
