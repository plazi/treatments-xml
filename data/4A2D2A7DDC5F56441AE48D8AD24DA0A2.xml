<taxonomicName id="B2EA18B1920B76429649F94A98A3E991" LSID="http://zoobank.org/CF8E0FB2-3F8F-4D6B-8AA3-3430D281B886" authority="Waterhouse, 1879" authorityName="Waterhouse" authorityYear="1879" class="Insecta" family="Lycidae" genus="Eniclases" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eniclases tikapurensis" order="Coleoptera" pageId="6" pageNumber="21" phylum="Arthropoda" rank="species" species="tikapurensis">Eniclases tikapurensis</taxonomicName>
<taxonomicNameLabel id="A8495F93553FB10F5D69CDB1AB34CC01" pageId="6" pageNumber="21">sp. n.</taxonomicNameLabel>
