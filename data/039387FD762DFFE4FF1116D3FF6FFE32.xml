species: 
<taxonomicName id="4C3A4D68762DFFE4FEA01728FE3AFEFE" authorityName="Denis &amp; Schiffermuller" authorityYear="1775" box="[294,439,292,316]" class="Insecta" family="Tineidae" genus="Tinea" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="302" phylum="Arthropoda" rank="species" species="vulgella">
