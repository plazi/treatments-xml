<heading id="D0D981A3FFE7FFC1FED7DF68EB0EFC3E" bold="true" box="[264,576,873,899]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<taxonomicName id="4C2E4D4CFFE7FFC1FED7DF68EB0EFC3E" authority="Beddard, 1886" authorityName="Beddard" authorityYear="1886" box="[264,576,873,899]" class="Malacostraca" family="Neotanaidae" genus="Neotanais" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
