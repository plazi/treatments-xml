<keyStep id="30EB2E16FFC8FFFAFF62EE2061EA81B2" pageId="1" pageNumber="594">
<paragraph id="8BA036B3FFC8FFFAFF62EE2061EA810A" blockId="1.[151,1437,1622,1996]" pageId="1" pageNumber="594">
<keyLead id="30EE9586FFC8FFFAFF62EE2061EA810A" pageId="1" pageNumber="594">
<number id="3DD72494FFC8FFFAFF62EE2064D58140" box="[151,163,1828,1849]" pageId="1" pageNumber="594" value="4.0">4</number>
