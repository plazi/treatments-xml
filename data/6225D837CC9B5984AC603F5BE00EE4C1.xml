<geoCoordinate id="4133DE4F3E4F9EE791C4BD12EBA7E66D" degrees="81.0661" direction="east" orientation="longitude" precision="5" value="81.0661">81.0661E</geoCoordinate>
, 4800m.&quot;; &quot;under stone, 2019. VII. 15, Shi HL lgt., Exp. BJFU 2019&quot;; &quot;DNA sample series number SHL2019--Xiz006&quot;; &quot;HOLOTYPE♂ 
<taxonomicName id="61039DDA156525C3B39C228A877637AF" authorityName="Yan &amp; Shi &amp; Liang" authorityYear="2020" class="Insecta" family="Carabidae" genus="Himalopenetretus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Himalopenetretus burangensis" order="Coleoptera" pageId="0" pageNumber="145" phylum="Arthropoda" rank="species" species="burangensis">
