<caption id="EBA64783605A9912FF3DF81E4316C8A6" ID-DOI="http://doi.org/10.5281/zenodo.6127890" ID-Zenodo-Dep="6127890" httpUri="https://zenodo.org/record/6127890/files/figure.png" pageId="5" pageNumber="142" startId="5.[151,264,1943,1967]" targetBox="[208,1380,195,1908]" targetPageId="5">
