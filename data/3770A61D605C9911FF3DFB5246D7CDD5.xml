<taxonomicName id="78D96C88605B9913FD41F94F4132C98C" authority="Liu &amp; Liu, 2022" authorityName="Liu &amp; Liu" authorityYear="2022" box="[747,869,1733,1757]" class="Insecta" family="Bucculatricidae" genus="Bucculatrix" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="141" phylum="Arthropoda" rank="species" species="coadnata" status="sp. n.">
