<taxonomicName id="E98B4D15FFDBFF94DFD23F63FDBBFCC0" box="[487,559,876,895]" class="Insecta" family="Ptiliidae" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="154" phylum="Arthropoda" rank="family">Ptiliidae</taxonomicName>
