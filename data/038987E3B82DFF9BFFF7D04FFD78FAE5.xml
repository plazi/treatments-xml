<taxonomicName id="4C204D76B82DFF9BFFC2D0B0FE1EFB4E" authorityName="Fall" authorityYear="1934" box="[185,433,1222,1248]" class="Insecta" family="Leiodidae" genus="Agathidium" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="377" phylum="Arthropoda" rank="species" species="depressum">
