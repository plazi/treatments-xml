<taxonomicName id="F3EBDB0D1AF00CBE6969422708440F15" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:35468" class="Insecta" family="Formicidae" genus="Prenolepis" kingdom="Animalia" lsidName-HNS="Prenolepis acuminata Forel" order="Hymenoptera" pageNumber="309" phylum="Arthropoda" rank="species" species="acuminata">Prenolepis acuminata</taxonomicName>
