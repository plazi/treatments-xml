<taxonomicName id="4C4E4D6DFFBFFFEA18E2FB0F6269FACE" authority="(Goding &amp; Froggatt, 1904)" baseAuthorityName="Goding &amp; Froggatt" baseAuthorityYear="1904" class="Insecta" family="Cicadidae" genus="Pauropsalta" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="538" phylum="Arthropoda" rank="genus">
