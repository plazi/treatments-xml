<taxonomicName id="95924D187225FFD9FCF0657BFBD9FB97" box="[885,1034,1095,1120]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="altiplanica">
<emphasis id="60E6EA897225FFD9FCF0657BFBD9FB97" box="[885,1034,1095,1120]" italics="true" pageId="28" pageNumber="29">S. altiplanica</emphasis>
</taxonomicName>
from all other 
<taxonomicName id="95924D187225FFD9FB376574FAC8FB97" box="[1202,1307,1095,1120]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
<emphasis id="60E6EA897225FFD9FB376574FAC8FB97" box="[1202,1307,1095,1120]" italics="true" pageId="28" pageNumber="29">Stigmella</emphasis>
</taxonomicName>
