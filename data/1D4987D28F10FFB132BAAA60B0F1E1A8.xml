<number id="232824E38F13FFB13025AAECB036E5B8" box="[520,582,969,995]" pageId="2" pageNumber="3" value="1907.0">1907</number>
</taxonomicName>
; 
<emphasis id="A794EAD68F13FFB13078AAECB62AE5B9" box="[597,1114,969,994]" italics="true" pageId="2" pageNumber="3">
<taxonomicName id="52E04D478F13FFB13078AAECB10DE5B9" box="[597,893,969,994]" class="Rhabditophora" family="Ilyplanidae" genus="Enterogonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="2" pageNumber="3" phylum="Platyhelminthes" rank="subSpecies" species="pigrans" subSpecies="var">Enterogonia pigrans var</taxonomicName>
