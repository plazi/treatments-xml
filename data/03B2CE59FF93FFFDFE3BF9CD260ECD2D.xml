</taxonomicName>
/ 
<taxonomicName id="4C1B04CCFF94FFFDFE44FECC26A5CE6B" authority="Woll." authorityName="Wollaston" authorityYear="1864" box="[478,655,347,371]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="2028" phylum="Arthropoda" rank="species" species="mendicus">
