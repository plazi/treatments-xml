<taxonomicName id="4C1B04CCFF94FFFDFEE1FE8D2604CE2A" authority="Woll." authorityName="Wollaston" authorityYear="1864" box="[379,558,282,306]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="2028" phylum="Arthropoda" rank="species" species="mendicus">
