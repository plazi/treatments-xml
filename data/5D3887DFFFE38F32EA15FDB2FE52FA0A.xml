<taxonomicName id="12914D4AFFE28F32E840FAD0FC6EFACA" baseAuthorityName="Silvestri" baseAuthorityYear="1927" box="[830,944,1341,1364]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="107" pageNumber="2204" phylum="Arthropoda" rank="species" species="strenua">
