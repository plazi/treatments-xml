<taxonomicName id="FBEF4D272B60FFF798ECF88BFDD1F895" authority="Bernhauer, 1938" authorityName="Bernhauer" authorityYear="1938" box="[409,527,1824,1846]" pageId="55" pageNumber="56" rank="species" species="subsericeus">
