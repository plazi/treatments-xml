<subSubSection id="07D195C4FF85FFBFFF1EFD05FBD372FC" box="[199,1143,642,665]" pageId="3" pageNumber="600" type="etymology">
<paragraph id="4F74C64FFF85FFBFFF1EFD05FBD372FC" blockId="3.[151,1436,152,728]" box="[199,1143,642,665]" pageId="3" pageNumber="600">
<emphasis id="7DBF1A5DFF85FFBFFF1EFD05FEE472FC" bold="true" box="[199,320,642,665]" pageId="3" pageNumber="600">Etymology</emphasis>
