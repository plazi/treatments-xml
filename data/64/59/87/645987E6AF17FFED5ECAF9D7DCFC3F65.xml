<collectingCountry box="[912,961,1983,2007]" name="Iran" pageId="54" pageNumber="55">Iran</collectingCountry>
, 
<collectingCountry box="[975,1024,1983,2007]" name="Iraq" pageId="54" pageNumber="55">Iraq</collectingCountry>
), Arabian Peninsula, tropical Africa (
<collectingCountry box="[158,293,151,176]" name="Cape Verde" pageId="55" pageNumber="56">Cape Verde</collectingCountry>
