<collectingCountry box="[1303,1432,1174,1199]" name="Tajikistan" pageId="54" pageNumber="55">Tadjikistan</collectingCountry>
, 
<collectingCountry box="[151,311,1210,1235]" name="Turkmenistan" pageId="54" pageNumber="55">Turkmenistan</collectingCountry>
).
</paragraph>
