<taxonomicName box="[809,952,295,320]" class="Insecta" family="Lymantridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="family">Lymantridae</taxonomicName>
) (in 
<collectingCountry box="[1005,1114,296,320]" name="Germany" pageId="11" pageNumber="12">Germany</collectingCountry>
), butterfly 
<taxonomicName box="[1235,1436,295,320]" class="Insecta" family="Noctuidae" genus="Mythimna" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="comma">
