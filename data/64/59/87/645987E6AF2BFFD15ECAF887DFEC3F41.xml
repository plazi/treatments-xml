<taxonomicName box="[1100,1229,260,285]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
: 
<taxonomicName box="[1240,1389,260,285]" class="Insecta" family="Scarabaeidae" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="family">Scarabaeidae</taxonomicName>
) (in 
<collectingCountry box="[151,219,295,320]" name="Spain" pageId="11" pageNumber="12">Spain</collectingCountry>
), caterpillars of 
<emphasis box="[399,644,295,320]" italics="true" pageId="11" pageNumber="12">Dasychiria selenotica</emphasis>
(
<taxonomicName box="[657,799,295,320]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
