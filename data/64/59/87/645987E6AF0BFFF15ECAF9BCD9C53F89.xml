<taxonomicName box="[767,1003,151,176]" class="Magnoliopsida" family="Malvaceae" genus="Corchorus" kingdom="Plantae" order="Malvales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="species" species="olitorius">
<emphasis box="[767,1003,151,176]" italics="true" pageId="43" pageNumber="44">Corchorus olitorius</emphasis>
</taxonomicName>
(
<taxonomicName box="[1034,1142,151,176]" class="Magnoliopsida" family="Tiliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Tiliaceae</taxonomicName>
), 
<taxonomicName box="[1176,1436,151,176]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="species" species="geniculata">
<emphasis box="[1176,1436,151,176]" italics="true" pageId="43" pageNumber="44">Euphorbia geniculata</emphasis>
</taxonomicName>
(Euphorpbiaceae), 
<taxonomicName box="[363,565,188,213]" class="Gastropoda" family="Muricidae" genus="Tribulus" kingdom="Animalia" order="Neogastropoda" pageId="43" pageNumber="44" phylum="Mollusca" rank="species" species="terrestris">
<emphasis box="[363,565,188,213]" italics="true" pageId="43" pageNumber="44">Tribulus terrestris</emphasis>
</taxonomicName>
(
<taxonomicName box="[579,761,188,213]" class="Magnoliopsida" family="Zygophyllaceae" kingdom="Plantae" order="Zygophyllales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Zygophyllaceae</taxonomicName>
) (Sakenin 
<emphasis box="[881,930,188,213]" italics="true" pageId="43" pageNumber="44">et al</emphasis>
. 
<number box="[943,1003,188,212]" pageId="43" pageNumber="44" value="2008.0">2008</number>
), 
<taxonomicName box="[1022,1078,189,212]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="genus">
<emphasis box="[1022,1078,189,212]" italics="true" pageId="43" pageNumber="44">Rosa</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[1130,1239,188,212]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
) (
<bibRefCitation author="Nateq" pageId="43" pageNumber="44" refString="Nateq Golestan, M., Safaralizadeh, M. H. &amp; Sarafrazi, A. M. (2011) Faunistic study on Pentatomid bugs (Hem., Heteroptera) in West Azarbaijan province, Iran. Journal of Entomological Research, 2 (4), 319 - 328. [in Persian, English summary]" type="journal article" year="2011">
Nateq Golestan 
<emphasis box="[151,202,223,248]" italics="true" pageId="43" pageNumber="44">et al</emphasis>
. 
<number box="[218,277,224,248]" pageId="43" pageNumber="44" value="2011.0">2011</number>
</bibRefCitation>
); on numerous herbs of the families 
<taxonomicName box="[701,796,224,247]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
, 
<taxonomicName box="[809,935,224,248]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
, 
<taxonomicName box="[948,1070,223,248]" class="Magnoliopsida" family="Urticaceae" kingdom="Plantae" order="Rosales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Urticaceae</taxonomicName>
, 
<taxonomicName box="[1084,1280,223,248]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
, 
<taxonomicName authority="Ribes &amp; Pagola Carte 2013" authorityName="Ribes &amp; Pagola Carte" authorityYear="2013" class="Magnoliopsida" family="Rhamnaceae" kingdom="Plantae" order="Rosales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">
Rhamnaceae (Ribes &amp; Pagola Carte 
<number box="[411,472,260,284]" pageId="43" pageNumber="44" value="2013.0">2013</number>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
<subSubSection pageId="43" pageNumber="44" type="discussion">
<paragraph blockId="43.[151,1436,151,429]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,335,295,320]" pageId="43" pageNumber="44">Comments.</emphasis>
<taxonomicName authority="Nees" authorityName="Nees" box="[342,680,295,320]" class="Insecta" family="Scelionidae" genus="Trissolcus" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="semistriatus">
<emphasis box="[342,597,295,320]" italics="true" pageId="43" pageNumber="44">Trissolcus semistriatus</emphasis>
(Nees)
</taxonomicName>
(
<taxonomicName box="[694,853,296,320]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
: 
<taxonomicName box="[863,995,295,320]" class="Insecta" family="Scelionidae" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="family">Scelionidae</taxonomicName>
) destroys the eggs of 
<taxonomicName box="[1241,1353,297,320]" class="Insecta" family="Pentatomidae" genus="Palomena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="prasina">
<emphasis box="[1241,1353,297,320]" italics="true" pageId="43" pageNumber="44">P. prasina</emphasis>
</taxonomicName>
; adults and nymphs are prey for 
<taxonomicName box="[445,517,332,355]" class="Insecta" family="Crabronidae" genus="Astata" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis box="[445,517,332,355]" italics="true" pageId="43" pageNumber="44">Astata</emphasis>
</taxonomicName>
spp. (
<taxonomicName box="[589,747,332,356]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
: Crabonidae), 
<taxonomicName box="[916,1075,332,357]" class="Arachnida" family="Theridiidae" genus="Enoplognatha" kingdom="Animalia" order="Araneae" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis box="[916,1075,332,357]" italics="true" pageId="43" pageNumber="44">Enoplognatha</emphasis>
</taxonomicName>
spp. (Aranea: 
<taxonomicName box="[1247,1378,332,357]" class="Arachnida" family="Theridiidae" kingdom="Animalia" order="Araneae" pageId="43" pageNumber="44" phylum="Arthropoda" rank="family">Theridiidae</taxonomicName>
) and 
<taxonomicName box="[151,279,367,392]" class="Insecta" family="Reduviidae" genus="Rhynocoris" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis box="[151,279,367,392]" italics="true" pageId="43" pageNumber="44">Rhynocoris</emphasis>
</taxonomicName>
spp. (
<taxonomicName box="[357,495,368,392]" class="Insecta" family="Sphaeroceridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">Heteroptera</taxonomicName>
: 
<taxonomicName box="[511,644,367,392]" class="Insecta" family="Reduviidae" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="family">Reduviidae</taxonomicName>
). Also 
<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[732,1130,367,392]" class="Insecta" family="Tachinidae" genus="Gymnosoma" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="rotundatum">
<emphasis box="[732,1015,367,392]" italics="true" pageId="43" pageNumber="44">Gymnosoma rotundatum</emphasis>
Linnaeus
</taxonomicName>
and 
<taxonomicName authority="Rond." authorityName="Rond." authorityYear="1830" class="Insecta" family="Tachinidae" genus="Alophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="bonapartei">
<emphasis box="[1195,1436,367,392]" italics="true" pageId="43" pageNumber="44">Alophora bonapartei</emphasis>
Rond.
</taxonomicName>
(
<taxonomicName box="[233,323,404,429]" class="Insecta" family="Sphaeroceridae" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
: 
<taxonomicName box="[333,458,404,429]" class="Insecta" family="Tachinidae" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="family">Tachinidae</taxonomicName>
) are known endoparasites of 
<taxonomicName authority="Ribes &amp; Pagola Carte 2013" authorityName="Ribes &amp; Pagola Carte" authorityYear="2013" box="[786,1234,404,429]" class="Insecta" family="Pentatomidae" genus="Palomena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="prasina">
<emphasis box="[786,900,405,428]" italics="true" pageId="43" pageNumber="44">P. prasina</emphasis>
(Ribes &amp; Pagola Carte 
<number box="[1166,1227,404,428]" pageId="43" pageNumber="44" value="2013.0">2013</number>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
