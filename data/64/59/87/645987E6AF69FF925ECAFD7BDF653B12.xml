<collectingCountry box="[554,633,1166,1191]" name="Russia" pageId="72" pageNumber="73">Russia</collectingCountry>
, 
<collectingCountry box="[644,737,1166,1191]" name="Ukraine" pageId="72" pageNumber="73">Ukraine</collectingCountry>
, European 
<collectingCountry box="[862,997,1166,1191]" name="Kazakhstan" pageId="72" pageNumber="73">Kazakhstan</collectingCountry>
), 
<collectingCountry box="[1016,1100,1166,1191]" name="Algeria" pageId="72" pageNumber="73">Algeria</collectingCountry>
