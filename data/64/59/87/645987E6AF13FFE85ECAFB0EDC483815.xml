<collectingCountry box="[774,839,1487,1512]" name="Libya" pageId="50" pageNumber="51">Libya</collectingCountry>
and 
<collectingCountry box="[894,961,1488,1512]" name="Egypt" pageId="50" pageNumber="51">Egypt</collectingCountry>
