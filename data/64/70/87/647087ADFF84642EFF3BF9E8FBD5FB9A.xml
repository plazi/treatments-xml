<emphasis box="[948,1033,1683,1704]" italics="true" pageId="14" pageNumber="513">P. azami</emphasis>
</taxonomicName>
(39), 
<taxonomicName box="[1095,1224,1682,1704]" class="Insecta" family="Pamphagidae" genus="Prionotropis" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="513" phylum="Arthropoda" rank="species" species="rhodanica">
<emphasis box="[1095,1224,1682,1704]" italics="true" pageId="14" pageNumber="513">P. rhodanica</emphasis>
</taxonomicName>
(40), 
<taxonomicName box="[1285,1381,1682,1704]" class="Insecta" family="Pamphagidae" genus="Prionotropis" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="513" phylum="Arthropoda" rank="species" species="appula">
<emphasis box="[1285,1381,1682,1704]" italics="true" pageId="14" pageNumber="513">P. appula</emphasis>
</taxonomicName>
(41), 
<taxonomicName box="[151,260,1713,1735]" class="Insecta" family="Pamphagidae" genus="Prionotropis" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="513" phylum="Arthropoda" rank="species" species="flexuosa">
