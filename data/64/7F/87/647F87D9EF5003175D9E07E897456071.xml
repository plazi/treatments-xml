<taxonomicName authority="Semenov, 1910" authorityName="Semenov" authorityYear="1910" box="[649,1122,436,459]" class="Insecta" family="Meloidae" genus="Lytta" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="843" phylum="Arthropoda" rank="species" species="sifanica" subGenus="Poreospasta">
<emphasis box="[649,703,436,459]" italics="true" pageId="18" pageNumber="843">Lytta</emphasis>
(
<emphasis box="[718,850,436,459]" italics="true" pageId="18" pageNumber="843">Poreospasta</emphasis>
) 
<emphasis box="[865,949,436,459]" italics="true" pageId="18" pageNumber="843">sifanica</emphasis>
Semenov, 1910
</taxonomicName>
