<taxonomicName authority="Marshall" authorityName="Marshall" box="[624,882,562,588]" class="Insecta" family="Curculionidae" genus="Sympiezomias" kingdom="Animalia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="serratipes">
<emphasis box="[624,770,562,587]" italics="true" pageId="46" pageNumber="47">S. serratipes</emphasis>
<collectingCountry box="[778,882,562,588]" name="Marshall Islands" pageId="46" pageNumber="47">Marshall</collectingCountry>
</taxonomicName>
and hence the name 
<taxonomicName box="[1128,1315,562,587]" class="Insecta" family="Curculionidae" genus="Sympiezomias" kingdom="Animalia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="subserratipes" status="sp. nov.">
