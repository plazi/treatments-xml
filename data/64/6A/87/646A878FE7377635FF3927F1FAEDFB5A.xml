. 
<emphasis box="[750,853,1382,1407]" italics="true" pageId="28" pageNumber="29">Breadth:</emphasis>
male, 
<number box="[932,968,1382,1408]" pageId="28" pageNumber="29" value="2.5">2.5</number>
â€“3.0 mm; female, 
<quantity box="[1183,1356,1382,1408]" metricMagnitude="-3" metricUnit="m" metricValue="3.425" metricValueMax="3.6" metricValueMin="3.25" pageId="28" pageNumber="29" unit="mm" value="3.425" valueMax="3.6" valueMin="3.25">3.25â€“3.60 mm</quantity>
