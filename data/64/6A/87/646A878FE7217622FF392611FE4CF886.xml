<quantity box="[366,525,1674,1700]" metricMagnitude="-3" metricUnit="m" metricValue="8.175" metricValueMax="8.85" metricValueMin="7.5" pageId="10" pageNumber="11" unit="mm" value="8.175" valueMax="8.85" valueMin="7.5">7.5–8.85 mm</quantity>
; 
<emphasis box="[537,639,1675,1700]" italics="true" pageId="10" pageNumber="11">Breadth:</emphasis>
