<taxonomicName box="[369,515,734,756]" class="Insecta" family="Curculionidae" genus="Ioniorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="130" phylum="Arthropoda" rank="genus">
