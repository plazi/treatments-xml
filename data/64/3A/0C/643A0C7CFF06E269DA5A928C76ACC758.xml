<taxonomicName class="Gastropoda" family="Hemisinidae" genus="Pyrgulifera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pyrgulifera" order="[unassigned] Caenogastropoda" pageId="134" pageNumber="135" phylum="Mollusca" rank="genus">
<emphasis italics="true" pageId="134" pageNumber="135">Pyrgulifera</emphasis>
</taxonomicName>
