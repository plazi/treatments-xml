<subSubSection pageId="12" pageNumber="13" type="nomenclature">
<paragraph pageId="12" pageNumber="13">
12. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:215ECCF4-792A-4809-BC55-2E702B970739" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus augur" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="augur">Trigonopterus augur Riedel</taxonomicName>
