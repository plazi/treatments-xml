<taxonomicName baseAuthorityName="Bhatti" baseAuthorityYear="1967" box="[1351,1436,1877,1898]" class="Insecta" family="Thripidae" genus="Astrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="269" phylum="Arthropoda" rank="species" species="asiaticus">
