<collectingRegion box="[359,522,333,359]" country="Brazil" name="Roraima" pageId="3" pageNumber="280">Roraima State</collectingRegion>
, 
<collectingCountry box="[533,601,333,359]" name="Brazil" pageId="3" pageNumber="280">Brazil</collectingCountry>
(
<figureCitation box="[617,744,333,359]" captionStart="FIGURE 1" captionStartId="1.[151,250,1499,1524]" captionTargetBox="[157,1427,453,1474]" captionTargetId="figure@1.[151,1436,445,1480]" captionTargetPageId="1" captionText="FIGURE 1. Sampled sites where Elmidae species were collected in Roraima State, Brazil." figureDoi="http://doi.org/10.5281/zenodo.3602420" httpUri="https://zenodo.org/record/3602420/files/figure.png" pageId="3" pageNumber="280">Fig. 1—P1</figureCitation>
