<subSubSection pageId="15" pageNumber="16" type="reference_group">
<paragraph pageId="15" pageNumber="16">
<taxonomicName class="Arachnida" family="Euscorpiidae" genus="Euscorpiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euscorpiops xui" order="Scorpiones" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="xui">Euscorpiops xui</taxonomicName>
