<subSubSection id="C3756545FFADFF50FF3BE18DFB6C0AB5" box="[197,1259,1458,1484]" pageId="14" pageNumber="15" type="materials_examined">
<paragraph id="8BD036CEFFADFF50FF3BE18DFB6C0AB5" blockId="14.[151,1436,1378,1964]" box="[197,1259,1458,1484]" pageId="14" pageNumber="15">
<emphasis id="B91BEADCFFADFF50FF3BE18DFEB50AB5" bold="true" box="[197,306,1458,1484]" pageId="14" pageNumber="15">Material</emphasis>
