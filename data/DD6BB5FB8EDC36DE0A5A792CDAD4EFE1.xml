<taxonomicName id="183106E34FBD90B846DC081648F475B4" class="Insecta" family="Scarabaeidae" genus="Phanaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phanaeus halffterorum" order="Coleoptera" pageId="0" pageNumber="113" phylum="Arthropoda" rank="species" species="halffterorum">Phanaeus halffterorum</taxonomicName>
(red triangle), 
<taxonomicName id="A8AF9FAD9B32DB4B152B80A9D3C777B2" genus="P." lsidName="P. bravoensis" pageId="0" pageNumber="113" rank="species" species="bravoensis">P. bravoensis</taxonomicName>
