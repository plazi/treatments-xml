<taxonomicName id="D89169DA9036EBA4F1495E926375C44A" authorityName="P.Fischer" authorityYear="1873" class="Gastropoda" family="Amphibulimidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="family">Amphibulimidae</taxonomicName>
, 
<taxonomicName id="203603371CDCAF732FB2B51F888426B9" class="Gastropoda" family="Amphibulimidae" genus="Plekocheilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="pentadinus" subGenus="Eurytus">
<emphasis id="043C0667802B09EB9371C7E673E68DB1" italics="true" pageId="17" pageNumber="18">Plekocheilus (Eurytus) pentadinus</emphasis>
</taxonomicName>
(d'Orbigny, 1835).
</paragraph>
</subSubSection>
