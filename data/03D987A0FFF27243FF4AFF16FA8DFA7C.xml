<taxonomicName id="4C704D35FFF17243FA8FFADEF98FFA20" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="species" species="alidaeii">
