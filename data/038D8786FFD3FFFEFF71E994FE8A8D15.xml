<quantity id="4CDC9B75FFD3FFFEFCE8EB87FCF88D79" box="[782,825,1004,1028]" metricMagnitude="0" metricUnit="m" metricValue="4.0" pageId="44" pageNumber="45" unit="m" value="4.0">4 m</quantity>
