<taxonomicName id="D7F64D08DF5E721FC39BFEC5FD577677" box="[465,695,332,356]" class="Insecta" family="Blastobasidae" genus="Blastobasis" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="custodis">
