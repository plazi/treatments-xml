<taxonomicName id="D7F64D08DF5E721FC079FE1DFD6F76BF" box="[563,655,404,428]" class="Insecta" family="Blastobasidae" genus="Blastobasis" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="custodis">
