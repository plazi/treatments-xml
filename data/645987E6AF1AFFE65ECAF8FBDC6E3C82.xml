<taxonomicName id="2BF04D73AF1DFFE65F51FD20DC643C8D" box="[268,374,657,680]" class="Magnoliopsida" family="Olacaceae" genus="Brachynema" kingdom="Plantae" order="Santalales" pageId="60" pageNumber="61" phylum="Tracheophyta" rank="species" species="triguttata">
