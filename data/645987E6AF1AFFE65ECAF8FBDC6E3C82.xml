</taxonomicName>
, 
<emphasis id="DE84EAE2AF1DFFE65F0AFDF8DCB93C45" box="[343,427,585,608]" italics="true" pageId="60" pageNumber="61">prtensa</emphasis>
, 
<emphasis id="DE84EAE2AF1DFFE65FE4FDF6DF363C45" box="[441,548,583,608]" italics="true" pageId="60" pageNumber="61">confluens</emphasis>
, 
<emphasis id="DE84EAE2AF1DFFE65C6FFDF6DFB43C45" box="[562,678,583,608]" italics="true" pageId="60" pageNumber="61">sctutellata</emphasis>
, 
<emphasis id="DE84EAE2AF1DFFE65CE9FDF6DE1C3C45" box="[692,782,583,608]" italics="true" pageId="60" pageNumber="61">caudata</emphasis>
, 
<emphasis id="DE84EAE2AF1DFFE65D41FDF8DE6B3C45" box="[796,889,585,608]" italics="true" pageId="60" pageNumber="61">connexa</emphasis>
, 
<emphasis id="DE84EAE2AF1DFFE65DDAFDF6DEF33C45" box="[903,993,583,608]" italics="true" pageId="60" pageNumber="61">ocellata</emphasis>
