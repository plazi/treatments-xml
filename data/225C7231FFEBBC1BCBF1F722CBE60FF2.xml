<materialsCitation id="1A9DC97AFFE5BC1BCADAF2EFCFA10F1A" ID-GBIF-Occurrence="3053091305" collectingDate="2006-02-03" collectionCode="MNT" collectorName="K. Hill &amp; D. Marshall &amp; M. Moulds" country="Australia" elevation="77" latitude="-14.9148" location="Mataranka on Roper Bar Hwy." longLatPrecision="9" longitude="133.713" pageId="8" pageNumber="583" specimenCount="1" stateProvince="Northern Territory" typeStatus="holotype">
