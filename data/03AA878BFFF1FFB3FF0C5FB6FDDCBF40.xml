<number id="3DCB24BAFFF3FFB3FA8C5D09FABBBD60" box="[1303,1354,354,380]" pageId="3" pageNumber="60" value="0.95">0.95</number>
+ 
<number id="3DCB24BAFFF3FFB3FAF25D09FA6DBD60" box="[1385,1436,354,380]" pageId="3" pageNumber="60" value="0.67">0.67</number>
+ 
<number id="3DCB24BAFFF3FFB3FF355DE1FF17BDB8" box="[174,230,394,420]" pageId="3" pageNumber="60" value="0.5">0.50</number>
); II 
<number id="3DCB24BAFFF3FFB3FE8E5DE1FEB8BDB8" box="[277,329,394,420]" pageId="3" pageNumber="60" value="2.45">2.45</number>
(
<number id="3DCB24BAFFF3FFB3FECC5DE1FE7DBDB8" box="[343,396,394,420]" pageId="3" pageNumber="60" value="0.7">0.70</number>
+ 
<number id="3DCB24BAFFF3FFB3FE305DE1FE2FBDB8" box="[427,478,394,420]" pageId="3" pageNumber="60" value="0.73">0.73</number>
+ 
<number id="3DCB24BAFFF3FFB3FE665DE1FDC1BDB8" box="[509,560,394,420]" pageId="3" pageNumber="60" value="0.57">0.57</number>
+ 
<number id="3DCB24BAFFF3FFB3FDD55DE1FD77BDB8" box="[590,646,394,420]" pageId="3" pageNumber="60" value="0.45">0.45</number>
); III 
<number id="3DCB24BAFFF3FFB3FD255DE1FD00BDB8" box="[702,753,394,420]" pageId="3" pageNumber="60" value="2.05">2.05</number>
(
<number id="3DCB24BAFFF3FFB3FC9A5DE1FCC7BDB8" box="[769,822,394,420]" pageId="3" pageNumber="60" value="0.57">0.57</number>
+ 
<number id="3DCB24BAFFF3FFB3FCCF5DE1FC76BDB8" box="[852,903,394,420]" pageId="3" pageNumber="60" value="0.55">0.55</number>
+ 
<number id="3DCB24BAFFF3FFB3FC3D5DE1FC2BBDB8" box="[934,986,394,420]" pageId="3" pageNumber="60" value="0.43">0.43</number>
+ 
<number id="3DCB24BAFFF3FFB3FC635DE1FBC1BDB8" box="[1016,1072,394,420]" pageId="3" pageNumber="60" value="0.5">0.50</number>
