<taxonomicName id="4C37F6C35A2CFF839D5B0361E70F080D" authorityName="Jovet" authorityYear="1941" box="[542,632,1838,1864]" class="Magnoliopsida" family="Rubiaceae" genus="Astiella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="1" pageNumber="2" phylum="Tracheophyta" rank="genus">
