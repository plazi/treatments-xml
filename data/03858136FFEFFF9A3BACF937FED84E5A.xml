<taxonomicName id="4C2C4BA3FFE1FF9B3AFAFEA9FD944E25" box="[449,578,296,320]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="463" phylum="Arthropoda" rank="species" species="littoralis">
