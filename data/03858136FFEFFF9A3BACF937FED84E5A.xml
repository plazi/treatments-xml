subgroup; 
<quantity id="4CD49DC5FFE1FF9B3F4DFEA6FB0F4E5A" box="[1142,1241,295,319]" metricMagnitude="-1" metricUnit="m" metricValue="4.445" metricValueMax="5.334" metricValueMin="3.556" pageId="8" pageNumber="463" unit="in" value="17.5" valueMax="21.0" valueMin="14.0">14–21 in</quantity>
