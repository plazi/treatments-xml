); 
<number id="3DE42207FFE1FF9B3F65FE85FB724E79" box="[1118,1188,260,284]" pageId="8" pageNumber="463" value="39.0" valueMax="45.0" valueMin="33.0">33–45</number>
femoral pores (
<quantity id="4CD49DC5FFE1FF9B3E6EFE85FF7B4E5A" metricMagnitude="-1" metricUnit="m" metricValue="7.366" metricValueMax="7.874" metricValueMin="6.858" pageId="8" pageNumber="463" unit="in" value="29.0" valueMax="31.0" valueMin="27.0">27–31 in</quantity>
<taxonomicName id="4C2C4BA3FFE1FF9B3B88FEA9FE9C4E25" box="[179,330,296,320]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="463" phylum="Arthropoda" rank="species" species="abaetensis">
