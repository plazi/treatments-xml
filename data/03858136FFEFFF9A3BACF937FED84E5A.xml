<taxonomicName id="4C2C4BA3FFE1FF9B39DBFEA9FCB14E25" box="[736,871,296,320]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="463" phylum="Arthropoda" rank="species" species="cyanurus">
