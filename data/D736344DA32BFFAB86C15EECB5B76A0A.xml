<taxonomicName id="989FFED8A32BFFAB822D5C87B4506D0D" box="[1147,1299,763,789]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="suaveolens">
