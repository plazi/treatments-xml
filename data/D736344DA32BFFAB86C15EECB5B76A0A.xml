<taxonomicName id="989FFED8A32BFFAB85105C87B2886D0D" box="[838,971,763,789]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="ochracea">
<emphasis id="6DEB5949A32BFFAB85105C87B2886D0D" box="[838,971,763,789]" italics="true" pageId="7" pageNumber="324">T. ochracea</emphasis>
</taxonomicName>
