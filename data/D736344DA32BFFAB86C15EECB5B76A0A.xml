(
<taxonomicName id="989FFED8A32BFFAB84FE5C87B2746D0D" box="[680,823,763,789]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="versicolor">
