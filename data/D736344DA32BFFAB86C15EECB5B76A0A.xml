<taxonomicName id="989FFED8A32BFFAB84795C87B3D56D0D" authorityName="E.M.Fries" authorityYear="1836" box="[559,662,764,789]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="genus">
