<taxonomicName id="989FFED8A32BFFAB855C5D64B2B66D21" box="[778,1013,799,825]" class="Agaricomycetes" family="Polyporaceae" genus="Hapalopilus" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="rutilans">
