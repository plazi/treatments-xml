<taxonomicName id="989FFED8A32BFFAB86C15D5BB01F6D21" box="[151,348,799,826]" class="Agaricomycetes" family="Polyporaceae" genus="Cerrena" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="unicolor">
