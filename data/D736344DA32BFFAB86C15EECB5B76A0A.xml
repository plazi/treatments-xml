<taxonomicName id="989FFED8A32BFFAB82505D5BB5A36D21" box="[1030,1248,799,825]" class="Agaricomycetes" family="Polyporaceae" genus="Fomes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="fomentarius">
