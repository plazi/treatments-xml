<taxonomicName id="989FFED8A32BFFAB834D5D5BB1BA6D45" class="Agaricomycetes" family="Hymenochaetaceae" genus="Fuscoporia" kingdom="Fungi" order="Hymenochaetales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="contigua">
