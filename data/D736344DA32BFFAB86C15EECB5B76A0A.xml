<taxonomicName id="989FFED8A32BFFAB858D5C87B5286D0D" box="[987,1131,763,789]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="324" phylum="Basidiomycota" rank="species" species="pubescens">
<emphasis id="6DEB5949A32BFFAB858D5C87B5286D0D" box="[987,1131,763,789]" italics="true" pageId="7" pageNumber="324">T. pubescens</emphasis>
</taxonomicName>
