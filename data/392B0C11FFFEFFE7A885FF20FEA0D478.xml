<taxonomicName id="7682C684FFFEFFE7A90DFF20FE2AD5F5" box="[287,466,152,178]" class="Insecta" family="Mantispidae" kingdom="Animalia" order="Neuroptera" pageId="7" pageNumber="408" phylum="Arthropoda" rank="subFamily" subFamily="Symphrasinae">Symphrasinae</taxonomicName>
