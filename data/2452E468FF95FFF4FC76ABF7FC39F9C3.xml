<subSection id="C5744C95FF90FFF4FF79A997FC39F9C3" pageId="4" pageNumber="5" type="multiple">
<paragraph id="AC44557EFF90FFF4FF79A997FDC6F943" blockId="4.[170,527,1722,1747]" box="[170,527,1722,1747]" pageId="4" pageNumber="5">
<emphasis id="9E8F896CFF90FFF4FF79A997FF38F943" bold="true" box="[170,241,1722,1747]" pageId="4" pageNumber="5">Color:</emphasis>
