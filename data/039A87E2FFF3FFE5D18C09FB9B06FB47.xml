<taxonomicName id="4C334D77FFF3FFE5D18C09FB9D6FFD7F" authority="Bergroth, 1905" authorityName="Bergroth" authorityYear="1905" box="[151,525,704,731]" class="Insecta" family="Reduviidae" genus="Diarthrotarsus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="385" phylum="Arthropoda" rank="genus">
<emphasis id="B947EAE6FFF3FFE5D18C09FB9D6FFD7F" bold="true" box="[151,525,704,731]" pageId="4" pageNumber="385">
<emphasis id="B947EAE6FFF3FFE5D18C09FB9E28FD7F" bold="true" box="[151,330,704,731]" italics="true" pageId="4" pageNumber="385">Diarthrotarsus</emphasis>
<bibRefCitation id="EFA24B05FFF3FFE5D04909FA9D6FFD7F" author="Bergroth, E." box="[338,525,705,731]" pageId="4" pageNumber="385" pagination="104 - 112" refId="ref10615" refString="Bergroth, E. (1905) Rhynchota Neotropica. Revue D'Entomologie, 24 (5 - 6), 104 - 112." type="journal article" year="1905">Bergroth, 1905</bibRefCitation>
