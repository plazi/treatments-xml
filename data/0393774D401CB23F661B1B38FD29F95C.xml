<taxonomicName id="4C3ABDD8401CB23F602D1AC8FAFFFA18" authority="Ng, 1995" authorityName="Ng" authorityYear="1995" box="[1141,1399,1417,1444]" class="Malacostraca" family="Hymenosomatidae" genus="Neorhynchoplax" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="dentata">
