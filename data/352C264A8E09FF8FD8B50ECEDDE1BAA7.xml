, empty shell in 
<quantity id="7A7D3AB98E09FF8FDC100C03DDF9BD91" box="[1111,1230,890,914]" metricMagnitude="2" metricUnit="m" metricValue="3.425" metricValueMax="3.5" metricValueMin="3.35" pageId="80" pageNumber="561" unit="m" value="342.5" valueMax="350.0" valueMin="335.0">335-350 m</quantity>
