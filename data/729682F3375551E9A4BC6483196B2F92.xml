<taxonomicName id="1636581C7E266C7DD7FB4CD1963D6FE5" class="Magnoliopsida" family="Rosaceae" genus="Sorbus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Sorbus insignis" order="Rosales" pageId="0" pageNumber="29" phylum="Tracheophyta" rank="species" species="insignis">
<emphasis id="AF6FDF8C521C8F4FF719F8F105B17581" italics="true" pageId="0" pageNumber="29">Sorbus insignis</emphasis>
