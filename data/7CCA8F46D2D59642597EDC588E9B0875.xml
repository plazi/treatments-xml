<taxonomicName id="8D43DCBD8677869349DDF7F98B010415" class="Insecta" family="Coccinellidae" genus="Coccinella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coccinella rufipes" order="Coleoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="rufipes">Coccinella rufipes</taxonomicName>
