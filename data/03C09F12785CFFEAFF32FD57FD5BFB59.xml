<taxonomicName id="4C695587785CFFEAFC84FBA7FB65FBF5" authority="(Kutz.) Kylin" authorityName="Kylin" baseAuthorityName="Kutz." box="[830,1241,1098,1120]" class="Florideophyceae" family="Delesseriaceae" genus="Erythroglossum" kingdom="Plantae" order="Ceramiales" pageId="6" pageNumber="130" phylum="Rhodophyta" rank="species" species="sandrianum">
