<taxonomicName id="4C695587785CFFEAFDA4FBA7FD2CFBCA" box="[542,656,1098,1119]" class="Florideophyceae" family="Delesseriaceae" genus="Asterocolax" kingdom="Plantae" order="Ceramiales" pageId="6" pageNumber="130" phylum="Rhodophyta" rank="genus">
