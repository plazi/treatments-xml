<treatmentCitation id="0A90C32D1972FF9E68C1953F1BEAFAC2" author="Mathews, G. M." box="[701,1227,1328,1348]" page="444" pageId="77" pageNumber="77" year="1912">
<taxonomicName id="4C319EBF1972FF9E68C1953F1BEAFAC2" authority="Mathews, 1912 a: 444" authorityName="Mathews" authorityPageNumber="444" authorityYear="1912" box="[701,1227,1328,1348]" class="Aves" family="Cracticidae" genus="Strepera" kingdom="Animalia" order="Passeriformes" pageId="77" pageNumber="77" phylum="Chordata" rank="subSpecies" species="melanoptera" subSpecies="howei">
<emphasis id="B945392E1972FF9E68C1953F1CC1FAC2" box="[701,992,1328,1348]" italics="true" pageId="77" pageNumber="77">Strepera melanoptera howei</emphasis>
<bibRefCitation id="EFA098CD1972FF9E6995953F1BEAFAC2" author="Mathews" box="[1001,1227,1329,1348]" firstAuthor="Mathews" page="444" pageId="77" pageNumber="77" pagination="171 - 446" refId="ref129241" refString="Mathews, G. M. 1912 a. A reference-list to the birds of Australia. Novitates Zoologicae 18: 171 - 446." type="journal article" year="1912" yearSuffix="a">Mathews, 1912a: 444</bibRefCitation>
</taxonomicName>
</treatmentCitation>
(Kow Plains, 
<collectingRegion id="49F52BDE1972FF9E691595451CE3FAD8" box="[873,962,1355,1374]" country="Australia" name="Victoria" pageId="77" pageNumber="77">Victoria</collectingRegion>
