<taxonomicName id="4C1086CEFFDB3C51628831BFFD0575F2" authority=", USNM PAL" authorityName="USNM PAL" class="Insecta" family="Atelestidae" genus="Alavesia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="longistylata">
<emphasis id="B964215FFFDB3C51628831BFFCBF75F2" italics="true" pageId="23" pageNumber="24">A. longistylata</emphasis>
, USNM PAL
