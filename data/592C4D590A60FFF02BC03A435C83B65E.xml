UF 
<number id="674DEE680A60FFF02BE83A025E61B5FE" box="[191,271,1403,1426]" pageId="0" pageNumber="66" value="121604.0">
<date id="A53BDA8F0A60FFF02BE83A025E61B5FE" box="[191,271,1403,1426]" pageId="0" pageNumber="66" value="04-12-16">121604</date>
</number>
