<taxonomicName id="4C35E5E8FF9DE643949FEB3AFE44F9C6" authority="Malicky 2006" authorityName="Malicky" authorityYear="2006" class="Insecta" family="Leptoceridae" genus="Ceraclea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="1" phylum="Arthropoda" rank="species" species="troilos">
