<emphasis id="D1E9EAF3FFFD7158979B987B04F8FCEA" bold="true" box="[151,490,799,825]" pageId="7" pageNumber="8">
<taxonomicName id="249D4D62FFFD7158979B987B0497FCEA" box="[151,389,799,825]" class="Insecta" family="Hybotidae" genus="Chersodromia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="leleji" status="sp. nov.">
<emphasis id="D1E9EAF3FFFD7158979B987B0497FCEA" bold="true" box="[151,389,799,825]" italics="true" pageId="7" pageNumber="8">Chersodromia leleji</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CADA5788FFFD71589680987B04F8FCEA" box="[396,490,799,825]" pageId="7" pageNumber="8" rank="species">sp. nov.</taxonomicNameLabel>
