<taxonomicName id="74EB4D7FFFFCFFC7FC4A28EAF52AF7D7" authority="Mendonca &amp; Nunes &amp; Fernandes, 2021" authorityName="Mendonca &amp; Nunes &amp; Fernandes" authorityYear="2021" box="[928,1083,2036,2055]" class="Insecta" family="Pentatomidae" genus="Hypoxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="300" phylum="Arthropoda" rank="species" species="bergrothi" status="sp. nov.">
