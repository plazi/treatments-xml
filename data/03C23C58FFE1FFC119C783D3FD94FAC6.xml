from Solnhofen: specimen in natural light (
<emphasis id="B91F515CFFE2FFDE190382ABFCF2F837" bold="true" box="[770,783,1964,1982]" pageId="14" pageNumber="655">A</emphasis>
), specimen in UV light (
<emphasis id="B91F515CFFE2FFDE19D482ABFC1FF837" bold="true" box="[981,994,1964,1982]" pageId="14" pageNumber="655">B</emphasis>
) and line drawing (
<emphasis id="B91F515CFFE2FFDE1E8082ABFB72F837" bold="true" box="[1153,1167,1964,1982]" pageId="14" pageNumber="655">C</emphasis>
); 
<emphasis id="B91F515CFFE2FFDE1E9E82ABFB50F837" bold="true" box="[1183,1197,1964,1982]" pageId="14" pageNumber="655">D</emphasis>
, specimen MNHN.F.B13445 of 
<taxonomicName id="4C6BF6CDFFE2FFDE1A8582C4FE52F85C" authority="(Schlotheim, 1822)" authorityName="" baseAuthorityName="Schlotheim" baseAuthorityYear="1822" box="[132,431,1987,2005]" class="Pezizomycetes" family="Pyronemataceae" genus="Pustulina" higherTaxonomySource="GBIF" kingdom="Fungi" order="Pezizales" pageId="14" pageNumber="655" phylum="Ascomycota" rank="species" species="minuta">
