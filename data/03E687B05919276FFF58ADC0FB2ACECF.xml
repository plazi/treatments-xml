<collectingCountry id="F35876365919276EFCE8AFADFCA8CBEC" box="[807,851,1936,1962]" name="Fiji" pageId="30" pageNumber="31">Fiji</collectingCountry>
, and the 
<collectingCountry id="F35876365919276EFC74AFADFBB9CBEC" box="[955,1090,1936,1962]" name="Philippines" pageId="30" pageNumber="31">Philippines</collectingCountry>
