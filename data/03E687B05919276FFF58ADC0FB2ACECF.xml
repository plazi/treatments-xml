Known from 
<collectingCountry id="F35876365919276EFDC3AFADFD3FCBEC" box="[524,708,1936,1962]" name="New Caledonia" pageId="30" pageNumber="31">New Caledonia</collectingCountry>
, 
<collectingCountry id="F35876365919276EFD1FAFADFCE7CBEC" box="[720,796,1936,1962]" name="Tonga" pageId="30" pageNumber="31">Tonga</collectingCountry>
