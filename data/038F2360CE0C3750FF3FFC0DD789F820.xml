<taxonomicName id="4C26E9F5CE0C3750FB24F841D75CF86D" authority="Just, 2019" authorityName="Just" authorityYear="2019" box="[1164,1353,1957,1979]" class="Malacostraca" family="Ischyroceridae" genus="Rhinoecetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="102" phylum="Arthropoda" rank="species" species="caetus" status="sp. nov.">
