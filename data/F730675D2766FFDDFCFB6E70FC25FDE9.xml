<taxonomicName id="B899ADC82767FFDDFB1E6D43FA44FDB0" box="[1239,1422,527,546]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="virginiana">Quercus virginiana</taxonomicName>
