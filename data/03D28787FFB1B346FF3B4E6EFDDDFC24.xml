<taxonomicName id="4C7B4D12FFB6B346FEE84A8DFE7DFC07" authorityName="C.A.Viraktamath" authorityYear="2011" box="[324,428,949,972]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="javana">
