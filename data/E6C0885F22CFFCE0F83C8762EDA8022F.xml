<taxonomicName id="E217C858F1C026779019794CF68E56BA" class="Lecanoromycetes" family="Parmeliaceae" genus="Platismatia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Platismatia glauca" order="Lecanorales" pageId="7" pageNumber="38" phylum="Ascomycota" rank="species" species="glauca">Platismatia glauca</taxonomicName>
