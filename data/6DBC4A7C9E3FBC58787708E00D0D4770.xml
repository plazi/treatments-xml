<pageStartToken id="EDA18F8827BC392B7A8AB8DEE12455B7" pageNumber="45">Figure</pageStartToken>
2. 
<taxonomicName id="C10F18885F28F1CAF0F6A4B5EE6D5746" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:247389" class="Insecta" family="Scelionidae" genus="Nixonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Nixonia mcgregori van Noort &amp; Johnson" order="Hymenoptera" pageNumber="45" phylum="Arthropoda" rank="species" species="mcgregori">Nixonia mcgregori van Noort &amp; Johnson</taxonomicName>
, 
<taxonomicNameLabel id="D3FC9CFCBC95EF9BC3AB15EEB1CD6F46" rank="species">sp. n.</taxonomicNameLabel>
, female, 
