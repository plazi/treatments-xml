<emphasis id="39A1EADFFF98C076FE4AF86CFDA6F86C" box="[470,623,1939,1960]" italics="true" pageId="12" pageNumber="324">
<taxonomicName id="CCD54D4EFF98C076FE4AF86CFDA2F86C" authorityName="Dobson" authorityYear="1875" box="[470,619,1939,1960]" class="Mammalia" family="Vespertilionidae" genus="Glauconycteris" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="324" phylum="Chordata" rank="genus">Glauconycteris</taxonomicName>
