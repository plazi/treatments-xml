<taxonomicName id="4C387A80FFE0CE7488122300F741FCB3" authority="Danai 1852" authorityName="Danai" authorityYear="1852" box="[868,1098,802,826]" class="Malacostraca" family="Caprellidae" genus="Protella" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="genus">
<emphasis id="B94CDD11FFE0CE7488122300F0BDFCB3" box="[868,950,802,826]" italics="true" pageId="16" pageNumber="1038">Protella</emphasis>
Danaı 1852
</taxonomicName>
and 
<taxonomicName id="4C387A80FFE0CE748BD62366F2CEFCD5" authority="Mayeri 1903" authorityName="Mayeri" authorityYear="1903" box="[160,453,836,860]" class="Malacostraca" family="Caprellidae" genus="Orthoprotella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="1038" phylum="Arthropoda" rank="genus">
