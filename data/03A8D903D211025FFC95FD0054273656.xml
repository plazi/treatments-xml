<taxonomicName id="4C011396D211025FFC02FC5B527330F8" authority="Morlon-Guyot et al. 1998" authorityName="Morlon-Guyot" authorityYear="1998" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="30" pageNumber="2812" phylum="Firmicutes" rank="species" species="manihotivorans">
