<taxonomicName id="4EB58FAAC1F6676D29C8006875C451D2" authorityName="Yin" authorityYear="2019" class="Insecta" family="Staphylinidae" genus="Chandleriella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chandleriella yunnanica" order="Coleoptera" pageId="0" pageNumber="135" phylum="Arthropoda" rank="species" species="yunnanica">Chandleriella yunnanica</taxonomicName>
