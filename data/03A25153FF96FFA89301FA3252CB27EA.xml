<taxonomicName id="4C0B9BC6FF96FFA99017F8B35209231C" authorityName="Schockaert &amp; Curini-Galletti &amp; Ridder &amp; Volonterio &amp; Artois" authorityYear="2009" box="[528,615,1850,1871]" class="Rhabditophora" family="Meidiamidae" genus="Serrula" kingdom="Animalia" order="Seriata" pageId="8" pageNumber="767" phylum="Platyhelminthes" rank="genus">
