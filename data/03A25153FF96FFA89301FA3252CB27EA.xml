<taxonomicName id="4C0B9BC6FF96FFA990D8F8D053F02250" box="[735,926,1518,1902]" class="Rhabditophora" family="Meidiamidae" genus="Serrula" kingdom="Animalia" order="Seriata" pageId="8" pageNumber="767" phylum="Platyhelminthes" rank="species" species="maxillaria">
