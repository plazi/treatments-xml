<taxonomicName id="4C764D47FF89AB109D1811FD45ED39D2" authorityName="Lane" authorityYear="1939" box="[839,944,472,497]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="flavitibia">
