<emphasis id="A9421113E1CF4805F1EE1909349C6CE5" bold="true" pageId="0" pageNumber="139">Figures 8-14.</emphasis>
<taxonomicName id="FD69DBEA61B3916FF30289971827FE63" authorityName="Nguyen &amp; Carpenter" authorityYear="2020" class="Insecta" family="Eumenidae" genus="Labus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labus obtusus" order="Hymenoptera" pageId="0" pageNumber="139" phylum="Arthropoda" rank="species" species="obtusus">
<emphasis id="D966FD5485074A9B654E1F1B41E79A26" italics="true" pageId="0" pageNumber="139">Labus obtusus</emphasis>
