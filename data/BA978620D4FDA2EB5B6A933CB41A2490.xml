<taxonomicName id="D16E921772AE47400969AFC00971AA09" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia indica" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="indica">Loboscelidia indica</taxonomicName>
