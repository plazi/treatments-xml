<date id="FF911029851EFF9EFBF31D31ECEA1CB4" box="[1139,1245,1901,1924]" pageId="0" pageNumber="81">
<number id="3DE724CE851EFF9EFBF31D31ECB71CB4" box="[1139,1152,1901,1924]" pageId="0" pageNumber="81" value="4.0">4</number>
January
</date>
, 
<number id="3DE724CE851EFF9EFB691D31ED291CB4" box="[1257,1310,1901,1924]" pageId="0" pageNumber="81" value="1950.0">1950</number>
(Record of State registration # 15, 50- 
<number id="3DE724CE851EFF9EFE371DCCE9D61C97" box="[439,481,1936,1959]" pageId="0" pageNumber="81" value="521.0">521</number>
