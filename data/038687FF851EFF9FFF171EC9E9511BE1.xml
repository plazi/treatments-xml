<taxonomicName id="4C2F4D6A851FFF9FFE461AC4EBF01B9F" authority="Bogdanowicz &amp; Woloszynova 1949" authorityName="Bogdanowicz &amp; Woloszynova" authorityYear="1949" box="[454,967,152,175]" class="Chromadorea" family="Cystidicolidae" genus="Dogielina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="1" pageNumber="82" phylum="Nematoda" rank="genus">
