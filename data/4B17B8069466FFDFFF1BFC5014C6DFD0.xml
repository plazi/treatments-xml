], 
<quantity id="0446A4F59466FFD8FEECFB7015CCDA08" box="[352,435,1267,1293]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="11" pageNumber="462" unit="m" value="1100.0">1100 m</quantity>
a.s.l., 
<date id="B7002FD09466FFD8FE70FB701603DA08" box="[508,636,1267,1293]" pageId="11" pageNumber="462" value="1967-06-23">23.06.1967</date>
, 
<specimenCount id="D5B8C2999466FFD8FD04FB7016BFDA08" box="[648,704,1267,1293]" pageId="11" pageNumber="462" type="generic">1 ex.</specimenCount>
, exp. Dr. Z. Kaszab (
<bibRefCitation id="A72F74E19466FFD8FC20FB701014DA08" author="Heyrovsky, L." box="[940,1131,1267,1293]" pageId="11" pageNumber="462" pagination="137 - 142" refId="ref22940" refString="Heyrovsky, L. (1970) Ergebnisse der zoologischen Forschungen von Dr. Z. Kaszab in der Mongolei, 199. Cerambycidae V (Coleoptera). Reichenbachia, 13 (13), 137 - 142." type="journal article" year="1970">Heyrovský 1970</bibRefCitation>
: as 
<taxonomicName id="04BE72939466FFD8FB18FB701126DA08" box="[1172,1369,1267,1293]" class="Insecta" family="Cerambycidae" genus="Asias" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="462" phylum="Arthropoda" rank="species" species="mongolicus">
