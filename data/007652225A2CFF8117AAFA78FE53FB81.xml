<collectionCode id="EECE7BF15A2CFF821679F846FE6EF814" box="[324,409,2009,2033]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="13" pageNumber="314">USNM</collectionCode>
