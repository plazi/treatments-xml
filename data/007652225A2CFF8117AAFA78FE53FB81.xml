<collectionCode id="EECE7BF15A2CFF821430F82AFC97F828" box="[781,864,1973,1997]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="13" pageNumber="314">USNM</collectionCode>
