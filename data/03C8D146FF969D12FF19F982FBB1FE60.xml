<quantity id="4C99CDB5FF959D13FC5DFF71FBFFFF31" box="[979,1068,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="38" pageNumber="189" unit="mm" value="1.7">1.7 mm</quantity>
