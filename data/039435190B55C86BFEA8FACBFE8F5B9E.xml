<taxonomicName id="4C3DFF8C0B54C868FDCEFA20F96E5CBD" authority="(Walker, 1856) (ROSSARO et al. 2003)" authorityName="ROSSARO et al." authorityYear="2003" baseAuthorityName="Walker" baseAuthorityYear="1856" box="[561,1085,1433,1456]" class="Insecta" family="Chironomidae" genus="Orthocladius" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="740" phylum="Arthropoda" rank="species" species="oblidens">
