<taxonomicName id="4C3DFF8C0B54C868FBD8FAC0FC2E5CBD" authority="Kieffer, 1911" authorityName="Kieffer" authorityYear="1911" class="Insecta" family="Chironomidae" genus="Orthocladius" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="740" phylum="Arthropoda" rank="species" species="rhyacobius">
