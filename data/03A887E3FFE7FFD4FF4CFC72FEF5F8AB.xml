<taxonomicName id="4C014D76FFE7FFD4FCBDF911FC21F895" box="[870,1008,1778,1804]" class="Insecta" family="Geometridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="47" phylum="Arthropoda" rank="family">Ennominae</taxonomicName>
