<taxonomicName id="4C1E3ED2FFD16D12FF4AFA4DFE65ECE0" authority="Rossmassler, 1838" authorityName="Rossmassler" authorityYear="1838" box="[151,490,1425,1447]" class="Bivalvia" family="Unionidae" genus="Unio" kingdom="Animalia" order="Unionoida" pageId="15" pageNumber="108" phylum="Mollusca" rank="species" species="muelleri">
