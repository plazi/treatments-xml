<taxonomicName id="00A34D6CF713FF99FEE7C79DFDBA33DE" authority="Friese" authorityName="Friese" box="[302,539,1757,1779]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="285" phylum="Arthropoda" rank="species" species="latipes">
<taxonomicName id="00A34D6CF713FF99FEE7C79DFE7133DE" box="[302,464,1757,1779]" class="Insecta" family="Colletidae" genus="Colletes" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="285" phylum="Arthropoda" rank="species" species="latipes">
