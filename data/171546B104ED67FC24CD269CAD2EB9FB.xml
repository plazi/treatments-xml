<taxonomicName id="1D9D8D77DFC3C5E440C1773053D5074B" LSID-ZBK="urn:lsid:zoobank.org:act:347CE897-3860-4DA6-85B8-033B2318B81A" class="Actinopterygii" family="Ariidae" genus="Aspistor" kingdom="Animalia" lsidName-ZBK="Aspistor Jordan &amp; Evermann 1898:2763" order="Siluriformes" pageNumber="60" phylum="Chordata" rank="genus">Aspistor</taxonomicName>
, 
<taxonomicName id="F18F79BCB53FD49CDF5EE5BFF6D69CA6" class="Actinopterygii" family="Ariidae" genus="Cathorops" kingdom="Animalia" order="Siluriformes" pageNumber="60" phylum="Chordata" rank="genus">Cathorops</taxonomicName>
, 
