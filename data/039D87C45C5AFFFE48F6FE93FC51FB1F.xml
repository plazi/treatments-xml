<taxonomicName id="4C344D515C5AFFFE48F6FE68FE2BFE31" authority="Evans, 1966" authorityName="Evans" authorityYear="1966" box="[151,485,438,461]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="trinitatis">
