<taxonomicName id="4C7A4DC69E1966554FCCFBB56BD8FBC7" authorityName="Chalumeau" authorityYear="1981" box="[428,656,1029,1052]" class="Insecta" family="Aphodiidae" genus="Haroldiataenius" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis id="B90EEA579E1966554FCCFBB56BD8FBC7" bold="true" box="[428,656,1029,1052]" italics="true" pageId="3" pageNumber="2">Haroldiataenius</emphasis>
