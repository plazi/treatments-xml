<taxonomicName id="4C3E468FFFAB0555FD75FB0FD41BFA8C" authorityName="Kraatz" authorityYear="1856" box="[767,885,1262,1283]" class="Insecta" family="Staphylinidae" genus="Oxypoda" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="125" phylum="Arthropoda" rank="species" species="formosa">
