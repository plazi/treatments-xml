<taxonomicName id="36C01A1BD78CF8307128F4C888FB7E73" authorityName="Planch &amp; Linden ex Hook" authorityYear="1862" class="Incertae sedis" family="Incertae sedis" genus="Heliconia" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Heliconia metallica" order="Incertae sedis" pageId="0" pageNumber="65" phylum="Incertae sedis" rank="species" species="metallica">
