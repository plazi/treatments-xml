<taxonomicName id="1FF94D24FFCFFF8BFEEA0B56FD8AA6EE" authority="Aube" authorityName="Aube" authorityYear="1836" box="[346,531,624,650]" class="Insecta" family="Haliplidae" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="251" phylum="Arthropoda" rank="family">Haliplidae Aubé</taxonomicName>
