<specimenCount id="7CA636A8FFF60D605B01C547FEA7CDCA" box="[315,351,1654,1676]" pageId="4" pageNumber="145" type="generic">
<emphasis id="58D42133FFF60D605B01C547FEB0CDCA" bold="true" box="[315,328,1654,1676]" pageId="4" pageNumber="145">4</emphasis>
