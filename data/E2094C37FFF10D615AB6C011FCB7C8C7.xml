<taxonomicName id="ADA086A2FFF10D675AB6C011FD8EC87C" authority="Jameson &amp; Wada, 2009" authorityName="Jameson &amp; Wada" authorityYear="2009" box="[140,630,800,827]" class="Insecta" family="Dynastidae" genus="Peltonotus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="144" phylum="Arthropoda" rank="species" species="cybele">
