, 
<taxonomicName id="4C604D6DFFA1FFA2BD71ADFEFD17F946" authority="Baldizzone &amp; van der Wolf" authorityName="Baldizzone &amp; van der Wolf" authorityYear="2011" box="[267,665,1681,1706]" class="Insecta" family="Coleophoridae" genus="Coleophora" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="etoshae">
<taxonomicName id="4C604D6DFFA1FFA2BD71ADFEFE61F946" authority="Baldizzone" authorityName="Baldizzone" box="[267,495,1681,1706]" genus="C." pageId="16" pageNumber="167" rank="species" species="etoshae">
