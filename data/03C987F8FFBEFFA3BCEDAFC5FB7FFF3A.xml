<taxonomicName id="4C604D6DFFA1FFA2BE5EAD27FD13F98E" authorityName="Hubner" authorityYear="1822" box="[548,669,1610,1634]" class="Insecta" family="Coleophoridae" genus="Coleophora" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="genus">
<emphasis id="B914EAFCFFA1FFA2BE5EAD27FD13F98E" box="[548,669,1610,1634]" italics="true" pageId="16" pageNumber="167">Coleophora</emphasis>
</taxonomicName>
spp. 
<emphasis id="B914EAFCFFA1FFA2BEA9AD24FD62F98E" bold="true" box="[723,748,1609,1634]" pageId="16" pageNumber="167">42</emphasis>
, 
<taxonomicName id="4C604D6DFFA1FFA2BE83AD26FC5DF98E" authority="Meyrik" authorityName="Meyrick" authorityYear="1921" box="[761,979,1609,1634]" class="Insecta" family="Coleophoridae" genus="Coleophora" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="presbytica">
