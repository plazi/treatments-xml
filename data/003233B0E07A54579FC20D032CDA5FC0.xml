<subSubSection id="D9E4289A98492D5E45357B6F716D31E9" pageId="0" pageNumber="49" type="etymology">
<paragraph id="7A4E25DD6E9702BC6E4810ADE9830575" pageId="0" pageNumber="49">Etymology.</paragraph>
<paragraph id="D4626F7EA93E8EC5B8C438304E64DEA3" pageId="0" pageNumber="49">
<emphasis id="DA211EECFCDC6E1E77DDB2A9461CB8DA" italics="true" pageId="0" pageNumber="49">Viklundi</emphasis>
