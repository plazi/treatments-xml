<taxonomicName id="4C3152ECFF92FF9BFEB34346FDB08A4A" baseAuthorityName="KRAATZ" baseAuthorityYear="1859" box="[355,554,1078,1099]" class="Insecta" family="Staphylinidae" genus="Rugilus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="80" phylum="Arthropoda" rank="species" species="ceylanensis">
