<quantity id="5922B2AFFFBEF51EFC1EFCADFF4246EB" metricMagnitude="3" metricUnit="m" metricValue="2.29" metricValueMax="2.3" metricValueMin="2.28" pageId="9" pageNumber="410" unit="m" value="2290.0" valueMax="2300.0" valueMin="2280.0">
