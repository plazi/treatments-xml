<location id="9B054991FFBEF51EFE15FBD0FD9C4145" LSID="urn:lsid:plazi:treatment:1673AE5CFFBEF51EFF7AFCECFD2041A6:9B054991FFBEF51EFE15FBD0FD9C4145" box="[504,566,1055,1079]" county="Specimen" municipality="The" name="There" pageId="9" pageNumber="410" stateProvince="Piemonte">There</location>
