<specimenCount id="88DCD4C3FFBEF51EFD35FBD0FCBB4145" box="[728,785,1053,1079]" pageId="9" pageNumber="410" type="female">17 ♀</specimenCount>
<typeStatus id="4161A1E8FFBEF51EFCF7FBD0FC2A4145" box="[794,896,1055,1079]" pageId="9" pageNumber="410">paratypes</typeStatus>
