<collectionCode id="F8CB878FFFBEF51EFE82FBB2FE6A41E7" box="[367,448,1149,1173]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="9" pageNumber="410" type="Museum">MHNG</collectionCode>
