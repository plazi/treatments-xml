<typeStatus id="1F75E7BC341D4711FECC45E574F0F845" box="[355,465,1973,1999]" pageId="6" pageNumber="60">Holotype</typeStatus>
11.8 x 
<quantity id="0736F4FB341D4711FD8445E577A8F845" box="[555,649,1973,1999]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" pageId="6" pageNumber="60" unit="mm" value="4.6">4.6 mm</quantity>
, aperture height 
<quantity id="0736F4FB341D4711FCFE45E57691F845" box="[849,944,1973,1999]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="6" pageNumber="60" unit="mm" value="3.9">3.9 mm</quantity>
; largest 
