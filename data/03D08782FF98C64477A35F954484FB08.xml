<taxonomicName id="4C794D17FF98C645735B597B42E2FE94" class="Magnoliopsida" family="Asteraceae" genus="Olearia" kingdom="Plantae" order="Asterales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="species" species="axillaris">
<emphasis id="B90DEA86FF98C645735B597B42E2FE94" italics="true" pageId="52" pageNumber="52">Olearia axillaris</emphasis>
</taxonomicName>
(
<taxonomicName id="4C794D17FF98C645740159654397FE94" box="[777,909,1150,1172]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
), and 
<taxonomicName id="4C794D17FF98C64574D1596642F8FEB1" class="Magnoliopsida" family="Asteraceae" genus="Oxybolium" kingdom="Plantae" order="Asterales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="species" species="arborescens">
<emphasis id="B90DEA86FF98C64574D1596642F8FEB1" italics="true" pageId="52" pageNumber="52">Oxybolium arborescens</emphasis>
</taxonomicName>
(
<taxonomicName id="4C794D17FF98C64575FA5980437CFEB1" box="[754,870,1179,1201]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
).
</paragraph>
</subSubSection>
