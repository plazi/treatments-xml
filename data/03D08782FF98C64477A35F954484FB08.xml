<taxonomicName id="4C794D17FF98C6457355595F4396FE77" class="Magnoliopsida" family="Myrtaceae" genus="Leptospermum" kingdom="Plantae" order="Myrtales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="species" species="lanigerum">
<emphasis id="B90DEA86FF98C6457355595F4396FE77" italics="true" pageId="52" pageNumber="52">Leptospermum lanigerum</emphasis>
</taxonomicName>
(
<taxonomicName id="4C794D17FF98C64574A5597A4437FE77" box="[941,1069,1121,1143]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="52" pageNumber="52" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
