<taxonomicName id="70CA4D3CFFAAE20E86F6FB7FD89961ED" authority="Browne 1902" authorityName="Browne" authorityYear="1902" box="[953,1431,1196,1221]" class="Hydrozoa" family="Proboscidactylidae" genus="Proboscidactyla" kingdom="Animalia" order="Anthoathecata" pageId="71" pageNumber="72" phylum="Cnidaria" rank="species" species="mutabilis">
