<taxonomicName id="06964D3FFF936225696DFF12FD8FFF00" authority="Traquair, 1896" authorityName="Traquair" authorityYear="1896" box="[291,546,216,242]" class="Pteraspidomorphi" family="Turiniidae" genus="Turinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thelodontida" pageId="13" pageNumber="558" phylum="Chordata" rank="genus">
