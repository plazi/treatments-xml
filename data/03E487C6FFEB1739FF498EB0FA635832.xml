<taxonomicName id="4C4D4D53FFEB1739FF198EDBFE6F5A5C" authority="Stal 1866" authorityName="Stal" authorityYear="1866" box="[199,400,1020,1042]" class="Malacostraca" family="Lysianassidae" genus="Pachynus" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
