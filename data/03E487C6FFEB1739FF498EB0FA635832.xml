<taxonomicName id="4C4D4D53FFEB1739FD828EF9FC6D5DBD" authority="Stal" authorityName="Stal" box="[604,914,989,1012]" class="Malacostraca" family="Lysianassidae" genus="Pachynus" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="bimaculicollis">
<emphasis id="B939EAC2FFEB1739FD828EF9FCA45DBA" box="[604,859,990,1012]" italics="true" pageId="6" pageNumber="7">Pachynus bimaculicollis</emphasis>
