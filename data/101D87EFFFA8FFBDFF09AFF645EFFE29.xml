<taxonomicName id="5FB44D7AFFA8FFBEFF09AFF647A9F904" box="[151,500,1727,1753]" class="Secernentea" family="Rhabditidae" genus="Quercorhabditis" kingdom="Animalia" order="Rhabditida" pageId="1" pageNumber="386" phylum="Nematoda" rank="species" species="rajouriensis" status="gen. n.">
<emphasis id="AAC0EAEBFFA8FFBEFF09AFF647A9F904" bold="true" box="[151,500,1727,1753]" italics="true" pageId="1" pageNumber="386">Quercorhabditis rajouriensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="B1F35790FFA8FFBEFE65AFF64413F904" box="[507,590,1727,1753]" pageId="1" pageNumber="386" rank="genus">gen. n.</taxonomicNameLabel>
