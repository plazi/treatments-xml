<paragraph id="A3FE73ED3A77C44B12CF74AE71F44629" pageId="0" pageNumber="1">
<bibRefCitation id="4A642E9C313BD25CFFBD09C354E1C58E" author="Krajcik, M" journalOrPublisher="X Supplement 5, Plzen" pageId="0" pageNumber="1" refId="B156" refString="Krajcik, M, 2012. Checklist of the world Scarabaeoidea. Animma. X Supplement 5, Plzen" title="Checklist of the world Scarabaeoidea. Animma." year="2012">Krajcik (2012</bibRefCitation>
, 
<bibRefCitation id="8DA9D74892BC845FFA2169A14AB5FD2E" author="Krajcik, M" journalOrPublisher="49 - 54, Plzen" pageId="0" pageNumber="1" refId="B157" refString="Krajcik, M, 2013. Addenda to the checklist of world Scarabaeoidea 2012. Animma.X. No. 49-54, Plzen" title="Addenda to the checklist of world Scarabaeoidea 2012. Animma. X. No." year="2013">2013</bibRefCitation>
) considered 
<taxonomicName id="8D3941AACA3A69E80A97A4133322ABD3" authorityName="Martinez" authorityYear="1953" class="Insecta" family="Scarabaeidae" genus="Microogenius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microogenius gutierrezi" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gutierrezi">
<emphasis id="32626F227E1A4D0DDE80016133363F00" italics="true" pageId="0" pageNumber="1">Microogenius gutierrezi</emphasis>
