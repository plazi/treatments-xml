<quantity id="413D9B2DFF9FC03136E7FBE4FB9AFB0D" box="[917,1033,1131,1157]" metricMagnitude="-1" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="1" pageNumber="2403" unit="cm" value="20.0" valueMax="30.0" valueMin="10.0">
