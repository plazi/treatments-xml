sp. (
<taxonomicName id="D04122C682ADD5EE0DCCBB960A9C7FE6" class="Dicotyledoneae" family="Rosaceae" genus="Rubus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Rosales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="family">Rosaceae</taxonomicName>
