<taxonomicName id="1C85CA29ADFB005AD8FCB063118391F8" class="Liliopsida" family="Colchicaceae" genus="Disporum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Liliales" pageId="0" pageNumber="115" phylum="Magnoliophyta" rank="family">Colchicaceae</taxonomicName>
