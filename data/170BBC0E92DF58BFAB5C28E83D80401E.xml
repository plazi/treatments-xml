<taxonomicName id="B7F3E4E07B8B541B2804AFCB01DD2DB7" class="Dicotyledoneae" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Castanopsis" order="Fagales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="genus">
<emphasis id="EB1FA30A69E2D8EE44AA66B35D54374B" italics="true" pageId="0" pageNumber="115">Castanopsis</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="2ACD76534DF3EE51D1D39C3EDED31D1F" class="Dicotyledoneae" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Fagales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="family">Fagaceae</taxonomicName>
