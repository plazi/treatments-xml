<taxonomicName id="0D484B3F32867AA8295B0ACF5F202558" class="Dicotyledoneae" family="Lamiaceae" genus="Notochaete" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Notochaete hamosa" order="Tubiflorae" pageId="0" pageNumber="115" phylum="Angiospermae" rank="species" species="hamosa">
<emphasis id="EB78692B071F013834D4A5DCCB883F74" italics="true" pageId="0" pageNumber="115">Notochaete hamosa</emphasis>
</taxonomicName>
Benth. (
<taxonomicName id="2C8CDB6F8F49089251BB6EAD26200D2D" class="Dicotyledoneae" family="Lamiaceae" genus="Notochaete" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Tubiflorae" pageId="0" pageNumber="115" phylum="Angiospermae" rank="family">Lamiaceae</taxonomicName>
