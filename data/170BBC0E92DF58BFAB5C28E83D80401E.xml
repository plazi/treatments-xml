<taxonomicName id="2E84BC10FA5ABAF53996D0E05E9D2550" class="Dicotyledoneae" family="Rosaceae" genus="Rubus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rubus" order="Rosales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="genus">
<emphasis id="80E8F6570C110C695C31371694F53EDE" italics="true" pageId="0" pageNumber="115">Rubus</emphasis>
