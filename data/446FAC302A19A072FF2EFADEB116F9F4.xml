<taxonomicName id="0BC666A52A19A072FBC7FA6DB5DBF9BC" authority="Lederer" authorityName="Lederer" box="[1150,1436,1517,1542]" class="Insecta" family="Noctuidae" genus="Sesamia" kingdom="Animalia" order="Lepidoptera" pageId="8" phylum="Arthropoda" rank="species" species="cretica">
<emphasis id="FEB2C1342A19A072FBC7FA6DB570F9BF" box="[1150,1335,1518,1541]" italics="true" pageId="8">Sesamia cretica</emphasis>
Lederer
</taxonomicName>
(
<taxonomicName id="0BC666A52A19A072FF27F992B16BF990" box="[158,300,1553,1578]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="8" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
