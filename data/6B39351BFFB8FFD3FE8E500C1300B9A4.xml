species: 
<taxonomicName id="2490FF8EFFB8FFD3FC56525F12C7BA7F" authorityName="Jouault &amp; Ngô-Muller &amp; Pouillon &amp; Nel" authorityYear="2021" box="[965,1271,1373,1395]" class="Insecta" family="Bethylidae" genus="Cretapristocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" phylum="Arthropoda" rank="species" species="longiscapa">Cretapristocera longiscapa</taxonomicName>
