<emphasis id="0991EAFFFFB6FF9EFF0CFEC2D744FCA6" bold="true" box="[151,581,752,779]" pageId="1" pageNumber="249">
Genus 
<taxonomicName id="FCE54D6EFFB6FF9EFF75FEC3D744FCA6" authority="Ausserer, 1871" authorityName="Ausserer" authorityYear="1871" box="[238,581,752,779]" class="Arachnida" family="Ctenizidae" genus="Cyclocosmia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="249" phylum="Arthropoda" rank="genus">
