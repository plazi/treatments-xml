<taxonomicName id="AAFB17CAFF850B64A652F8A621543F7C" box="[285,574,1875,1897]" class="Malacostraca" family="Dynomenidae" genus="Graptocarcinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="48" phylum="Arthropoda" rank="species" species="urbasaensis" status="n. sp.">
<emphasis id="5F8FB05BFF850B64A652F8A621543F7C" box="[285,574,1875,1897]" italics="true" pageId="8" pageNumber="48">Graptocarcinus urbasaensis</emphasis>
</taxonomicName>
