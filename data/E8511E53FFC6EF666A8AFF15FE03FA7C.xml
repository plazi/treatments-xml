<paragraph id="6047AF45FFC6EF666A8AFC6CFEA4FBEB" blockId="26.[192,1440,943,1039]" pageId="26" pageNumber="25">
<emphasis id="528C7357FFC6EF666A8AFC6CFE90FC2D" bold="true" box="[192,348,943,969]" pageId="26" pageNumber="25">Distribution.</emphasis>
<collectingRegion id="A23C61A7FFC6EF666B29FC73FE1BFC2E" box="[355,471,944,970]" country="United States of America" name="Wyoming" pageId="26" pageNumber="25">Wyoming</collectingRegion>
, 
<collectingRegion id="A23C61A7FFC6EF666BA8FC73FD87FC2E" box="[482,587,944,970]" country="United States of America" name="Montana" pageId="26" pageNumber="25">Montana</collectingRegion>
, 
<collectingRegion id="A23C61A7FFC6EF66681DFC73FD36FC2E" box="[599,762,944,970]" country="United States of America" name="North Dakota" pageId="26" pageNumber="25">North Dakota</collectingRegion>
, south through 
<collectingRegion id="A23C61A7FFC6EF6669FBFC73FC26FC2E" box="[945,1002,944,970]" country="United States of America" name="Utah" pageId="26" pageNumber="25">Utah</collectingRegion>
(Grand, 
<collectingRegion id="A23C61A7FFC6EF666E18FC73FB75FC2E" box="[1106,1209,944,970]" country="Trinidad and Tobago" name="San Juan/Laventille" pageId="26" pageNumber="25">San Juan</collectingRegion>
, 
<collectingRegion id="A23C61A7FFC6EF666E8FFC73FA9CFC2E" box="[1221,1360,944,970]" country="United States of America" name="Washington" pageId="26" pageNumber="25">Washington</collectingRegion>
(
<bibRefCitation id="0469D2B4FFC6EF666F15FC73FEFEFC08" author="Monarch" pageId="26" pageNumber="25" refId="ref135978" refString="Monarch. 2021. Monarch Collections Search Results Table. Available at https: // monarch. calacademy. org / collections / listtabledisplay. php? taxa = Culicoides; Leptoconops &amp; thes = 1 &amp; type = 1 &amp; occindex = 1 &amp; sortfield 1 = Scientific Name &amp; sortfield 2 = &amp; sortorder = asc (Last accessed 10 April 2021.)" type="url" year="2021">Monarch 2021</bibRefCitation>
