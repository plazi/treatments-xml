<emphasis id="528C7357FFC6EF666BFDFC11FD4CFC08" bold="true" box="[439,640,978,1004]" pageId="26" pageNumber="25">new state record</emphasis>
), 
<collectingRegion id="A23C61A7FFC6EF6668D1FC11FCCBFC08" box="[667,775,978,1004]" country="United States of America" name="Colorado" pageId="26" pageNumber="25">Colorado</collectingRegion>
, 
<collectingRegion id="A23C61A7FFC6EF66695DFC11FC48FC08" box="[791,900,978,1004]" country="United States of America" name="Nebraska" pageId="26" pageNumber="25">Nebraska</collectingRegion>
, and 
<collectingRegion id="A23C61A7FFC6EF666980FC11FB89FC08" box="[970,1093,978,1004]" country="United States of America" name="Oklahoma" pageId="26" pageNumber="25">Oklahoma</collectingRegion>
