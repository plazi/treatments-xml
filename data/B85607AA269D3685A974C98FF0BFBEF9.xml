<taxonomicName id="E7877A6C57DF9B244027A743D9BB4B20" genus="A." lsidName="A. paragogus" pageId="0" pageNumber="1" rank="species" species="paragogus">
<emphasis id="F1339020E67660E133322B00663E2BE0" italics="true" pageId="0" pageNumber="1">A. paragogus</emphasis>
</taxonomicName>
, 
<taxonomicName id="79618C7B274CD37FE09A8C5726C4612E" baseAuthorityName="Blandford" baseAuthorityYear="1894" class="Insecta" family="Curculionidae" genus="Xylosandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xylosandrus germanus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="germanus">
