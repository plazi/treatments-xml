<collectionCode id="ADBFAE13A83BFFE4FF15FA9FFF600C45" box="[128,221,1354,1375]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="35" pageNumber="35">AMNH</collectionCode>
