<paragraph id="F9DE7453F1AD7B438B5E9090C4FA163C" pageId="9" pageNumber="46">
<taxonomicName id="3A6D05FC681C8CB3FD986024C4DF5584" class="Hexapoda" family="Vespidae" genus="Vespula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="flavopilosa">
