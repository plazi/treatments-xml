<taxonomicName id="4C014D11FFE793605D9015A29A6EFD5A" box="[1217,1387,704,730]" class="Insecta" family="Cicadidae" genus="Psithyristria" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="crassinervis">
<emphasis id="B975EA80FFE793605D9015A29BD5FD5A" box="[1217,1232,704,729]" italics="true" pageId="19" pageNumber="20">P</emphasis>
. 
<emphasis id="B975EA80FFE793605D8F15A29A6EFD5A" box="[1246,1387,704,729]" italics="true" pageId="19" pageNumber="20">crassinervis</emphasis>
</taxonomicName>
