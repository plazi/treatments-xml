<collectingCountry id="9DB1761BFFC7C82BF0998B821B26C3D0" box="[494,554,437,461]" name="India" pageId="10" pageNumber="371">India</collectingCountry>
: 
<collectingRegion id="2762F869FFC7C82BF34D8B821BCAC3D0" box="[570,710,437,461]" country="India" name="Maharashtra" pageId="10" pageNumber="371">Maharashtra</collectingRegion>
