pur (
<geoCoordinate id="8092504CFFC4C828F2178DC71AD1CA14" box="[864,989,2032,2057]" degrees="23" direction="north" minutes="15" orientation="latitude" pageId="9" pageNumber="370" precision="15" seconds="57" value="23.265833">23°15′57″N</geoCoordinate>
