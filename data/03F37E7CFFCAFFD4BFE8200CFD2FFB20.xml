<taxonomicName id="4C5AB4E9FFCAFFD5BFE820B9FD4CFA55" authority="Breddin, 1903: 375" authorityName="Breddin" authorityPageNumber="375" authorityYear="1903" box="[151,673,1434,1456]" class="Insecta" family="Coreidae" genus="Ctenomelynthus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="573" phylum="Arthropoda" rank="species" species="brunneiventris">
