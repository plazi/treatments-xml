<taxonomicName id="4C7A4D1BFF8DFF85E82410B6FB1952F8" authority="Wagner, 1840" authorityName="Wagner" authorityYear="1840" box="[841,1171,1192,1216]" pageId="13" pageNumber="13" rank="subFamily" subFamily="Desmodontinae">Desmodontinae Wagner, 1840</taxonomicName>
