<taxonomicName id="4C62D3B4FF83A04B3F38F84B9ABAF991" box="[284,509,1971,1992]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="265" phylum="Arthropoda" rank="species" species="irregularis" status="sp. nov.">
<emphasis id="B9167425FF83A04B3F38F84B9ABAF991" box="[284,509,1971,1992]" italics="true" pageId="10" pageNumber="265">
<taxonomicName id="4C62D3B4FF83A04B3F38F84B9ACFF991" box="[284,392,1971,1992]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="265" phylum="Arthropoda" rank="genus">Culicoides</taxonomicName>
<taxonomicName id="4C62D3B4FF83A04B3FB4F84B9ABAF991" box="[400,509,1971,1992]" pageId="10" pageNumber="265" rank="species" species="irregularis" status="sp. nov.">irregularis</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A225C95EFF83A04B3C20F84B9911F991" box="[516,598,1971,1992]" pageId="10" pageNumber="265" rank="species">
