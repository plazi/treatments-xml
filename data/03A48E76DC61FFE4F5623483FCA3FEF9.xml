<typeStatus id="54B681C2DC61FFE5F64A3530FD5DFAF1" box="[627,717,1374,1394]" pageId="12" pageNumber="789">Paratypus</typeStatus>
<taxonomicName id="4C0D44E3DC61FFE5F6EB3530FCFEFAF1" authorityName="PACE" authorityYear="1996" box="[722,878,1374,1394]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="789" phylum="Arthropoda" rank="species" species="ventosa">Geostiba ventosa</taxonomicName>
