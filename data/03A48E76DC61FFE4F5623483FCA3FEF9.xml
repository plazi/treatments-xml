<taxonomicName id="4C0D44E3DC61FFE5F0733530FB76FAF1" authorityName="PACE" authorityYear="1996" box="[1098,1254,1374,1394]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="789" phylum="Arthropoda" rank="species" species="ventosa" status="sp. n.">Geostiba ventosa</taxonomicName>
