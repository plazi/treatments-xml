<taxonomicName id="4C714D292164F55BFD2BB743FC49FDED" box="[732,902,522,547]" class="Maxillopoda" family="Austrobalanidae" genus="Austrobalanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="3" pageNumber="42" phylum="Arthropoda" rank="genus">
