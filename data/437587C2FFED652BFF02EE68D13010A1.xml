<materialsCitation id="7BB43C89FFED6528FAB9EEB8D006130A" ID-GBIF-Occurrence="2451575410" collectionCode="ICZN" collectorName="Altonaer Museum &amp; O. Semper &amp; Cuming. We" pageId="41" pageNumber="42" specimenCount="4" typeStatus="lectotype">
