<taxonomicName id="A0C01EAFFF97FFDBFF74FAD3FE93FAB3" authorityName="Kim and Pak" authorityYear="1965" box="[151,383,1368,1392]" class="Insecta" family="Eupelmidae" genus="Anastatus" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="397" phylum="Arthropoda" rank="species" species="dendrolimus">
<emphasis id="55B4B93EFF97FFDBFF74FAD3FE93FAB3" box="[151,383,1368,1392]" italics="true" pageId="46" pageNumber="397">Anastatus dendrolimus</emphasis>
</taxonomicName>
