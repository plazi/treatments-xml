<materialsCitation id="D5B6D90EFFB6D320D2B52AE4FAA4744B" ID-GBIF-Occurrence="1585970307" country="Papua New Guinea" location="Pictosciara Mohrig" pageId="14" pageNumber="465" specimenCount="1" typeStatus="holotype">
<emphasis id="57AA0F41FFB6D320D2B52AE4FEF67793" bold="true" box="[199,328,433,458]" pageId="14" pageNumber="465">Comments</emphasis>
. 
<taxonomicName id="A2DEA8D0FFB6D320D3242AE7FE547790" authorityName="Mohrig. A. Hypopygium" authorityYear="2004" box="[342,490,434,457]" class="Insecta" family="Sciaridae" genus="Cratyna" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="species" species="vera">
