<taxonomicName id="71A54D3DFFDAFF8DFF24F97DA8154D83" box="[150,333,1745,1766]" class="Insecta" family="Ichneumonidae" genus="Thyreodon" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="335" phylum="Arthropoda" rank="species" species="undefined-1">
