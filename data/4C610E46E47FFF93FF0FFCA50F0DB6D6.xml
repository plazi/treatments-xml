<collectionCode id="A2D92795E47FFF93FBF9FAE709A0B667" box="[1121,1177,1283,1307]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="22" pageNumber="23">SMF</collectionCode>
and labeled “ 
<collectingRegion id="060C71B2E47FFF93FAAAFAE708A4B667" box="[1330,1437,1283,1307]" country="Paraguay" name="Asuncion" pageId="22" pageNumber="23">
<collectingCountry id="BCDFFFC0E47FFF93FAAAFAE708A4B667" box="[1330,1437,1283,1307]" name="Paraguay" pageId="22" pageNumber="23">Paraguay</collectingCountry>
</collectingRegion>
// Sa. 
<collectingCountry id="BCDFFFC0E47FFF93FF42FAC20C00B643" box="[218,313,1318,1343]" name="Trinidad and Tobago" pageId="22" pageNumber="23">Trinidad</collectingCountry>
// 
