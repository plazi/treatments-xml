<taxonomicName id="03C8C4D3E47FFF93FBB0FA8A0958B6FB" box="[1064,1121,1390,1415]" class="Insecta" family="Syrphidae" genus="Baccha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pulla">
