<taxonomicName id="4C4D4D48C50CFFEBFF5BFA487885FA73" authority="(Herrich-Schaffer, 1847)" baseAuthorityName="Herrich-Schaffer" baseAuthorityYear="1847" box="[144,680,1444,1471]" class="Insecta" family="Geometridae" genus="Scopula" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="18" phylum="Arthropoda" rank="species" species="confinaria">
<emphasis id="B939EAD9C50CFFEBFF5BFA487885FA73" bold="true" box="[144,680,1444,1471]" pageId="9" pageNumber="18">
<emphasis id="B939EAD9C50CFFEBFF5BFA487B5EFA72" bold="true" box="[144,371,1444,1471]" italics="true" pageId="9" pageNumber="18">Scopula confinaria</emphasis>
