<taxonomicName id="428EE8C29EC5E3BFFD1CFBBED862670F" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[710,862,1095,1119]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="pectunguis" status="sp. nov.">
<emphasis id="B7FA4F539EC5E3BFFD1CFBBED862670F" box="[710,862,1095,1119]" italics="true" pageId="160" pageNumber="161">A. pectunguis</emphasis>
</taxonomicName>
