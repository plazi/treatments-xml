<taxonomicName id="4C424D1CFFE8C32FA7CF0384403866AC" box="[151,306,226,251]" class="Ascidiacea" family="Styelidae" genus="Amphicarpa" higherTaxonomySource="CoL" kingdom="Animalia" order="Stolidobranchia" pageId="8" pageNumber="59" phylum="Chordata" rank="genus">
