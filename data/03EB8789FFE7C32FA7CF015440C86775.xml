<taxonomicName id="4C424D1CFFE8C32FA4F9038442D166AB" box="[929,987,226,252]" class="Magnoliopsida" family="Fabaceae" genus="Glycine" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="59" phylum="Tracheophyta" rank="species" species="soya">soya</taxonomicName>
