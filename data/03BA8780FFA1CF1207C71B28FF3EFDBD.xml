<taxonomicName id="4C134D15FFA6CF12043C1C38FC08FDFA" box="[876,995,513,536]" class="Arachnida" family="Halacaridae" genus="Halacarus" kingdom="Animalia" order="Trombidiformes" pageId="20" pageNumber="38" phylum="Arthropoda" rank="species" species="actenos">
