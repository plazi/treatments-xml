<paragraph id="449190823E41A3272E901AF91A9844A3" pageId="0" pageNumber="81">
<taxonomicName id="04A6D49FA442CD14F5FD29FE19AFACE6" authorityName="Lahey" authorityYear="2021" class="Insecta" family="Platygastridae " genus="Alfredella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alfredella mephisto" order="Hymenoptera" pageId="0" pageNumber="81" phylum="Arthropoda" rank="species" species="mephisto">
