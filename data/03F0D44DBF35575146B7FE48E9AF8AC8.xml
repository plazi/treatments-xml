<taxonomicName id="4C591ED8BF35575147E4FDD2EEBA8A38" box="[452,671,548,573]" class="Magnoliopsida" family="Rhizophoraceae" genus="Rhizophora" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="12" pageNumber="233" phylum="Tracheophyta" rank="species" species="mangle">
