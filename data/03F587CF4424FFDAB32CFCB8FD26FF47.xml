(=GDMCC 1.1745 
<superScript id="7C299B914424FFDBB548F89DFADDF8F1" attach="none" box="[1368,1378,1869,1883]" fontSize="6" pageId="6" pageNumber="4828">T</superScript>
=KACC 21659 
<superScript id="7C299B914424FFDBB36CF8BCFC39F8D0" attach="none" box="[892,902,1900,1914]" fontSize="6" pageId="6" pageNumber="4828">T</superScript>
