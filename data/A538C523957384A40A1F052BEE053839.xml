<taxonomicName id="03E3041D307A4D4CA8A4567312005976" class="Magnoliopsida" family="Fabaceae" genus="Ononis" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="506" phylum="Tracheophyta" rank="species" species="pusilla">
<italic id="630E23BA24A3F474A4EB2D23BEFDCC9C" pageId="null" pageNumber="506">O. pusilla</italic>
