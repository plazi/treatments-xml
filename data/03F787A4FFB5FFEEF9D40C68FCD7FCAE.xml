<taxonomicName id="4C5E4D31FFB4FFE1FBB308F9FC9AF931" authorityName="Horn" authorityYear="1889" box="[679,832,1744,1765]" class="Insecta" family="Chrysomelidae" genus="Hemiphrynus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="genus">
