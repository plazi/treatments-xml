<collectingCountry id="F35383E88A143B02F25FFC58B828FC44" box="[291,367,965,986]" name="Mexico" pageId="4" pageNumber="95">México</collectingCountry>
: 
<collectingRegion id="49800D9A8A143B02F205FC58B8F8FC44" box="[377,447,965,986]" country="Mexico" name="Sonora" pageId="4" pageNumber="95">Sonora</collectingRegion>
, 
<location id="8E9B95A38A143B02F2B4FC58BB60FC44" LSID="urn:lsid:plazi:treatment:03ED726E8A123B03F20CFDBBBB68FD28:8E9B95A38A143B02F2B4FC58BB60FC44" box="[456,551,965,986]" country="Mexico" municipality="Based" name="Tezopaco" pageId="4" pageNumber="95" stateProvince="Sonora">Tezopaco</location>
