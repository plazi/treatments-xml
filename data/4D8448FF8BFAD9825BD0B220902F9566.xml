<taxonomicName id="2E9DC2DC4C75D68A0AB4510DBC956EC0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:141919" class="Insecta" family="Formicidae" genus="Paratrechina" kingdom="Animalia" lsidName-HNS="Paratrechina (Nylanderia) arlesi Bernard" order="Hymenoptera" pageNumber="257" phylum="Arthropoda" rank="species" species="arlesi" subGenus="Nylanderia">Paratrechina (Nylanderia) arlesi</taxonomicName>
