<subSubSection id="EEF3C7C8340DA6E1ABFBA91822F4A14F" pageId="null" pageNumber="48" type="reference_group">
<paragraph id="2C7DF14158BDF9B9051A560355C3449E" pageId="null" pageNumber="48">
(
<taxonomicName id="F5002AC8ECB3A66365AC428772B34EE9" authority="R. Br." authorityName="R. Br." class="Magnoliopsida" family="Convolvulaceae" genus="Cuscuta" kingdom="Plantae" order="Solanales" pageId="null" pageNumber="48" phylum="Tracheophyta" rank="species" species="australis">
<emphasis id="FEF2DC860C0D8B60ED7F48869F66E607" italics="true" pageId="null" pageNumber="48">C. australis</emphasis>
R. Br.
</taxonomicName>
var. 
<taxonomicName id="B2018083C0313D2643A0FCED7B0C3A30" class="Magnoliopsida" family="Convolvulaceae" genus="Cuscuta" kingdom="Plantae" order="Solanales" pageId="null" pageNumber="48" phylum="Tracheophyta" rank="species" species="cesatiana">
