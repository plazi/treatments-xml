SUBFAMILY 
<taxonomicName id="4C504D78CD22FFADFC2B2B8B1D3FEC0E" authority="BLEEKER, 1858" authorityName="Bleeker" authorityYear="1858" box="[1019,1347,1021,1045]" class="Actinopterygii" family="Cetopsidae" kingdom="Animalia" order="Siluriformes" pageId="46" pageNumber="801" phylum="Chordata" rank="subFamily" subFamily="Cetopsinae">CETOPSINAE BLEEKER, 1858</taxonomicName>
