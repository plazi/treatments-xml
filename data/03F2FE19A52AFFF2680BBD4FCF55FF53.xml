<taxonomicName id="4C5B348CA52AFF8D6E45BD14CFC8FCFF" authorityName="Mengual" authorityYear="2012" box="[1339,1434,833,854]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="63" pageNumber="162" phylum="Arthropoda" rank="species" species="triton">
<emphasis id="B92F931DA52AFF8D6E45BD14CFC8FCFF" box="[1339,1434,833,854]" italics="true" pageId="63" pageNumber="162">C. triton</emphasis>
