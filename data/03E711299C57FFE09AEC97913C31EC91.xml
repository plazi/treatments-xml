<taxonomicName id="4C4EDBBC9C57FFE39BA697FC3C52E97C" authority=", Olivier 1795: 42" authorityName=", Olivier" authorityPageNumber="42" authorityYear="1795" box="[241,632,1522,1544]" class="Insecta" family="Cerambycidae" genus="Cerambix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" phylum="Arthropoda" rank="species" species="bidens">
<emphasis id="B93A7C2D9C57FFE39BA697FC3F8EE973" box="[241,420,1522,1543]" italics="true" pageId="5">Cerambix bidens</emphasis>
