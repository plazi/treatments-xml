<bibRefCitation id="2D715BC0E21FAC61E108FDA1FDF2651E" author="Munro HK" box="[361,528,522,546]" pageId="22" pageNumber="2446" pagination="139 - 153" refId="ref18633" refString="Munro HK. 1939 b. Some new species of South African Trypetidae (Diptera), including one from Madagascar. J Entomol Soc South Afr. 2: 139 - 153." type="journal article" year="1939">Munro, 1939b</bibRefCitation>
</taxonomicName>
(relationship 
<taxonomicName id="8EE05DB2E21FAC61E2D6FDA0FCAD651E" authorityName="Bezzi" authorityYear="1924" box="[695,847,522,546]" class="Insecta" family="Tephritidae" genus="Perilampsis" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="2446" phylum="Arthropoda" rank="species" species="diademata">
