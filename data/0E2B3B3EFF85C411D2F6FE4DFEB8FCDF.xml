<collectingCountry id="FE95CAB8FF85C411D3A4FD3CFEDFFD5F" box="[151,293,649,673]" name="Saudi Arabia" pageId="6" pageNumber="431">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="FE95CAB8FF85C411D203FD3CFE94FD5F" box="[304,366,649,673]" name="Israel" pageId="6" pageNumber="431">Israel</collectingCountry>
, 
<collectingCountry id="FE95CAB8FF85C411D24BFD3CFE45FD5F" box="[376,447,649,673]" name="Jordan" pageId="6" pageNumber="431">Jordan</collectingCountry>
