<taxonomicName id="4D26FDA886A9836FA46AAC6432FBF19B" class="Magnoliopsida" family="Lythraceae" genus="Lagerstroemia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lagerstroemia" order="Myrtales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="genus">Lagerstroemia</taxonomicName>
