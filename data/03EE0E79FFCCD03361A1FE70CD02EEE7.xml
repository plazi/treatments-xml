<quantity id="4CBF128AFFCCD0336409FC5BCDA3EE76" box="[1343,1431,1015,1039]" metricMagnitude="3" metricUnit="m" metricValue="2.7" pageId="5" pageNumber="186" unit="m" value="2700.0">2700 m</quantity>
, 
<date id="FFF999AFFFCCD03361A1FBB6C921EE4B" box="[151,277,1050,1075]" pageId="5" pageNumber="186" value="2015-09-17">
<number id="3D8FAD48FFCCD03361A1FBB6C885EE4B" box="[151,177,1050,1075]" pageId="5" pageNumber="186" value="17.0">17</number>
-ix- 
<number id="3D8FAD48FFCCD03361EFFBB6C921EE4B" box="[217,277,1050,1075]" pageId="5" pageNumber="186" value="2015.0">2015</number>
