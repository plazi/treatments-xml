<collectingCountry id="F37976783140C0174FDB8E67FC0C740D" box="[941,991,328,351]" name="Peru" pageId="9" pageNumber="278">Peru</collectingCountry>
(
<quantity id="4C969B0D3140C0174F998E67FBE5740D" box="[1007,1078,328,352]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="9" pageNumber="278" unit="m" value="800.0">800 m</quantity>
