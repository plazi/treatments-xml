<quantity id="4C969B0D3140C01748B58E67FACB7432" box="[1219,1304,328,352]" metricMagnitude="5" metricUnit="m" metricValue="6.0" pageId="9" pageNumber="278" unit="km" value="600.0">600 km</quantity>
