<taxonomicName id="4C6E4D6B3140C0174F108FCBFC0B75A9" authorityName="Ihering" authorityYear="1905" box="[870,984,228,251]" class="Gastropoda" family="Solaropsidae" genus="Solaropsis" kingdom="Animalia" order="Stylommatophora" pageId="9" pageNumber="278" phylum="Mollusca" rank="species" species="juruana">
