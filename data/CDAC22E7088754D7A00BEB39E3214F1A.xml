<taxonomicName id="7AF81B5F930360360F89B4B2D7399CD1" authorityName="McKenzie" authorityYear="1991" family="Chaetosphaeriaceae" genus="Chalarodes" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Chalarodes bisetis" order="Chaetosphaeriales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="bisetis">
