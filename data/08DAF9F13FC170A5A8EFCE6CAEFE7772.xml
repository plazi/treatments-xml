<taxonomicName id="D4C322E773E9BB5C5EC0393867278D0E" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis Ramburi" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="Ramburi">Chrysis Ramburi</taxonomicName>
