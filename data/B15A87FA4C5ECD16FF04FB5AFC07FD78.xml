<taxonomicName id="FEF34D6F4C59CD14FE8AF844FE1CF83A" authority="Xu &amp; Zheng &amp; Yao, 2020" authorityName="Xu &amp; Zheng &amp; Yao" authorityYear="2020" box="[281,416,1946,1970]" class="Arachnida" family="Pholcidae" genus="Khorata" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="160" phylum="Arthropoda" rank="species" species="nani" status="sp. nov.">
