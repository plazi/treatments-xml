<superScript id="89EDEA6AFFDFBC7AFA8E0CA56937FF77" attach="none" box="[1355,1363,152,166]" fontSize="6" pageId="3" pageNumber="70">o</superScript>
58´W
</geoCoordinate>
) and Cape Cod (
<collectingCountry id="068F07B2FFDFBC7AFE860C806DE4FF06" box="[323,384,189,215]" name="United States of America" pageId="3" pageNumber="70">USA</collectingCountry>
), to 
<collectingRegion id="BC5C89C0FFDFBC7AFE6A0C816EF8FF06" box="[431,668,188,215]" country="Brazil" name="Rio Grande do Norte" pageId="3" pageNumber="70">Rio Grande do Norte</collectingRegion>
