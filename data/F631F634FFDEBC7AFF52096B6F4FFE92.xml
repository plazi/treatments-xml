, usually bellow 
<specimenCount id="689E8CABFFDFBC7AFCA10CDC6FD0FF2A" box="[868,948,225,251]" pageId="3" pageNumber="70" type="generic">
<quantity id="B960EAC7FFDFBC7AFCA10CDC6FD4FF2A" box="[868,944,225,251]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="70" unit="m" value="100.0">100 m</quantity>
