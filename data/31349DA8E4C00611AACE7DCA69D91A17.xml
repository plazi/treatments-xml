<subSubSection id="67AC5C1D56E03F2E6F243378B8ABE40B" pageId="9" pageNumber="272" type="etymology">
<paragraph id="BD079ED4EBFDCCFE45DD7E68C48FB980" pageId="9" pageNumber="272">Etymology.</paragraph>
<paragraph id="C3E08F9EAD2563B8B4B7BB9B49084B7E" pageId="9" pageNumber="272">
Latin incompletus, 
<normalizedToken id="71D1AD00D475983A7F9D6711DD2F6F27" originalValue="“incomplete”">&quot;incomplete&quot;</normalizedToken>
