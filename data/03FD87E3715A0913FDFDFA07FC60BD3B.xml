<taxonomicName id="4C544D7671550912FC99FB16FB3ABAF7" authority="(Vanhoffen, 1910)" baseAuthorityName="Vanhoffen" baseAuthorityYear="1910" box="[859,1268,1201,1223]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="40" pageNumber="2692" phylum="Cnidaria" rank="species" species="juncea">
