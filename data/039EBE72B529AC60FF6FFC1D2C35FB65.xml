<subSubSection id="C32D5CEFB529AC6EFF3FFAFD2F8AFAA2" pageId="6" pageNumber="544" type="etymology">
<paragraph id="8B880F64B529AC6EFF3FFAFD2F8AFAA2" blockId="6.[151,1437,1122,2013]" pageId="6" pageNumber="544">
<emphasis id="B943D376B529AC6EFF3FFAFD2E16FAC6" bold="true" box="[199,334,1303,1329]" pageId="6" pageNumber="544">Etymology.</emphasis>
