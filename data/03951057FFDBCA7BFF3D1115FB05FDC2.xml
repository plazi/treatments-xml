<taxonomicName id="4C3CDAC2FFDBCA7BFF3D114EFCA5FE8E" authority="Takaoka et al., 2014 a: 10" authorityName="Takaoka et al." authorityPageNumber="10" authorityYear="2014" box="[151,817,324,349]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="azhari" subGenus="Gomphostilbia">
