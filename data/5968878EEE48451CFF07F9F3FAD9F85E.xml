<taxonomicName id="16C14D1BEE4B451DFEDAF974FDF7F911" box="[330,516,1733,1755]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="serrula">
<emphasis id="E3B5EA8AEE4B451DFEDAF974FDF7F911" box="[330,516,1733,1755]" italics="true" pageId="26" pageNumber="27">
Megachile 
<taxonomicName id="16C14D1BEE4B451DFE2BF974FDF7F911" box="[443,516,1733,1755]" pageId="26" pageNumber="27" rank="species" species="serrula">serrula</taxonomicName>
