<materialsCitation id="3B6D3CA9FFE9FF9FFD47FDD03ADBFBE2" collectingDate="1911-06-17" collectionCode="AMNH" collectorName="Mathews &amp; Fitzroy River' &amp; J. P. Rogers &amp; Mungi Rock Hole" country="Australia" county="West Kimberley" location="Mathews" municipality="The" pageId="82" pageNumber="82" specimenCode="AMNH 694860, AMNH 694857, AMNH 694858" specimenCount="1" typeStatus="holotype">
