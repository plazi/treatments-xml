<taxonomicName id="4C214D6598509415FEECD668D83C2B53" authority="Hughes, 2009" authorityName="Hughes" authorityYear="2009" box="[288,516,1869,1892]" class="Malacostraca" family="Synopiidae" genus="Syrrhoe" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="886" phylum="Arthropoda" rank="species" species="petitaserrata" status="sp. nov.">
<emphasis id="B955EAF498509415FEECD668D83C2B53" box="[288,516,1869,1892]" italics="true" pageId="6" pageNumber="886">Syrrhoe petitaserrata</emphasis>
</taxonomicName>
<emphasis id="B955EAF498509415FDC7D669D8582B53" bold="true" box="[523,608,1868,1892]" pageId="6" pageNumber="886">
