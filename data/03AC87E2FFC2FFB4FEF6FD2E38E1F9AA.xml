<collectionCode id="ED14AE31FFC2FFB4FE15FBDA3B20FBD6" box="[490,583,1117,1138]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="121" pageNumber="121">AMNH</collectionCode>
