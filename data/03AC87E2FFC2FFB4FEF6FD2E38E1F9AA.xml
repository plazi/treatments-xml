males, 
<specimenCount id="9D03FD7DFFC2FFB4FD9CFB773997FA87" pageId="121" pageNumber="121" type="immature">one immature</specimenCount>
male, 
<specimenCount id="9D03FD7DFFC2FFB4FEC0FA8A38F8FA87" box="[319,415,1293,1315]" pageId="121" pageNumber="121" type="adult">11 adult</specimenCount>
