<specimenCount id="9D03FD7DFFC2FFB4FD8DFACF3992FADF" pageId="121" pageNumber="121" type="generic" typeStatus="paratypes">22 paratypes</specimenCount>
: 
<emphasis id="B971EAE6FFC2FFB4FEFEFAE23B72FADE" bold="true" box="[257,533,1381,1402]" pageId="121" pageNumber="121">
<specimenCode id="DBA39E8FFFC2FFB4FEFEFAE23B72FADE" box="[257,533,1381,1402]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="121" pageNumber="121">AMNH 343083–343104</specimenCode>
