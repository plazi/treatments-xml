; about 1/ 
<quantity id="4CCF9B59E506DF1614CEF9A6FCBDA422" box="[736,780,1621,1647]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="1" pageNumber="178" unit="in" value="3.0">3 in</quantity>
<taxonomicName id="4C374D3FE506DF16153DF9A5FC29A422" authorityName="Xu, Zhang &amp; Yao" authorityYear="2019" box="[787,920,1622,1647]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="178" phylum="Arthropoda" rank="species" species="mixiaoqii">
