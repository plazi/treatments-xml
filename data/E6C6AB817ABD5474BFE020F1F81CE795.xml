<taxonomicName id="33E7EB967F95702609F6F86893E7111A" authority="Cockerell, 1926" authorityName="Cockerell" authorityYear="1926" class="Insecta" family="Apidae" genus="Asaropoda" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Asaropoda dawsoni" order="Hymenoptera" pageId="0" pageNumber="45" phylum="Arthropoda" rank="species" species="dawsoni">Asaropoda dawsoni</taxonomicName>
