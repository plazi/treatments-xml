<taxonomicName id="5FE781BEFA158B57EF2627DC1E3CCE1C" authority="Bar, 1873" authorityName="Bar" authorityYear="1873" class="Insecta" family="Erebidae" genus="Palustra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Palustra uruguayensis" order="Lepidoptera" pageId="14" pageNumber="79" phylum="Arthropoda" rank="species" species="uruguayensis">Palustra uruguayensis</taxonomicName>
