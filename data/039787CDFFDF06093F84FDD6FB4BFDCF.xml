<taxonomicName id="4C3E4D58FFDF060A3D4FF9E2FB4CF9D8" authority="Li, Gao and Chen, 2010" authorityName="Li, Gao and Chen" authorityYear="2010" box="[790,1156,1547,1572]" class="Insecta" family="Drosophilidae" genus="Pseudostegana" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="2101" phylum="Arthropoda" rank="species" species="bilobata">
