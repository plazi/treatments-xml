<taxonomicName id="129AA3D76177FFC827CBB5E2FBFBFC75" authority="Moriuti" authorityName="Moriuti" box="[931,1140,926,951]" class="Insecta" family="Plutellidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="cristata">
