2. 
<taxonomicName id="F74A9E183939E593242DEE626589AD38" class="Arachnida" family="Liacaridae" genus="Liacarus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="87" phylum="Arthropoda" rank="species" species="gracilis">Liacarus gracilis</taxonomicName>
