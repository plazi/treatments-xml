<geoCoordinate id="9654550B14B9B578664EF3CE80BAF928" direction="south" orientation="latitude" precision="5" value="-26.9438">26.9438S</geoCoordinate>
; 
<geoCoordinate id="59D4E71524476645426C7010BA5E32F1" direction="east" orientation="longitude" precision="5" value="120.3847">120.3847E</geoCoordinate>
