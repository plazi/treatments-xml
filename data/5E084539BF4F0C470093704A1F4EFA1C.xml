<taxonomicName id="5769807EC11A9FD19A637B445184FBDE" class="Magnoliopsida" family="Thymelaeaceae" genus="Pimelea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pimelea" order="Malvales" pageId="27" pageNumber="136" phylum="Tracheophyta" rank="genus">Pimelea</taxonomicName>
