<geoCoordinate id="EE745033FE2D2BCAFDC0D7DAFD13FE28" box="[617,754,404,431]" degrees="18" direction="south" minutes="01" orientation="latitude" pageId="386" pageNumber="387" precision="15" seconds="35" value="-18.026388">18°01’35’’S</geoCoordinate>
