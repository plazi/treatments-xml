, 
<taxonomicName id="4C754D4EFFF0FFC371BDFC14FDA5F9C2" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[415,516,909,932]" class="Aves" family="Picidae" genus="Dendrocopos" kingdom="Animalia" order="Piciformes" pageId="124" pageNumber="125" phylum="Chordata" rank="species" species="major">D. major</taxonomicName>
</emphasis>
and 
<taxonomicName id="4C754D4EFFF0FFC37218FC14FD61F9C2" box="[570,704,909,932]" class="Aves" family="Picidae" genus="Picus" kingdom="Animalia" order="Piciformes" pageId="124" pageNumber="125" phylum="Chordata" rank="species" species="canus">
<emphasis id="B901EADFFFF0FFC37218FC14FD61F9C2" box="[570,704,909,932]" italics="true" pageId="124" pageNumber="125">Picus canus</emphasis>
