<subSubSection id="C36F6546FFF0FFC370E5FCF1FD69F9C5" pageId="124" pageNumber="125" type="biology_ecology">
<paragraph id="8BCA36CDFFF0FFC370E5FCF1FD69F9C5" blockId="124.[151,1438,151,1292]" pageId="124" pageNumber="125">
<emphasis id="B901EADFFFF0FFC370E5FCF1FE46F9E6" bold="true" box="[199,487,871,896]" inLineHeading="true" pageId="124" pageNumber="125" reason="1">
