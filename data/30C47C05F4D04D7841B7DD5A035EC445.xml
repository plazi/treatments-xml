<taxonomicName id="4C38DFC087C3EAFEF5B4EDFBCF5C618B" class="Demospongiae" family="Irciniidae" genus="Ircinia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ircinia retidermata" order="Dictyoceratida" pageId="22" pageNumber="23" phylum="Porifera" rank="species" species="retidermata">Ircinia retidermata</taxonomicName>
