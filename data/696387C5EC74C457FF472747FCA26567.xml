<taxonomicName id="26CA4D50EC71C457FCA52708FBC2642B" baseAuthorityName="Morrison" baseAuthorityYear="1929" box="[885,1085,562,587]" class="Insecta" family="Formicidae" genus="Azteca" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="longiceps">
