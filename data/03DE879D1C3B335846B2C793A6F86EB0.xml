<quantity id="4C8F9B6E1C3B33584567C361A1DF6E1D" box="[834,986,1544,1571]" metricMagnitude="2" metricUnit="m" metricValue="4.865" metricValueMax="5.68" metricValueMin="4.05" pageId="42" pageNumber="454" unit="m" value="486.5" valueMax="568.0" valueMin="405.0">405 to 568 m</quantity>
.
