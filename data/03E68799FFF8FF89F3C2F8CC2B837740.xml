(
<figureCitation id="13742A0AFFF8FF8EF3CAF8A92EF57353" box="[159,253,1876,1900]" captionStart="FIGURE 16" captionStartId="195.[151,244,1126,1147]" captionTargetBox="[157,1433,196,1096]" captionTargetId="figure-77@195.[151,1436,191,1104]" captionTargetPageId="195" captionText="FIGURE 16. Alara schmidti, ♀ paratype in BMNH from Papua (Kokoda), lateral (inverted) view." pageId="27" pageNumber="28">Figs. 16</figureCitation>
, 
<figureCitation id="13742A0AFFF8FF8EF25DF8A92F3B7353" box="[264,307,1876,1900]" captionStart="FIGURE 145" captionStartId="229.[151,244,451,472]" captionText="FIGURE 145. Alara schmidti, holotype ♂." pageId="27" pageNumber="28">145</figureCitation>
