<quantity id="4CC79B5BD715644DC561E4A02BF2C622" box="[321,422,1266,1288]" metricMagnitude="-3" metricUnit="m" metricValue="7.25" metricValueMax="7.5" metricValueMin="7.0" pageId="13" pageNumber="488" unit="mm" value="7.25" valueMax="7.5" valueMin="7.0">7-7,5 mm</quantity>
