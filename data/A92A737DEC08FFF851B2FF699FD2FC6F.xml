<taxonomicName id="E683B9E8EC0FFFF85225FE8F9FACFE83" baseAuthorityName="Hinton" baseAuthorityYear="1945" box="[856,947,265,287]" class="Insecta" family="Elmidae" genus="Cylloepus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="274" phylum="Arthropoda" rank="genus">
<emphasis id="13F71E79EC0FFFF85225FE8F9FACFE83" box="[856,947,265,287]" italics="true" pageId="5" pageNumber="274">Cylloepus</emphasis>
