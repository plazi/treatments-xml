<taxonomicName id="E683B9E8EC0FFFF850D9FF5D9E38FF6D" authorityName="Shepard &amp; Sites &amp; Rodrigues" authorityYear="2021" box="[420,551,219,241]" class="Insecta" family="Elmidae" genus="Cylloepus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="274" phylum="Arthropoda" rank="species" species="dimorphus">
<emphasis id="13F71E79EC0FFFF850D9FF5D9E38FF6D" box="[420,551,219,241]" italics="true" pageId="5" pageNumber="274">C. dimorphus</emphasis>
