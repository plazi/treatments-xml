<quantity id="E67B6F8EEC0EFFF9529AFAD89879FAEF" box="[999,1126,1373,1395]" metricMagnitude="-3" metricUnit="m" metricValue="3.15" metricValueMax="3.6" metricValueMin="2.7" pageId="4" pageNumber="273" unit="mm" value="3.15" valueMax="3.6" valueMin="2.7">2.7–3.6 mm</quantity>
long, 
<quantity id="E67B6F8EEC0EFFF953BEFAFF9F26FA12" box="[707,825,1400,1422]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" metricValueMax="1.8" metricValueMin="1.6" pageId="4" pageNumber="273" unit="mm" value="1.7" valueMax="1.8" valueMin="1.6">1.6–1.8 mm</quantity>
