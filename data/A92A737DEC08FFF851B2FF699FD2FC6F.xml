<taxonomicName id="E683B9E8EC0FFFF85080FE139E7CFE37" authorityName="Shepard &amp; Sites &amp; Rodrigues" authorityYear="2021" box="[509,611,405,427]" class="Insecta" family="Elmidae" genus="Cylloepus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="274" phylum="Arthropoda" rank="species" species="dimorphus">
<emphasis id="13F71E79EC0FFFF85080FE139E7CFE37" box="[509,611,405,427]" italics="true" pageId="5" pageNumber="274">dimorphus</emphasis>
