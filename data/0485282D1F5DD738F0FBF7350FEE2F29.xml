<taxonomicName id="E5DC10482AD6CD7E86FDBCABEE3E6511" class="Magnoliopsida" family="Acanthaceae" genus="Avicennia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Avicennia" order="Lamiales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">Avicennia</taxonomicName>
