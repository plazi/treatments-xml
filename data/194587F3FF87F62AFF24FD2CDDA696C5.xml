, 
<quantity id="56149B00FF87F62DFDFCFC5FDCE396B1" box="[591,664,897,922]" metricMagnitude="2" metricUnit="m" metricValue="3.05" pageId="2" pageNumber="393" unit="m" value="305.0">305 m</quantity>
, 
