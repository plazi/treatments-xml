<collectingCountry id="61C8764CE74F0447AFD0FD2DFBC0FD18" box="[990,1076,644,669]" name="Turkey" pageId="2" pageNumber="199">
<emphasis id="2BABEACEE74F0447AFD0FD2DFBC0FD18" bold="true" box="[990,1076,644,669]" pageId="2" pageNumber="199">Turkey</emphasis>
</collectingCountry>
(
<collectingRegion id="DB1BF83EE74F0447A84CFD2DFB56FD18" box="[1090,1186,644,669]" country="Turkey" name="Istanbul" pageId="2" pageNumber="199">İstanbul</collectingRegion>
: 
<bibRefCitation id="7D4E4B2DE74F0447A8A2FD2DFABAFD18" box="[1196,1358,644,669]" pageId="2" pageNumber="199" refId="ref5883" refString="Omodeo, P. (1952) Oligocheti della Turchia. Annuario Museo Zoologico, Universita Di Napoli, 4, 1 - 20." type="journal article">Omodeo 1952</bibRefCitation>
; 
<collectingRegion id="DB1BF83EE74F0447A957FD2CFA68FD18" box="[1369,1436,645,669]" country="Turkey" name="Hatay" pageId="2" pageNumber="199">
