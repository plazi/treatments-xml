<taxonomicName id="0BD525A2FF88804FFE48F8CCE5C10531" box="[483,708,1886,1909]" class="Liliopsida" family="Bromeliaceae" genus="Werauhia" higherTaxonomySource="CoL" kingdom="Plantae" order="Poales" pageId="4" pageNumber="322" phylum="Tracheophyta" rank="species" species="gigantea">
