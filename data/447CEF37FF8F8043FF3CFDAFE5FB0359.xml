<taxonomicName id="0BD525A2FF8F8048FEF1FDAFE5F80013" authority="Chassain" authorityName="Chassain" box="[346,765,573,599]" class="Insecta" family="Elateridae" genus="Platycrepidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="321" phylum="Arthropoda" rank="species" species="dewynteri">
<emphasis id="FEA18233FF8F8048FEF1FDAFE5860013" bold="true" box="[346,643,573,599]" italics="true" pageId="3" pageNumber="321">Platycrepidius dewynteri</emphasis>
