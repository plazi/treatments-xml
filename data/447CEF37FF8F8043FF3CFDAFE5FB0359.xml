<subSubSection id="84CF0DAAFF8B8043FF6CF90AE5FB0359" lastPageId="8" lastPageNumber="326" pageId="7" pageNumber="325" type="materials_examined">
<paragraph id="CC6A5E21FF8B804CFF6CF90AE58905CD" blockId="7.[151,1437,1688,2038]" pageId="7" pageNumber="325">
<emphasis id="FEA18233FF8B804CFF6CF90AE6B304F5" bold="true" box="[199,438,1688,1713]" pageId="7" pageNumber="325">Material examined.</emphasis>
