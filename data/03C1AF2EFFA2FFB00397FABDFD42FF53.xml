<paragraph id="8BD71E38FFA2FFB30397FAD5FEA5FA78" blockId="4.[151,1437,1400,1982]" pageId="4" pageNumber="5">
<emphasis id="B91CC22AFFA2FFB30397FAD5FE94FA10" bold="true" box="[151,290,1400,1425]" pageId="4" pageNumber="5">Dimensions</emphasis>
. 
<typeStatus id="54D3A09AFFA2FFB30231FAD5FE2FFA10" box="[305,409,1400,1425]" pageId="4" pageNumber="5">Holotype</typeStatus>
female length 
<number id="3DA00C1FFFA2FFB30146FAD5FDC8FA10" box="[582,638,1400,1425]" pageId="4" pageNumber="5" value="1366.0">1366</number>
Μm, breadth 
<number id="3DA00C1FFFA2FFB30018FAD5FCF4FA10" box="[792,834,1400,1425]" pageId="4" pageNumber="5" value="656.0">656</number>
Μm; 
<typeStatus id="54D3A09AFFA2FFB30080FAD4FC57FA10" box="[896,993,1401,1425]" pageId="4" pageNumber="5">paratype</typeStatus>
females (
<emphasis id="B91CC22AFFA2FFB30753FAD4FBD7FA11" box="[1107,1121,1401,1424]" italics="true" pageId="4" pageNumber="5">n</emphasis>
= 
<number id="3DA00C1FFFA2FFB30782FAD5FB26FA11" box="[1154,1168,1400,1424]" pageId="4" pageNumber="5" value="8.0">8</number>
) mean length 
<number id="3DA00C1FFFA2FFB30636FAD5FAD8FA11" box="[1334,1390,1400,1424]" pageId="4" pageNumber="5" value="1387.0">1387</number>
Μm (range 
<number id="3DA00C1FFFA2FFB303E6FA36FED2FA32" box="[230,356,1435,1459]" pageId="4" pageNumber="5" value="1376.0" valueMax="1449.0" valueMin="1303.0">
<date id="FFD638F8FFA2FFB303E6FA36FED2FA32" box="[230,356,1435,1459]" pageId="4" pageNumber="5" value="1303" valueMax="1449">1303–1449</date>
</number>
Μm); mean breadth 
<number id="3DA00C1FFFA2FFB3014AFA36FDC2FA32" box="[586,628,1435,1459]" pageId="4" pageNumber="5" value="712.0">712</number>
