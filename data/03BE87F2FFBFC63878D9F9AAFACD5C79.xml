<quantity id="4CEF9B01FFBEC63B792EFF68FE6A5FD1" box="[352,463,229,255]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" pageId="13" pageNumber="14" unit="mm" value="1.45">1.45 mm</quantity>
