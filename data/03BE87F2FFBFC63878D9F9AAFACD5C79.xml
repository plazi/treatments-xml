<quantity id="4CEF9B01FFBEC63B7A34FF68FD4F5FD1" box="[634,746,229,255]" metricMagnitude="-4" metricUnit="m" metricValue="8.8" pageId="13" pageNumber="14" unit="mm" value="0.88">0.88 mm</quantity>
