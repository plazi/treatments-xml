locality in 
<collectingCountry id="6E83ED8CFB2EFFACFBF3FC831D7EFC09" box="[1030,1126,873,899]" name="Ecuador" pageId="32" pageNumber="33">Ecuador</collectingCountry>
. 
<location id="134BFBC7FB2EFFACFB85FC831D99FC09" LSID="urn:lsid:plazi:treatment:9E3D1C0AFB11FFACFF62F9B719EEFC2D:134BFBC7FB2EFFACFB85FC831D99FC09" box="[1136,1153,873,899]" country="Ecuador" name="It" pageId="32" pageNumber="33">It</location>
