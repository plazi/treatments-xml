<taxonomicName id="F12572B5B8D4601AC9384A3AE40C69C9" class="Insecta" family="Chrysididae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis fuscipennis" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="fuscipennis">
