<quantity id="4CF296D5FF91FFC6FDBEF98BF07199FD" box="[585,635,1652,1669]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="416" unit="mm" value="1.0">1 mm</quantity>
