<taxonomicName id="04B211ABC942FF895C09FC8FFC1719BA" authorityName="Regan" authorityYear="1908" box="[769,1022,880,904]" class="Actinopterygii" family="Amblycipitidae" genus="Liobagrus" kingdom="Animalia" order="Siluriformes" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="formosanus">
