<taxonomicName id="63BC4D09FFCF992EFBB6FB40BEB1592C" baseAuthorityName="Schreber" baseAuthorityYear="1780" box="[1136,1412,1268,1293]" class="Mammalia" family="Muridae" genus="Clethrionomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="213" phylum="Chordata" rank="species" species="glareolus">
