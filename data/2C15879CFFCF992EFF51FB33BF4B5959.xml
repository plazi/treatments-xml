<taxonomicName id="63BC4D09FFCF992EFCBAFB40BF77592C" baseAuthorityName="Schreber" baseAuthorityYear="1780" box="[892,1090,1268,1293]" class="Mammalia" family="Muridae" genus="Myodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="213" phylum="Chordata" rank="species" species="glareolus">
