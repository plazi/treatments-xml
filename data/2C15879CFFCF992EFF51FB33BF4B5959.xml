<taxonomicName id="63BC4D09FFCF992EFD7CFB41B850592D" box="[698,869,1269,1292]" class="Mammalia" family="Cricetidae" genus="Pitymys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="213" phylum="Chordata" rank="species" species="majori">
