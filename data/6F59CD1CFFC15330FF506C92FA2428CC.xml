<emphasis id="D584A018FFC15330FC336B5FFBE72875" box="[1012,1106,1123,1149]" italics="true" pageId="22" pageNumber="411">Quercus</emphasis>
</taxonomicName>
(
<taxonomicName id="20F00789FFC15330FBA66B5EFB7A2874" authorityName="Dumortier" authorityYear="1829" box="[1121,1231,1122,1148]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
), 
<taxonomicName id="20F00789FFC15330FB256B5EFA8C2874" box="[1250,1337,1122,1148]" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="genus">
<emphasis id="D584A018FFC15330FB256B5EFA8C2874" box="[1250,1337,1122,1148]" italics="true" pageId="22" pageNumber="411">Juglans</emphasis>
</taxonomicName>
