<taxonomicName id="20F00789FFC15330FA8F6B5EFF5228A9" class="Magnoliopsida" family="Juglandaceae" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="family">Juglandaceae</taxonomicName>
), 
<taxonomicName id="20F00789FFC15330FF3D6BBBFE8D28A8" box="[250,312,1159,1184]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="genus">
<emphasis id="D584A018FFC15330FF3D6BBBFE8D28A8" box="[250,312,1159,1184]" italics="true" pageId="22" pageNumber="411">Ficus</emphasis>
</taxonomicName>
(
<taxonomicName id="20F00789FFC15330FE816BBAFE0F28A8" box="[326,442,1158,1184]" class="Magnoliopsida" family="Moraceae" kingdom="Plantae" order="Rosales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="family">Moraceae</taxonomicName>
), 
<taxonomicName id="20F00789FFC15330FE0A6BBAFD9C28A8" box="[461,553,1158,1184]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="genus">
<emphasis id="D584A018FFC15330FE0A6BBAFD9C28A8" box="[461,553,1158,1184]" italics="true" pageId="22" pageNumber="411">Populus</emphasis>
</taxonomicName>
(
<taxonomicName id="20F00789FFC15330FDF16BBAFD0528A9" box="[566,688,1158,1185]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
) and 
<taxonomicName id="20F00789FFC15330FD2B6BBBFCA828A8" box="[748,797,1159,1184]" class="Magnoliopsida" family="Vitaceae" genus="Vitis" kingdom="Plantae" order="Vitales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="genus">
<emphasis id="D584A018FFC15330FD2B6BBBFCA828A8" box="[748,797,1159,1184]" italics="true" pageId="22" pageNumber="411">Vitis</emphasis>
</taxonomicName>
(
<taxonomicName id="20F00789FFC15330FCEC6BBAFC3A28A9" box="[811,911,1158,1185]" class="Magnoliopsida" family="Vitaceae" kingdom="Plantae" order="Vitales" pageId="22" pageNumber="411" phylum="Tracheophyta" rank="family">Vitaceae</taxonomicName>
) (
<bibRefCitation id="836101FBFFC15330FC626BBAFBFB28A9" author="Liu, L. Y. &amp; Ghahari, H. &amp; Beaver, R. A." box="[933,1102,1158,1185]" pageId="22" pageNumber="411" pagination="1 - 22" refId="ref17819" refString="Liu, L. Y., Ghahari, H. &amp; Beaver, R. A. (2016) An annotated synopsis of the powder post beetles of Iran (Coleoptera: Bostrichoidea: Bostrichidae). Journal of Insect Biodiversity, 4 (14), 1 - 22. https: // doi. org / 10.12976 / jib / 2016.4.14" type="journal article" year="2016">
Liu 
<emphasis id="D584A018FFC15330FC146BBBFBBE28A8" box="[979,1035,1158,1184]" italics="true" pageId="22" pageNumber="411">et al.</emphasis>
2016
</bibRefCitation>
