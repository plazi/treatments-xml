<collectingRegion id="49B6F82B702DFFE1FCF2FDA55A3CDDB2" box="[816,872,626,652]" country="Argentina" name="Salta" pageId="14" pageNumber="59">Salta</collectingRegion>
(
<collectingRegion id="49B6F82B702DFFE1FCB8FDA55AE2DDB2" box="[890,950,626,652]" country="Argentina" name="Salta" pageId="14" pageNumber="59">Salta</collectingRegion>
, 
<collectingCountry id="F3657659702DFFE1FC00FDA55D67DDB3" box="[962,1075,626,653]" name="Argentina" pageId="14" pageNumber="59">Argentina</collectingCountry>
