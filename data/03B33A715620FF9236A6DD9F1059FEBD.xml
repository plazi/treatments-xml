<taxonomicName id="4C1AF0E45620FF9336A6DD9F172CFE19" authority="(Lamarck, 1819)" baseAuthorityName="Lamarck" baseAuthorityYear="1819" box="[423,841,404,428]" class="Bivalvia" family="Limidae" genus="Ctenoides" kingdom="Animalia" order="Limida" pageId="20" pageNumber="3194" phylum="Mollusca" rank="species" species="annulata">
<emphasis id="B96E57755620FF9336A6DD9F161AFE19" bold="true" box="[423,639,404,428]" italics="true" pageId="20" pageNumber="3194">Ctenoides annulata</emphasis>
