<taxonomicName id="54984D4FFFA9FFB43FC9F978FEDCFBF2" authorityName="Thorell" authorityYear="1890" box="[151,261,1045,1071]" class="Arachnida" family="Liocranidae" genus="Sphingius" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="557" phylum="Arthropoda" rank="genus">
