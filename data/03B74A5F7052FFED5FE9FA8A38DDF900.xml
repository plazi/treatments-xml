<taxonomicName id="4C1E80CA7052FFED5E5BFAC23C99FAAC" box="[293,465,1375,1397]" class="Insecta" family="Aleyrodidae" genus="Lukotekia" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="328" phylum="Arthropoda" rank="species" species="menae" status="sp. n." subOrder="Sternorrhyncha">
<emphasis id="B96A275B7052FFED5E5BFAC23C99FAAC" box="[293,465,1375,1397]" italics="true" pageId="9" pageNumber="328">Lukotekia menae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2599A207052FFED5EA6FAC23F59FAAD" box="[472,529,1375,1396]" pageId="9" pageNumber="328" rank="species">
