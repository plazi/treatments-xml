<taxonomicName id="4C6B35E7FFC2FFE6378B8ADADEDF0D8E" authority="HROMADKA 2012" authorityName="HROMADKA" authorityYear="2012" class="Insecta" family="Staphylinidae" genus="Philonthus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="716" phylum="Arthropoda" rank="species" species="centropyge">
<emphasis id="B91F9276FFC2FFE6378B8ADADDA00D91" box="[703,937,972,993]" italics="true" pageId="3" pageNumber="716">Philonthus centropyge</emphasis>
<bibRefCitation id="EFFA3395FFC2FFE636808ADADEDF0D8E" author="HROMADKA L" pageId="3" pageNumber="716" pagination="137 - 178" refId="ref2221" refString="HROMADKA L. (2012): Revision of Afrotropical species of the Philonthus interocularis species group (Coleoptera: Staphylinidae: Philonthina). - Studies and Reports, Taxonomical Series 8: 137 - 178." type="book chapter" year="2012">HROMÁDKA 2012</bibRefCitation>
</taxonomicName>
(
<taxonomicName id="4C6B35E7FFC2FFE635DC8AFEDF820D8D" authorityName="Bernhauer" authorityYear="1915" box="[232,395,1000,1021]" class="Insecta" family="Staphylinidae" genus="Philonthus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="716" phylum="Arthropoda" rank="species" species="interocularis">
<emphasis id="B91F9276FFC2FFE635DC8AFEDF820D8D" box="[232,395,1000,1021]" italics="true" pageId="3" pageNumber="716">P. interocularis</emphasis>
</taxonomicName>
group – cf. 
<bibRefCitation id="EFFA3395FFC2FFE637228AFEDD0E0D8E" author="HROMADKA L" box="[534,775,1000,1022]" pageId="3" pageNumber="716" pagination="137 - 178" refId="ref2221" refString="HROMADKA L. (2012): Revision of Afrotropical species of the Philonthus interocularis species group (Coleoptera: Staphylinidae: Philonthina). - Studies and Reports, Taxonomical Series 8: 137 - 178." type="book chapter" year="2012">HROMÁDKA 2012: 144</bibRefCitation>
), which is a junior primary homonym of 
<taxonomicName id="4C6B35E7FFC2FFE6340C8D12DCE30A6A" authority="HROMADKA 2011" authorityName="HROMADKA" authorityYear="2011" box="[312,746,1028,1050]" class="Insecta" family="Staphylinidae" genus="Philonthus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="716" phylum="Arthropoda" rank="species" species="centropyge">
