<taxonomicName id="4C334D77FFFFFFE9D18C0F119D30FBE0" authority="Miller, 1951" authorityName="Miller" authorityYear="1951" box="[151,594,1066,1093]" class="Insecta" family="Reduviidae" genus="Graptocleptes" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="389" phylum="Arthropoda" rank="species" species="formosus">
