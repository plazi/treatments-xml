<elevation id="000BD1F2FFF5A212AD5FFDF9FB252061" box="[1142,1221,526,552]" metricMagnitude="2" metricUnit="m" metricValue="2.2199999999999998" pageId="36" pageNumber="37" unit="m" value="222.0">222 m</elevation>
