<quantity id="FD27F997EA16FFFEFA8045152977B063" box="[1323,1405,865,889]" metricMagnitude="-2" metricUnit="m" metricValue="4.6" pageId="3" pageNumber="4" unit="mm" value="46.0">46 mm</quantity>
