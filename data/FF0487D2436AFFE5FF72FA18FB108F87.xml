<paragraph id="771236C4436AFFE5FF22F98BFCEF8E89" blockId="10.[151,1436,1577,1924]" box="[199,890,1649,1674]" pageId="10" pageNumber="551">
<materialsCitation id="C7C53C99436AFFE5FF22F98BFCEF8E89" ID-GBIF-Occurrence="1326058831" box="[199,890,1649,1674]" country="Brazil" location="Rio Solimoes" pageId="10" pageNumber="551" specimenCount="1" stateProvince="Amazonas">
Type-locality: Manacapuru, 
<location id="7272601F436AFFE5FDE0F98BFD0A8E89" LSID="urn:lsid:plazi:treatment:FF0487D2436AFFE5FF72FA18FB108F87:7272601F436AFFE5FDE0F98BFD0A8E89" box="[517,671,1649,1674]" country="Brazil" name="Rio Solimoes" pageId="10" pageNumber="551" stateProvince="Amazonas">Rio Solimões</location>
, 
<collectingRegion id="B569F826436AFFE5FD4FF988FCB68E89" box="[682,803,1650,1674]" country="Colombia" name="Amazonas" pageId="10" pageNumber="551">Amazonas</collectingRegion>
, 
<collectingCountry id="0FBA7654436AFFE5FCCBF98BFCE38E89" box="[814,886,1649,1674]" name="Brazil" pageId="10" pageNumber="551">Brazil</collectingCountry>
