<taxonomicName id="D9B34D78FFE5FFADFC34FCE3FB756A0A" authority="Pomel, 1883" authorityName="Pomel" authorityYear="1883" box="[1000,1272,845,869]" class="Hydrozoa" family="Bythotiaridae" genus="Heterotiara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecatae" pageId="4" pageNumber="153" phylum="Cnidaria" rank="genus">
