<taxonomicName id="D9B34D78FFE5FFADFDFEFCDCFCA46AE7" authority="Maas, 1905" authorityName="Maas" authorityYear="1905" box="[546,809,880,904]" class="Hydrozoa" family="Bythotiaridae" genus="Heterotiara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecatae" pageId="4" pageNumber="153" phylum="Cnidaria" rank="genus">
<emphasis id="2CC7EAE9FFE5FFADFDFEFCDCFD126AE7" box="[546,671,881,904]" italics="true" pageId="4" pageNumber="153">Heterotiara</emphasis>
<bibRefCitation id="7A224B0AFFE5FFADFD76FCDDFCA46AE7" author="Maas, O." box="[682,809,880,904]" journalOrPublisher="Siboga-Expeditie" pageId="4" pageNumber="153" pagination="1 - 84" part="10" refString="Maas, O. 1905. Die Craspedoten Medusen der Siboga-Expeditie. Siboga-Expeditie, 10: 1 - 84." title="Die Craspedoten Medusen der Siboga-Expeditie" type="journal article" year="1905">
Maas, 
<number id="A87B24DCFFE5FFADFD28FCDDFCA46AE7" box="[756,809,880,904]" pageId="4" pageNumber="153" value="1905.0">1905</number>
</bibRefCitation>
</taxonomicName>
