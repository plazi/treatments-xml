<taxonomicName id="D9B34D78FFE5FFADFE42FCE3FDAF6A0A" box="[414,546,846,869]" class="Hydrozoa" family="Bythotiaridae" genus="Protiaropsis" kingdom="Animalia" order="Anthoathecata" pageId="4" pageNumber="153" phylum="Cnidaria" rank="genus">
<emphasis id="2CC7EAE9FFE5FFADFE42FCE3FDAF6A0A" box="[414,546,846,869]" italics="true" pageId="4" pageNumber="153">Protiaropsis</emphasis>
</taxonomicName>
to replace 
<emphasis id="2CC7EAE9FFE5FFADFD7CFCE3FC936A0A" box="[672,798,846,869]" italics="true" pageId="4" pageNumber="153">Heteratiara</emphasis>
<bibRefCitation id="7A224B0AFFE5FFADFCFBFCE3FC246A0A" author="Maas, O." box="[807,937,845,869]" journalOrPublisher="Siboga-Expeditie" pageId="4" pageNumber="153" pagination="1 - 84" part="10" refString="Maas, O. 1905. Die Craspedoten Medusen der Siboga-Expeditie. Siboga-Expeditie, 10: 1 - 84." title="Die Craspedoten Medusen der Siboga-Expeditie" type="journal article" year="1905">
Maas, 
<number id="A87B24DCFFE5FFADFCADFCE0FC246A0A" box="[881,937,845,869]" pageId="4" pageNumber="153" value="1905.0">1905</number>
</bibRefCitation>
