<taxonomicName id="4C1FF9D6005AFFA99576BE5DFB45FCC1" authorityName="Pekarsky" authorityYear="2011" box="[1059,1215,829,853]" class="Insecta" family="Noctuidae" genus="Stenoloba" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="315" phylum="Arthropoda" rank="species" species="viridicollar">
