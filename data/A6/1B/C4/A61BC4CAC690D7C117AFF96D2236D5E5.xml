<taxonomicName baseAuthorityName="Kirby" baseAuthorityYear="1837" class="Hexapoda" family="Carabidae" genus="Pterostichus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="503" pageNumber="504" phylum="Arthropoda" rank="species" species="nitidus">
