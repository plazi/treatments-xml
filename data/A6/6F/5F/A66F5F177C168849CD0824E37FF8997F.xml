<paragraph pageId="null" pageNumber="540">
<taxonomicName authority="Schkuhr" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="540" phylum="Tracheophyta" rank="species" species="uliginosus">
