<paragraph blockId="1.[151,482,706,733]" box="[151,482,706,733]" pageId="1" pageNumber="56">
<heading bold="true" box="[151,482,706,733]" fontSize="11" level="1" pageId="1" pageNumber="56" reason="1">
<emphasis bold="true" box="[151,482,706,733]" pageId="1" pageNumber="56">
<taxonomicName box="[151,415,707,733]" class="Arachnida" family="Damaeidae" genus="Porobelba" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="weigmanni" status="n.sp.">
<emphasis bold="true" box="[151,415,707,733]" italics="true" pageId="1" pageNumber="56">Porobelba weigmanni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[423,482,706,732]" pageId="1" pageNumber="56" rank="species">n.sp.</taxonomicNameLabel>
</emphasis>
