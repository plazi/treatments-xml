<collectingCountry box="[544,653,1277,1301]" name="Argentina" pageId="13" pageNumber="1388">Argentina</collectingCountry>
and 
<collectingCountry box="[705,767,1277,1301]" name="Chile" pageId="13" pageNumber="1388">Chile</collectingCountry>
