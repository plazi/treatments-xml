Center 
<collectingCountry box="[403,511,1903,1927]" name="Colombia" pageId="12" pageNumber="1387">Colombia</collectingCountry>
, northeastern and southwestern 
<collectingCountry box="[267,336,1938,1962]" name="Brazil" pageId="12" pageNumber="1387">Brazil</collectingCountry>
, southern 
<collectingCountry box="[463,545,1938,1962]" name="Bolivia" pageId="12" pageNumber="1387">Bolivia</collectingCountry>
, northern 
<collectingCountry box="[671,773,1938,1962]" name="Paraguay" pageId="12" pageNumber="1387">Paraguay</collectingCountry>
, eastern 
<collectingCountry box="[253,362,1972,1996]" name="Argentina" pageId="12" pageNumber="1387">Argentina</collectingCountry>
and 
<collectingCountry box="[414,476,1972,1996]" name="Chile" pageId="12" pageNumber="1387">Chile</collectingCountry>
.
</paragraph>
