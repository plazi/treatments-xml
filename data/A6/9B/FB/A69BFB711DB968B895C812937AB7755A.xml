<geoCoordinate direction="north" orientation="latitude" precision="5" value="46.1152">46.1152°N</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" precision="5" value="-67.7632">67.7632°W</geoCoordinate>
