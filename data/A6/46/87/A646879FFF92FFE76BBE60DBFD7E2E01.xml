6. 
<taxonomicName authorityName="EDWARDS" authorityYear="1838" box="[177,339,470,492]" class="Stenolaemata" family="Horneridae" genus="Hornera" kingdom="Animalia" order="Cyclostomatida" pageId="10" pageNumber="35" phylum="Bryozoa" rank="species" species="striata">
