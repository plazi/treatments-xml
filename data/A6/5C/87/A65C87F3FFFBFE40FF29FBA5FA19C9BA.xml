</taxonomicName>
(
<typeStatus box="[387,432,1198,1219]" pageId="1" pageNumber="55">type</typeStatus>
species 
<taxonomicName authority="Wang 1990" authorityName="Wang" authorityYear="1990" box="[518,811,1197,1219]" class="Insecta" family="Tephritidae" genus="Acrorellia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="55" phylum="Arthropoda" rank="species" species="sinica">
