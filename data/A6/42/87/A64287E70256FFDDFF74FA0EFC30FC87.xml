<geoCoordinate box="[1221,1330,260,284]" direction="east" orientation="longitude" pageId="47" pageNumber="198" precision="925" value="128.16667">
<number box="[1221,1266,260,284]" pageId="47" pageNumber="198" value="128.0">128</number>
Â° 
<number box="[1276,1305,260,284]" pageId="47" pageNumber="198" value="10.0">10</number>
â€™E
</geoCoordinate>
) to Cape Pallarenda, Townsville, Qld (
<geoCoordinate box="[486,595,295,320]" direction="east" orientation="longitude" pageId="47" pageNumber="198" precision="925" value="146.76666">
<number box="[486,531,295,320]" pageId="47" pageNumber="198" value="146.0">146</number>
Â° 
<number box="[540,571,295,320]" pageId="47" pageNumber="198" value="46.0">46</number>
â€™E
</geoCoordinate>
) and Irian Jaya, western New 
<collectingCountry box="[951,1035,295,320]" name="Guinea-Bissau" pageId="47" pageNumber="198">Guinea</collectingCountry>
