<taxonomicName authorityName="Larivière &amp; Larochelle" authorityYear="2018" box="[422,589,1995,2017]" class="Insecta" family="Saldidae" genus="Kiwisaldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="januszkiewiczi">
