<emphasis box="[1265,1364,1960,1981]" italics="true" pageId="5" pageNumber="156">K. yangae</emphasis>
</taxonomicName>
, (2) 
<taxonomicName authorityName="Larivière &amp; Larochelle" authorityYear="2018" class="Insecta" family="Saldidae" genus="Kiwisaldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="cranshawi">
<emphasis italics="true" pageId="5" pageNumber="156">K. cranshawi</emphasis>
</taxonomicName>
, (3) 
<taxonomicName authorityName="Larivière &amp; Larochelle" authorityYear="2018" box="[297,376,1996,2017]" class="Insecta" family="Saldidae" genus="Kiwisaldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="ryani">
