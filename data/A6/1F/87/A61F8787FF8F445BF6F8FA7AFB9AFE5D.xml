<taxonomicName authority="(Carter, 1882)" authorityName="Carter" authorityYear="1882" baseAuthorityName="Carter" baseAuthorityYear="1882" box="[453,829,296,320]" class="Demospongiae" family="Phloeodictyidae" genus="Oceanapia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplosclerida" pageId="7" pageNumber="370" phylum="Porifera" rank="species" species="isodictyiformis">
