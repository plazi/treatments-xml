<emphasis bold="true" box="[1343,1436,188,213]" pageId="7" pageNumber="370">sp. nov.</emphasis>
</taxonomicNameLabel>
<taxonomicName authority="(Van Soest &amp; Sass, 1981)" authorityName="Van Soest &amp; Sass" authorityYear="1981" baseAuthorityName="Van Soest &amp; Sass" baseAuthorityYear="1981" box="[151,780,224,248]" class="Demospongiae" family="Phloeodictyidae" genus="Oceanapia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplosclerida" pageId="7" pageNumber="370" phylum="Porifera" rank="species" species="penicilliformis">
