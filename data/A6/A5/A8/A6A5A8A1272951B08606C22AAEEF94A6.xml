<paragraph pageId="0" pageNumber="1">Type species.</paragraph>
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Germar" authorityYear="1818" class="Insecta" family="Staphylinidae" genus="Scaphidium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Scaphidium haemorrhoidalis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="haemorrhoidalis">
