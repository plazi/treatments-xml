<paragraph blockId="9.[151,671,1898,1924]" box="[151,671,1898,1924]" pageId="9" pageNumber="10">
<heading bold="true" box="[151,671,1898,1924]" fontSize="11" level="1" pageId="9" pageNumber="10" reason="1">
<emphasis bold="true" box="[151,671,1898,1924]" pageId="9" pageNumber="10">
<taxonomicName authority="Reshchikov" authorityName="Reshchikov" box="[151,571,1898,1924]" class="Insecta" family="Ichneumonidae" genus="Lathrolestes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="thomsoni" status="sp. nov.">
<emphasis bold="true" box="[151,418,1898,1924]" italics="true" pageId="9" pageNumber="10">Lathrolestes thomsoni</emphasis>
Reshchikov
</taxonomicName>
<taxonomicNameLabel box="[578,671,1898,1924]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
