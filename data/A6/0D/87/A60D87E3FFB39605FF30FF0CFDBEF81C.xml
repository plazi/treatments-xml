<paragraph blockId="5.[151,1437,259,2009]" pageId="5" pageNumber="397">
<taxonomicName box="[199,342,1030,1054]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="5" pageNumber="397" phylum="Nematoda" rank="genus">
