<emphasis box="[619,631,914,937]" italics="true" pageId="5" pageNumber="397">c</emphasis>
<number box="[631,639,928,942]" pageId="5" pageNumber="397" value="1.0">1</number>
&lt;
<emphasis box="[655,667,914,937]" italics="true" pageId="5" pageNumber="397">c</emphasis>
<number box="[668,676,928,942]" pageId="5" pageNumber="397" value="1.0">1</number>
– 
<emphasis box="[689,703,913,937]" italics="true" pageId="5" pageNumber="397">d</emphasis>
<number box="[703,711,928,942]" pageId="5" pageNumber="397" value="1.0">1</number>
