(
<number box="[1049,1066,1284,1308]" pageId="5" pageNumber="397" value="2.0">2</number>
)- 
<number box="[1084,1099,1284,1308]" pageId="5" pageNumber="397" value="5.0">5</number>
(
<number box="[1109,1120,1284,1308]" pageId="5" pageNumber="397" value="1.0">1</number>
); II: 
<number box="[1180,1215,1284,1308]" pageId="5" pageNumber="397" value="2.0" valueMax="3.0" valueMin="1.0">1-3</number>
- 
<number box="[1225,1243,1284,1308]" pageId="5" pageNumber="397" value="2.0">2</number>
(
<number box="[1253,1264,1284,1308]" pageId="5" pageNumber="397" value="1.0">1</number>
)- 
<number box="[1284,1299,1284,1308]" pageId="5" pageNumber="397" value="3.0">3</number>
(
<number box="[1309,1320,1284,1308]" pageId="5" pageNumber="397" value="1.0">1</number>
), III: 
<number box="[1389,1404,1284,1308]" pageId="5" pageNumber="397" value="2.0">2</number>
- 
