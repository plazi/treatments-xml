<taxonomicName authorityName="Leerat., Chalermglin &amp; R. M. K. Saunders" authorityYear="2021" class="Magnoliopsida" family="Annonaceae" genus="Goniothalamus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Goniothalamus roseipetalus" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="roseipetalus">
<emphasis italics="true" pageId="0" pageNumber="1">Goniothalamus roseipetalus</emphasis>
</taxonomicName>
and 
<taxonomicName genus="G." lsidName="G. sukhirinensis" pageId="0" pageNumber="1" rank="species" species="sukhirinensis">
