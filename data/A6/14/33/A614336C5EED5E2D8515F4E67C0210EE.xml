<subSubSection pageId="0" pageNumber="1" type="etymology">
<paragraph pageId="0" pageNumber="1">Etymology.</paragraph>
<paragraph pageId="0" pageNumber="1">In reference to the red pigmentation of the petals.</paragraph>
</subSubSection>
