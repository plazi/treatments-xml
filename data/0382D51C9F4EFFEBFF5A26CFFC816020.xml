<number id="3DE3762D9F4EFFEDFEA827B2FE69633E" box="[357,418,490,514]" pageId="19" pageNumber="38" value="2007.0">2007</number>
</date>
, S05 (
<quantity id="4CD3C9EF9F4EFFEDFE3C27B2FDFE633E" box="[497,565,490,514]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="38" unit="m" value="20.0">20 m</quantity>
