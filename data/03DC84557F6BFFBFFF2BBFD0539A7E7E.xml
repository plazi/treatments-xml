species: 
<taxonomicName id="4C754EC07F6BFFBFFEB3BF8954457BE5" authority="Hartlaub, 1907" authorityName="Hartlaub" authorityYear="1907" box="[272,630,980,1002]" class="Hydrozoa" family="Eucodoniidae" genus="Eucodonium" kingdom="Animalia" order="Anthoathecata" pageId="1" pageNumber="49" phylum="Cnidaria" rank="species" species="brownei">
