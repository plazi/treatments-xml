<collectionCode id="FF8AAE7E9B7CFFF9FEB3F9FAFEEEF9D7" box="[311,347,1549,1572]" pageId="37" pageNumber="38">BL</collectionCode>
<quantity id="5E639B5E9B7CFFF9FEE1F9FBFDB0F9D6" box="[357,517,1548,1573]" metricMagnitude="-3" metricUnit="m" metricValue="2.025" metricValueMax="2.2" metricValueMin="1.85" pageId="37" pageNumber="38" unit="mm" value="2.025" valueMax="2.2" valueMin="1.85">1.85–2.20mm</quantity>
