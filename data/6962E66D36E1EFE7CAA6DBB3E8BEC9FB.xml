, Llano Bonito, Refugio, 
<geoCoordinate id="1BEDC60CF580593061D00B41698A7365" direction="north" orientation="latitude" precision="15" value="9.454444">
09°27'16&quot;
<normalizedToken id="601117913BCF8E63A7C3B4770702C751" originalValue="N–">N-</normalizedToken>
</geoCoordinate>
<geoCoordinate id="C8371F56EB8C634FA05D51B4C896E37B" direction="west" orientation="longitude" precision="15" value="-83.54472">083°32'41&quot;W</geoCoordinate>
