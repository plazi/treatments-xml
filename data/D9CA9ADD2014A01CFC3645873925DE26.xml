<taxonomicName id="54B6760E07A0613593EBB03CC4CE56A8" class="Magnoliopsida" family="Brassicaceae" genus="Cardamine" kingdom="Plantae" order="Brassicales" pageId="null" pageNumber="211" phylum="Tracheophyta" rank="species" species="palustris">
