<normalizedToken id="3658A8C774EDA3054921AA297FF4B917" originalValue="Früchte" pageId="null" pageNumber="211">Fruechte</normalizedToken>
; 
<normalizedToken id="456FB146A958AF0A6A7CB35201C1B47D" originalValue="Kelchblätter" pageId="null" pageNumber="211">Kelchblaetter</normalizedToken>
