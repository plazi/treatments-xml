, 
<taxonomicName id="4C1C4D48C106B170CD15CBB8FCBDFE6D" box="[597,772,438,463]" class="Reptilia" family="Typhlopidae" genus="Anilios" higherTaxonomySource="CoL" kingdom="Animalia" order="Squamata" pageId="60" pageNumber="61" phylum="Chordata" rank="species" species="troglodytes">
