<emphasis id="B94EEAE19569465AFD9FFAE28829FAC1" box="[621,639,1349,1374]" italics="true" pageId="50" pageNumber="51">E</emphasis>
. 
<emphasis id="B94EEAE19569465AFD7CFAE2889AFAC1" box="[654,716,1349,1374]" italics="true" pageId="50" pageNumber="51">incus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C3A4D709569465AFD29FAE28901FAC1" authorityName="Mello-Leitao" authorityYear="1931" box="[731,855,1349,1375]" class="Arachnida" family="Gonyleptidae" genus="Eusarcus" kingdom="Animalia" order="Opiliones" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="pusillus">
<emphasis id="B94EEAE19569465AFD29FAE288BBFAC1" box="[731,749,1349,1374]" italics="true" pageId="50" pageNumber="51">E</emphasis>
