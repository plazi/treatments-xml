<subSubSection id="C3556504FFFAFF8CF392FA472ADB7225" pageId="25" pageNumber="26" type="diagnosis">
<paragraph id="8BF0368FFFFAFF8CF392FA472ADB7225" blockId="25.[151,1436,926,1563]" pageId="25" pageNumber="26">
<emphasis id="B93BEA9DFFFAFF8CF392FA472F4971EC" bold="true" box="[199,321,1466,1491]" inLineHeading="true" pageId="25" pageNumber="26" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFFFAFF8CF212FA462FDC71EC" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[327,468,1467,1491]" class="Insecta" family="Derbidae" genus="Alara" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="lingua">
