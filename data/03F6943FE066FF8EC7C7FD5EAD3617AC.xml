<quantity id="4CA788CCE066FF8EC539FA20AAE31595" box="[617,706,1482,1507]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="19" pageNumber="521" unit="mm" value="2.6">2.6 mm</quantity>
. 
<emphasis id="B92BF93BE066FF8EC59CFA20AB021594" bold="true" box="[716,803,1482,1507]" pageId="19" pageNumber="521">Female</emphasis>
