<paragraph id="569195B7ECED1D9F7821F18433CCA5F9" pageId="3" pageNumber="68">
<normalizedToken id="AF957DB0585F0B87F1278EDDDFF89C6F" originalValue="“Brasilien”">&quot;
<materialsCitation id="E1325A46564BAFB758BB40826EE29E84" collectingDate="2065-03" country="Brasilien" location="Regnel" municipality="Caldas" specimenCount="1" stateProvince="Minas Gerais" typeStatus="holotype">
