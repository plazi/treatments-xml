<collectingMunicipality id="2F1ABEDE65C1A9FD4D3CACC49EF77FEE">Caldas</collectingMunicipality>
, 
<emphasis id="9148B467666270AAD13366F2991953B4" italics="true" pageId="3" pageNumber="68">
<location id="241C37775AAF3A9E910EAA239015B401" LSID="urn:lsid:plazi:treatment:88E383C9675E54628F7339E3031B8745:241C37775AAF3A9E910EAA239015B401" country="Brasilien" municipality="Caldas" name="Regnel" stateProvince="Minas Gerais">Regnel</location>
<collectingDate id="2A1528429FF6067AA6454351E9165B6A" value="2065-03">III 2065</collectingDate>
</emphasis>
