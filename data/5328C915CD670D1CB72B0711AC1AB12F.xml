<emphasis id="E9F5A411CD670D18B6200419AC51B56B" bold="true" box="[593,625,1635,1652]" pageId="9" pageNumber="52">A -E</emphasis>
, 
<taxonomicName id="1C810380CD670D18B60C0419ACD7B56B" authorityName="C.E.Weiss" authorityYear="1876" box="[637,759,1635,1652]" class="Polypodiopsida" family="Calamitaceae" genus="Palaeostachya" higherTaxonomySource="GBIF" kingdom="Plantae" order="Equisetales" pageId="9" pageNumber="52" phylum="Tracheophyta" rank="genus">
