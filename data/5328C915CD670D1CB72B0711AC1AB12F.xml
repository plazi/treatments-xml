<taxonomicName id="1C810380CD650D1AB57A0403AC26B595" authority="Corda" authorityName="Corda" box="[267,518,1657,1674]" class="Polypodiopsida" family="Psalixochlaenaceae" genus="Anachoropteris" higherTaxonomySource="GBIF" kingdom="Plantae" order="Zygopteridales" pageId="11" pageNumber="54" phylum="Tracheophyta" rank="species" species="pulchra">
