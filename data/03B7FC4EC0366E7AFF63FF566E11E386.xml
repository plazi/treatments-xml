<quantity id="4CE6E0BDC0366E7AFE9DFE7B6D66E25D" box="[368,524,506,530]" metricMagnitude="-1" metricUnit="m" metricValue="9.84" metricValueMax="12.0" metricValueMin="7.68" pageId="11" pageNumber="336" unit="mm" value="984.0" valueMax="1200.0" valueMin="768.0">768 to 1200 mm</quantity>
