<geoCoordinate id="EE6950329D71FFC6FE3DF9D630CAF980" box="[407,497,1592,1617]" degrees="07" direction="north" minutes="47" orientation="latitude" pageId="13" pageNumber="664" precision="925" value="7.7833333">07°47′N</geoCoordinate>
