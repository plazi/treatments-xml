<taxonomicName id="30AB2FA5FFEDFFB369B9406F7D4C6A46" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[198,361,1555,1580]" class="Aves" family="Paridae" genus="Periparus" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="ater">
