</taxonomicName>
from 
<taxonomicName id="30AB2FA5FFEDFFB36D9D4393794B6A62" authorityName="Linnaeus" authorityYear="1758" box="[1250,1390,1519,1544]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="major">
<emphasis id="C5DF8834FFEDFFB36D9D4393794B6A62" box="[1250,1390,1519,1544]" italics="true" pageId="31" pageNumber="32">Parus major</emphasis>
</taxonomicName>
