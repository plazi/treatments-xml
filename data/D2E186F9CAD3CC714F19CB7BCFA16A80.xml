<normalizedToken id="FBABD4B196D6B0CE951FFB2D69DED433" originalValue="São">Sao</normalizedToken>
<normalizedToken id="2608640212D4A8FD09784F52BF5CD9BD" originalValue="João">Joao</normalizedToken>
