<typeStatus id="54CD884EFFDB4C4DFEE8EDE76EFDF86B" box="[354,461,1965,1991]" pageId="39" pageNumber="40">Holotype</typeStatus>
, length 
<quantity id="4C8E9B09FFDB4C4DFDAFEDE76DB4F864" box="[549,644,1965,1992]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" pageId="39" pageNumber="40" unit="mm" value="1.25">1.25mm</quantity>
, height 
<quantity id="4C8E9B09FFDB4C4DFD57EDE76C0CF86B" box="[733,828,1965,1991]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="39" pageNumber="40" unit="mm" value="0.8">0.80mm</quantity>
, width 
<quantity id="4C8E9B09FFDB4C4DFC04EDE76CDEF864" box="[910,1006,1965,1992]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" pageId="39" pageNumber="40" unit="mm" value="0.58">0.58mm</quantity>
.
</paragraph>
