<taxonomicName id="E68F4D65FFA1FFFB01C7826AFCEEF99C" authority="(Bloch)" authorityName="Bloch" baseAuthorityName="Bloch" baseAuthorityYear="1795" box="[501,840,1607,1632]" class="Actinopterygii" family="Carangidae" genus="Parastromateus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="378" phylum="Chordata" rank="species" species="niger">
