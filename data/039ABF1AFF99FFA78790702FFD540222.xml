<collectingCountry id="F3244E9CFF99FFA7865270E6FE430493" box="[368,487,663,683]" name="Belarus" pageId="25" pageNumber="1320">Weissrussland</collectingCountry>
(MIN), 
<collectingCountry id="F3244E9CFF99FFA7856570E6FDDD0493" box="[583,633,663,683]" name="Poland" pageId="25" pageNumber="1320">Polen</collectingCountry>
(SAW, WAR), 
<collectingCountry id="F3244E9CFF99FFA7840A70E6FC2F0493" box="[808,907,663,683]" name="Czech Republic" pageId="25" pageNumber="1320">Tschechien</collectingCountry>
(
<collectingCountry id="F3244E9CFF99FFA7848370E6FC4A0493" box="[929,1006,663,683]" name="Czech Republic" pageId="25" pageNumber="1320">Böhmen</collectingCountry>
) (LIN), 
<collectingCountry id="F3244E9CFF99FFA787FD70DEFEE804FB" box="[223,332,687,707]" name="Germany" pageId="25" pageNumber="1320">Deutschland</collectingCountry>
(BER, DES, LUN, HAM, MÜN, SNE, ZSM), 
<collectingCountry id="F3244E9CFF99FFA7843170DEFCD904FB" box="[787,893,687,707]" name="Netherlands" pageId="25" pageNumber="1320">Niederlande</collectingCountry>
(LEI, ZSM, ZWA), Grossbritannien (
<collectingCountry id="F3244E9CFF99FFA7865070B6FE1904E3" box="[370,445,711,731]" name="United Kingdom" pageId="25" pageNumber="1320">England</collectingCountry>
) (EDI), 
<collectingCountry id="F3244E9CFF99FFA7852970B6FDF104E3" box="[523,597,711,731]" name="Switzerland" pageId="25" pageNumber="1320">Schweiz</collectingCountry>
(ZWA), 
<collectingCountry id="F3244E9CFF99FFA7858E70B6FCA204E3" box="[684,774,711,731]" name="Austria" pageId="25" pageNumber="1320">Österreich</collectingCountry>
