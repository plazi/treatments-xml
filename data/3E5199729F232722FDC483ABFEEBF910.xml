<subSubSection id="FEE27BEF9F232725FF168253FAC5FEF4" lastPageId="35" lastPageNumber="36" pageId="34" pageNumber="35" type="diagnosis">
<paragraph id="B64728649F232724FF168253FE9CF8E3" blockId="34.[189,310,1861,1887]" box="[189,310,1861,1887]" pageId="34" pageNumber="35">
<heading id="ED0F9F089F232724FF168253FE9CF8E3" bold="true" box="[189,310,1861,1887]" fontSize="11" level="3" pageId="34" pageNumber="35" reason="3">
