<taxonomicName box="[1042,1126,1888,1914]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="7" pageNumber="57" phylum="Magnoliopsida" rank="genus">Acacia</taxonomicName>
thorn scrub (
<number box="[1290,1301,1888,1914]" pageId="7" pageNumber="57" value="1.0">1</number>
F, 
<collectionCode box="[1336,1424,1888,1914]" pageId="7" pageNumber="57">RLWE</collectionCode>
), Hwy. 190, 5 km W of Tehuantepec, 
<date box="[575,709,1926,1952]" pageId="7" pageNumber="57" value="1992-12-07">
<number box="[575,644,1926,1952]" pageId="7" pageNumber="57" value="12.07">12.07</number>
. 
<number box="[647,709,1926,1952]" pageId="7" pageNumber="57" value="1992.0">1992</number>
</date>
, G.H. Nelson (
<number box="[882,893,1926,1952]" pageId="7" pageNumber="57" value="1.0">1</number>
M, 
<collectionCode box="[937,1016,1926,1952]" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="7" pageNumber="57">FSCA</collectionCode>
