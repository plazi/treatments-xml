, 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:3EF9A59B-2473-44CF-BB0F-3846968A6AB8" authority="Weber, 1913" authorityName="Weber" authorityYear="1913" class="Actinopterygii" family="Ariidae" genus="Doiichthys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Doiichthys Weber 1913:532" order="Siluriformes" pageNumber="78" phylum="Chordata" rank="genus">Doiichthys</taxonomicName>
, 
