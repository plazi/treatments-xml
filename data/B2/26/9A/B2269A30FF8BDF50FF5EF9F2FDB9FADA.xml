<emphasis bold="true" box="[199,376,943,968]" pageId="26" pageNumber="27">Type material.</emphasis>
<materialsCitation collectionCode="MNHN" country="Brazil" location="Minas Gerais" pageId="26" pageNumber="27" specimenCount="2" stateProvince="Minas Gerais" typeStatus="holotype">
