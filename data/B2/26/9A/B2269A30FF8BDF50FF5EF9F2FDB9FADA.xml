<typeStatus box="[661,759,943,967]" pageId="26" pageNumber="27">holotype</typeStatus>
(
<figureCitation box="[777,908,943,967]" captionStart="FIGURES 89–97" captionStartId="25.[150,263,1927,1950]" captionTargetBox="[187,1382,267,1897]" captionTargetId="figure-97@25.[187,1399,245,1906]" captionTargetPageId="25" captionText="FIGURES 89–97. 89, Andraegoidus homoplatus, lectotype, male; 90, Andraegoidus homoplatus, male (holotype of A. hassenteufeli); 91, Andraegoidus homoplatus, female; 92–93, Andraegoidus distinguendus, holotype, male; 94–95, Andraegoidus distinguendus, paratype, female; 96–97, Andraegoidus translucidus, holotype, male." pageId="26" pageNumber="27">Figs 92–93</figureCitation>
) and the female 
<typeStatus box="[1108,1204,944,968]" pageId="26" pageNumber="27">paratype</typeStatus>
(
<figureCitation box="[1223,1364,943,967]" captionStart="FIGURES 89–97" captionStartId="25.[150,263,1927,1950]" captionTargetBox="[187,1382,267,1897]" captionTargetId="figure-97@25.[187,1399,245,1906]" captionTargetPageId="25" captionText="FIGURES 89–97. 89, Andraegoidus homoplatus, lectotype, male; 90, Andraegoidus homoplatus, male (holotype of A. hassenteufeli); 91, Andraegoidus homoplatus, female; 92–93, Andraegoidus distinguendus, holotype, male; 94–95, Andraegoidus distinguendus, paratype, female; 96–97, Andraegoidus translucidus, holotype, male." pageId="26" pageNumber="27">Figs. 94–95</figureCitation>
), both from 
<collectingCountry box="[212,280,980,1004]" name="Brazil" pageId="26" pageNumber="27">Brazil</collectingCountry>
(
