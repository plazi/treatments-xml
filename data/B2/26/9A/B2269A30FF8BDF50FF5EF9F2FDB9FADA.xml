<collectionCode box="[471,563,980,1004]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="26" pageNumber="27">MNHN</collectionCode>
