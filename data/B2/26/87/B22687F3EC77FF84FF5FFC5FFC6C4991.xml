<number box="[1288,1304,332,356]" pageId="9" pageNumber="510" value="3.0">3</number>
, along with 
<taxonomicName box="[151,320,367,392]" class="Insecta" family="Chrysomelidae" genus="Sennius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="leucostauros">
