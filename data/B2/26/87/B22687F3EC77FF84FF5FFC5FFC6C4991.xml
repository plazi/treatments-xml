. 
<emphasis box="[178,320,367,392]" italics="true" pageId="9" pageNumber="510">leucostauros</emphasis>
</taxonomicName>
. 
<taxonomicName box="[333,516,367,392]" class="Insecta" family="Chrysomelidae" genus="Sennius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="bondari">
<emphasis box="[333,516,367,392]" italics="true" pageId="9" pageNumber="510">Sennius bondari</emphasis>
