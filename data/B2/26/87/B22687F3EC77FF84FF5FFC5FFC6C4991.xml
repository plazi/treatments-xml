</taxonomicName>
and 
<taxonomicName box="[1177,1308,295,320]" class="Insecta" family="Chrysomelidae" genus="Sennius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="lojaensis">
