<taxonomicName authority="Fieber, 1858" authorityName="Fieber" authorityYear="1858" box="[683,813,1800,1824]" class="Insecta" family="Miridae" genus="Agalliastes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="473" phylum="Arthropoda" rank="species" species="absinthii">
