</taxonomicName>
to 
<taxonomicName box="[399,549,1764,1788]" class="Insecta" family="Miridae" genus="Compsidolon" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="473" phylum="Arthropoda" rank="genus">
<emphasis box="[399,549,1764,1788]" italics="true" pageId="4" pageNumber="473">Compsidolon</emphasis>
</taxonomicName>
by 
<bibRefCitation author="Wagner" box="[608,932,1764,1789]" pageId="4" pageNumber="473" refString="Wagner, E. &amp; Weber, H. H. (1964) Heteropteres Miridae. Faune de Frannce, 67, 489 - 492." type="journal article" year="1964">
Wagner and Weber (
<number box="[863,924,1764,1789]" pageId="4" pageNumber="473" value="1964.0">1964</number>
)
</bibRefCitation>
, and 
<bibRefCitation author="Wagner" box="[1009,1186,1764,1789]" pageId="4" pageNumber="473" refString="Wagner, E. (1965) Die Gattung Compsidolon Ruter, 1899 (Het. Miridae). Notulae Entomologicae, 45, 113 - 137." type="journal article" year="1965">
