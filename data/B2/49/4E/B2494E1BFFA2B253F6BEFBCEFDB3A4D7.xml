<taxonomicName authority="Lendenfeld, 1891" authorityName="Lendenfeld" authorityYear="1891" box="[836,1127,1026,1052]" class="Calcarea" family="Sycanthidae" genus="Sycantha" kingdom="Animalia" order="Leucosolenida" pageId="25" pageNumber="228" phylum="Porifera" rank="genus">
