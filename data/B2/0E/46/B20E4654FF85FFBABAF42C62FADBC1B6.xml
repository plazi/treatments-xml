<taxonomicName baseAuthorityName="Gnaspini" baseAuthorityYear="1996" box="[499,645,1343,1369]" class="Insecta" family="Leiodidae" genus="Paulipalpina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
