<taxonomicName authority="Jeannel, 1936" authorityName="Jeannel" authorityYear="1936" box="[349,666,1886,1911]" class="Insecta" family="Leiodidae" genus="Adelopsis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="ovalis">
