<typeStatus box="[151,252,425,452]" pageId="33" pageNumber="34">holotype</typeStatus>
] in 
<collectionCode box="[294,380,426,452]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="33" pageNumber="34">MNHN</collectionCode>
(Gnaspini, 1996: 541). Labels: “ 
<collectingRegion box="[747,817,425,451]" country="Brazil" name="Bahia" pageId="33" pageNumber="34">Bahia</collectingRegion>
, 
<collectingCountry box="[829,896,425,451]" name="Brazil" pageId="33" pageNumber="34">Brésil</collectingCountry>
(tabacs) / 
<collectorName box="[1011,1160,425,452]" pageId="33" pageNumber="34">A. Grouvelle</collectorName>
