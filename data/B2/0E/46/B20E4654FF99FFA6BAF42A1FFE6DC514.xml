// 
<taxonomicName authority="Prt." authorityName="Prt." class="Insecta" family="Leiodidae" genus="Ptomaphagus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="dispar">Ptomaphagus dispar Prt.</taxonomicName>
