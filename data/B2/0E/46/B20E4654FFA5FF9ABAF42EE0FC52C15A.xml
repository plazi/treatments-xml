male and 
<specimenCount box="[797,896,1620,1647]" pageId="14" pageNumber="15" type="female">1 female</specimenCount>
in 
<collectionCode box="[937,1013,1621,1647]" pageId="14" pageNumber="15">CBCV</collectionCode>
. 
<location LSID="urn:lsid:plazi:treatment:B20E4654FFA5FF9ABAF42EE0FC52C15A:3F78A199FFA5FF9BBE612D3AFBDEC041" box="[1026,1105,1620,1646]" municipality="Probable" name="Labels" pageId="14" pageNumber="15" stateProvince="Falcon">Labels</location>
: “Sierra de 
<collectorName box="[1241,1419,1620,1647]" pageId="14" pageNumber="15">S. Luis Hueque</collectorName>
/ Cueva 1 / m. 700 Edo. FA. [
<collectingRegion box="[459,604,1656,1683]" country="Venezuela" name="Falcon" pageId="14" pageNumber="15">Falcón State</collectingRegion>
