<collectingCountry box="[1083,1198,1731,1753]" name="Colombia" pageId="27" pageNumber="28">Colombia</collectingCountry>
, south to 
<collectingRegion box="[1319,1438,1731,1753]" country="Ecuador" name="Pichincha" pageId="27" pageNumber="28">Pichincha</collectingRegion>
, 
<collectingCountry box="[827,922,1762,1784]" name="Ecuador" pageId="27" pageNumber="28">Ecuador</collectingCountry>
