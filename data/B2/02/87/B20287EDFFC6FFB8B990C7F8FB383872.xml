+ 
<number box="[1248,1297,1050,1075]" pageId="34" pageNumber="35" value="27.6">27.6</number>
+ 
<number box="[1327,1365,1051,1075]" pageId="34" pageNumber="35" value="3.1">3.1</number>
), tibia 
<number box="[151,168,1087,1111]" pageId="34" pageNumber="35" value="2.0">2</number>
: 
<number box="[179,216,1087,1111]" pageId="34" pageNumber="35" value="8.9">8.9</number>
, tibia 
<number box="[284,301,1087,1111]" pageId="34" pageNumber="35" value="3.0">3</number>
: 
<number box="[312,349,1086,1111]" pageId="34" pageNumber="35" value="6.3">6.3</number>
