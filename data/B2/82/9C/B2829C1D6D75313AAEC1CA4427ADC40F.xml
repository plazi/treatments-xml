<taxonomicName authorityName="Ferrari" authorityYear="1867" class="Insecta" family="Symplocaceae" genus="Anisandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Symplocaceae</taxonomicName>
