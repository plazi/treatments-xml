(30.6), 
<taxonomicName authorityName="Taylor" authorityYear="1942" box="[151,287,405,428]" class="Reptilia" family="Gekkonidae" genus="Phyllodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="176" phylum="Chordata" rank="subSpecies" species="tuberculosus" subSpecies="magnus">
