<taxonomicName box="[433,553,367,392]" class="Reptilia" family="Gekkonidae" genus="Phyllodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="176" phylum="Chordata" rank="species" species="isabelae">
<emphasis box="[433,553,367,392]" italics="true" pageId="25" pageNumber="176">P. isabelae</emphasis>
</taxonomicName>
(27.8), 
<taxonomicName authorityName="Smith" authorityYear="1935" box="[640,720,367,392]" class="Reptilia" family="Gekkonidae" genus="Phyllodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="176" phylum="Chordata" rank="species" species="lanei">
<emphasis box="[640,720,367,392]" italics="true" pageId="25" pageNumber="176">P. lanei</emphasis>
</taxonomicName>
(30), 
<taxonomicName box="[789,901,369,392]" class="Reptilia" family="Gekkonidae" genus="Phyllodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="25" pageNumber="176" phylum="Chordata" rank="species" species="rupinus">
<emphasis box="[789,901,369,392]" italics="true" pageId="25" pageNumber="176">P. rupinus</emphasis>
