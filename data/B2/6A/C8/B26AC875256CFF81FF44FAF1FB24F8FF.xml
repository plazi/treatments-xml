is endemic of the Marietas islands, 
<collectingRegion box="[1029,1116,1793,1818]" country="Mexico" name="Nayarit" pageId="24" pageNumber="175">Nayarit</collectingRegion>
, 
<collectingCountry box="[1127,1216,1793,1818]" name="Mexico" pageId="24" pageNumber="175">
<collectingRegion box="[1127,1216,1793,1818]" country="Mexico" name="Mexico" pageId="24" pageNumber="175">Mexico</collectingRegion>
</collectingCountry>
