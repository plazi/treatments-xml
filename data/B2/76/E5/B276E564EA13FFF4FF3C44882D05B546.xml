<subSubSection pageId="9" pageNumber="10" type="etymology">
<paragraph blockId="9.[151,1436,1063,1628]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[199,334,1208,1232]" pageId="9" pageNumber="10">Etymology.</emphasis>
