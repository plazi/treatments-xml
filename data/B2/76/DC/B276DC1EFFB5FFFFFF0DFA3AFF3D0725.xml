<quantity box="[522,672,1428,1453]" metricMagnitude="-3" metricUnit="m" metricValue="9.85" metricValueMax="11.5" metricValueMin="8.2" pageId="24" pageNumber="65" unit="mm" value="9.85" valueMax="11.5" valueMin="8.2">8.2–11.5 mm</quantity>
