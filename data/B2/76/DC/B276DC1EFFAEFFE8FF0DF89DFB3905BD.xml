<taxonomicName authorityName="Westwood" authorityYear="1842" box="[1192,1313,1949,1971]" class="Insecta" family="Scarabaeidae" genus="Macroderes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis box="[1192,1313,1949,1971]" italics="true" pageId="13" pageNumber="54">Macroderes</emphasis>
</taxonomicName>
species. 46, 49, 51, 
<taxonomicName authority="Frolov &amp; Scholtz, 2005" authorityName="Frolov &amp; Scholtz" authorityYear="2005" box="[227,593,1984,2006]" class="Insecta" family="Scarabaeidae" genus="Macroderes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="54" phylum="Arthropoda" rank="species" species="foveatus">
<emphasis box="[227,344,1984,2006]" italics="true" pageId="13" pageNumber="54">M. foveatus</emphasis>
Frolov &amp; Scholtz, 2005
</taxonomicName>
; 47, 48, 50, 
<taxonomicName authority="Frolov &amp; Scholtz, 2005" authorityName="Frolov &amp; Scholtz" authorityYear="2005" box="[717,1089,1984,2006]" class="Insecta" family="Scarabaeidae" genus="Macroderes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="54" phylum="Arthropoda" rank="species" species="cornutus">
