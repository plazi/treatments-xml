<keyLead pageId="2" pageNumber="451">
7a. Scales 
<quantity box="[293,379,561,583]" metricMagnitude="-1" metricUnit="m" metricValue="9.779" metricValueMax="10.16" metricValueMin="9.398" pageId="2" pageNumber="451" unit="in" value="38.5" valueMax="40.0" valueMin="37.0">37–40 in</quantity>
