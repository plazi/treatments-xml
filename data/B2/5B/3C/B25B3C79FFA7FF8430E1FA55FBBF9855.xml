<emphasis bold="true" box="[151,281,699,721]" pageId="24" pageNumber="473">FIGURE 15</emphasis>
. 
<taxonomicName authority="Hensley, 2003" authorityName="Hensley" authorityYear="2003" box="[293,762,699,721]" class="Actinopterygii" family="Bothidae" genus="Engyprosopon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pleuronectiformes" pageId="24" pageNumber="473" phylum="Chordata" rank="species" species="mozambiquense">
<taxonomicName authority="Hensley" authorityName="Hensley" box="[293,698,699,721]" class="Actinopterygii" family="Bothidae" genus="Engyprosopon" higherTaxonomySource="CoL" kingdom="Animalia" order="Pleuronectiformes" pageId="24" pageNumber="473" phylum="Chordata" rank="species" species="mozambiquense">
<taxonomicName box="[293,608,699,721]" class="Actinopterygii" family="Bothidae" genus="Engyprosopon" higherTaxonomySource="CoL" kingdom="Animalia" order="Pleuronectiformes" pageId="24" pageNumber="473" phylum="Chordata" rank="species" species="mozambiquense">
