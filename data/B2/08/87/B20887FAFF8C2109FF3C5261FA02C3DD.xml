</taxonomicName>
. 
<emphasis bold="true" box="[653,703,1272,1294]" pageId="4" pageNumber="3">1–4)</emphasis>
<taxonomicName authorityName="Alten" authorityYear="2014" box="[710,922,1272,1294]" class="Insecta" family="Cerambycidae" genus="Xylotrechus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="rameyi">
