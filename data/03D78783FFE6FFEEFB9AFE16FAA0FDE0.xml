<location id="8EA1604EFFE6FFEEFE51FDD1FE17FDC2" LSID="urn:lsid:plazi:treatment:03D78783FFE6FFEEFB9AFE16FAA0FDE0:8EA1604EFFE6FFEEFE51FDD1FE17FDC2" county="The" municipality="University of Kansas" name="Snow Entomological Museum" pageId="0" pageNumber="600">Snow Entomological Museum</location>
