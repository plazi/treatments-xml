<taxonomicName id="64D7DDA37733FFF2FDD7FE46097A4866" box="[526,587,511,537]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="8" pageNumber="389" phylum="Mollusca" rank="genus">
