<emphasis id="DE93EAFF257EFFE5FE026EB42C00FD07" box="[462,576,693,718]" italics="true" pageId="23" pageNumber="24">japonicus</emphasis>
</taxonomicName>
, 
<taxonomicName id="2BE74D6E257EFFE5FD826EB42C8EFD07" box="[590,718,693,719]" class="Insecta" family="Stratiomyidae" genus="Ptecticus" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="remeans">
