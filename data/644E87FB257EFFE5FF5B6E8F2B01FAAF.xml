<taxonomicName id="2BE74D6E257EFFE5FEFD6EB42FE1FD07" box="[305,417,693,719]" class="Insecta" family="Stratiomyidae" genus="Ptecticus" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="erectus">
<emphasis id="DE93EAFF257EFFE5FEFD6EB42F00FD07" box="[305,320,693,718]" italics="true" pageId="23" pageNumber="24">P</emphasis>
