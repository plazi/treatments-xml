<taxonomicName id="2BE74D6E257EFFE5FE7C6EB42C00FD07" box="[432,576,693,719]" class="Insecta" family="Stratiomyidae" genus="Ptecticus" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="japonicus">
<emphasis id="DE93EAFF257EFFE5FE7C6EB42FFFFD07" box="[432,447,693,718]" italics="true" pageId="23" pageNumber="24">P</emphasis>
