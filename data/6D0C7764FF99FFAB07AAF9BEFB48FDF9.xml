<materialsCitation id="55CDCC2FFF98FFAB07AAFF5FFBD0FE85" ID-GBIF-Occurrence="2990869439" collectingDate="2008-10-13" collectionCode="V" country="Mexico" county="Cerros" location="Don Martin Dam" municipality="Burras" pageId="43" pageNumber="44" specimenCount="1" stateProvince="Coahuila de Zaragoza">
