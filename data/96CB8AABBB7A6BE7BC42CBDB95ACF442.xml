<date id="8E1332EF57506B5750C467337B27CCA3" pageId="5" pageNumber="41" value="1928-11-11">
<collectingDate id="3D6F6746B02CA8DFD78D4EE06827436F" pageId="5" pageNumber="41" value="1928-11-11">11.XI.28</collectingDate>
