<date id="EE263497540B2E671E09CB91FE98CD19" pageId="17" pageNumber="418" value="2011-10">x. 2011</date>
<taxonomicName id="5D9869D4540B2E671C01CA70FDB1CD19" baseAuthorityName="Tressler" baseAuthorityYear="1950" box="[301,526,1297,1319]" class="Ostracoda" family="Cyprididae" genus="Tanycypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="17" pageNumber="418" phylum="Arthropoda" rank="species" species="pedroensis">
