</taxonomicName>
old trees in 
<collectingCountry box="[1117,1302,1516,1541]" name="Netherlands" pageId="1" pageNumber="135">the Netherlands</collectingCountry>
; 
<taxonomicName authority="Triapitsyn" authorityName="Triapitsyn" authorityYear="2008" class="Insecta" family="Mymaridae" genus="Cleruchus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="135" phylum="Arthropoda" rank="species" species="puchus">
