<emphasis bold="true" box="[964,1436,300,322]" pageId="2" pageNumber="136">
<taxonomicName authority="Manickavasagam &amp; Palanivel" authorityName="Manickavasagam &amp; Triapitsyn &amp; Palanivel" authorityYear="2018" box="[964,1375,300,322]" class="Insecta" family="Mymaridae" genus="Cleruchus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="136" phylum="Arthropoda" rank="species" species="indicus" status="sp. n.">
