<number box="[573,610,548,574]" pageId="4" pageNumber="29" value="3.5">3.5</number>
(
<number box="[626,675,548,574]" pageId="4" pageNumber="29" value="3.5" valueMax="5.0" valueMin="2.0">2–5</number>
