(
<figureCitation box="[990,1066,238,260]" captionStart="Figure 5" captionStartId="8.[164,243,587,606]" captionTargetBox="[323,1277,201,557]" captionTargetId="figure-576@8.[323,1283,197,557]" captionTargetPageId="8" captionText="Figure 5. Map of north portion of South America showing geographical distribution of Apteronotus anu (1 = holotype locality); Apteronotus baniwa (2 = holotype locality); Apteronotus ferrarisi (3 = holotype locality); Apteronotus galvisi (4 = holotype locality), and Apteronotus leptorhynchus (5 = holotype locality). Some symbols represent more than one locality and/or lot of specimens." figureDoi="http://doi.org/10.5281/zenodo.5280856" httpUri="https://zenodo.org/record/5280856/files/figure.png" pageId="11" pageNumber="575">FIGS 5</figureCitation>
, 
<figureCitation box="[1082,1098,238,259]" captionStart="Figure 7" captionStartId="11.[144,223,1457,1476]" captionTargetBox="[145,1414,954,1420]" captionTargetId="figure-365@11.[144,1425,949,1427]" captionTargetPageId="11" captionText="Figure 7. Apteronotus baniwa, 155 mm total length, male, holotype, AUM 43275; Venezuela, Amazonas, Río Orinoco, 147 km east-south-east of San Fernando de Atabapo, 03°18′23′′N, 66°36′12′′W." figureDoi="http://doi.org/10.5281/zenodo.5293378" httpUri="https://zenodo.org/record/5293378/files/figure.png" pageId="11" pageNumber="575">7</figureCitation>
, 
<figureCitation box="[1113,1129,238,259]" captionStart="Figure 8" captionStartId="11.[144,223,1853,1872]" captionTargetBox="[148,1420,1559,1818]" captionTargetId="figure-401@11.[144,1425,1555,1823]" captionTargetPageId="11" captionText="Figure 8. Apteronotus baniwa, 101 mm total length, female, paratype, MGNC 52994; Venezuela, Amazonas, Río Orinoco, 147 km east-south-east of San Fernando de Atabapo, 03°18′23′′N, 66°36′12′′W." figureDoi="http://doi.org/10.5281/zenodo.5293382" httpUri="https://zenodo.org/record/5293382/files/figure.png" pageId="11" pageNumber="575">8</figureCitation>
; 
