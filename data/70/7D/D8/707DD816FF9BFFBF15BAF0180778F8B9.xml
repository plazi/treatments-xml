<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[151,1436,666,1892]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[197,330,1786,1812]" pageId="21" pageNumber="22">Etymology</emphasis>
