<subSubSection pageId="14" pageNumber="15" type="nomenclature">
<paragraph blockId="14.[151,570,751,812]" box="[151,570,751,777]" pageId="14" pageNumber="15">
<heading bold="true" box="[151,570,751,777]" fontSize="11" level="1" pageId="14" pageNumber="15" reason="1">
<emphasis bold="true" box="[151,570,751,777]" pageId="14" pageNumber="15">
<taxonomicName authority="Troubridge" authorityName="Troubridge" authorityYear="2008" box="[151,496,751,777]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="sesmu" status="sp. n.">
<emphasis bold="true" box="[151,347,751,777]" italics="true" pageId="14" pageNumber="15">Sympistis sesmu</emphasis>
Troubridge
</taxonomicName>
