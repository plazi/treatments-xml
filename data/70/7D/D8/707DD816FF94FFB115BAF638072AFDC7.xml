<subSubSection pageId="27" pageNumber="28" type="etymology">
<paragraph blockId="27.[151,1437,152,538]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[197,330,431,457]" pageId="27" pageNumber="28">Etymology</emphasis>
