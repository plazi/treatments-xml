<subSubSection box="[197,1211,631,658]" pageId="41" pageNumber="42" type="etymology">
<paragraph blockId="41.[151,1436,151,738]" box="[197,1211,631,658]" pageId="41" pageNumber="42">
<emphasis bold="true" box="[197,330,631,657]" pageId="41" pageNumber="42">Etymology</emphasis>
