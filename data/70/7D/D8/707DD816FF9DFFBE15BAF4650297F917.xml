<paragraph blockId="19.[151,601,1626,1687]" box="[151,601,1626,1652]" pageId="19" pageNumber="20">
<heading bold="true" box="[151,601,1626,1652]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[151,601,1626,1652]" pageId="19" pageNumber="20">
