<location LSID="urn:lsid:plazi:treatment:704187ADE15D0E60FF1CF8DA1D1DF900:FD376060E15F0E60FF4CF9C71D0FF9B1" box="[199,313,1600,1625]" country="South Korea" municipality="All" name="Elevation" pageId="15" pageNumber="16">Elevation</location>
