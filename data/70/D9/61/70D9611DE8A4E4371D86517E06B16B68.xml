</paragraph>
<paragraph pageId="16" pageNumber="17">
<taxonomicName authorityName="Copel" authorityYear="1931" class="Magnoliopsida" family="Oleandraceae" genus="Oleandra" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Myrtales" pageId="16" pageNumber="17" phylum="Magnoliophyta" rank="species" species="pubescens">Oleandra pubescens</taxonomicName>
