<taxonomicName authority="Saussure, 1896" authorityName="Saussure" authorityYear="1896" box="[868,1222,1346,1372]" class="Insecta" family="Ripipterygidae" genus="Ripipteryx" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="carbonaria">
<emphasis box="[868,1029,1347,1372]" italics="true" pageId="3" pageNumber="26">R.carbonaria</emphasis>
<bibRefCitation author="Saussure" box="[1036,1222,1346,1372]" pageId="3" pageNumber="28" refString="Saussure, H. de (1896) Orthoptera. Fam. Gryllidae. Biologia Centrali-Americana, 1, 201 - 216." type="journal article" year="1896">
Saussure, 
<number box="[1159,1222,1346,1372]" pageId="3" pageNumber="26" value="1896.0">1896</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Gunther, 1962" authorityName="Gunther" authorityYear="1962" class="Insecta" family="Ripipterygidae" genus="Ripipteryx" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="ecuadoriensis">
