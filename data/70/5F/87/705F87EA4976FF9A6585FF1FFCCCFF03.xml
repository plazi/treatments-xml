<emphasis bold="true" box="[647,679,2010,2032]" pageId="7" pageNumber="244">16.</emphasis>
<taxonomicName authority="Russell-Smith, 2017" authorityName="Russell-Smith" authorityYear="2017" box="[685,874,2010,2032]" class="Arachnida" family="Gnaphosidae" genus="Drassyllus" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="244" phylum="Arthropoda" rank="species" species="cyprius" status="sp. n.">
<emphasis box="[685,874,2010,2032]" italics="true" pageId="7" pageNumber="244">Drassyllus cyprius</emphasis>
</taxonomicName>
<emphasis bold="true" box="[880,937,2010,2032]" pageId="7" pageNumber="244">
<taxonomicNameLabel box="[880,937,2010,2032]" pageId="7" pageNumber="244" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
