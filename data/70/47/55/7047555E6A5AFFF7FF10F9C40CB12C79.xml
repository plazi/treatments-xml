<paragraph blockId="20.[674,1210,470,812]" box="[677,807,470,496]" pageId="20" pageNumber="929">REMARQUES</paragraph>
<paragraph blockId="20.[674,1210,470,812]" pageId="20" pageNumber="929">
<taxonomicName baseAuthorityName="Bosquet" baseAuthorityYear="1852" box="[677,956,501,527]" class="Ostracoda" family="Cuneocytheridae" genus="Cuneocythere" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="20" pageNumber="929" phylum="Arthropoda" rank="species" species="foveolata" subGenus="Monsmirabilia">
