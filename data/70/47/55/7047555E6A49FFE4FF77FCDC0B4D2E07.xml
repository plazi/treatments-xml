<taxonomicName baseAuthorityName="Roemer" baseAuthorityYear="1838" box="[113,269,1092,1116]" class="Ostracoda" family="Cytherellidae" genus="Cytherella" kingdom="Animalia" order="Platycopida" pageId="7" pageNumber="916" phylum="Arthropoda" rank="species" species="gyrosa">
