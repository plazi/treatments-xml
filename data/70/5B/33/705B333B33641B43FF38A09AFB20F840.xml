<date box="[1089,1215,1686,1710]" pageId="13" pageNumber="58" value="1857" valueMax="1894">
<number box="[1089,1215,1686,1710]" pageId="13" pageNumber="58" value="1875.5" valueMax="1894.0" valueMin="1857.0">1857–1894</number>
