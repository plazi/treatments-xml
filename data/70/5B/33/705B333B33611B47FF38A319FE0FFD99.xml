<collectingCountry box="[844,912,511,535]" name="Japan" pageId="9" pageNumber="54">Japan</collectingCountry>
”) as the 
<typeStatus box="[1007,1055,512,536]" pageId="9" pageNumber="54">type</typeStatus>
