<taxonomicName authority="Lee &amp; Bezdek, 2020" authorityName="Lee &amp; Bezděk" authorityYear="2020" baseAuthorityName="Gressitt &amp; Kimoto" baseAuthorityYear="1963" class="Insecta" family="Chrysomelidae" genus="Pseudotheopea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudotheopea smaragdina" order="Coleoptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="species" species="smaragdina">
