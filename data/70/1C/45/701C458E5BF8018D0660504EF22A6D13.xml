<normalizedToken originalValue="Außenkelch" pageId="null" pageNumber="352">Aussenkelch</normalizedToken>
; 
<normalizedToken originalValue="Hüllblätter" pageId="null" pageNumber="352">Huellblaetter</normalizedToken>
so lang oder 
<normalizedToken originalValue="länger" pageId="null" pageNumber="352">laenger</normalizedToken>
als die 
<normalizedToken originalValue="äußern" pageId="null" pageNumber="352">aeussern</normalizedToken>
<normalizedToken originalValue="Blüten" pageId="null" pageNumber="352">Blueten</normalizedToken>
</td>
<td align="left" pageId="null" pageNumber="352" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Caprifoliaceae" genus="Scabiosa" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="352" phylum="Tracheophyta" rank="species" species="vestina">
