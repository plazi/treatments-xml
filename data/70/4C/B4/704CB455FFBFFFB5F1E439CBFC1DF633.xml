<collectingCountry box="[775,875,1236,1275]" name="China" pageId="55" pageNumber="218">China</collectingCountry>
Sea; 
<collectingCountry box="[1014,1207,1236,1275]" name="Philippines" pageId="55" pageNumber="218">Philippines</collectingCountry>
; 
<collectingCountry box="[236,394,1282,1321]" name="Australia" pageId="55" pageNumber="218">Australia</collectingCountry>
; 
<collectingCountry box="[421,655,1282,1321]" name="New Zealand" pageId="55" pageNumber="218">New Zealand</collectingCountry>
; questionably from off Durban, 
<collectingCountry box="[238,465,1328,1367]" name="South Africa" pageId="55" pageNumber="218">South Africa</collectingCountry>
(see remarks). Depth: 
<quantity box="[902,1164,1328,1367]" metricMagnitude="2" metricUnit="m" metricValue="3.6399999999999997" metricValueMax="5.05" metricValueMin="2.23" pageId="55" pageNumber="218" unit="m" value="364.0" valueMax="505.0" valueMin="223.0">
