<taxonomicName authority="Gnanamuthu, 1953" authorityName="Gnanamuthu" authorityYear="1953" class="Maxillopoda" family="Pennellidae" genus="Lernaeenicus" kingdom="Animalia" order="Siphonostomatoida" pageId="4" pageNumber="196" phylum="Arthropoda" rank="species" species="stromatei">
<emphasis box="[1302,1436,405,428]" italics="true" pageId="4" pageNumber="196">L. stromatei</emphasis>
<bibRefCitation author="Gnanamuthu" box="[151,367,439,464]" pageId="4" pageNumber="196" refString="Gnanamuthu, C. P. (1953) Three lernaeid copepods parasitic on South Indian fishes. Journal of Parasitology, 39, 14 - 21. http: // dx. doi. org / 10.2307 / 3274053" type="journal article" year="1953">Gnanamuthu, 1953</bibRefCitation>
</taxonomicName>
(host: black pomfret 
<taxonomicName authority="Bloch, 1795" authorityName="Bloch" authorityYear="1795" box="[612,1025,439,464]" class="Actinopterygii" family="Carangidae" genus="Parastromateus" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="196" phylum="Chordata" rank="species" species="niger">
