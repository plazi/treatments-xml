<subSubSection pageId="null" pageNumber="200" type="vernacular_name">
<paragraph pageId="null" pageNumber="200">
<normalizedToken originalValue="Dichtblättriges" pageId="null" pageNumber="200">Dichtblaettriges</normalizedToken>
Laichkraut
</paragraph>
