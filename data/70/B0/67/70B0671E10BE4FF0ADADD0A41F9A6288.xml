<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="309" phylum="Tracheophyta" rank="species" species="verum">G. verum</taxonomicName>
</italic>
(Nr. 12)
</td>
</tr>
<tr pageId="null" pageNumber="309">
<td align="left" pageId="null" pageNumber="309" valign="top">
13*. Krone 
<normalizedToken originalValue="weiß" pageId="null" pageNumber="309">weiss</normalizedToken>
bis hell 
<normalizedToken originalValue="gelbgrün" pageId="null" pageNumber="309">gelbgruen</normalizedToken>
; Stengel kahl oder nur im untern Teil behaart; 
<normalizedToken originalValue="Blätter" pageId="null" pageNumber="309">Blaetter</normalizedToken>
