<taxonomicName id="D6A46FBCFF88FF91FE8841BF145FFC13" box="[345,518,992,1013]" class="Insecta" family="Dynastidae" genus="Pucaya" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="473" phylum="Arthropoda" rank="species" species="castanea">
