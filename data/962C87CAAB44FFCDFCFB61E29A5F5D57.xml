<emphasis id="2CF1EACEAB47FFCBF832616F9C775B26" bold="true" box="[1041,1073,1293,1314]" pageId="26" pageNumber="27">71.</emphasis>
<taxonomicName id="D9854D5FAB47FFCBF81D616C99CF5B39" authority="(Fabricius)" baseAuthorityName="Fabricius" baseAuthorityYear="1804" class="Insecta" family="Halictidae" genus="Pseudaugochlora" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="graminea">
<emphasis id="2CF1EACEAB47FFCBF81D616C994C5B39" italics="true" pageId="26" pageNumber="27">Pseudaugochlora graminea</emphasis>
