<taxonomicName id="FD8D059CFFBD9B66EE9BFB16355CFB04" authority="Chappuis, 1929" authorityName="Chappuis" authorityYear="1929" box="[733,1034,1229,1251]" class="Maxillopoda" family="Canthocamptidae" genus="Elaphoidella" kingdom="Animalia" order="Harpacticoida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="genus">
