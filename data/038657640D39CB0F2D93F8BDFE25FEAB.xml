<taxonomicName id="4C2F9DF10D36CB0F2E15FEB0FC32FEF7" authorityName="Dyar" authorityYear="1919" box="[785,964,259,285]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="269" phylum="Arthropoda" rank="species" species="intrudens">
