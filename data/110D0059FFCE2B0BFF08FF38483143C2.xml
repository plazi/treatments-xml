<number id="2F6CA368FFCE2B0BFDE0FD204E8544FF" box="[639,688,655,679]" pageId="21" pageNumber="222" value="0.42">0.42</number>
+ 
<number id="2F6CA368FFCE2B0BFD4FFD214F3444FF" box="[720,769,654,679]" pageId="21" pageNumber="222" value="3.64">3.64</number>
