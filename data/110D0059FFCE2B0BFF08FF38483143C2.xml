<number id="2F6CA368FFCE2B0BFE41FD214E2844FF" box="[478,541,654,679]" pageId="21" pageNumber="222" value="14.63">14.63</number>
(
<number id="2F6CA368FFCE2B0BFDB3FD214E6A44FF" box="[556,607,654,679]" pageId="21" pageNumber="222" value="4.36">4.36</number>
