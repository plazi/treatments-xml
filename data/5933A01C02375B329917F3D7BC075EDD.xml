<taxonomicName id="A4194D92AEB8CBA21646850FB7BC5A8E" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Vaccinium vitis-idaea" order="Ericales" pageId="11" pageNumber="34" phylum="Spermatophyta" rank="species" species="vitis-idaea">
