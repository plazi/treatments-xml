<emphasis id="CC9CD28B6E59EC99E7860285DB516D1B" italics="true" pageId="11" pageNumber="34">Empetrum nigrum</emphasis>
</taxonomicName>
L., 
<taxonomicName id="71FE244A6A47F35CE181E111BE4A707F" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhododendron lapponicum" order="Ericales" pageId="11" pageNumber="34" phylum="Spermatophyta" rank="species" species="lapponicum">
