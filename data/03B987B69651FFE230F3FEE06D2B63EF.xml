<taxonomicName id="4C104D239651FFE233B8FCD26B1A6327" box="[988,1087,861,884]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="10" pageNumber="45" phylum="Tracheophyta" rank="species" species="cerris">
