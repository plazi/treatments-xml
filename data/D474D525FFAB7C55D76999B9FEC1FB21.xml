<taxonomicName id="9BDD1FB0FFAA7C55D7619F31FE78FC58" authority="Thunberg, 1818" authorityName="Thunberg" authorityYear="1818" box="[98,506,874,901]" class="Insecta" family="Scarabaeidae" genus="Ateuchus" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="spinipes">
