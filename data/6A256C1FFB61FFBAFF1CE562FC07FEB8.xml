<collectingCountry id="9A9B9D99FB61FFB9FE8CE407FEA2F8C0" box="[263,333,1888,1914]" name="China" pageId="13" pageNumber="14">China</collectingCountry>
(including Tibet), north 
<collectingCountry id="9A9B9D99FB61FFB9FDF9E407FD5EF8C0" box="[626,689,1888,1914]" name="India" pageId="13" pageNumber="14">India</collectingCountry>
, 
<collectingCountry id="9A9B9D99FB61FFB9FD36E407FCE9F8C0" box="[701,774,1888,1914]" name="Nepal" pageId="13" pageNumber="14">Nepal</collectingCountry>
, 
<collectingCountry id="9A9B9D99FB61FFB9FC99E407FC92F8C0" box="[786,893,1888,1914]" name="Australia" pageId="13" pageNumber="14">Australia</collectingCountry>
(introduced). – In 
<collectingCountry id="9A9B9D99FB61FFB9FBD7E407FB64F8C0" box="[1116,1163,1888,1914]" name="Iran" pageId="13" pageNumber="14">Iran</collectingCountry>
(Pl. 
<number id="5444CF2EFB61FFB9FB4EE407FB0CF8C0" box="[1221,1251,1888,1914]" pageId="13" pageNumber="14" value="11.0">11</number>
