<taxonomicName id="70248A997D01C7C8412FB5B239280752" authorityName="LeConte" authorityYear="1846" class="Hexapoda" family="Carabidae" genus="Dyschirius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="294" pageNumber="295" phylum="Arthropoda" rank="species" species="terminatus">Dyschirius terminatus</taxonomicName>
