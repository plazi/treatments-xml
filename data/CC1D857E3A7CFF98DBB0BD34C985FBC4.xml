<emphasis id="76C0E87A3A7CFF98DBE0BD83C955FC20" bold="true" box="[199,335,977,1002]" pageId="8" pageNumber="159">Comments.</emphasis>
<taxonomicName id="83B44FEB3A7CFF98DA70BD83C9C6FC20" box="[343,476,977,1002]" class="Insecta" family="Cerambycidae" genus="Mimozotale" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="159" phylum="Arthropoda" rank="genus">
