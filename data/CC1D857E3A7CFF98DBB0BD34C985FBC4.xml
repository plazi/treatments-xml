<taxonomicName id="83B44FEB3A7CFF98D85BBD83CBE6FC20" box="[892,1020,977,1002]" class="Insecta" family="Cerambycidae" genus="Euseboides" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="159" phylum="Arthropoda" rank="genus">
