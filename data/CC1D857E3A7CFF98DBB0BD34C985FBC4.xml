<taxonomicName id="83B44FEB3A7CFF98DACABD83CA72FC20" box="[493,616,977,1002]" class="Insecta" family="Coleoptera" genus="Parazotale" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="159" phylum="Arthropoda" rank="genus">
