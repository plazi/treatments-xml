(
<emphasis id="DEFCC55DFFC24F2EE3D40FCDB561FE2C" box="[754,876,448,471]" italics="true" pageId="45" pageNumber="62">Austromactra</emphasis>
) 
<emphasis id="DEFCC55DFFC24F2EE25E0FCDB5DDFE2C" box="[888,976,448,471]" italics="true" pageId="45" pageNumber="62">compressa</emphasis>
(
<bibRefCitation id="881964BEFFC24F2EE2FD0FCDB27AFE23" author="DESHAYES G. - P." box="[987,1143,448,472]" pageId="45" pageNumber="62" refId="ref41695" refString="DESHAYES G. - P. 1832. - Encyclopedie Methodique. Histoire naturelle des Vers. Tome 2, fascicule 2. Agasse, Paris, 594 p." type="book" year="1832">Deshayes, 1832</bibRefCitation>
)
</taxonomicName>
]. Correction pour homonymie primaire, pro 
<taxonomicName id="2B8862CCFFC24F2EE5000FD6B533FDF6" authority="Deshayes, 1824" authorityName="Deshayes" authorityYear="1824" class="Bivalvia" family="Mactridae" genus="Mactra" kingdom="Animalia" order="Venerida" pageId="45" pageNumber="62" phylum="Mollusca" rank="species" species="depressa">
