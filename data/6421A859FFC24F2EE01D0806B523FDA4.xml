<taxonomicName id="2B8862CCFFC24F2EE3830FCDB273FE23" authority="(Deshayes, 1832)" baseAuthorityName="Deshayes" baseAuthorityYear="1832" box="[677,1150,448,472]" class="Bivalvia" family="Mactridae" genus="Spisula" kingdom="Animalia" order="Venerida" pageId="45" pageNumber="62" phylum="Mollusca" rank="species" species="compressa" subGenus="Austromactra">
