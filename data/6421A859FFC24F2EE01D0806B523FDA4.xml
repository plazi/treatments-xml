<taxonomicName id="2B8862CCFFC24F2EE3830C27B541FDBA" box="[677,844,554,577]" class="Bivalvia" family="Mactridae" genus="Mactra" kingdom="Animalia" order="Venerida" pageId="45" pageNumber="62" phylum="Mollusca" rank="species" species="compressa">
