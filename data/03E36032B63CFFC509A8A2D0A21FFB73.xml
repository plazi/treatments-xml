<emphasis id="B93E0D36B601FFC50DD3A54CA567FE5B" box="[1260,1288,369,395]" italics="true" pageId="34" pageNumber="503">ad</emphasis>
<quantity id="4CB27CC1B601FFC50C37A5BFA542FE5B" box="[1288,1325,369,400]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="34" pageNumber="503" unit="in" value="1.0">
<emphasis id="B93E0D36B601FFC50C37A5BFA57FFE40" box="[1288,1296,386,400]" italics="true" pageId="34" pageNumber="503">
<subScript id="17CED361B601FFC50C37A5BFA57FFE40" attach="left" box="[1288,1296,386,400]" fontSize="6" pageId="34" pageNumber="503">1</subScript>
</emphasis>
in
</quantity>
posterior, 
<emphasis id="B93E0D36B601FFC509A8A5A8A0DCFE7F" box="[151,179,405,431]" italics="true" pageId="34" pageNumber="503">ad</emphasis>
<quantity id="4CB27CC1B601FFC5098CA59BA0B7FE7F" box="[179,216,405,436]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="34" pageNumber="503" unit="in" value="2.0">
<emphasis id="B93E0D36B601FFC5098CA59BA0D4FE64" box="[179,187,422,436]" italics="true" pageId="34" pageNumber="503">
<subScript id="17CED361B601FFC5098CA59BA0D4FE64" attach="left" box="[179,187,422,436]" fontSize="6" pageId="34" pageNumber="503">2</subScript>
</emphasis>
in
</quantity>
lateral, 
<emphasis id="B93E0D36B601FFC5080CA5A8A120FE7F" box="[307,335,405,431]" italics="true" pageId="34" pageNumber="503">ad</emphasis>
<quantity id="4CB27CC1B601FFC50870A59BA11BFE7F" box="[335,372,405,436]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="34" pageNumber="503" unit="in" value="3.0">
<emphasis id="B93E0D36B601FFC50870A59BA138FE64" box="[335,343,422,436]" italics="true" pageId="34" pageNumber="503">
<subScript id="17CED361B601FFC50870A59BA138FE64" attach="left" box="[335,343,422,436]" fontSize="6" pageId="34" pageNumber="503">3</subScript>
