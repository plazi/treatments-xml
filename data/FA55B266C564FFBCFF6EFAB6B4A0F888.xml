<taxonomicName id="B5FC78F3C563FFBCFEB6F96EB7CFF918" authority="(Signoret)" baseAuthorityName="Signoret" box="[335,677,1742,1766]" class="Insecta" family="Acanthosomatidae" genus="Hyperbius" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="672" phylum="Arthropoda" rank="species" species="geniculatus">
