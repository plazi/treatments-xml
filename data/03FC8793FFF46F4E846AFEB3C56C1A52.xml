<treatmentCitationGroup id="AB4511ABFFF46F4E846AFE20C47F1C9E" box="[264,634,398,422]" pageId="37" pageNumber="38">
<taxonomicName id="4C554D06FFF46F4E846AFE20C4731C9E" authority="Mosely 1954: 341" authorityName="Mosely" authorityPageNumber="341" authorityYear="1954" box="[264,630,398,422]" class="Insecta" family="Glossosomatidae" genus="Culoptila" kingdom="Animalia" order="Trichoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="rusia">
