<quantity id="4C8E9B91FFD5FFBDFEF2798CFE73C4A0" box="[367,511,1986,2010]" metricMagnitude="2" metricUnit="m" metricValue="8.41" metricValueMax="15.25" metricValueMin="1.5699999999999998" pageId="24" pageNumber="25" unit="m" value="841.0" valueMax="1525.0" valueMin="157.0">157–1525 m</quantity>
