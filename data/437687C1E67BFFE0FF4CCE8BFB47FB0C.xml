<taxonomicName id="0CDF4D54E67BFFE0FE4CC9BEFDEEFB0C" authorityName="Blumenbach" authorityYear="1779" box="[407,538,1210,1236]" class="Mammalia" kingdom="Animalia" order="Chiroptera" pageId="97" pageNumber="98" phylum="Chordata" rank="order">Chiroptera</taxonomicName>
