<taxonomicName id="2B8862CCFFF44F18E1570817B46FF9C9" authority="d'Orbigny, 1850 b: 317" authorityName="d'Orbigny" authorityPageNumber="317" authorityYear="1850" box="[113,610,1562,1586]" class="Gastropoda" family="Fasciolariidae" genus="Fusus" kingdom="Animalia" order="Neogastropoda" pageId="27" pageNumber="44" phylum="Mollusca" rank="species" species="subfusiformis">
