<pageBreakToken id="CE13E0BD1B8DA243210D797DEDA86728" pageId="null" pageNumber="722">Artengruppe</pageBreakToken>
des 
<taxonomicName id="1B67E923851D83ADB91FE145B242CC14" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="722" phylum="Tracheophyta" rank="genus">Rumex</taxonomicName>
