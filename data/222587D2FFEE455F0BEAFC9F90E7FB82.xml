<taxonomicName id="6D8C4D47FFEE455F0AB6FC169071FBBC" box="[459,553,1002,1024]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="406" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
