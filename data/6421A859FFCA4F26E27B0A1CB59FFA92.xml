<paragraph id="EC37194FFFCA4F26E3830A45B5ECFAE2" blockId="37.[677,1210,1096,1385]" pageId="37" pageNumber="54">
<taxonomicName id="2B8862CCFFCA4F26E3830A45B2B8FB9B" authority="d'Orbigny, 1850 b: 325" authorityName="d'Orbigny" authorityPageNumber="325" authorityYear="1850" box="[677,1205,1096,1120]" class="Bivalvia" family="Veneridae" genus="Pectunculus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Venerida" pageId="37" pageNumber="54" phylum="Mollusca" rank="species" species="pseudopulvinatus">
