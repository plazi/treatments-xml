<quantity id="4C929B09FFF9FFEABBD9FC4B7F77FC00" box="[186,290,908,932]" metricMagnitude="-3" metricUnit="m" metricValue="2.55" pageId="5" pageNumber="53" unit="mm" value="2.55">2.55 mm</quantity>
