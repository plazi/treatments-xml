<number id="3DA224CBFFF9FFEABF59FC837A00FCF8" box="[1082,1109,836,860]" pageId="5" pageNumber="53" value="10.0">10</number>
, w/l mesonotum 
<number id="3DA224CBFFF9FFEABE75FC837B61FCF8" box="[1302,1332,836,860]" pageId="5" pageNumber="53" value="34.0">34</number>
