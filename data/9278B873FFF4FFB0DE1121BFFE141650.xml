<materialsCitation id="AAB90338FFF5FFB0DC6327C3FE901619" collectorName="La Cruz" country="Argentina" latitude="-29.150833" location="Corrientes" longLatPrecision="20" longitude="-56.821667" pageId="11" pageNumber="426" specimenCount="1" stateProvince="Corrientes">
