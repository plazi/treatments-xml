<collectingRegion id="D815C787FFF5FFB0DC3B27EDFE9D163A" box="[256,373,233,257]" country="Argentina" name="Corrientes" pageId="11" pageNumber="426">Corrientes</collectingRegion>
, 
<collectorName id="B7246CB3FFF5FFB0DCBA27EDFE36163A" box="[385,478,233,257]" pageId="11" pageNumber="426">La Cruz</collectorName>
, Paraje Tres Cerros; 
<geoCoordinate id="7FE56FA2FFF5FFB0DFF827E3FF371619" degrees="29" direction="south" minutes="09" orientation="latitude" pageId="11" pageNumber="426" precision="15" seconds="03" value="-29.150833">29°09′ 03′′S</geoCoordinate>
, 
<geoCoordinate id="7FE56FA2FFF5FFB0DDD1260DFE901619" box="[234,376,265,290]" degrees="056" direction="west" minutes="49" orientation="longitude" pageId="11" pageNumber="426" precision="15" seconds="18" value="-56.821667">056°49′18′′W</geoCoordinate>
</materialsCitation>
.
</paragraph>
<paragraph id="1A6E0965FFF5FFB0DD932635FE141650" blockId="11.[168,778,159,363]" pageId="11" pageNumber="426">
<emphasis id="28A5D577FFF5FFB0DD932635FEA21672" bold="true" box="[168,330,305,329]" pageId="11" pageNumber="426">Identification.</emphasis>
