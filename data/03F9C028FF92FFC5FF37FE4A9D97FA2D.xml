(
<number id="3D986319FF93FFC5FB2EFF4D999CFF15" box="[1166,1242,152,178]" pageId="7" pageNumber="46" value="89.0" valueMax="93.0" valueMin="85.0">85–93</number>
) thick; light yellow. Gnathosoma 
<number id="3D986319FF93FFC5FECEFF159CCDFF7D" box="[366,395,192,218]" pageId="7" pageNumber="46" value="68.0">68</number>
(
<number id="3D986319FF93FFC5FE3DFF159CAAFF7D" box="[413,492,192,218]" pageId="7" pageNumber="46" value="67.5" valueMax="70.0" valueMin="65.0">65–70</number>
), pedipalp coxal setae (
<emphasis id="B924AD2CFF93FFC5FCA5FF159E67FF7E" box="[773,801,192,217]" italics="true" pageId="7" pageNumber="46">ep</emphasis>
) 
<number id="3D986319FF93FFC5FC94FF159E04FF7D" box="[820,834,192,218]" pageId="7" pageNumber="46" value="3.0">3</number>
(
<number id="3D986319FF93FFC5FCF5FF159EC2FF7D" box="[853,900,192,218]" pageId="7" pageNumber="46" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
