<quantity id="4CEF9B25FF28FF0C9D63FC69FF2F33B3" box="[1310,1390,923,950]" metricMagnitude="-1" metricUnit="m" metricValue="7.0104" pageId="185" pageNumber="186" unit="in" value="27.6">27.6 in</quantity>
