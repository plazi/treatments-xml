<quantity id="4CEF9B25FF28FF0C9CB6FC69FF5133B0" box="[1227,1296,923,949]" metricMagnitude="-1" metricUnit="m" metricValue="7.0" pageId="185" pageNumber="186" unit="cm" value="70.0">70 cm</quantity>
