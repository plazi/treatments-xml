<taxonomicName id="71DF4D7DA542FFF7AD1940BBFD331EBC" baseAuthorityName="Smith" baseAuthorityYear="1852" box="[554,686,1703,1728]" class="Insecta" family="Ichneumonidae" genus="Callajoppa" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="1161" phylum="Arthropoda" rank="species" species="pepsoides">
<emphasis id="84ABEAECA542FFF7AD1940BBFD331EBC" box="[554,686,1703,1728]" italics="true" pageId="47" pageNumber="1161">T. pepsoides</emphasis>
</taxonomicName>
the 
<typeStatus id="6964885CA542FFF7ADD140BBFC891EBC" box="[738,788,1703,1728]" pageId="47" pageNumber="1161">type</typeStatus>
