<taxonomicName id="B88F4D71FFBEFFE4FEF1F8E4FD334B1C" authority="Bologna" authorityName="Bologna &amp; Amore &amp; Pitzalis" authorityYear="2018" box="[313,709,1830,1855]" class="Insecta" family="Meloidae" genus="Paramimesthes" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="namibicus" status="sp. n.">
