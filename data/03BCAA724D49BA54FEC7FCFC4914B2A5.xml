at depths between 
<date id="FFAB3DA44D4ABA54FCE8FD764EB7B2A5" box="[807,939,724,750]" pageId="2" pageNumber="61" value="1005" valueMax="1463">
<number id="3DDD09434D4ABA54FCE8FD764EB7B2A5" box="[807,939,724,750]" pageId="2" pageNumber="61" value="1234.0" valueMax="1463.0" valueMin="1005.0">1005–1463</number>
