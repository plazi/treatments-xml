Central 
<collectingCountry id="F376EA86AE30FFA7FE5AFD04DB7BFD46" box="[451,546,655,681]" name="Ecuador" pageId="10" pageNumber="311">Ecuador</collectingCountry>
, 
<collectingRegion id="49A564F4AE30FFA7FDB4FD04DA47FD46" box="[557,798,655,681]" country="Ecuador" name="Tungurahua" pageId="10" pageNumber="311">Tungurahua Province</collectingRegion>
