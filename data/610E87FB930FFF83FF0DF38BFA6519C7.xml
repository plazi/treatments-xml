Key to 
<taxonomicName id="2EA74D6E930FFF83FF6AF38AFECD1AC1" authorityName="Evsyukov &amp; Golovatch &amp; Reip" authorityYear="2018" box="[240,307,1232,1258]" class="Diplopoda" family="Julidae" genus="Julus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Julida" pageId="25" pageNumber="114" phylum="Arthropoda" rank="genus">
