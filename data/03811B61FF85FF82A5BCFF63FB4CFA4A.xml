<quantity id="4CD00792FF85FF82A179FEF9FB58FA4A" box="[1106,1196,1409,1434]" metricMagnitude="3" metricUnit="m" metricValue="1.15" pageId="12" pageNumber="353" unit="m" value="1150.0">1150 m</quantity>
