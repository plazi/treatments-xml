<taxonomicName id="4C28D1F4FF85FF82A749FEFAFD44FA49" box="[610,688,1410,1433]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="12" pageNumber="353" phylum="Tracheophyta" rank="genus">
<emphasis id="B95C7665FF85FF82A749FEFAFD44FA49" box="[610,688,1410,1433]" italics="true" pageId="12" pageNumber="353">Acacia</emphasis>
