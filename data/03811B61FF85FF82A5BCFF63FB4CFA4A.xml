<taxonomicName id="4C28D1F4FF85FF82A6B7FE26FB77FAA6" box="[924,1155,1373,1398]" class="Magnoliopsida" family="Cactaceae" genus="Opuntia" kingdom="Plantae" order="Caryophyllales" pageId="12" pageNumber="353" phylum="Tracheophyta" rank="species" species="ficus-indica">
