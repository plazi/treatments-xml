<quantity id="4CD00792FF85FF82A446FEF9FE30FA4A" box="[365,452,1409,1434]" metricMagnitude="3" metricUnit="m" metricValue="1.614" pageId="12" pageNumber="353" unit="m" value="1614.0">1614 m</quantity>
