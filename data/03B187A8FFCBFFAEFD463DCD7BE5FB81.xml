<emphasis id="B96CEAACFFC8FFB0FB0E3DE57BF5FB39" bold="true" box="[1188,1212,1206,1230]" pageId="78">A.</emphasis>
<taxonomicName id="4C184D3DFFC8FFB0FFC63D877EE7FB1B" authority=", AMNH" authorityName="AMNH" box="[108,430,1236,1260]" class="Insecta" family="Phoridae" genus="Eosciadocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="78" phylum="Arthropoda" rank="species" species="pauciseta">
