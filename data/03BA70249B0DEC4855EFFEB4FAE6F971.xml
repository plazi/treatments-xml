<specimenCount id="9D150ABB9B0DEC485639FA93FCEDFA75" box="[833,869,1386,1412]" pageId="17" pageNumber="218" type="male">4♂</specimenCount>
, 
<specimenCount id="9D150ABB9B0DEC485608FA93FC1DFA75" box="[880,917,1386,1412]" pageId="17" pageNumber="218" type="female">4♀</specimenCount>
