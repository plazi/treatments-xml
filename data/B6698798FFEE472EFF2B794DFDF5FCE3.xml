<taxonomicName id="F9C04D0DFFEE472EFE9579A0FE41FCC0" box="[268,457,757,779]" class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="937" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="0CB4EA9CFFEE472EFE9579A0FEF2FCC0" box="[268,378,757,779]" italics="true" pageId="8" pageNumber="937">Astragalus</emphasis>
species
</taxonomicName>
(
<taxonomicName id="F9C04D0DFFEE472EFE4079A0FDF1FCC0" box="[473,633,757,779]" class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="937" phylum="Tracheophyta" rank="species" species="angustifolius">
<emphasis id="0CB4EA9CFFEE472EFE4079A0FDF1FCC0" box="[473,633,757,779]" italics="true" pageId="8" pageNumber="937">A. angustifolius</emphasis>
</taxonomicName>
and a yet unidentified 
<taxonomicName id="F9C04D0DFFEE472EFCF079A0FC72FCC0" box="[873,1018,757,779]" class="Magnoliopsida" family="Fabaceae" genus="Astragalus" kingdom="Plantae" order="Fabales" pageId="8" pageNumber="937" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="0CB4EA9CFFEE472EFCF079A0FC5FFCC0" box="[873,983,757,779]" italics="true" pageId="8" pageNumber="937">Astragalus</emphasis>
