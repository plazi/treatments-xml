<taxonomicName id="4C454D7E1D39FFF7FF74FE52FE3CFE0B" baseAuthorityName="Forel" baseAuthorityYear="1904" box="[178,415,401,422]" class="Insecta" family="Formicidae" genus="Cataglyphis" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="919" phylum="Arthropoda" rank="species" species="adenensis">
