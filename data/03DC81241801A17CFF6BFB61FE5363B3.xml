<taxonomicName id="4C754BB11801A17AFF12FB7EFE9B678F" box="[238,372,1231,1257]" class="Insecta" family="Hymenopodidae" genus="Astyliasula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mantodea" pageId="10" pageNumber="253" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B901EC201801A17AFF12FB7EFE9B678F" bold="true" box="[238,372,1231,1257]" italics="true" pageId="10" pageNumber="253">Astyliasula</emphasis>
