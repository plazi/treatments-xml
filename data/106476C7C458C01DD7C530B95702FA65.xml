<geoCoordinate id="05E9386DD66309BB8A43CDA6163E38F3" direction="south" orientation="latitude" precision="15" value="-32.123886">
32°07
<normalizedToken id="76BF7E067E7BE378C3539E4777671E24" originalValue="’26”">'26&quot;</normalizedToken>
S
</geoCoordinate>
