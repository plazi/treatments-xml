<typeStatus id="A7648871FF87D261FEBFF944FE198A8E" box="[376,424,1702,1728]" pageId="24" pageNumber="325">type</typeStatus>
locality of 
<taxonomicName id="BFDF4D50FF87D261FDE9F944FD208AF1" authorityName="Smith" authorityYear="1869" box="[558,657,1701,1727]" class="Malacostraca" family="Panopeidae" genus="Panopeus" kingdom="Animalia" order="Decapoda" pageId="24" pageNumber="325" phylum="Arthropoda" rank="species" species="harttii">
