<typeStatus id="54F78837FFA3FFF146BAFAF5FF03FA99" box="[151,201,1294,1318]" pageId="61" pageNumber="62">Type</typeStatus>
species.— 
<taxonomicName id="4C4C4D16FFA3FFF14714FAF5FCCBFA99" authority="Boulard, 1980 b: 50" authorityName="Boulard" authorityPageNumber="50" authorityYear="1980" box="[313,769,1294,1318]" class="Insecta" family="Cicadidae" genus="Malgachialna" kingdom="Animalia" order="Hemiptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="noualhieri">
