, 
<taxonomicName id="00BD8237A704B6FDD408CFE61E4878FD" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="95" pageNumber="96" phylum="Mollusca" rank="species" species="marmarinus" subGenus="Drymaeus">
<emphasis id="2978B8DE4CCCB76F9303D35A01657784" italics="true" pageId="95" pageNumber="96">Drymaeus (Drymaeus) marmarinus</emphasis>
</taxonomicName>
(
<normalizedToken id="4E70CF7990BCCDCD699DEBDAF12004FC" originalValue="d’Orbigny">d'Orbigny</normalizedToken>
