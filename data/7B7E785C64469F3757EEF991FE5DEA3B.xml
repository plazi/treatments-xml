<taxonomicName id="34D7B2C964469F3654E8F816FBBCEE9A" authority="Zetterstedt" authorityName="Zetterstedt" authorityYear="1838" box="[913,1142,2002,2026]" class="Insecta" family="Chironomidae" genus="Rhamphomyia" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="obscura">
