<taxonomicName id="4C64ACA8FFE8FFED2784B8D1FDF7CB83" authority="Napp &amp; Monne" authorityName="Napp &amp; Monne" authorityYear="2008" box="[151,605,1727,1753]" class="Insecta" family="Cerambycidae" genus="Caperonotus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="tucurui" status="New species">
<emphasis id="B9100B39FFE8FFED2784B8D1FE39CB83" bold="true" box="[151,403,1727,1753]" italics="true" pageId="1" pageNumber="26">Caperonotus tucurui</emphasis>
