<taxonomicName id="4C6D4D5EED7E381AFF3EF969FEB6F900" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[251,371,1703,1729]" class="Maxillopoda" family="Notodelphyidae" genus="Gallincola" kingdom="Animalia" order="Cyclopoida" pageId="559" pageNumber="560" phylum="Arthropoda" rank="genus" status="gen. nov.">
