<quantity id="4CFC08C5E57CFFE4FDC1FBBCFDC5FBEE" box="[566,626,1070,1089]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="4" pageNumber="101" unit="mm" value="13.0">13 mm</quantity>
