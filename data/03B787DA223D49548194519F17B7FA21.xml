<taxonomicName id="4C1E4D4F223D495485FE551A1601FA51" authority="(Mound, 2007)" baseAuthorityName="Mound" baseAuthorityYear="2007" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Thysanoptera" pageId="15" pageNumber="62" phylum="Tracheophyta" rank="order">
