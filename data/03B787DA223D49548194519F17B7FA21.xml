of 
<taxonomicName id="4C1E4D4F223D4954830B554B14C2FAD9" authority="Karny" authorityName="Karny" authorityYear="1925" box="[520,904,1282,1308]" class="Insecta" family="Thripidae" genus="Anaphothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="62" phylum="Arthropoda" rank="species" species="plynopygus">
