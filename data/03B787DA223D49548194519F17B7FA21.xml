<taxonomicName id="4C1E4D4F223D495481945563157FFA81" authority="Anathakrishnan" authorityName="Ananthakrishnan" authorityYear="1968" box="[151,565,1322,1348]" class="Insecta" family="Thripidae" genus="Zonothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="62" phylum="Arthropoda" rank="species" species="luridus">
