<taxonomicName id="B6D290951A4A384D0F13383BFD5F4528" authority="McIntosh, 1885" authorityName="McIntosh" authorityYear="1885" box="[299,711,290,312]" class="Polychaeta" family="Terebellidae" genus="Euthelepus" kingdom="Animalia" order="Terebellida" pageId="11" pageNumber="251" phylum="Annelida" rank="species" species="setubalensis">
