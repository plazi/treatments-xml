<number id="3DEE24934F19FF9968D23FE46760FB3C" box="[764,793,1239,1265]" pageId="15" pageNumber="16" value="49.0">49</number>
(
<number id="3DEE24934F19FF9969033FE46702FB3C" box="[813,891,1239,1265]" pageId="15" pageNumber="16" value="48.0" valueMax="51.0" valueMin="45.0">45–51</number>
