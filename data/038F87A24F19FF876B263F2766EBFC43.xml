<number id="3DEE24934F19FF996E843E1460BEFA8C" box="[1194,1223,1319,1345]" pageId="15" pageNumber="16" value="25.0">25</number>
(
<number id="3DEE24934F19FF996EF83E14615BFA8C" box="[1238,1314,1319,1345]" pageId="15" pageNumber="16" value="25.5" valueMax="26.0" valueMin="25.0">25–26</number>
