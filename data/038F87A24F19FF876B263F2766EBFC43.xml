<number id="3DEE24934F19FF9968C73E44677FFA5C" box="[745,774,1399,1425]" pageId="15" pageNumber="16" value="18.0">18</number>
(
<number id="3DEE24934F19FF9969393E44671DFA5C" box="[791,868,1399,1425]" pageId="15" pageNumber="16" value="18.5" valueMax="20.0" valueMin="17.0">17–20</number>
