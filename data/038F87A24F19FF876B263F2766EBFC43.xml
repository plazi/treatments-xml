<emphasis id="B952EAA64F19FF9968953E4466A1FA5D" box="[699,728,1399,1424]" italics="true" pageId="15" pageNumber="16">
<number id="3DEE24934F19FF9968953E4466B1FA5D" box="[699,712,1399,1424]" pageId="15" pageNumber="16" value="1.0">1</number>
