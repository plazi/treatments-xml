<number id="3DEE24934F19FF996E753FCC601FFAD4" box="[1115,1126,1279,1305]" pageId="15" pageNumber="16" value="1.0">1</number>
/ 
<number id="3DEE24934F19FF996E5F3FCC60F9FAD4" box="[1137,1152,1279,1305]" pageId="15" pageNumber="16" value="3.0">3</number>
