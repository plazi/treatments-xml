<taxonomicName id="2F8453B5B7F68C97FBF05EA75F3E3D05" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleurodicus dugesii" order="Hemiptera" pageId="5" pageNumber="38" phylum="Arthropoda" rank="species" species="dugesii">Aleurodicus dugesii</taxonomicName>
