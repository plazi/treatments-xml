<taxonomicName id="BF4AE48DDA1A2F095A8CA2FE82E69D60" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleurodicus dugesii" order="Hemiptera" pageId="5" pageNumber="38" phylum="Arthropoda" rank="species" species="dugesii">Aleurodicus dugesii</taxonomicName>
