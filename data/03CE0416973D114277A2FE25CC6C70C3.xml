<geoCoordinate id="EE53D3C7973D114675C4FD95CEB27467" box="[753,945,548,575]" degrees="114" direction="east" minutes="10" orientation="longitude" pageId="2" pageNumber="27" precision="1" seconds="53.7" value="114.18158">E 114° 10’ 53.7’’</geoCoordinate>
