<collectionCode id="ED762DC5973D1146748AFDF9CEEC743B" box="[959,1007,585,611]" country="China" httpUri="http://grbio.org/cool/vxkr-andy" name="Chengdu Institute of Biology" pageId="2" pageNumber="27">CIB</collectionCode>
