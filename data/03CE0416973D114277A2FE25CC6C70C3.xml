<collectionCode id="ED762DC5973D1146740AFD21CE6C74F3" box="[831,879,657,683]" country="China" httpUri="http://grbio.org/cool/vxkr-andy" name="Chengdu Institute of Biology" pageId="2" pageNumber="27">CIB</collectionCode>
<date id="FFD993C0973D11467442FD21CECD74F3" box="[887,974,657,683]" pageId="2" pageNumber="27" value="27-03-14">140327</date>
