<collectionCode id="ED762DC5973D11467206FDDDC86174DF" box="[1331,1378,621,647]" country="China" httpUri="http://grbio.org/cool/vxkr-andy" name="Chengdu Institute of Biology" pageId="2" pageNumber="27">CIB</collectionCode>
