<collectingCountry id="F370F590973D114673BCFDB1C9CC7443" box="[1161,1231,513,539]" name="China" pageId="2" pageNumber="27">China</collectingCountry>
, 
<collectingRegion id="49A37BE2973D114673EFFDB1C89B7443" box="[1242,1432,513,539]" country="China" name="Jiangxi" pageId="2" pageNumber="27">Jiangxi Province</collectingRegion>
