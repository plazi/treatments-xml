<taxonomicName id="4C67CE83973D1146755CFDB1CFCB7443" box="[617,712,513,539]" class="Amphibia" kingdom="Animalia" order="Caudata" pageId="2" pageNumber="27" phylum="Chordata" rank="order">Caudata</taxonomicName>
