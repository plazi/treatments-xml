</taxonomicName>
(
<taxonomicName id="4C67CE83973D114676DFFDB1CF5C7443" box="[490,607,513,539]" class="Amphibia" kingdom="Animalia" pageId="2" pageNumber="27" phylum="Chordata" rank="class">Amphibia</taxonomicName>
