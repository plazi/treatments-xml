<taxonomicName id="3B671E6B5708F4BD85252EFE8F5B229D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29496" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera jeanneli (Santschi)" order="Hymenoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="jeanneli">jeanneli</taxonomicName>
