<taxonomicName id="AE2FF62F46A3F469613B615C401F348C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298369" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera surda Bolton &amp; Fisher" order="Hymenoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="surda">surda</taxonomicName>
