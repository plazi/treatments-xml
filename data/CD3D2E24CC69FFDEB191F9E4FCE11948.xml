<emphasis id="77E04320CC69FFD0B1C1F80CFD9B1BAC" bold="true" box="[199,605,1980,2007]" pageId="38" pageNumber="239">Measurements.Az inside diameter</emphasis>
, 
<geoCoordinate id="20A0F9F5CC69FFD0B36CF80DFD6E1BAC" box="[618,680,1980,2007]" degrees="0.055" direction="north" orientation="latitude" pageId="38" pageNumber="239" precision="55" value="0.055">0.055</geoCoordinate>
<geoCoordinate id="20A0F9F5CC69FFD0B3AEF80DFD301BAC" box="[680,758,1980,2007]" degrees="0.080" direction="west" orientation="longitude" pageId="38" pageNumber="239" precision="55" value="-0.08">–0.080</geoCoordinate>
(0.068 ±0.007); 
<emphasis id="77E04320CC69FFD0B2A8F80DFB711BAC" bold="true" box="[942,1207,1980,2007]" pageId="38" pageNumber="239">alveoli inside diameter</emphasis>
, 
<geoCoordinate id="20A0F9F5CC69FFD0B5C2F80DFAC51BAC" box="[1220,1283,1980,2007]" degrees="0.069" direction="north" orientation="latitude" pageId="38" pageNumber="239" precision="55" value="0.069">0.069</geoCoordinate>
