<taxonomicName id="4442DE0BFF897339C8B32211775F34E9" class="Insecta" family="Staphylinidae" genus="Remus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Remus" order="Coleoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">Remus</taxonomicName>
