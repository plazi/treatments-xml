<taxonomicName id="93E36AF6E56A7F09A6D97D5A5C11591F" class="Insecta" family="Staphylinidae" genus="Remus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Remus" order="Coleoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">Remus</taxonomicName>
