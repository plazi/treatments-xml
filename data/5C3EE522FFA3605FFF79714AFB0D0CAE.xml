<number id="625F4613FFA36059FF777294FF090AE6" box="[153,188,1956,1980]" pageId="2" pageNumber="49" value="1.5">1.5</number>
times as long as 
<emphasis id="E6E38826FFA36059FE6A7296FE150AE7" box="[388,416,1958,1981]" italics="true" pageId="2" pageNumber="49">ag</emphasis>
<number id="625F4613FFA36059FE4E7284FE1D0A9B" box="[416,424,1972,1985]" pageId="2" pageNumber="49" value="1.0">
<emphasis id="E6E38826FFA36059FE4E7284FE1D0A9B" box="[416,424,1972,1985]" italics="true" pageId="2" pageNumber="49">1</emphasis>
</number>
; setae 
<emphasis id="E6E38826FFA36059FE167296FDA10AE7" box="[504,532,1958,1981]" italics="true" pageId="2" pageNumber="49">an</emphasis>
<number id="625F4613FFA36059FDFA7284FDA90A9B" box="[532,540,1972,1985]" pageId="2" pageNumber="49" value="1.0">
<emphasis id="E6E38826FFA36059FDFA7284FDA90A9B" box="[532,540,1972,1985]" italics="true" pageId="2" pageNumber="49">1</emphasis>
</number>
and 
<emphasis id="E6E38826FFA36059FDBB7296FDC40AE7" box="[597,625,1958,1981]" italics="true" pageId="2" pageNumber="49">an</emphasis>
<number id="625F4613FFA36059FD9F7284FDCC0A9B" box="[625,633,1972,1985]" pageId="2" pageNumber="49" value="2.0">
<emphasis id="E6E38826FFA36059FD9F7284FDCC0A9B" box="[625,633,1972,1985]" italics="true" pageId="2" pageNumber="49">2</emphasis>
</number>
