<subSubSection id="C3126569FFB6FFD79EDE013C7CF6F8BE" box="[201,1437,1866,1891]" pageId="19" pageNumber="20" type="etymology">
<paragraph id="8BB736E2FFB6FFD79EDE013C7CF6F8BE" blockId="19.[151,1437,1650,2034]" box="[201,1437,1866,1891]" pageId="19" pageNumber="20">
<emphasis id="B97CEAF0FFB6FFD79EDE013C7824F8BF" bold="true" box="[201,335,1867,1891]" pageId="19" pageNumber="20">Etymology.</emphasis>
