<paragraph id="8BC43691FFC4B334FF3B4F1FFD40F9F4" blockId="38.[151,1436,1575,2034]" box="[151,657,1575,1600]" pageId="38" pageNumber="39">
<emphasis id="B90FEA83FFC4B334FF3B4F1FFF31F98B" bold="true" box="[151,224,1575,1600]" inLineHeading="true" pageId="38" pageNumber="39" reason="1">Color.</emphasis>
