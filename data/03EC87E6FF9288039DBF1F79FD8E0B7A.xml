<quantity id="4CBD9B15FF92880D9CC51E36FDA00965" box="[493,597,1809,1834]" metricMagnitude="-3" metricUnit="m" metricValue="2.68" pageId="23" pageNumber="324" unit="mm" value="2.68">2.68 mm</quantity>
