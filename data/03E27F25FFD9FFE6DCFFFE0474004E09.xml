<quantity id="4CB363D6FFD7FFE6DFCBFF4373CC4E21" box="[931,990,232,258]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="41" pageNumber="42" unit="m" value="20.0">20 m</quantity>
