<taxonomicName id="4C7857C4FF93FFAB6F95FDFFFA81FDCA" box="[1235,1300,603,624]" class="Arachnida" family="Cunaxidae" genus="Riscus" kingdom="Animalia" order="Prostigmata" pageId="18" pageNumber="40" phylum="Arthropoda" rank="genus">Riscus</taxonomicName>
