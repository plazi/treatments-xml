<typeStatus id="D330F9ADC500BD41FF45FE67506AFE50" box="[199,258,459,485]" pageId="16" pageNumber="247">Type</typeStatus>
locality:
</emphasis>
<collectingCountry id="749C079FC500BD41FEF2FE6050B5FE53" box="[368,477,460,486]" name="Paraguay" pageId="16" pageNumber="247">Paraguay</collectingCountry>
: 
<collectingRegion id="CE4F89EDC500BD41FE65FE6753D5FE50" box="[487,701,459,485]" country="Paraguay" name="Guaira" pageId="16" pageNumber="247">Guairá department</collectingRegion>
: 
<location id="095411D4C500BD41FD45FE675259FE50" LSID="urn:lsid:plazi:treatment:8422F619C500BD41FF15FF3B5340FD98:095411D4C500BD41FD45FE675259FE50" box="[711,817,459,485]" country="Paraguay" name="Villarrica" pageId="16" pageNumber="247" stateProvince="Guaira">Villarrica</location>
(“Villa Rica”)
