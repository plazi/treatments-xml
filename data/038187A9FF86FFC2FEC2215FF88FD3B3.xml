<taxonomicName id="4C284D3CFF86FFC2FEB92182F8C7D2EB" authority="Ewing, 1917" authorityName="Ewing" authorityYear="1917" box="[346,665,727,752]" class="Arachnida" family="Euphthiracaridae" genus="Euphthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageId="23" pageNumber="979" phylum="Arthropoda" rank="genus">
