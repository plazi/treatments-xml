<treatmentCitation id="0AC0525AE72EE954FFC7B12BFDE2FD0E" author="" journal="F Gris, Bull. Soc. bot. France" page="77" pageId="5" pageNumber="248" score="0.6039806827376195" volume="9" year="1862">
<taxonomicName id="4C610FC8E72EE954FFC7B12BFEF3FD0E" authority="Brongn. F Gris, Bull. Soc." class="Magnoliopsida" family="Cunoniaceae" genus="Codia" kingdom="Plantae" order="Oxalidales" pageId="5" pageNumber="248" phylum="Tracheophyta" rank="variety" species="obcordata" variety="discolor">
<emphasis id="B915A859E72EE954FFC7B12BFEE0FD24" box="[113,271,625,648]" italics="true" pageId="5" pageNumber="248">Codia obcordata</emphasis>
Brongn. F Gris var. 
<emphasis id="B915A859E72EE954FE53B12BFDC2FD24" box="[485,557,625,648]" italics="true" pageId="5" pageNumber="248">discolor</emphasis>
Brongn. F Gris, Bull. Soc.
</taxonomicName>
bot. 
<collectingCountry id="F37634DBE72EE954FEFAB1D1FE61FD0E" box="[332,398,651,674]" name="France" pageId="5" pageNumber="248">France</collectingCountry>
