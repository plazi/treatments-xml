<location id="8EBE2290E72EE954FDA9B102FD6FFDC3" LSID="urn:lsid:plazi:treatment:03C8C55DE72EE954FFC7B144FF1AFADF:8EBE2290E72EE954FDA9B102FD6FFDC3" box="[543,640,600,623]" country="France" municipality="Type" name="Basionym" pageId="5" pageNumber="248">Basionym</location>
