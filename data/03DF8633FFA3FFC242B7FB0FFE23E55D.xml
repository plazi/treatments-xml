<taxonomicName id="4C764CA6FFA3FFC34350F9D1FD9BE20E" box="[368,633,1633,1657]" class="Insecta" family="Psyllidae" genus="Arytainilla" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="spartiophila">
