<taxonomicName id="FDC5AC5EECC23312550C6DB7144CF3D4" authority="A.Fleischer, 1897" authorityName="A.Fleischer" authorityYear="1897" class="Insecta" family="Carabidae" genus="Cyclothorax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cyclothorax" order="Coleoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="genus">Cyclothorax</taxonomicName>
