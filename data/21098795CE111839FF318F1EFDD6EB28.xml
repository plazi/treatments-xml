<taxonomicName id="6EA04D00CE121839FC9B8BC7FB8DECF6" authority="Dantas &amp; Hamada, 2019" authorityName="Dantas &amp; Hamada" authorityYear="2019" box="[829,1080,482,506]" class="Insecta" family="Chironomidae" genus="Ablabesmyia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="122" phylum="Arthropoda" rank="species" species="monticola" status="sp. n.">
<emphasis id="9BD4EA91CE121839FC9B8BC7FB8DECF6" box="[829,1080,482,506]" italics="true" pageId="11" pageNumber="122">Ablabesmyia monticola</emphasis>
