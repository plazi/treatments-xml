<taxonomicName id="CAAF5485FFE47F5DE4136C9CAAF5FF4B" baseAuthorityName="Fisher" baseAuthorityYear="1938" box="[611,752,189,215]" class="Insecta" family="Keroplatidae" genus="Rocetelion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="579" phylum="Arthropoda" rank="species" species="fenestrale">
