<taxonomicName id="4C534D25FF99773FA56E2E45FCFAFBD9" box="[552,776,1047,1068]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="moseleyae">
