<taxonomicName id="4C534D25FF99773FA2CE2E45FEF7FBBC" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="unicoi">
