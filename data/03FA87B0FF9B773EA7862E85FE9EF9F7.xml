<taxonomicName id="4C534D25FF99773FA4AD2E45FB90FBD9" authority="Sokolov, 2011" authorityName="Sokolov" authorityYear="2011" box="[1003,1122,1047,1068]" class="Insecta" family="Carabidae" genus="Anillinus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="carltoni" status="new species">
