near 
<taxonomicName id="1FEF4D6EC75EFFA0FB84FEA63CF1FEE2" box="[1142,1422,338,363]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="blakeii">
