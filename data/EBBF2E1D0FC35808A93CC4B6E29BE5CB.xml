<taxonomicName id="BD715F96F51BFC5386E1D766216D308A" LSID="EBBF2E1D-0FC3-5808-A93C-C4B6E29BE5CB" authority="Aube, 1833" authorityName="Aube" authorityYear="1833" class="Insecta" family="Staphylinidae" genus="Bibloplectus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bibloplectus minutissimus" order="Coleoptera" pageId="0" pageNumber="53" phylum="Arthropoda" rank="species" species="minutissimus">
