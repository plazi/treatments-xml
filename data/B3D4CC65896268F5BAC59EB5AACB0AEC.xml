5. 
<taxonomicName id="8587E948FD580C614A87774F05AB4525" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31944" class="Insecta" family="Formicidae" genus="Myrmecia" kingdom="Animalia" lsidName-HNS="Myrmecia vindex Smith" order="Hymenoptera" pageId="144" pageNumber="144" phylum="Arthropoda" rank="species" species="vindex">Myrmecia vindex</taxonomicName>
