<taxonomicName id="CAF0E0B3EF76FF90E3F4FD4FA023FD3B" authority="(Winnertz, 1852)" baseAuthorityName="Winnertz" baseAuthorityYear="1852" box="[653,1025,676,703]" class="Insecta" family="Ceratopogonidae" genus="Ceratopogon" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="versicolor">
