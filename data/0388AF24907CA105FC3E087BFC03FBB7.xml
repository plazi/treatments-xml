<taxonomicName id="4C2165B1907DA106FE480898FD8AFA4E" authorityName="Gregor &amp; Povolny" authorityYear="1954" box="[462,567,1479,1498]" class="Insecta" family="Gelechiidae" genus="Caryocolum" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="genus">
<emphasis id="B955C220907DA106FE480898FD8AFA4E" box="[462,567,1479,1498]" italics="true" pageId="5" pageNumber="70">Caryocolum</emphasis>
</taxonomicName>
species recorded from Altai Mts. 17 – 
<taxonomicName id="4C2165B1907DA106FC0F0897FB30FA4E" authority="Povolny, 1969" authorityName="Povolny" authorityYear="1969" box="[905,1165,1479,1499]" class="Insecta" family="Gelechiidae" genus="Caryocolum" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="mongolense">
<emphasis id="B955C220907DA106FC0F0897FBB5FA4E" box="[905,1032,1479,1499]" italics="true" pageId="5" pageNumber="70">C. mongolense</emphasis>
Povolný, 1969
</taxonomicName>
, gen. prep. Bidzilya 192/18; 18 – 
<taxonomicName id="4C2165B1907DA106FF2E08BDFE99FA60" authority="Šumpich &amp; Huemer &amp; Bidzilya, 2020" authorityName="Šumpich &amp; Huemer &amp; Bidzilya" authorityYear="2020" box="[168,292,1505,1525]" class="Insecta" family="Gelechiidae" genus="Caryocolum" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="procurvella" status="sp. nov.">
<emphasis id="B955C220907DA106FF2E08BDFE99FA60" box="[168,292,1505,1525]" italics="true" pageId="5" pageNumber="70">C. procurvella</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2667F5B907DA106FEAC08BEFED2FA60" box="[298,367,1505,1524]" pageId="5" pageNumber="70" rank="species">sp. nov.</taxonomicNameLabel>
; gen. prep. Šumpich 19979; 19 – 
<taxonomicName id="4C2165B1907DA106FD1708BDFD63FA60" authority="Šumpich &amp; Huemer &amp; Bidzilya, 2020" authorityName="Šumpich &amp; Huemer &amp; Bidzilya" authorityYear="2020" box="[657,734,1506,1525]" class="Insecta" family="Gelechiidae" genus="Caryocolum" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="atrum" status="sp. nov.">
