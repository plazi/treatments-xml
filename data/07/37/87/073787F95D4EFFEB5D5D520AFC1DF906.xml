</taxonomicName>
<taxonomicNameLabel box="[515,569,1446,1467]" pageId="7" pageNumber="6" rank="species">
<emphasis bold="true" box="[515,538,1446,1467]" pageId="7" pageNumber="6">sp</emphasis>
. 
<emphasis bold="true" box="[550,563,1446,1467]" pageId="7" pageNumber="6">n</emphasis>
.
</taxonomicNameLabel>
<emphasis bold="true" box="[577,686,1445,1467]" pageId="7" pageNumber="6">Figures 33</emphasis>
, 
<emphasis bold="true" box="[700,760,1445,1467]" pageId="7" pageNumber="6">38–40</emphasis>
