<emphasis box="[1151,1264,1589,1615]" italics="true" pageId="32" pageNumber="33">
<collectionCode box="[1151,1172,1590,1615]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15534" lsid="urn:lsid:biocol.org:col:15534" name="Botanischer Garten und Botanisches Museum Berlin-Dahlem, Zentraleinrichtung der Freien Universitaet" pageId="32" pageNumber="33" type="Herbarium">
