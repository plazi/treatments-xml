<quantity box="[980,1051,872,896]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="11" pageNumber="129" unit="m" value="800.0">800 m</quantity>
to 
<quantity box="[1085,1173,871,896]" metricMagnitude="3" metricUnit="m" metricValue="2.6" pageId="11" pageNumber="129" unit="m" value="2600.0">2600 m</quantity>
