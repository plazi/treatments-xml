</paragraph>
<paragraph blockId="1.[151,1436,361,1466]" box="[151,678,361,386]" pageId="1" pageNumber="588">
<typeStatus box="[151,207,362,386]" pageId="1" pageNumber="588">Type</typeStatus>
species: 
<taxonomicName box="[309,608,361,385]" class="Insecta" family="Aradidae" genus="Phamaptera" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="588" phylum="Arthropoda" rank="species" species="lamdongensis" status="n. sp.">
<emphasis box="[309,608,361,385]" italics="true" pageId="1" pageNumber="588">Phamaptera lamdongensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[615,678,361,386]" pageId="1" pageNumber="588">
<taxonomicNameLabel box="[615,678,361,386]" pageId="1" pageNumber="588" rank="species">n. sp.</taxonomicNameLabel>
