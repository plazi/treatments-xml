<quantity box="[673,753,1844,1869]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="4" pageNumber="591" unit="m" value="1500.0">1500m</quantity>
in South 
<collectingCountry box="[882,981,1844,1869]" name="Vietnam" pageId="4" pageNumber="591">Vietnam</collectingCountry>
