<subSubSection pageId="5" pageNumber="592" type="diagnosis">
<paragraph blockId="5.[151,1436,1681,2030]" pageId="5" pageNumber="592">
<emphasis bold="true" box="[151,273,1681,1706]" pageId="5" pageNumber="592">Diagnosis.</emphasis>
