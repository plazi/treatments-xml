/ 
<collectionCode box="[323,352,1683,1705]" pageId="1" pageNumber="2">IN</collectionCode>
= 
<quantity box="[375,447,1683,1705]" metricMagnitude="-2" metricUnit="m" metricValue="1.6256" pageId="1" pageNumber="2" unit="in" value="0.64">0.64 in</quantity>
<emphasis box="[458,550,1683,1705]" italics="true" pageId="1" pageNumber="2">P. oyatabu</emphasis>
; 
