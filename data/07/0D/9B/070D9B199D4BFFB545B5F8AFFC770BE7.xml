<quantity metricMagnitude="-2" metricUnit="m" metricValue="2.0066" metricValueMax="2.032" metricValueMin="1.9812" pageId="1" pageNumber="2" unit="in" value="0.79" valueMax="0.8" valueMin="0.78">0.78– 0.80 in</quantity>
<emphasis box="[228,370,1767,1789]" italics="true" pageId="1" pageNumber="2">P. kathismaphlox</emphasis>
