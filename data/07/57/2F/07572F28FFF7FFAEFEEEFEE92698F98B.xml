<heading bold="true" box="[264,537,353,379]" fontSize="11" level="1" pageId="6" pageNumber="7" reason="1">
<emphasis bold="true" box="[264,537,353,379]" pageId="6" pageNumber="7">
<number box="[264,301,353,379]" pageId="6" pageNumber="7" value="2.1">2.1</number>
<taxonomicName box="[308,481,353,379]" class="Insecta" family="Baetidae" genus="Potamocloeon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[308,481,353,379]" italics="true" pageId="6" pageNumber="7">Potamocloeon</emphasis>
