<collectingCountry box="[343,398,1357,1380]" name="Malta" pageId="1" pageNumber="66">Male</collectingCountry>
(
<typeStatus box="[412,512,1357,1380]" pageId="1" pageNumber="66">holotype</typeStatus>
): Prosoma length 
<number box="[703,738,1357,1380]" pageId="1" pageNumber="66" value="1.8">1.8</number>
