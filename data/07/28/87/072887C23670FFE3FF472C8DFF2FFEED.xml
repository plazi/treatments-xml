<taxonomicName authority="Skoracki, 2011" authorityName="Skoracki" authorityYear="2011" box="[1095,1399,1887,1911]" class="Arachnida" family="Syringophilidae" genus="Neoaulonastus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="370" phylum="Arthropoda" rank="species" species="picidus">
