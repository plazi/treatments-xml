<quantity box="[407,475,344,367]" metricMagnitude="-1" metricUnit="m" metricValue="2.0" pageId="5" pageNumber="6" unit="cm" value="20.0">20 cm</quantity>
above the ground on young 
<taxonomicName box="[786,910,344,367]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="altissima">
<emphasis box="[786,910,344,367]" italics="true" pageId="5" pageNumber="6">S. altissima</emphasis>
</taxonomicName>
, 
<taxonomicName box="[924,1026,344,367]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="rugosa">
<emphasis box="[924,1026,344,367]" italics="true" pageId="5" pageNumber="6">S. rugosa</emphasis>
</taxonomicName>
, or 
<taxonomicName box="[1071,1216,344,367]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="canadensis">
<emphasis box="[1071,1216,344,367]" italics="true" pageId="5" pageNumber="6">S. canadensis</emphasis>
</taxonomicName>
