<taxonomicName authority="(Neboiss)" authorityName="Cartwright" authorityYear="2008" baseAuthorityName="Cartwright" baseAuthorityYear="2008" box="[362,692,1621,1645]" class="Insecta" family="Ecnomidae" genus="Daternomina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="merga">
<emphasis box="[362,577,1622,1645]" italics="true" pageId="19" pageNumber="20">Daternomina merga</emphasis>
