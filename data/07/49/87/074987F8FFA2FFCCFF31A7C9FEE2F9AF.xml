<emphasis bold="true" box="[192,808,195,220]" pageId="3" pageNumber="2">
<taxonomicName authority="(Anthonomus)" box="[192,557,195,220]" class="Insecta" family="Curculionidae" genus="Anthonomus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="subGenus" subGenus="Anthonomus">
<emphasis bold="true" box="[192,363,197,220]" italics="true" pageId="3" pageNumber="2">Anthonomus</emphasis>
(
<emphasis bold="true" box="[377,548,197,220]" italics="true" pageId="3" pageNumber="2">Anthonomus</emphasis>
)
</taxonomicName>
juniperinus group
</emphasis>
</heading>
</paragraph>
