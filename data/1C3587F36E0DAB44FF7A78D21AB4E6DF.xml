<subSubSection id="DC86656E6E0DAB41FF2B7FFF1C4FE291" box="[198,1106,1175,1202]" pageId="11" pageNumber="12" type="etymology">
<paragraph id="942336E56E0DAB41FF2B7FFF1C4FE291" blockId="11.[151,1437,1063,2023]" box="[198,1106,1175,1202]" pageId="11" pageNumber="12">
<emphasis id="A6E8EAF76E0DAB41FF2B7FFF194EE292" bold="true" box="[198,339,1175,1201]" pageId="11" pageNumber="12">Etymology.</emphasis>
