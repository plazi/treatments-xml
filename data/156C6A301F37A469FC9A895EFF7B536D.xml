<taxonomicName id="5AC5A0A51F30A469FED4888DFDA7528C" box="[295,546,228,249]" class="Bryopsida" family="Mniaceae" genus="Rhizomnium" kingdom="Plantae" order="Bryales" pageId="28" pageNumber="1184" phylum="Bryophyta" rank="species" species="hattorii">
<emphasis id="AFB107341F30A469FED4888DFDA7528C" box="[295,546,228,249]" italics="true" pageId="28" pageNumber="1184">Rhizomnium hattorii</emphasis>
</taxonomicName>
and 
<taxonomicName id="5AC5A0A51F30A469FDAC888CFF7B536D" authority="T. J. Kop." authorityName="T. J. Kop." class="Bryopsida" family="Mniaceae" genus="Rhizomnium" kingdom="Plantae" order="Bryales" pageId="28" pageNumber="1184" phylum="Bryophyta" rank="species" species="tuomikoskii">
<emphasis id="AFB107341F30A469FDAC888CFC8F528C" box="[607,778,228,250]" italics="true" pageId="28" pageNumber="1184">R. tuomikoskii</emphasis>
