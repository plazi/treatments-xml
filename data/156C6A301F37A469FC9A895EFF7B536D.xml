<taxonomicName id="5AC5A0A51F37A46EFBD58EB1FB1C549B" box="[1062,1177,1752,1774]" class="Bryopsida" family="Mniaceae" kingdom="Plantae" order="Bryales" pageId="27" pageNumber="1183" phylum="Bryophyta" rank="family">Mniaceae</taxonomicName>
(
<taxonomicName id="5AC5A0A51F37A46EFB588EB1FA8F549B" box="[1195,1290,1752,1774]" class="Bryopsida" kingdom="Plantae" order="Bryales" pageId="27" pageNumber="1183" phylum="Bryophyta" rank="order">Bryales</taxonomicName>
