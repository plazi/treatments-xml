<taxonomicName id="5AC5A0A51F30A469FD1A88ACFE9C528C" class="Bryopsida" family="Mniaceae" genus="Plagiomnium" kingdom="Plantae" order="Bryales" pageId="28" pageNumber="1184" phylum="Bryophyta" rank="species" species="vesicatum">
<emphasis id="AFB107341F30A469FD1A88ACFE9C528C" italics="true" pageId="28" pageNumber="1184">Pl. vesicatum</emphasis>
