<taxonomicName id="5AC5A0A51F30A469FEA888ACFD5C52AE" authority="(Sakurai) T. J. Kop." authorityName="T. J. Kop." baseAuthorityName="Sakurai" box="[347,729,197,219]" class="Bryopsida" family="Mniaceae" genus="Plagiomnium" kingdom="Plantae" order="Bryales" pageId="28" pageNumber="1184" phylum="Bryophyta" rank="species" species="tezukae">
<emphasis id="AFB107341F30A469FEA888ACFE6052AF" box="[347,485,197,218]" italics="true" pageId="28" pageNumber="1184">Pl. tezukae</emphasis>
