, park rd. km 29, 
<geoCoordinate id="EE795060FFF4FFE5237731A7FA65FBD8" box="[1316,1436,1018,1044]" degrees="20.3363" direction="north" orientation="latitude" pageId="17" pageNumber="258" precision="5" value="20.3363">20.3363ºN</geoCoordinate>
<geoCoordinate id="EE795060FFF4FFE526C43640FEE4FBF4" box="[151,285,1053,1080]" degrees="105.6094" direction="east" orientation="longitude" pageId="17" pageNumber="258" precision="5" value="105.6094">105.6094ºE</geoCoordinate>
