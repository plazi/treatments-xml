<collectionCode id="ED5CAE62FFF4FFE5258B3640FBCDFBFB" box="[984,1076,1053,1079]" country="Viet Nam" httpUri="http://grbio.org/cool/65ma-2bzq" name="Vietnam National Museum of Nature" pageId="17" pageNumber="258">VNMN</collectionCode>
