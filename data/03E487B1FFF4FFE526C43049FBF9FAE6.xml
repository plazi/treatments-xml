<quantity id="4CB59B42FFF4FFE5277B3640FE93FBF4" box="[296,362,1053,1080]" metricMagnitude="2" metricUnit="m" metricValue="3.5" pageId="17" pageNumber="258" unit="m" value="350.0">
<elevation id="0060D194FFF4FFE5277B3640FE93FBF4" box="[296,362,1053,1080]" metricMagnitude="2" metricUnit="m" metricValue="3.5" pageId="17" pageNumber="258" unit="m" value="350.0">350m</elevation>
