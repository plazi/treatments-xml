, 
<taxonomicName id="DA1F9F850ECEC462A736D78459CBE21B" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1855" class="Gastropoda" family="Bulimulidae" genus="Thaumastus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thaumastus plumbeus" order="Stylommatophora" pageId="24" pageNumber="41" phylum="Mollusca" rank="species" species="plumbeus">
