<typeStatus id="675E8879FFA4FFBCFE3FB416FE64C60F" box="[387,501,351,377]" pageId="98" pageNumber="99">Holotype</typeStatus>
</emphasis>
: ad 
<specimenCount id="AEE3FD52FFA4FFBCFD9AB416FDABC605" box="[550,570,351,371]" pageId="98" pageNumber="99" type="female">♀</specimenCount>
(
<collectionCode id="DEF4AE1EFFA4FFBCFDF5B429FD03C60C" box="[585,658,352,378]" country="India" httpUri="http://grbio.org/cool/qfpa-5ik3" name="National Pusa Collections" pageId="98" pageNumber="99">INPC</collectionCode>
)
</materialsCitation>
. 
<materialsCitation id="088D3C86FFA4FFBCFD19B416FC89C6D4" collectionCode="INPC, USNM" pageId="98" pageNumber="99" specimenCount="2" specimenCount-female="2" typeStatus="paratype">
<emphasis id="8A91EAC9FFA4FFBCFD19B416FCB3C60F" bold="true" box="[677,802,351,377]" pageId="98" pageNumber="99">
