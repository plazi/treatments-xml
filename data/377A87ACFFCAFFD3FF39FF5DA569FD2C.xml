<quantity id="782B9B5FFFCAFFD3FEF8FEC8A5A7FE90" box="[342,424,258,283]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="4" pageNumber="162" unit="mm" value="4.5">4.5mm</quantity>
