<quantity id="4C8792B1A11AFFAAFB1CF807C62343DA" box="[1212,1338,1937,1962]" metricMagnitude="0" metricUnit="m" metricValue="5.1181" metricValueMax="5.207" metricValueMin="5.0292" pageId="20" pageNumber="471" unit="in" value="201.5" valueMax="205.0" valueMin="198.0">198–205 in</quantity>
