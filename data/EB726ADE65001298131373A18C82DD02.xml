<taxonomicName id="6B2F128F87FF9F44CFC031799351226E" authority="Claparède, 1861" authorityName="Claparède" authorityYear="1861" class="Clitellata" family="Enchytraeidae" genus="Pachydrilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pachydrilus helgolandicus" order="Enchytraeida" pageId="27" pageNumber="42" phylum="Annelida" rank="species" species="helgolandicus">
<pageBreakToken id="8984F5DE15A966CAFB69D514EF8C744F" pageId="27" pageNumber="42" start="start">Pachydrilus</pageBreakToken>
