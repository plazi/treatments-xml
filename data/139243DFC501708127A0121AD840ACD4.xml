<taxonomicName id="1EA27BB0B9CD6CEA1055CECD004198CF" authority="Foerster, 1863" authorityName="Foerster" authorityYear="1863" class="Insecta" family="Braconidae" genus="Peristenus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Peristenus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">
Peristenus 
