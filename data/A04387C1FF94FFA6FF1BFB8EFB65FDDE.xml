<subSubSection id="60F0655CFF96FFA6FF4BFE9DFB65FDDE" pageId="18" pageNumber="469" type="distribution">
<paragraph id="285536D7FF96FFA6FF4BFE9DFD4DFE09" blockId="18.[151,1437,151,608]" box="[199,766,367,392]" pageId="18" pageNumber="469">
<emphasis id="1A9EEAC5FF96FFA6FF4BFE9DFED3FE09" bold="true" box="[199,352,367,392]" pageId="18" pageNumber="469">Distribution.</emphasis>
<collectingCountry id="50FD7647FF96FFA6FEEBFE9DFE7DFE09" box="[359,462,367,392]" name="Australia" pageId="18" pageNumber="469">Australia</collectingCountry>
(southeast, 
<bibRefCitation id="4C7B4B26FF96FFA6FDD8FE9DFD41FE09" author="Neboiss" box="[596,754,367,392]" pageId="18" pageNumber="469" refString="Neboiss, A. (1986) Atlas of Trichoptera of the SW Pacific - Australian region. Dr. W. Junk Publishers, Dordrecht, 286 pp. http: // dx. doi. org / 10.1007 / 978 - 94 - 009 - 4814 - 3" type="book" year="1986">
Neboiss 
<number id="9E2224F0FF96FFA6FD3AFE9DFD41FE09" box="[694,754,367,392]" pageId="18" pageNumber="469" value="1986.0">1986</number>
