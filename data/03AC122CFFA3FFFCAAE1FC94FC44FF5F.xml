<quantity id="4CFD0EDFFFA1FFFCAFFBF9F7FF10FF5F" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="6" pageNumber="48" unit="mm" value="5.0">5 µm</quantity>
