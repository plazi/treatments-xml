<quantity id="4CFD0EDFFFA2FFFFA8E8FE23FD54F8E3" box="[670,762,1868,1894]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="5" pageNumber="47" unit="mm" value="600.0">600 µm</quantity>
