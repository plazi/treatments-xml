<quantity id="4CFD0EDFFFA2FFFFA94BFE23FC36F8E3" box="[829,920,1868,1894]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="5" pageNumber="47" unit="mm" value="300.0">300 µm</quantity>
