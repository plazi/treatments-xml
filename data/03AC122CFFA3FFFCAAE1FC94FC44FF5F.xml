<quantity id="4CFD0EDFFFA2FFFFAAE1FE1BFF5AF80B" box="[151,244,1908,1934]" metricMagnitude="-1" metricUnit="m" metricValue="3.5" pageId="5" pageNumber="47" unit="mm" value="350.0">350 µm</quantity>
