<taxonomicName id="4C174D43FF3BFF1F98CEF9E0F85F3628" authority="(Gervais, 1848)" baseAuthorityName="Gervais" baseAuthorityYear="1848" box="[179,542,1554,1581]" class="Actinopterygii" family="Cichlidae" genus="Coptodon" kingdom="Animalia" order="Perciformes" pageId="170" pageNumber="171" phylum="Chordata" rank="species" species="zillii">
