<taxonomicName id="36F24D7FCF438B7E0838FE9EEF541BC1" authorityName="Macquart" authorityYear="1838" class="Insecta" family="Asilidae" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="457" phylum="Arthropoda" rank="subFamily" subFamily="Laphriinae">Laphriinae</taxonomicName>
