<paragraph id="8BB136F1FFC99414B38D9A1AFDDDF97B" blockId="35.[151,595,1661,1687]" box="[151,595,1661,1687]" pageId="35" pageNumber="336">
<heading id="D0F9819DFFC99414B38D9A1AFDDDF97B" bold="true" box="[151,595,1661,1687]" fontSize="11" level="1" pageId="35" pageNumber="336" reason="1">
<taxonomicName id="4C0E4D72FFC99414B38D9A1AFDDDF97B" authority="Bohart" authorityName="Bohart" authorityYear="1949" box="[151,595,1661,1687]" class="Insecta" family="Vespidae" genus="Polistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="336" phylum="Arthropoda" rank="species" species="rossi" subGenus="Fuscopolistes">
<emphasis id="B97AEAE3FFC99414B38D9A1AFDDDF97B" bold="true" box="[151,595,1661,1687]" pageId="35" pageNumber="336">
<emphasis id="B97AEAE3FFC99414B38D9A1AFF7AF97B" bold="true" box="[151,244,1661,1687]" italics="true" pageId="35" pageNumber="336">Polistes</emphasis>
(
<emphasis id="B97AEAE3FFC99414B21E9A1AFE26F97B" bold="true" box="[260,424,1661,1687]" italics="true" pageId="35" pageNumber="336">Fuscopolistes</emphasis>
) 
<emphasis id="B97AEAE3FFC99414B2A09A1AFE7DF97B" bold="true" box="[442,499,1661,1687]" italics="true" pageId="35" pageNumber="336">rossi</emphasis>
