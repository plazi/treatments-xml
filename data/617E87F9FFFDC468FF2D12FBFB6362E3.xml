<number id="5F1F24C8FFF8C468FDAE1049FDAF658B" box="[532,546,878,902]" pageId="4" pageNumber="553" value="3.0">3</number>
(m 
<number id="5F1F24C8FFF8C468FDF71049FDD7658B" box="[589,602,878,902]" pageId="4" pageNumber="553" value="3.0">3</number>
, m 
<number id="5F1F24C8FFF8C468FD3B1049FD03658B" box="[641,654,878,902]" pageId="4" pageNumber="553" value="3.0">3</number>
e, a 
<number id="5F1F24C8FFF8C468FD0C1049FD49658B" box="[694,708,878,902]" pageId="4" pageNumber="553" value="2.0">2</number>
) central and 
<number id="5F1F24C8FFF8C468FCE71049FCE6658B" box="[861,875,878,902]" pageId="4" pageNumber="553" value="1.0">1</number>
(m 
<number id="5F1F24C8FFF8C468FC2C104AFC2E658B" box="[918,931,877,902]" pageId="4" pageNumber="553" value="5.0">5</number>
) lateral mac. Abd.III with 
<number id="5F1F24C8FFF8C468FB601049FB65658B" box="[1242,1256,878,902]" pageId="4" pageNumber="553" value="1.0">1</number>
(m 
<number id="5F1F24C8FFF8C468FAA91049FAAD658B" box="[1299,1312,878,902]" pageId="4" pageNumber="553" value="3.0">3</number>
) central, 
<number id="5F1F24C8FFF8C468FA341049FA11658B" box="[1422,1436,878,902]" pageId="4" pageNumber="553" value="2.0">2</number>
(pm 
<number id="5F1F24C8FFF8C468FF7D10B6FF5865A7" box="[199,213,913,938]" pageId="4" pageNumber="553" value="6.0">6</number>
, p 
<number id="5F1F24C8FFF8C468FF5510B6FF7065A7" box="[239,253,913,938]" pageId="4" pageNumber="553" value="6.0">6</number>
) lateral mac, 
<number id="5F1F24C8FFF8C468FE2C10B5FE2965A7" box="[406,420,914,938]" pageId="4" pageNumber="553" value="1.0">1</number>
ms and 
<number id="5F1F24C8FFF8C468FDB910B5FD9C65A7" box="[515,529,914,938]" pageId="4" pageNumber="553" value="2.0">2</number>
