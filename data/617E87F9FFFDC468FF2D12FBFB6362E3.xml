and a 
<date id="9D69102FFFF8C468FD60106EFD62656F" box="[730,751,841,866]" pageId="4" pageNumber="553">
<number id="5F1F24C8FFF8C468FD60106EFD65656F" box="[730,744,841,866]" pageId="4" pageNumber="553" value="6.0">6</number>
i
</date>
as mac (
<figureCitation id="71EC2A6AFFF8C468FCE8106EFC28656F" box="[850,933,841,866]" captionStart="FIGURES 16 – 19" captionStartId="3.[151,264,1990,2013]" captionTargetBox="[167,1420,204,1959]" captionTargetId="figure@3.[151,1436,193,1969]" captionTargetPageId="3" captionText="FIGURES 16 – 19. Sinella longisensilla sp. nov.: Tergal chaetotaxy. 16, thorax; 17, Abd. I – III; 18, Abd. IV; 19, Abd. V." httpUri="https://zenodo.org/record/218240/files/figure.png" pageId="4" pageNumber="553">
