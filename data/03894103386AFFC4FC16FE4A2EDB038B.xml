<taxonomicName id="4C208B96386AFFC4FAE3FB292EF1030B" authorityName="Gill" authorityYear="1903" box="[1367,1451,1267,1291]" class="Actinopterygii" family="Erythrinidae" genus="Hoplias" kingdom="Animalia" order="Characiformes" pageId="45" pageNumber="46" phylum="Chordata" rank="genus">
<emphasis id="B9542C07386AFFC4FAE3FB292EF1030B" box="[1367,1451,1267,1291]" italics="true" pageId="45" pageNumber="46">Hoplias</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="4C208B96386AFFC4FCE8FAC928E2032B" authorityName="Miranda-Ribeiro" authorityYear="1918" box="[860,952,1299,1323]" class="Actinopterygii" family="Erythrinidae" genus="Hoplias" kingdom="Animalia" order="Characiformes" pageId="45" pageNumber="46" phylum="Chordata" rank="species" species="lacerdae">
<emphasis id="B9542C07386AFFC4FCE8FAC928E2032B" box="[860,952,1299,1323]" italics="true" pageId="45" pageNumber="46">lacerdae</emphasis>
</taxonomicName>
