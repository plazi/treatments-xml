<emphasis id="B9542C07386AFFC4FC99FB2E28C1030C" bold="true" box="[813,923,1268,1292]" pageId="45" pageNumber="46">Remarks.</emphasis>
<taxonomicName id="4C208B96386AFFC4FC13FB292FDE030B" baseAuthorityName="Gunther" baseAuthorityYear="1864" box="[935,1156,1267,1291]" class="Actinopterygii" family="Erythrinidae" genus="Hoplias" kingdom="Animalia" order="Characiformes" pageId="45" pageNumber="46" phylum="Chordata" rank="species" species="intermedius">
