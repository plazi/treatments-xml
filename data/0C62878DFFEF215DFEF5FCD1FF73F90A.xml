<taxonomicName id="43CB4D18FFEF215DFEA7FA1DFE39F9C9" box="[338,432,1532,1556]" class="Insecta" family="Aphodiidae" genus="Aphodius" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis id="B6BFEA89FFEF215DFEA7FA1DFE39F9C9" box="[338,432,1532,1556]" italics="true" pageId="1" pageNumber="44">Aphodius</emphasis>
</taxonomicName>
