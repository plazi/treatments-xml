species: 
<taxonomicName id="6FFD4D74A5446674FEF60C8D22DD8994" authority="Breddin, 1903" authorityName="Breddin" authorityYear="1903" box="[294,680,222,245]" class="Insecta" family="Cicadellidae" genus="Tettigonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="286" phylum="Arthropoda" rank="species" species="vespertinula">
