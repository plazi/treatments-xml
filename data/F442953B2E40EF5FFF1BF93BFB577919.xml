<taxonomicName id="BBEB5FAE2E40EF5FFEBCF910FE107939" authorityName="Swainson" authorityYear="1839" box="[282,421,1784,1808]" class="Actinopterygii" family="Tetraodontidae" genus="Canthigaster" kingdom="Animalia" order="Tetraodontiformes" pageId="61" pageNumber="62" phylum="Chordata" rank="genus">
