<collectionCode id="88F0AE62FFF2FF94FDC4FCF6D11AFCDF" LSID="urn:lsid:biocol.org:col:15536" box="[559,620,799,823]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15536" name="Department of Natural Resources, Environment, The Arts and Sport" pageId="43" pageNumber="44">DNA</collectionCode>
sequences (
