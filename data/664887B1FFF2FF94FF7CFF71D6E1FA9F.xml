<taxonomicName id="29E14D24FFF2FF94FF7CFF71D739FF5A" authority="Castroviejo-Fisher, Padial, Chaparro, Aguayo" authorityName="Castroviejo-Fisher, Padial, Chaparro, Aguayo" box="[151,1103,152,178]" class="Amphibia" family="Centrolenidae" genus="Hyalinobatrachium" kingdom="Animalia" order="Anura" pageId="43" pageNumber="44" phylum="Chordata" rank="species" species="carlesvilai">
