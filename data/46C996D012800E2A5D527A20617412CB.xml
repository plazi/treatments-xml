<taxonomicName id="7A3323C60CD7C7AB8219BFFDB2DB2A14" LSID="http://zoobank.org/F5E7EEC2-A203-491D-8C2C-5DE90C1F2404" class="Anthozoa" family="Xeniidae" genus="Ovabunda" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ovabunda andamanensis" order="Alcyonacea" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="andamanensis">Ovabunda andamanensis</taxonomicName>
