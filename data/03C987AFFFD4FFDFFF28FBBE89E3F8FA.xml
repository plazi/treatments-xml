<taxonomicName id="4C604D3AFFD4FFDFFD07FA888E86FA39" box="[696,761,1398,1419]" class="Magnoliopsida" family="Asteraceae" genus="Bidens" kingdom="Plantae" order="Asterales" pageId="1" pageNumber="281" phylum="Tracheophyta" rank="genus">
