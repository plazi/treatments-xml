<taxonomicName id="4C604D3AFFD4FFDFFA8DFAFE89EDFAA7" box="[1330,1426,1280,1301]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="1" pageNumber="281" phylum="Tracheophyta" rank="tribe" tribe="Cardueae">Cardueae</taxonomicName>
