<taxonomicName id="F147F1E8F64469C51BA7E346695C40FB" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia nigriceps" order="Coleoptera" pageId="90" pageNumber="91" phylum="Arthropoda" rank="species" species="nigriceps">Cephaloleia nigriceps</taxonomicName>
