<taxonomicName id="181D2A8811671033FC613BC72E0BF5BF" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia lojaensis" order="Coleoptera" pageId="90" pageNumber="91" phylum="Arthropoda" rank="species" species="lojaensis">Cephaloleia lojaensis</taxonomicName>
