<taxonomicName id="4C204D1FFFAE2152D9DAFBF9FD1766E1" authorityName="Abo-Elnaga &amp; Kandler" authorityYear="1965" box="[237,696,1135,1159]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="35" pageNumber="2817" phylum="Firmicutes" rank="subSpecies" species="coryniformis" subSpecies="torquens">
