<taxonomicName id="BAD94D67853DFFA0FF6F9AF4020DFD0C" authority="Audinet-Serville, 1835" authorityName="Audinet-Serville" authorityYear="1835" box="[151,523,723,750]" class="Insecta" family="Cerambycidae" genus="Oreodera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="544" phylum="Arthropoda" rank="genus">
