<taxonomicName id="4C674D05FF86FFDCFEB0FB0B25B72F5E" authority="Houart &amp; Héros, 2012" authorityName="Houart &amp; Héros" authorityYear="2012" box="[355,476,1261,1287]" class="Gastropoda" family="Muricidae" genus="Pazinotus" kingdom="Animalia" order="Neogastropoda" pageId="7" pageNumber="28" phylum="Mollusca" rank="species" species="chionodes" status="n. sp.">
<emphasis id="B913EA94FF86FFDCFEB0FB0B25B72F5E" box="[355,476,1261,1287]" italics="true" pageId="7" pageNumber="28">P. chionodes</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22057EFFF86FFDCFE31FB0B26772F51" box="[482,540,1262,1288]" pageId="7" pageNumber="28" rank="species">n. sp.</taxonomicNameLabel>
