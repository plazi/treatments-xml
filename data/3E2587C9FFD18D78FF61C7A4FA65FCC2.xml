<collectionCode id="D09DAE1AFFD18D78FD4FC650FCDFFC26" box="[697,810,1878,1904]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="10" pageNumber="453" type="Museum">NHMUK</collectionCode>
