<taxonomicName id="4C5D4D1EB810B47BFE17745EFC5FF909" authority="(Feruglio, 1954)" authorityName="Feruglio" authorityYear="1954" baseAuthorityName="Feruglio" baseAuthorityYear="1954" box="[488,815,1662,1687]" class="Bivalvia" family="Carditidae" genus="Scalaricardita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carditoida" pageId="23" pageNumber="74" phylum="Mollusca" rank="species" species="laciarina">
