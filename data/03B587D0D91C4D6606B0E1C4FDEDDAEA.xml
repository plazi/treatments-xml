<quantity id="4CE49B23D91C4D6407C5E299FD95DAB8" box="[482,550,1688,1715]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="154" unit="mm" value="4.0">4 mm</quantity>
