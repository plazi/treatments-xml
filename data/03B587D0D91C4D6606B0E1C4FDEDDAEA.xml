<quantity id="4CE49B23D91C4D640480E371FCB0DB80" box="[679,771,1904,1931]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="12" pageNumber="154" unit="mm" value="2.1">2.1 mm</quantity>
; pronotum length 
<quantity id="4CE49B23D91C4D6405F1E371FB82DB80" box="[982,1073,1904,1931]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="154" unit="mm" value="0.4">0.4 mm</quantity>
