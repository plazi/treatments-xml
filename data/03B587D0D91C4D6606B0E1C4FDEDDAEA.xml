<quantity id="4CE49B23D91C4D6404F5E395FC98DBA4" box="[722,811,1940,1967]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="12" pageNumber="154" unit="mm" value="0.8">0.8 mm</quantity>
