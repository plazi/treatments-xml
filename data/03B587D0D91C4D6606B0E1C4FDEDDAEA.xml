<quantity id="4CE49B23D91C4D64035EE371FF75DBA4" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="12" pageNumber="154" unit="mm" value="0.5">0.5 mm</quantity>
; elytra length 
<quantity id="4CE49B23D91C4D640741E395FE0CDBA4" box="[358,447,1940,1967]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="12" pageNumber="154" unit="mm" value="1.4">1.4 mm</quantity>
