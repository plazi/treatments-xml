<taxonomicName id="4C0749E7C82942548550A193F3B4FA5B" authority="Tseng &amp; Hsiao &amp; Hsu, 2018" authorityName="Tseng &amp; Hsiao &amp; Hsu" authorityYear="2018" box="[568,962,1485,1507]" class="Insecta" family="Curculionidae" genus="Omophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="591" phylum="Arthropoda" rank="species" species="wallacei" status="sp. nov." subGenus="Sinomophorus">
