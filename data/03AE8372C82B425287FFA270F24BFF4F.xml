<taxonomicName id="4C0749E7C8294254824CA193F270F9BA" authority="Voss, 1960" authorityName="Voss" authorityYear="1960" class="Insecta" family="Curculionidae" genus="Omophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="591" phylum="Arthropoda" rank="species" species="biroi" subGenus="Pangomophorus">
