<taxonomicName id="8ACAC29EFFCB3A67FF7569D7F24CFC1E" authority="Reitter, 1883" authorityName="Reitter" authorityYear="1883" box="[195,611,974,1000]" class="Insecta" family="Derodontidae" genus="Laricobius" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="sahlbergi">
