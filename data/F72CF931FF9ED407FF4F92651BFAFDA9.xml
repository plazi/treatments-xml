<caption id="2BFA18AFFF9CD400FF4F91061FF7F85B" ID-DOI="http://doi.org/10.5281/zenodo.4754106" ID-Zenodo-Dep="4754106" httpUri="https://zenodo.org/record/4754106/files/figure.png" pageId="3" pageNumber="486" startId="3.[151,216,1792,1818]" targetBox="[232,1336,193,1768]" targetPageId="3">
<paragraph id="7F3A4827FF9CD400FF4F91061FF7F85B" blockId="3.[151,1437,1792,1999]" pageId="3" pageNumber="486">
