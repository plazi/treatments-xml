<taxonomicName id="4C35DCDBFFFEFFF2DCBEFEFDFE0E7E1C" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[140,404,1002,1025]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="127" pageNumber="128" phylum="Arthropoda" rank="species" species="grossus" status="new species">
<emphasis id="B9417B4AFFFEFFF2DCBEFEFDFE0E7E1C" bold="true" box="[140,404,1002,1025]" italics="true" pageId="127" pageNumber="128">Paramblynotus grossus</emphasis>
</taxonomicName>
, 
