<quantity id="6E3A9B01FFF92B03FE463234FDEFE9EE" box="[460,528,1910,1933]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="15" pageNumber="16" unit="mm" value="7.0">7 mm</quantity>
