<taxonomicName id="AF18AC9058081B9B7DA0C0686BD0DB45" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145405" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sima sahlbergii Forel" lsidName-HNS="Sima sahlbergii Forel" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="species" species="sahlbergii">Sima sahlbergii</taxonomicName>
