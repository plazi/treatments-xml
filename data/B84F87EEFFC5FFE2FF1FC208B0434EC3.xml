<specimenCount id="26E0FD71FFC5FFE0FC7FC43AB0074A46" box="[1015,1097,1080,1106]" pageId="4" pageNumber="31" type="generic">
<quantity id="F71E9B1DFFC5FFE0FC7FC43AB00A4A46" box="[1015,1092,1080,1106]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="4" pageNumber="31" unit="m" value="400.0">400 m</quantity>
