<taxonomicName id="9B8B2EDEFFA2F1116BDA26B0FC0403AE" authority="Butler, 1878" authorityName="Butler" authorityYear="1878" box="[700,988,151,176]" class="Diplopoda" family="Sphaerotheriidae" genus="Cyliosoma" kingdom="Animalia" order="Sphaerotheriida" pageId="4" pageNumber="367" phylum="Arthropoda" rank="species" species="larvalis">
