<taxonomicName id="9B8B2EDEFFA2F111685C26B0FD5A03AE" authority="Silvestri, 1895" authorityName="Silvestri" authorityYear="1895" box="[314,642,151,176]" class="Diplopoda" family="Zephronidae" genus="Zephronia" kingdom="Animalia" order="Sphaerotheriida" pageId="4" pageNumber="367" phylum="Arthropoda" rank="species" species="albertisii">
