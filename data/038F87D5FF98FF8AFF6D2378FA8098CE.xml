<taxonomicName id="4C264D40FF98FF8AFCCF206DFC5698E2" authorityName="Attems" authorityYear="1907" box="[821,986,634,660]" class="Diplopoda" family="Diplomaragnidae" genus="Diplomaragna" kingdom="Animalia" order="Chordeumatida" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
