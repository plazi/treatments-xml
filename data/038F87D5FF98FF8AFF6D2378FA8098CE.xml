<taxonomicName id="4C264D40FF98FF8AFC452089FA8498CE" authority="(Mikhaljova 2000)" baseAuthorityName="Mikhaljova" baseAuthorityYear="2000" box="[959,1288,670,696]" class="Diplopoda" family="Diplomaragnidae" genus="Orientyla" kingdom="Animalia" order="Chordeumatida" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
