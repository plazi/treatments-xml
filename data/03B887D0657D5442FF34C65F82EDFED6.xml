<date id="FFAF1006657A5442FCE2C28285A9FF08" box="[833,912,152,178]" pageId="4" pageNumber="47">
<number id="3DD924E1657A5442FCE2C2828569FF08" box="[833,848,152,178]" pageId="4" pageNumber="47" value="7.0">7</number>
