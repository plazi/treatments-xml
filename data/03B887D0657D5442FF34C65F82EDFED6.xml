<date id="FFAF1006657A5442FD01C2A78510FF6D" box="[674,809,189,215]" pageId="4" pageNumber="47" value="1923-05-25">
<number id="3DD924E1657A5442FD01C2A784D2FF6D" box="[674,747,189,215]" pageId="4" pageNumber="47" value="24.0" valueMax="25.0" valueMin="23.0">23–25</number>
