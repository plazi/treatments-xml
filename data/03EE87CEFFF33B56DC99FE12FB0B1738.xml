<taxonomicName id="4C474D5BFFF53B56DE7FFEE0FD521215" authorityName="Dunin" authorityYear="1992" box="[622,761,305,330]" class="Arachnida" family="Dysderidae" genus="Dysdera" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="102" phylum="Arthropoda" rank="species" species="pamirica">
