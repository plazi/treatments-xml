, 
<taxonomicName id="00F44D1E254EE067FC3BBC78FB9CF9D1" authorityName="Sukaczev" authorityYear="1911" box="[981,1137,1593,1619]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="platyphylla">
