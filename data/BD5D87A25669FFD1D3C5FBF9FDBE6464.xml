<taxonomicName id="F2F44D375669FFDCD502F9E9FD43633C" authorityName="Thomas" authorityYear="1909" class="Mammalia" family="Phyllostomidae" genus="Vampyressa" kingdom="Animalia" order="Chiroptera" pageId="117" pageNumber="117" phylum="Chordata" rank="species" species="thyone">
<emphasis id="0780EAA65669FFDCD502F9E9FD43633C" italics="true" pageId="117" pageNumber="117">Vampyressa thyone</emphasis>
</taxonomicName>
(identified as 
<taxonomicName id="F2F44D375669FFDCD2A0F9CEFBB5633C" baseAuthorityName="Wagner" baseAuthorityYear="1843" box="[923,1026,1579,1603]" class="Mammalia" family="Phyllostomidae" genus="Vampyressa" kingdom="Animalia" order="Chiroptera" pageId="117" pageNumber="117" phylum="Chordata" rank="species" species="pusilla">
