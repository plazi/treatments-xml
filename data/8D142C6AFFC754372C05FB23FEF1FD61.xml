<emphasis id="37C9416EFFC754362C6DF833FE78F9FA" box="[352,500,1604,1629]" italics="true" pageId="6" pageNumber="7">
<taxonomicName id="C2BDE6FFFFC754362C6DF833FE7DF9FA" authorityName="Abeille de Perrin" authorityYear="1897" box="[352,497,1604,1629]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="croaticus">A. croaticus</taxonomicName>
