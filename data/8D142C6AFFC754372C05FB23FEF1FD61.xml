<taxonomicName id="C2BDE6FFFFC754362FC8F86BFCCBF992" authorityName="Obenberger" authorityYear="1927" box="[709,839,1564,1589]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="serbicus">
