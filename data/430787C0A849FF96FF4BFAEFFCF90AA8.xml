<collectionCode id="ADBFAE13A849FF96FE8FF986FECC0F72" box="[282,369,1619,1640]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="81" pageNumber="81" type="Museum">USNM</collectionCode>
