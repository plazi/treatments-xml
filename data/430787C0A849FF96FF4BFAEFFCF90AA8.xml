<typeStatus id="14158874A849FF96FF09F9C2FEA10F37" box="[156,284,1559,1583]" pageId="81" pageNumber="81">HOLOTYPE</typeStatus>
: 
<emphasis id="F9DAEAC4A849FF96FEA2F9CCFE5A0F34" bold="true" box="[311,487,1561,1582]" pageId="81" pageNumber="81">
<specimenCode id="9B089EADA849FF96FEA2F9CCFE5A0F34" box="[311,487,1561,1582]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="81" pageNumber="81">AMNH 40802</specimenCode>
</emphasis>
