, general appearance, male. 
<collectionCode id="60DCAE68FFFEFF8CFB39F926FA8404B2" box="[1277,1293,1761,1783]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="11" type="Museum">V</collectionCode>
, 
<taxonomicName id="C1CD4D2EFFFEFF8CFAD3F926FED00551" authority="(Chevrolat)" baseAuthorityName="Chevrolat" class="Insecta" family="Lycidae" genus="Rhyncheros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" phylum="Arthropoda" rank="species" species="loripes">
