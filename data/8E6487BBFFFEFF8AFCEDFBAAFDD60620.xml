<taxonomicName id="C1CD4D2EFFFEFF8CFD61F8FEFC0E050A" authority="(Duges)" baseAuthorityName="Duges" box="[677,903,1849,1871]" class="Insecta" family="Coleoptera (awaiting allocation)" genus="Neolycus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" phylum="Arthropoda" rank="species" species="fernandezi">
