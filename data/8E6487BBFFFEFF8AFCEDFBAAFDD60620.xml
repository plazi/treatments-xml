<taxonomicName id="C1CD4D2EFFFEFF8CFCB8F939FBA60551" box="[892,1071,1790,1812]" class="Insecta" family="Lycidae" genus="Rhyncheros" kingdom="Animalia" order="Coleoptera" pageId="11" phylum="Arthropoda" rank="species" species="undefined-3">
