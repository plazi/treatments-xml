<number id="3D9C24E5FFB23D78FD4DC4DDF8D5FC58" box="[692,736,908,932]" pageId="5" pageNumber="506" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
, pd 
<number id="3D9C24E5FFB23D78FCF6C4DDF928FC58" box="[783,797,908,932]" pageId="5" pageNumber="506" value="1.0">1</number>
