<taxonomicName id="53804D47FF91CF4955ABFEE8810FFE9C" authorityName="Scheffrahn" authorityYear="2021" box="[221,338,295,319]" class="Insecta" family="Kalotermitidae" genus="Cryptotermes" kingdom="Animalia" order="Isoptera" pageId="1" pageNumber="146" phylum="Arthropoda" rank="species" species="camelus">
