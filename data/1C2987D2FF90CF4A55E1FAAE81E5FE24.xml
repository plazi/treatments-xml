<taxonomicName id="53804D47FF91CF4955C2FE8C821AFEC0" authority="(Silvestri)" baseAuthorityName="Silvestri" baseAuthorityYear="1901" box="[180,583,331,355]" class="Insecta" family="Kalotermitidae" genus="Tauritermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="1" pageNumber="146" phylum="Arthropoda" rank="species" species="taurocephalus">
