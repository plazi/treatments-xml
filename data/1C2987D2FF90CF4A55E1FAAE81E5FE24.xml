<taxonomicName id="53804D47FF91CF495148FE8C85C4FEC0" authority="(Silvestri)" baseAuthorityName="Silvestri" baseAuthorityYear="1901" box="[1086,1433,331,355]" class="Insecta" family="Kalotermitidae" genus="Neotermes" kingdom="Animalia" order="Isoptera" pageId="1" pageNumber="146" phylum="Arthropoda" rank="species" species="fulvescens">
