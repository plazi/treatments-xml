<th id="C69B7629ACF2942221BE064DF3BCA689" colspan="1" pageId="15" pageNumber="64" rowspan="1">Characters</th>
<th id="57CB16E861A38FDF707DCE5333305CF5" colspan="1" pageId="15" pageNumber="64" rowspan="1">
<taxonomicName id="4789ACE6F800081176F6E64FF5975557" authorityName="A. K. Monro" authorityYear="2012" class="Dicotyledoneae" family="Solanaceae" genus="Cestrum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Tubiflorae" pageId="15" pageNumber="64" phylum="Angiospermae" rank="species" species="haberii">
