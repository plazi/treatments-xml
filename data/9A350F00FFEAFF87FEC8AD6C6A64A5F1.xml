<taxonomicName id="D59CC595FFE9FF86FF6BAA196900A057" authorityName=": Csuzdi" authorityYear="1995" box="[240,356,1364,1383]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" kingdom="Animalia" order="Haplotaxida" pageId="6" pageNumber="3044" phylum="Annelida" rank="species" species="feai" subGenus="Dichogaster">
