<caption id="46E3EE9EFFEAFF85FF11A92B69ACA3A3" ID-DOI="http://doi.org/10.5281/zenodo.5220107" ID-Zenodo-Dep="5220107" httpUri="https://zenodo.org/record/5220107/files/figure.png" pageId="5" pageNumber="3043" startId="5.[138,198,1638,1657]" targetBox="[138,1185,882,1602]" targetPageId="5">
