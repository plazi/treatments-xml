<emphasis id="B918EAC3430AFFEAFE94F92C4973F910" bold="true" box="[296,319,1712,1730]" pageId="14" pageNumber="355">dp</emphasis>
, dorsal process; 
<emphasis id="B918EAC3430AFFEAFE72F92C49AAF910" bold="true" box="[462,486,1712,1730]" pageId="14" pageNumber="355">mr</emphasis>
, medial ridge; 
<emphasis id="B918EAC3430AFFEAFDDDF92C4AC4F910" bold="true" box="[609,648,1712,1730]" pageId="14" pageNumber="355">muc</emphasis>
, muscular crest; 
<emphasis id="B918EAC3430AFFEAFCABF92C4B61F910" bold="true" box="[791,813,1712,1730]" pageId="14" pageNumber="355">nc</emphasis>
, neural canal; 
<emphasis id="B918EAC3430AFFEAFC1AF92C4BF7F910" bold="true" box="[934,955,1712,1730]" pageId="14" pageNumber="355">ns</emphasis>
, neural spine; 
<emphasis id="B918EAC3430AFFEAFB89F92C4C0BF910" bold="true" box="[1077,1095,1712,1730]" pageId="14" pageNumber="355">nt</emphasis>
