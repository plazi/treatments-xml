<taxonomicName id="26AA7695A377FFDFAA65E6F92AD5F835" authority="(Fabricius, 1794)" baseAuthorityName="Fabricius" baseAuthorityYear="1794" class="Insecta" family="Cicadellidae" genus="Cicadula" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="quadrinotata">
