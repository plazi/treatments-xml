<taxonomicName id="4C344D6FFFE3E5206143BBDA48B72DAE" box="[1042,1181,1695,1722]" class="Magnoliopsida" family="Platanaceae" kingdom="Plantae" order="Proteales" pageId="10" pageNumber="530" phylum="Tracheophyta" rank="family">Platanaceae</taxonomicName>
