<paragraph id="C86036AAFFABFFFFCEF6FBDBFAB6FB0B" blockId="1.[151,1436,728,1570]" box="[1012,1395,1066,1215]" pageId="1" pageNumber="41">
<emphasis id="FAABEAB8FFABFFFFCEF6FBDBFAB6FB0B" box="[1012,1395,1066,1215]" italics="true" pageId="1" pageNumber="41">
<taxonomicName id="0FDF4D29FFABFFFFCEF6FBDBFBCFFB0A" box="[1012,1034,1111,1215]" class="Insecta" family="Cetoniidae" genus="Octocollis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="41" phylum="Arthropoda" rank="genus">Octocollis</taxonomicName>
<taxonomicName id="0FDF4D29FFABFFFFC93FFBE3FB5EFB0A" authority="Neoclithria" authorityName="Neoclithria" box="[1085,1179,1100,1215]" class="Insecta" family="Cetoniidae" genus="Clithria" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="41" phylum="Arthropoda" rank="genus">Clithria Neoclithria</taxonomicName>
