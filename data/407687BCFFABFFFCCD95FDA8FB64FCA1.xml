<taxonomicName id="0FDF4D29FFABFFFFC85FFBCAFAB6FB0B" box="[1373,1395,1094,1214]" class="Insecta" family="Cetoniidae" genus="Schizorhina" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="41" phylum="Arthropoda" rank="genus">Schizorhina</taxonomicName>
