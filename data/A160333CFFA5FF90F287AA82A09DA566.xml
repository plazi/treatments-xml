<taxonomicName id="EEC9F9A9FFA5FF90F12DA8B3A1B6A082" box="[829,998,973,998]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="saligna">
<emphasis id="1BBD5E38FFA5FF90F12DA8B3A1B6A082" box="[829,998,973,998]" italics="true" pageId="13" pageNumber="14">Acacia saligna</emphasis>
</taxonomicName>
(as 
<taxonomicName id="EEC9F9A9FFA5FF90F606A8B3A6EFA082" box="[1046,1215,973,998]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="cyanophylla">
