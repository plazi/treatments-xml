<quantity id="EE312FCFFFA5FF90F142A88CA1F7A76E" box="[850,935,1009,1034]" metricMagnitude="1" metricUnit="m" metricValue="5.0165" pageId="13" pageNumber="14" unit="in" value="1975.0">1975 in</quantity>
the 
<location id="2C16D4F1FFA5FF90F1C6A88FA62FA76D" LSID="urn:lsid:plazi:treatment:A160333CFFA5FF90F287AA82A09DA566:2C16D4F1FFA5FF90F1C6A88FA62FA76D" box="[982,1151,1010,1033]" country="Australia" county="Table" municipality="Western Australian" name="Esperance area" pageId="13" pageNumber="14">Esperance area</location>
by 
<collectorName id="843CE7FCFFA5FF90F6B8A88FA734A76E" box="[1192,1380,1009,1034]" pageId="13" pageNumber="14">M. van den Berg</collectorName>
(
<collectorName id="843CE7FCFFA5FF90F762A88FA2AFA74A" pageId="13" pageNumber="14">van den Berg</collectorName>
, 1980) and from seeds of 
<taxonomicName id="EEC9F9A9FFA5FF90F00FAF68A164A74A" box="[543,820,1045,1070]" class="Magnoliopsida" family="Fabaceae" genus="Paraserianthes" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="lophanta">
<emphasis id="1BBD5E38FFA5FF90F00FAF68A164A74A" box="[543,820,1045,1070]" italics="true" pageId="13" pageNumber="14">Paraserianthes lophanta</emphasis>
</taxonomicName>
in 
<collectingDate id="4D335D02FFA5FF90F147AF6BA1ADA74A" box="[855,1021,1045,1070]" pageId="13" pageNumber="14" value="1984" valueMax="1985">
<date id="5D77A4EAFFA5FF90F147AF6BA1ADA74A" box="[855,1021,1045,1070]" pageId="13" pageNumber="14" value="1984" valueMax="1985">
1984 and 
<quantity id="EE312FCFFFA5FF90F1D5AF68A1ADA74A" box="[965,1021,1045,1070]" metricMagnitude="1" metricUnit="m" metricValue="5.0419" pageId="13" pageNumber="14" unit="in" value="1985.0">1985</quantity>
</date>
</collectingDate>
in the 
<location id="2C16D4F1FFA5FF90F659AF68A680A749" LSID="urn:lsid:plazi:treatment:A160333CFFA5FF90F287AA82A09DA566:2C16D4F1FFA5FF90F659AF68A680A749" box="[1097,1232,1045,1070]" country="Australia" county="Table" municipality="Western Australian" name="Albany area" pageId="13" pageNumber="14">Albany area</location>
by 
<collectorName id="843CE7FCFFA5FF90F6EAAF6BA73DA74A" box="[1274,1389,1045,1070]" pageId="13" pageNumber="14">M. Morris</collectorName>
and 
<collectorName id="843CE7FCFFA5FF90F287AF47A378A735" box="[151,296,1081,1106]" pageId="13" pageNumber="14">G. Dennill. It</collectorName>
was released in 
