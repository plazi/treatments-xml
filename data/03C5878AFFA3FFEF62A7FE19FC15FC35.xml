<taxonomicName id="4C6C4D1FFFA3FFE960C6FF9AFDD2FA1B" authorityName="Houart &amp; Héros" authorityYear="2013" box="[677,960,1451,1478]" class="Gastropoda" family="Muricidae" genus="Favartia" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="514" phylum="Mollusca" rank="species" species="tantelyi" subGenus="Favartia">
