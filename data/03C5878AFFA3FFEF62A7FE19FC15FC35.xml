<taxonomicName id="4C6C4D1FFFA3FFE961BBFCBCFA6AF97D" baseAuthorityName="Broderip" baseAuthorityYear="1833" box="[984,1144,1673,1700]" class="Gastropoda" family="Muricidae" genus="Favartia" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="514" phylum="Mollusca" rank="species" species="tetragona" subGenus="Favartia">
