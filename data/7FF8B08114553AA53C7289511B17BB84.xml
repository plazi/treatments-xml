<taxonomicName id="4621E028F158BBEB4EEF49C974B60E02" LSID="urn:lsid:zoobank.org:act:F97E7775-669A-42CB-81ED-5578B8997191" class="Arachnida" family="Neopilionidae" genus="Forsteropsalis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Forsteropsalis pureora" order="Opiliones" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="pureora">Forsteropsalis pureora</taxonomicName>
