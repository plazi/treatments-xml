<taxonomicName id="4C6503AD660FFF82FF1643294C67FDDF" authority="(Butler)" baseAuthorityName="Butler" baseAuthorityYear="1870" box="[151,364,532,556]" class="Insecta" family="Castniidae" genus="Telchin" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="277" phylum="Arthropoda" rank="species" species="diva">
