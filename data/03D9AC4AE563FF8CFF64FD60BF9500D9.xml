(
<figureCitation id="134B01D9E563FF8DFF6CFC8DBBE3007D" box="[154,241,792,815]" captionStart="Fig" captionStartId="5.[146,173,1283,1300]" captionTargetBox="[153,1435,148,1247]" captionTargetId="figure@5.[152,1435,147,1248]" captionTargetPageId="5" captionText="Fig. 4. Maps showing the known distribution ranges of Afrotropical Discothyrea species." figureDoi="http://doi.org/10.5281/zenodo.3542140" httpUri="https://zenodo.org/record/3542140/files/figure.png" pageId="54" pageNumber="55">Figs. 4L</figureCitation>
, 
<figureCitation id="134B01D9E563FF8DFF08FC8DBA0A007D" box="[254,280,792,815]" captionStart="Fig" captionStartId="10.[146,173,1454,1471]" captionTargetBox="[155,1434,149,1418]" captionTargetId="figure@10.[154,1434,147,1419]" captionTargetPageId="10" captionText="Fig. 6. Diagnostic plate showing still images from surface volume renderings of the head in full-face view (the remainder of the body virtually removed). All Afrotropical species are shown, species names are embedded in each particular image for better comparison and orientation.Specimen codes are given inTable 1." figureDoi="http://doi.org/10.5281/zenodo.3542144" httpUri="https://zenodo.org/record/3542144/files/figure.png" pageId="54" pageNumber="55">6L</figureCitation>
, 
<figureCitation id="134B01D9E563FF8DFED0FC8DBA52007D" box="[294,320,792,815]" captionStart="Fig" captionStartId="11.[146,173,1457,1474]" captionTargetBox="[153,1435,149,1421]" captionTargetId="figure@11.[152,1435,147,1422]" captionTargetPageId="11" captionText="Fig. 7. Diagnostic plate showing still images from surface volume renderings of the head in profile (the remainder of the body virtually removed).All Afrotropical species are shown, species names are embedded in each particular image for better comparison and orientation. Specimen codes are given inTable 1." figureDoi="http://doi.org/10.5281/zenodo.3542148" httpUri="https://zenodo.org/record/3542148/files/figure.png" pageId="54" pageNumber="55">7L</figureCitation>
, 
<figureCitation id="134B01D9E563FF8DFEB8FC8DBA7A007D" box="[334,360,792,815]" captionStart="Fig" captionStartId="12.[146,172,1457,1474]" captionTargetBox="[153,1435,149,1421]" captionTargetId="figure@12.[152,1435,147,1422]" captionTargetPageId="12" captionText="Fig. 8. Diagnostic plate showing still images from surface volume renderings of the right mandible in dorsal view (the remainder of the body virtually removed).All Afrotropical species are shown,species names are embedded in each particular image for better comparison and orientation.Specimen codes are given inTable 1." figureDoi="http://doi.org/10.5281/zenodo.3542150" httpUri="https://zenodo.org/record/3542150/files/figure.png" pageId="54" pageNumber="55">8L</figureCitation>
, 
<figureCitation id="134B01D9E563FF8DFE80FC8DBA82007D" box="[374,400,792,815]" captionStart="Fig" captionStartId="13.[146,173,1457,1474]" captionTargetBox="[153,1435,149,1421]" captionTargetId="figure@13.[152,1435,147,1422]" captionTargetPageId="13" captionText="Fig. 9. Diagnostic plate showing still images from surface volume renderings of the mesosoma in profile (the remainder of the body virtually removed). All Afrotropical species are shown, species names are embedded in each particular image for better comparison and orientation.Specimen codes are given inTable 1." figureDoi="http://doi.org/10.5281/zenodo.3542152" httpUri="https://zenodo.org/record/3542152/files/figure.png" pageId="54" pageNumber="55">9L</figureCitation>
