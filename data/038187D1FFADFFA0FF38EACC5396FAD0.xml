<taxonomicName id="4C284D44FFADFFA0FD9DEEAE52F4FAD0" authorityName="Tang &amp; Jiang" authorityYear="2018" box="[562,730,1269,1294]" class="Insecta" family="Staphylinidae" genus="Stenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="302" phylum="Arthropoda" rank="species" species="communicatus">communicatus</taxonomicName>
