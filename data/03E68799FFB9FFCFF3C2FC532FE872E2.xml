<taxonomicName id="4C4F4D0CFFB9FFCFF676F9832B9372AA" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1315,1435,1661,1685]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="90" pageNumber="91" phylum="Arthropoda" rank="species" species="globosa" status="sp. nov.">
