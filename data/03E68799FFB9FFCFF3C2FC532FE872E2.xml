<taxonomicName id="4C4F4D0CFFB9FFCFF271F95F2C0A7286" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[292,514,1697,1721]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="90" pageNumber="91" phylum="Arthropoda" rank="species" species="novahiberniensis" status="sp. nov.">
