<taxonomicName id="4C004D2CD52AB21BFC3EFE1CBE2A2D7B" authorityName="Sasa et Okazawa" authorityYear="1992" box="[968,1292,489,513]" class="Insecta" family="Chironomidae" genus="Bryophaenocladius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="518" phylum="Arthropoda" rank="species" species="togafelix">
