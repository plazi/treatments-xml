. – 
<taxonomicName id="4C5BC6DC1D172B4E599BFC5D724252F2" box="[1185,1374,942,966]" class="Liliopsida" family="Pandanaceae" genus="Benstonea" kingdom="Plantae" order="Pandanales" pageId="10" pageNumber="333" phylum="Tracheophyta" rank="species" species="foetida">
