<materialsCitation id="A5E93CA9FFA1FF80FF17FA2D2009FCDA" ID-GBIF-Occurrence="2332675084" box="[199,1314,1412,1439]" collectionCode="USNM" pageId="2" pageNumber="129" specimenCode="USNM 1569237-1569245" specimenCount="2" typeStatus="paratype">
