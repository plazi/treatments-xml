</taxonomicName>
<taxonomicNameLabel id="B69CC251782DFFECFF013E65AF81EB7A" box="[140,216,273,299]" pageId="42" pageNumber="43" rank="genus">gen. n.</taxonomicNameLabel>
, 
<taxonomicName id="58DBD8BB782DFFECFF6B3E64AEFEEB7B" authority="Wesener &amp; Enghoff &amp; Sierwald, 2009" authorityName="Wesener &amp; Enghoff &amp; Sierwald" authorityYear="2009" box="[230,423,272,298]" class="Diplopoda" family="Pachybolidae" genus="Pseudocentrobolus" kingdom="Animalia" order="Spirobolida" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus" status="gen. n.">
