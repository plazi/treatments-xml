<taxonomicName id="71DCFCC6C228FFFD24BFFA4FFE20FEA7" authority="Gill, 1859" authorityName="Gill" authorityYear="1859" box="[151,421,1410,1437]" class="Actinopterygii" family="Gobiidae" genus="Rhinogobius" kingdom="Animalia" order="Perciformes" pageId="31" pageNumber="110" phylum="Chordata" rank="genus">
<emphasis id="84A85B57C228FFFD24BFFA4FFE20FEA7" bold="true" box="[151,421,1410,1437]" pageId="31" pageNumber="110">
<emphasis id="84A85B57C228FFFD24BFFA4FFEAEFEA6" bold="true" box="[151,299,1410,1437]" italics="true" pageId="31" pageNumber="110">Rhinogobius</emphasis>
<bibRefCitation id="D24DFAB4C228FFFD251AFA4FFE20FEA7" author="Gill, T. N." box="[306,421,1410,1436]" pageId="31" pageNumber="110" pagination="144 - 150" refId="ref27075" refString="Gill, T. N. (1859) Notes on a collection of Japanese fishes, made by Dr. J. Morrow. Proceedings of the Academy of Natural Sciences of Philadelphia, 11, 144 - 150." type="journal article" year="1859">Gill, 1859</bibRefCitation>
