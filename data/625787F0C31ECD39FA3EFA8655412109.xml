<taxonomicName id="2DFE4D65C319CD38FA51FC4456F723C2" authorityName="El-Hawagry" authorityYear="2002" box="[257,392,927,950]" class="Insecta" family="Bombyliidae" genus="Anthrax" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="609" phylum="Arthropoda" rank="species" species="galali">
