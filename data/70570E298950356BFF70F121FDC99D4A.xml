<subSubSection id="B0E4ECB48950356BFF45F374FEBE9AD6" pageId="7" pageNumber="38" type="biology_ecology">
<paragraph id="F841BF3F8950356BFF45F374FEBE9AD6" blockId="7.[140,1108,764,1144]" pageId="7" pageNumber="38">
