<taxonomicName id="A7A53B0A05F5B248392EA23DD1DB45FB" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="nummularia">
