<quantity id="4CB79B0FFFA5FF8648C4FA85FEF2FACF" box="[192,332,1382,1406]" metricMagnitude="-2" metricUnit="m" metricValue="4.775" metricValueMax="5.1" metricValueMin="4.45" pageId="5" pageNumber="870" unit="mm" value="47.75" valueMax="51.0" valueMin="44.5">
