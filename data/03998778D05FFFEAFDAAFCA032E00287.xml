<elevation id="001DD15DD05FFFEBFB91FB3434DE0788" box="[1105,1184,1230,1255]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="10" pageNumber="346" unit="m" value="1500.0">1500 m</elevation>
