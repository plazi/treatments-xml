: 
<taxonomicName id="ABE2E5A4FFBFE46C07A8EA94FCF5E8A5" authority="(Pascoe, 1888)" authorityName="Santos-Silva &amp; Galileo &amp; Mcclarin" authorityYear="2018" baseAuthorityName="Pascoe" baseAuthorityYear="1888" box="[418,876,973,998]" class="Insecta" family="Cerambycidae" genus="Francisparna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="macilenta" status="comb. nov.">
<emphasis id="5E964235FFBFE46C07A8EA94FD2DE8A6" box="[418,692,973,997]" italics="true" pageId="8" pageNumber="9">Francisparna macilenta</emphasis>
(Pascoe, 1888)
</taxonomicName>
, 
<emphasis id="5E964235FFBFE46C0571EA97FC66E8A5" bold="true" box="[891,1023,973,998]" pageId="8" pageNumber="9">
<taxonomicNameLabel id="45A5FF4EFFBFE46C0571EA97FC66E8A5" box="[891,1023,973,998]" pageId="8" pageNumber="9">comb. nov.</taxonomicNameLabel>
