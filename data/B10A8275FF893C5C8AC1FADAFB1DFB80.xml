<taxonomicName id="FEA348E0FF893C5D8AC1FA71FDC6FA55" authority="Broun, 1893 b: 1309" authorityName="Broun, 1893 b:" authorityPageNumber="1309" authorityYear="1309" box="[151,606,1481,1506]" class="Insecta" family="Chrysomelidae" genus="Caccomolpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="maculatus">
