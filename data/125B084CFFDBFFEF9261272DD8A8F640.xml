<quantity id="5D0A14BFFFD4FFEF90FE20B1D81CF0CD" box="[919,1039,444,470]" metricMagnitude="1" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="8" pageNumber="9" unit="m" value="16.0" valueMax="17.0" valueMin="15.0">15–17 m</quantity>
