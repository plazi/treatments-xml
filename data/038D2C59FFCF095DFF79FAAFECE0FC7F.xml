<taxonomicName id="4C24E6CCFFCF095BFE91FAAEE8B2FA90" box="[383,462,1321,1347]" class="Elasmobranchii" family="Myliobatidae" genus="Manta" kingdom="Animalia" order="Myliobatiformes" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">
