<taxonomicName id="AFD9A5961252FFDFFD89FDB4FBF3FD9B" authority="Monterosato, 1878" authorityName="Monterosato" authorityYear="1878" box="[586,1110,581,606]" class="Gastropoda" family="Barleeiidae" genus="Barleeia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="40" pageNumber="41" phylum="Mollusca" rank="variety" species="rubra" variety="major">
