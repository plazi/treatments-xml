<subSubSection id="50BD656AFFAC9608FF5DFA45B7D6377C" pageId="15" pageNumber="788" type="materials_examined">
<paragraph id="181836E1FFAC9608FF5DFA45B4083636" blockId="15.[168,775,1483,1542]" pageId="15" pageNumber="788">
<emphasis id="2AD3EAF3FFAC9608FF5DFA45B4B835D3" bold="true" box="[168,405,1483,1507]" pageId="15" pageNumber="788">Material examined.</emphasis>
<collectionCode id="7EB6AE24FFAC9608FE53FA42B4DE35D4" box="[422,499,1484,1508]" country="0" httpUri="http://grbio.org/cool/42td-7yhx" name="Universidad Nacional de San Agustin, Museo de Historia Natural (Peru)" pageId="15" pageNumber="788">MUSA</collectionCode>
4098; Transect TN22, 
<date id="6C191021FFAC9608FF5DFA60B40C3636" box="[168,289,1518,1542]" pageId="15" pageNumber="788" value="2013-09-25">25.IX.2013</date>
