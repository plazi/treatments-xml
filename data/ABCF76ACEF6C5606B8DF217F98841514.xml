<taxonomicName id="31B1EC93F6BF17E15F7892718D9DA674" LSID="ABCF76AC-EF6C-5606-B8DF-217F98841514" authority="(Hellen, 1948)" authorityName="Newman" authorityYear="1837" baseAuthorityName="Hellén" baseAuthorityYear="1948" class="Insecta" family="Tenthredinidae" genus="Euura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euura pyramidalis" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="pyramidalis">
