<taxonomicName id="4C053082FFCEFFCDF317E8DAFF0B8081" class="Insecta" family="Ectobiidae" genus="Chorisoneura" kingdom="Animalia" order="Blattodea" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="minuta">
