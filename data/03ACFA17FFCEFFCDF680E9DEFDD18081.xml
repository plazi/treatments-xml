<taxonomicName id="4C053082FFCEFFCDF492E8DAFC5480E4" box="[645,954,403,427]" class="Insecta" family="Ectobiidae" genus="Chorisoneura" kingdom="Animalia" order="Blattodea" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="rochaesilvae">
