<collectionCode id="ED14D3C4FFCEFFCDF75BE8DAFE7980E5" box="[332,407,403,426]" httpUri="http://grbio.org/cool/zi1i-a0b5" name="Museu Nacional/Universidade Federal de Rio de Janeiro" pageId="27" pageNumber="28">MNRJ</collectionCode>
