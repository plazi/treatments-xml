<taxonomicName id="4C053082FFCEFFCDF513E826FC9C80C9" box="[772,882,367,390]" class="Insecta" family="Ectobiidae" genus="Chorisoneura" kingdom="Animalia" order="Blattodea" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="minuta">
