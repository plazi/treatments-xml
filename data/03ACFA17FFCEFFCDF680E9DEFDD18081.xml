<collectingCountry id="F3120B91FFCEFFCDF680E8DBFF3580E5" box="[151,219,402,426]" name="Brazil" pageId="27" pageNumber="28">Brazil</collectingCountry>
(
<typeStatus id="54BEF5A3FFCEFFCDF6F9E8DBFECE80E5" box="[238,288,402,426]" pageId="27" pageNumber="28">type</typeStatus>
