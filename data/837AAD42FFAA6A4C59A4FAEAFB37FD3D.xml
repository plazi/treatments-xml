<typeStatus id="D468A2F6FFAA6A535B53F92DFD19F97E" box="[608,656,1686,1712]" pageId="14" pageNumber="445">type</typeStatus>
locality (
<figureCitation id="93E800D1FFAA6A535BC8F92EFCC6F97E" box="[763,847,1685,1712]" captionStart="FIGURE 57" captionStartId="21.[151,250,1794,1819]" captionTargetBox="[151,1435,1094,1769]" captionTargetId="figure@21.[151,1435,1094,1769]" captionTargetPageId="21" captionText="FIGURE 57. Distribution map of Ampliphallus chimalapaensis gen. nov., sp. nov. (white circle) and two species of Guelaguetzia gen.nov.: G. cuicateca sp. nov. (black square) and G. serrana sp. nov. (white square)." figureDoi="http://doi.org/10.5281/zenodo.3701061" httpUri="https://zenodo.org/record/3701061/files/figure.png" pageId="14" pageNumber="445">Fig. 57</figureCitation>
