<taxonomicName id="CCD367D7FFAA6A535A9CF901FBB5F91D" authority="Cruz-López &amp; Francke, 2020" authorityName="Cruz-López &amp; Francke" authorityYear="2020" box="[943,1084,1722,1748]" class="Arachnida" family="Stygnopsidae" genus="Guelaguetzia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="14" pageNumber="445" phylum="Arthropoda" rank="species" species="cuicateca" status="sp. nov.">
