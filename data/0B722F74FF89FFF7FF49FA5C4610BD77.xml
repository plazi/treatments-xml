<treatmentCitationGroup id="A3CBB94CFF89FFF4FF49FA2347C3B9FA" box="[151,664,1504,1527]" pageId="13" pageNumber="14">
? 
<taxonomicName id="44DBE5E1FF89FFF4FF77FA234741B9FA" authority="Karl, 1943: 77" authorityName="Karl" authorityPageNumber="77" authorityYear="1943" box="[169,538,1504,1527]" class="Insecta" family="Anthomyiidae" genus="Pegomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="alpestris">
<emphasis id="B1AF4270FF89FFF4FF77FA23442CB9FA" box="[169,375,1504,1527]" italics="true" pageId="13" pageNumber="14">Pegomyia alpestris</emphasis>
<treatmentCitation id="027AB873FF89FFF4FEA3FA234741B9FA" author="Karl" box="[381,538,1504,1527]" page="77" pageId="13" pageNumber="14" year="1943">
<bibRefCitation id="E74AE393FF89FFF4FEA3FA2344A8B9FA" author="Karl" box="[381,499,1504,1527]" pageId="13" pageNumber="24" refString="Karl, O. (1943) Erganzungen und Berichtigungen zu meiner Arbeit uber die Musciden (Prof. Dr. Fr. Dahl, Die Tierwelt Deutschlands, Teil 13) VI. Stettiner Entomologische Zeitung, 104, 64 - 77." type="journal article" year="1943">
