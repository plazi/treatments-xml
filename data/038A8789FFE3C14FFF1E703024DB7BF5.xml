<taxonomicName id="4C234D1CFFE2C148FB07742527C47D94" authority="M. Sars, 1850" authorityName="M. Sars" authorityYear="1850" class="Hydrozoa" family="Campanulariidae" genus="Clytia" kingdom="Animalia" order="Leptothecata" pageId="11" pageNumber="12" phylum="Cnidaria" rank="species" species="gracilis">
