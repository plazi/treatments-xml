<taxonomicName id="4C18C4FBFFAED217D2A6FF1BFE725786" authority="Hood, 1954" authorityName="Hood" authorityYear="1954" box="[151,464,151,178]" class="Insecta" family="Thripidae" genus="Scirtidothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="genus">
<emphasis id="B96C636AFFAED217D2A6FF1BFE725786" bold="true" box="[151,464,151,178]" pageId="4" pageNumber="391">
<emphasis id="B96C636AFFAED217D2A6FF1BFE9E5785" bold="true" box="[151,316,151,177]" italics="true" pageId="4" pageNumber="391">Scirtidothrips</emphasis>
<bibRefCitation id="EF89C289FFAED217D372FF14FE725786" author="Hood" box="[323,464,152,178]" pageId="4" pageNumber="391" refString="Hood, J. D. (1954) Brazilian Thysanoptera V. Proceedings of the Biological Society of Washington, 67, 195 - 214." type="journal article" year="1954">
Hood, 
<number id="3DD0AD5FFFAED217D3A4FF14FE725786" box="[405,464,152,178]" pageId="4" pageNumber="391" value="1954.0">1954</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
