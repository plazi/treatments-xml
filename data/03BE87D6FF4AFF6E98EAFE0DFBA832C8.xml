<taxonomicName id="4C174D43FF4AFF6E9C8BFD7DFBA432C8" authority="(Gilbert, 1890)" baseAuthorityName="Gilbert" baseAuthorityYear="1890" class="Actinopterygii" family="Pleuronectidae" genus="Embassichthys" kingdom="Animalia" order="Pleuronectiformes" pageId="219" pageNumber="220" phylum="Chordata" rank="species" species="bathybius">
<emphasis id="B963EAD2FF4AFF6E9C8BFD7DFB7232C8" italics="true" pageId="219" pageNumber="220">Embassichthys bathybius</emphasis>
