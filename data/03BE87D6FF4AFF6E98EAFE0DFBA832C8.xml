<geoCoordinate id="EE235007FF4AFF6E98B2FDB5FB733264" box="[207,306,583,609]" degrees="32" direction="north" minutes="38" orientation="latitude" pageId="219" pageNumber="220" precision="925" value="32.633335">32°38’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FF4AFF6E993BFDB5FBFA3264" box="[326,443,583,609]" degrees="119" direction="west" minutes="24" orientation="longitude" pageId="219" pageNumber="220" precision="925" value="-119.4">119°24’W</geoCoordinate>
) (Personal communication: Los Angeles County Museum of Natural History Fish Collection, Los Angeles, California). Benthic; depth: 
<quantity id="4CEF9B25FF4AFF6E9B56FD99F9F23280" box="[811,947,619,645]" metricMagnitude="2" metricUnit="m" metricValue="8.92" metricValueMax="17.43" metricValueMin="0.41" pageId="219" pageNumber="220" unit="m" value="892.0" valueMax="1743.0" valueMin="41.0">41–1,743 m</quantity>
