<quantity id="5A261FAD80D0FF73EEE5C8751A880808" box="[910,981,1628,1652]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="212" pageNumber="213" unit="m" value="1.5" valueMax="3.0" valueMin="0.0">0–3 m</quantity>
