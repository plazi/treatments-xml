in the species 
<taxonomicName id="4C0E4D37F210F176EFE01706FECE5C0C" authorityName="Latham" authorityYear="1801" box="[186,335,360,383]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="36" phylum="Chordata" rank="species" species="pectoralis">
