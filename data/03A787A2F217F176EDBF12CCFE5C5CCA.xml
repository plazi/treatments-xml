<taxonomicName id="4C0E4D37F210F176EE0017C8FE575CCA" authorityName="G. R. Gray" authorityYear="1860" box="[346,470,420,441]" class="Aves" family="Pachycephalidae" genus="Pachycephala" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="36" phylum="Chordata" rank="species" species="vitiensis">
<emphasis id="B97AEAA6F210F176EE0017C8FE575CCA" box="[346,470,420,441]" italics="true" pageId="36" pageNumber="36">P. vitiensis</emphasis>
