<taxonomicName id="6DDC4D63D371D069BC25FBD9FC839D18" authority="Valley" authorityName="Valley" class="Insecta" family="Staphylinidae" genus="Numinbah" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="173" phylum="Arthropoda" rank="genus">Numinbah Valley</taxonomicName>
(
<collectingRegion id="6818F802D371D069BB6BFBBCFCFE9D18" box="[778,893,1114,1135]" country="Australia" name="Queensland" pageId="2" pageNumber="173">Queensland</collectingRegion>
) and 
<taxonomicName id="6DDC4D63D371D069BBCEFBBCFB909D18" box="[943,1043,1114,1135]" class="Insecta" family="Staphylinidae" genus="Numinbah" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="174" phylum="Arthropoda" rank="genus">Numinbah</taxonomicName>
Nature Reserve (
<collectingRegion id="6818F802D371D069BAA8FB92FCFF9DFE" box="[713,892,1140,1161]" country="Australia" name="New South Wales" pageId="2" pageNumber="173">New South Wales</collectingRegion>
