<collectingCountry id="F31312BEFF99FFB7CB47FCADFDC6B7EC" box="[527,632,878,904]" name="Brazil" pageId="12" pageNumber="205">BRAZIL</collectingCountry>
. 
<emphasis id="B9708E3CFF99FFB7CBCAFCAEFCBDB7EC" bold="true" box="[642,771,877,904]" pageId="12" pageNumber="205">
<collectingRegion id="49C09CCCFF99FFB7CBCAFCAEFD40B7EC" box="[642,766,877,904]" country="Brazil" name="Sao Paulo" pageId="12" pageNumber="205">São Paulo</collectingRegion>
:
</emphasis>
<location id="8EDB04F5FF99FFB7CA42FCADFCC4B7EC" LSID="urn:lsid:plazi:treatment:03ADE338FF99FFB7C9C0FD79FA1CB790:8EDB04F5FF99FFB7CA42FCADFCC4B7EC" box="[778,890,878,904]" country="Brazil" name="Angatuba" pageId="12" pageNumber="205" stateProvince="Sao Paulo">Angatuba</location>
, 
<date id="FFBA74EEFF99FFB7CACCFCADFBE3B7EC" box="[900,1117,878,904]" pageId="12" pageNumber="205" value="2012-12-18">
<collectingDate id="EFFE8D06FF99FFB7CACCFCADFBE3B7EC" box="[900,1117,878,904]" pageId="12" pageNumber="205" value="2012-12-18">18 December 2012</collectingDate>
