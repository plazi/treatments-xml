<emphasis id="B909EA80A92FFFB2FF351337FE67FAEE" bold="true" box="[151,483,1371,1397]" pageId="4" pageNumber="5">
Subgenus 
<emphasis id="B909EA80A92FFFB2FEB51337FE67FAEE" bold="true" box="[279,483,1371,1397]" italics="true" pageId="4" pageNumber="5">
<taxonomicName id="4C7D4D11A92FFFB2FEB51337FE32FAEE" box="[279,438,1371,1397]" class="Arachnida" family="Limnocharidae" genus="Limnochares" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus" sensu="stricto">Limnochares</taxonomicName>
<taxonomicNameLabel id="A23A57FBA92FFFB2FE1C1337FE67FAEE" box="[446,483,1371,1397]" pageId="4" pageNumber="5" sensu="stricto">s.s.</taxonomicNameLabel>
