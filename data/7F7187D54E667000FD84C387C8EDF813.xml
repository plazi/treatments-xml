<taxonomicName id="30D84D404E667000FF41C41ECE2EFB82" authority="Huber, 2011: 144" authorityName="Huber" authorityPageNumber="144" authorityYear="2011" box="[189,569,1120,1146]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="satun">
