<collectingRegion id="710F6CF33F25A1596066F998B001421B" box="[803,882,1576,1597]" country="Mexico" name="Chiapas" pageId="4" pageNumber="451">Chiapas</collectingRegion>
, 
<collectingRegion id="710F6CF33F25A159603BF998B0B4421B" box="[894,967,1576,1597]" country="Mexico" name="Oaxaca" pageId="4" pageNumber="451">Oaxaca</collectingRegion>
