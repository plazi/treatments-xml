<paragraph id="B374A2113F25A159619EF9BDB7DA42E4" blockId="4.[707,1193,1496,1730]" pageId="4" pageNumber="451">
<materialsCitation id="03A3A84C3F25A159619EF9BDB7DA42E4" country="Mexico" location="Mexico" pageId="4" pageNumber="451" specimenCount="1" stateProvince="Mexico">
