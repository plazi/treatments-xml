<taxonomicName id="7D94A99DFFBDF30CFF2FF9AA599BF9D1" authority="Verhoeff, 1943: 226" authorityName="Verhoeff" authorityPageNumber="226" authorityYear="1943" box="[151,510,1620,1642]" class="Diplopoda" family="Julidae" genus="Parancistrum" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="1" pageNumber="502" phylum="Arthropoda" rank="genus">
<emphasis id="88E00E0CFFBDF30CFF2FF9AA5947F9D1" box="[151,290,1621,1642]" italics="true" pageId="1" pageNumber="502">Parancistrum</emphasis>
Verhoeff, 
<number id="0C5CC039FFBDF30CFE2AF9AB59ACF9D1" box="[402,457,1620,1642]" pageId="1" pageNumber="502" value="1943.0">1943</number>
