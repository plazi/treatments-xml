<taxonomicName id="7D94A99DFFBEF30FFDA8F9C75AFDF9EB" box="[528,664,1592,1616]" class="Diplopoda" family="Julidae" genus="Phauloiulus" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="genus">
<emphasis id="88E00E0CFFBEF30FFDA8F9C75AFDF9EB" box="[528,664,1592,1616]" italics="true" pageId="2" pageNumber="503">Phauloiulus</emphasis>
</taxonomicName>
