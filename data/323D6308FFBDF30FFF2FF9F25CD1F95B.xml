</taxonomicName>
), 
<taxonomicName id="7D94A99DFFBEF30FFE58FA9E5A08FAC3" box="[480,621,1377,1400]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="serratum">
<emphasis id="88E00E0CFFBEF30FFE58FA9E5A08FAC3" box="[480,621,1377,1400]" italics="true" pageId="2" pageNumber="503">M. serratum</emphasis>
</taxonomicName>
, 
<taxonomicName id="7D94A99DFFBEF30FFDC5FA9E5B4DFAC3" box="[637,808,1376,1400]" class="Diplopoda" family="Julidae" genus="Chromatoiulus" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="tenenbaumi">
<emphasis id="88E00E0CFFBEF30FFDC5FA9E5B4DFAC3" box="[637,808,1376,1400]" italics="true" pageId="2" pageNumber="503">M. tenenbaumi</emphasis>
</taxonomicName>
and 
<taxonomicName id="7D94A99DFFBEF30FFCDBFA9E5B8BFACC" box="[867,1006,1376,1400]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="turcicum">
<emphasis id="88E00E0CFFBEF30FFCDBFA9E5B8BFACC" box="[867,1006,1376,1400]" italics="true" pageId="2" pageNumber="503">M. turcicum</emphasis>
</taxonomicName>
to the subgenus 
<taxonomicName id="7D94A99DFFBEF30FFB0BFA9F58AAFA27" authority="Attems, 1940" authorityName="Attems" authorityYear="1940" class="Diplopoda" family="Julidae" genus="Phauloiulus" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="genus">
