<taxonomicName id="7D94A99DFFBEF30FFB0CF9C65D25F9EB" box="[1204,1344,1592,1616]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="curvifolii">
