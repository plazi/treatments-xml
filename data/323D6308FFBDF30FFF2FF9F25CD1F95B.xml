</taxonomicName>
, a misprint), 
<taxonomicName id="7D94A99DFFBEF30FFF2FF9EA59A8F996" authority="Attems, 1905" authorityName="Attems" authorityYear="1905" box="[151,461,1556,1581]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="pentheri">
<emphasis id="88E00E0CFFBEF30FFF2FF9EA597FF997" box="[151,282,1556,1580]" italics="true" pageId="2" pageNumber="503">M. pentheri</emphasis>
(Attems, 
<number id="0C5CC039FFBEF30FFE32F9EB59A0F996" box="[394,453,1556,1581]" pageId="2" pageNumber="503" value="1905.0">1905</number>
)
</taxonomicName>
, 
<taxonomicName id="7D94A99DFFBEF30FFE62F9EA5B4EF997" authority="Attems, 1940" authorityName="Attems" authorityYear="1940" box="[474,811,1556,1580]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="seditiosum">
<emphasis id="88E00E0CFFBEF30FFE62F9EA5A1DF997" box="[474,632,1556,1580]" italics="true" pageId="2" pageNumber="503">M. seditiosum</emphasis>
(Attems, 
<number id="0C5CC039FFBEF30FFD50F9EB5B41F997" box="[744,804,1556,1580]" pageId="2" pageNumber="503" value="1940.0">1940</number>
)
</taxonomicName>
, 
<taxonomicName id="7D94A99DFFBEF30FFC80F9EA5C08F997" authority="Attems, 1932" authorityName="Attems" authorityYear="1932" box="[824,1133,1556,1580]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="tetricum">
<emphasis id="88E00E0CFFBEF30FFC80F9EA5BDEF997" box="[824,955,1557,1580]" italics="true" pageId="2" pageNumber="503">M. tetricum</emphasis>
(Attems, 
<number id="0C5CC039FFBEF30FFB93F9EB5C01F997" box="[1067,1124,1556,1580]" pageId="2" pageNumber="503" value="1932.0">1932</number>
)
</taxonomicName>
and 
<taxonomicName id="7D94A99DFFBEF30FFB1CF9EA58BFF9EB" authority="Verhoeff, 1903" authorityName="Verhoeff" authorityYear="1903" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="vicinum">
