<taxonomicName id="7D94A99DFFBEF30FFDC8F9A25B62F9CF" box="[624,775,1628,1652]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="adanense">
