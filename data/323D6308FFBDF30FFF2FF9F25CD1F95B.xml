Attems (
<number id="0C5CC039FFBEF30FFE7EFAC35A65FAEF" box="[454,512,1340,1364]" pageId="2" pageNumber="503" value="1940.0">1940</number>
) assigned the species 
<taxonomicName id="7D94A99DFFBEF30FFC97FAC25BACFAEF" box="[815,969,1340,1364]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="curvifolii">
<emphasis id="88E00E0CFFBEF30FFC97FAC25BACFAEF" box="[815,969,1340,1364]" italics="true" pageId="2" pageNumber="503">M. curvifolii</emphasis>
</taxonomicName>
, 
<taxonomicName id="7D94A99DFFBEF30FFC5CFAC25CAAFAEF" box="[996,1231,1340,1364]" class="Diplopoda" family="Julidae" genus="Megaphyllum" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="genezarethanum">
<emphasis id="88E00E0CFFBEF30FFC5CFAC25CAAFAEF" box="[996,1231,1340,1364]" italics="true" pageId="2" pageNumber="503">M. genezarethanum</emphasis>
</taxonomicName>
(referred to as 
<taxonomicName id="7D94A99DFFBEF30FFF2FFA9F59A2FAC3" box="[151,455,1376,1400]" class="Diplopoda" family="Julidae" genus="Chromatoiulus" kingdom="Animalia" order="Julida" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="palaestinus">
