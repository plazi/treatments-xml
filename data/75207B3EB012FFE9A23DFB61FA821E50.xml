<emphasis id="CFFD163AB012FFE9A25BFAE0FC601C7C" box="[858,992,1374,1397]" italics="true" pageId="17" pageNumber="385">
<typeStatus id="2232748AB012FFE9A25BFAE0FC0C1C7C" box="[858,908,1374,1397]" pageId="17" pageNumber="385">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="3A89B1ABB012FFE9A2EEFAE2FA141C7D" authority="Caporiacco, 1947" authorityName="Caporiacco" authorityYear="1947" box="[1007,1428,1372,1397]" class="Arachnida" family="Agelenidae" genus="Agelena" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="385" phylum="Arthropoda" rank="species" species="bornemiszai">
<emphasis id="CFFD163AB012FFE9A2EEFAE2FB501C7D" box="[1007,1232,1372,1396]" italics="true" pageId="17" pageNumber="385">Agelena bornemiszai</emphasis>
Caporiacco, 1947
</taxonomicName>
