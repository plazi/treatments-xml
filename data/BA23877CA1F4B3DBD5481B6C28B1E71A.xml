<paragraph id="742344CE19E6C4B55BD4A564F5DF2D97" pageNumber="93">
<taxonomicName id="338D1195FD974616C8301BB69875381A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:149224" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Crematogaster senegalensis goliathula Forel" order="Hymenoptera" pageNumber="93" phylum="Arthropoda" rank="subSpecies" species="senegalensis" subSpecies="goliathula">Crematogaster senegalensis Rog. r. goliathula</taxonomicName>
