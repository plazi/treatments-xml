<taxonomicName id="4C1E4D1BFFCBFFC8FD4BA03D2D01AA60" box="[681,854,1305,1330]" class="Insecta" family="Cicadellidae" genus="Varta" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="rubrostriata">
