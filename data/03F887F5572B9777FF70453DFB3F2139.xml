<taxonomicName id="4C514D60572B9777FCA5465FFB452035" authority="Handlirsch, 1906" authorityName="Handlirsch" authorityYear="1906" box="[834,1251,575,600]" class="Insecta" family="Limoniidae" genus="Eotipula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="180" phylum="Arthropoda" rank="species" species="lapidaria">
