<taxonomicName id="4C7A4D6CFFFEFF95FB2BFEB5D45B1A98" authorityName="Christoffersen" authorityYear="1984" box="[1212,1379,368,392]" class="Malacostraca" family="Alpheidae" genus="Alpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="337" phylum="Arthropoda" rank="species" species="estuariensis">
<emphasis id="B90EEAFDFFFEFF95FB2BFEB5D45B1A98" box="[1212,1379,368,392]" italics="true" pageId="6" pageNumber="337">
<collectingRegion id="49BEF80DFFFEFF95FB2BFEB5D5E81A97" box="[1212,1232,368,391]" country="Nigeria" name="Rivers" pageId="6" pageNumber="337">A</collectingRegion>
