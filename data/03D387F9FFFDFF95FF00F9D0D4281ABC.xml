<collectionCode id="ED6BAE2AFFFEFF95FD9DFEB5D35A1A98" box="[522,610,368,392]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="6" pageNumber="337">RMNH</collectionCode>
