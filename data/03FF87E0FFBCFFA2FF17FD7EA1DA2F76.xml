<subSubSection id="C34C657DFFBCFFA1FF17FD0BA6C72C83" box="[151,657,736,759]" pageId="1" pageNumber="64" type="reference_group">
<paragraph id="8BE936F6FFBCFFA1FF17FD0BA6C72C83" blockId="1.[151,657,736,759]" box="[151,657,736,759]" pageId="1" pageNumber="64">
<typeStatus id="54ED8854FFBCFFA1FF17FD0BA49A2C83" box="[151,204,736,759]" pageId="1" pageNumber="64">Type</typeStatus>
species: 
<taxonomicName id="4C564D75FFBCFFA1FEADFD0BA6DB2C83" authority="Rolston, 1992" authorityName="Rolston" authorityYear="1992" box="[301,653,736,759]" class="Insecta" family="Pentatomidae" genus="Stalius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="trisinuatus">
<emphasis id="B922EAE4FFBCFFA1FEADFD0BA5B82C83" box="[301,494,736,759]" italics="true" pageId="1" pageNumber="64">Stalius trisinuatus</emphasis>
<bibRefCitation id="EFC74B07FFBCFFA1FE74FD0BA6DB2C83" author="Rolston" box="[500,653,736,759]" pageId="1" pageNumber="68" refString="Rolston, L. H. (1992) Key and diagnoses for the genera of Ochlerini (Hemiptera: Pentatomidae: Discocephalinae). Journal of the New York Entomological Society, 100, 1 - 41." type="journal article" year="1992">
Rolston, 
<number id="3D9E24D1FFBCFFA1FDD5FD0BA6DB2C83" box="[597,653,736,759]" pageId="1" pageNumber="64" value="1992.0">1992</number>
