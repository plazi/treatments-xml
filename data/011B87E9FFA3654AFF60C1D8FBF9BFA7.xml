<geoCoordinate id="EC865038FFA3654BFD1CC044FCA5BC74" box="[747,869,261,288]" degrees="149.150" direction="east" orientation="longitude" pageId="50" pageNumber="51" precision="55" value="149.15">149.150°E</geoCoordinate>
