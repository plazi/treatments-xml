<elevation id="029FD1CCFFA3654BFC87C044FC18BC4B" box="[880,984,261,288]" metricMagnitude="2" metricUnit="m" metricValue="5.779999999999999" pageId="50" pageNumber="51" unit="m" value="578.0">
