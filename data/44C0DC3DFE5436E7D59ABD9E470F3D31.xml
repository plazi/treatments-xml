<geoCoordinate id="B18F2059DE158135D6C947DF61791F9E" direction="north" orientation="latitude" precision="1" value="27.151133">27°09.068'N</geoCoordinate>
, 
<geoCoordinate id="FC5D09D008D6A1AB9E22C6000FE50253" direction="east" orientation="longitude" precision="1" value="108.12307">108°07.384'E</geoCoordinate>
, net sweeping on a 
<taxonomicName id="A99A6909D0B5A5D228B45FC240F65046" class="Magnoliopsida" family="Adoxaceae" genus="Viburnum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Viburnum" order="Dipsacales" pageId="4" pageNumber="15" phylum="Tracheophyta" rank="genus">Viburnum</taxonomicName>
sp. plant, S. Yang col. (GZNULS). Paratype ♀ from Leigongshan, Leishan County, Guizhou Province, CHINA, 
<geoCoordinate id="39C109778210B18EC9A01D15E06A63CA" direction="north" orientation="latitude" precision="1" value="26.379683">26°22.781'N</geoCoordinate>
, 
