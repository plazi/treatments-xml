<paragraph id="004E36B4FFC48A22F577F3FBF32CD4EB" blockId="11.[2746,3547,3056,3623]" pageId="11" pageNumber="290">
<taxonomicName id="C7F14D37FFC48A22F577F3FBF3D2D5E2" authorityName="Ruppell" authorityYear="1842" box="[2747,3159,3162,3201]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="290" phylum="Chordata" rank="species" species="fumigatus">
