<typeStatus id="512487EBFF9F7255FD8B529AFD47839B" box="[616,714,1681,1706]" pageId="46" pageNumber="47">holotype</typeStatus>
from Borneo: 
<collectingRegion id="4C5BF7ABFF9F7255FC96529AFC33839B" box="[885,958,1681,1706]" country="Malaysia" name="Sabah" pageId="46" pageNumber="47">Sabah</collectingRegion>
