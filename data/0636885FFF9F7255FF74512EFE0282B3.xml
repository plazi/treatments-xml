<taxonomicName id="499F42CAFF9F7255FA625265FE818398" authorityName=": Hancock" authorityYear="1915" class="Insecta" family="Tetrigidae" genus="Hexocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="sexspicata">
