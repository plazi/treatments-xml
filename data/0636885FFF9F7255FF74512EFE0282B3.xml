<typeStatus id="512487EBFF9F7255FE135241FD098353" box="[496,644,1610,1634]" pageId="46" pageNumber="47">HOLOTYPE</typeStatus>
Sakoembang (
<collectionCode id="E88EA18CFF9F7255FCC95241FCE48353" box="[810,873,1610,1634]" country="Italy" httpUri="http://grbio.org/cool/0m6x-31ev" name="National Culture Bank" pageId="46" pageNumber="47">NCB</collectionCode>
