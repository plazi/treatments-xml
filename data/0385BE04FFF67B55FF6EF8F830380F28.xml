<taxonomicName id="4C2C7491FFF67B55FEBCF79C360B015A" authorityName="Prout" authorityYear="1925" box="[378,549,2067,2091]" class="Insecta" family="Noctuidae" genus="Athyrma" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="1114" phylum="Arthropoda" rank="species" species="ptocha">
