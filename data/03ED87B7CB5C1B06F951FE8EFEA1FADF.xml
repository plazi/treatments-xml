<taxonomicName id="4C444D22CB5C1B06FA6DFDFFFD58FDA8" authorityName="Thomson" authorityYear="1864" box="[552,689,601,622]" class="Insecta" family="Cerambycidae" genus="Eurynassa" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="73" phylum="Arthropoda" rank="genus">
