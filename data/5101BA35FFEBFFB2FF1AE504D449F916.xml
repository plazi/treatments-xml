<number id="6F601904FFEBFFB0FD3FE463D5DCFAF7" box="[690,720,1383,1407]" pageId="73" pageNumber="74" value="35.0">35</number>
′S
</geoCoordinate>
, 
<geoCoordinate id="BC9C6DE4FFEBFFB0FD62E463D445FAF7" box="[751,841,1383,1407]" direction="east" orientation="longitude" pageId="73" pageNumber="74" precision="925" value="28.683332">
<number id="6F601904FFEBFFB0FD62E463D401FAF7" box="[751,781,1383,1407]" pageId="73" pageNumber="74" value="28.0">28</number>
