<number id="3DE3F107FFEEFFA5FCAE6A3CFC2FF89E" box="[886,901,1833,1857]" pageId="12" pageNumber="272" value="4.0">4</number>
, m 
<date id="FF95C5E0FFEEFFA5FC706A3DFC15F89E" box="[936,959,1832,1857]" pageId="12" pageNumber="272">
<number id="3DE3F107FFEEFFA5FC706A3DFC1DF89E" box="[936,951,1832,1857]" pageId="12" pageNumber="272" value="4.0">4</number>
i
</date>
, m 
<number id="3DE3F107FFEEFFA5FC3A6A3CFC5BF89E" box="[994,1009,1833,1857]" pageId="12" pageNumber="272" value="4.0">4</number>
p), 
<number id="3DE3F107FFEEFFA5FBCB6A3CFBF0F89E" box="[1043,1114,1833,1857]" pageId="12" pageNumber="272" value="21.0" valueMax="22.0" valueMin="20.0">20−22</number>
posterior mac, 
<number id="3DE3F107FFEEFFA5FAD06A3CFABCF89E" box="[1288,1302,1833,1857]" pageId="12" pageNumber="272" value="1.0">1</number>
ms and 
<number id="3DE3F107FFEEFFA5FAAB6A3CFA2BF89E" box="[1395,1409,1833,1857]" pageId="12" pageNumber="272" value="2.0">2</number>
schaetae. Th.III with about 
<number id="3DE3F107FFEEFFA5FE1B6A58FE75F8BA" box="[451,479,1869,1893]" pageId="12" pageNumber="272" value="34.0">34</number>
mac and 
<number id="3DE3F107FFEEFFA5FD956A58FDF1F8BA" box="[589,603,1869,1893]" pageId="12" pageNumber="272" value="2.0">2</number>
lateral s-chaetae (
<figureCitation id="1310FFA5FFEEFFA5FCF16A59FCD4F8BA" box="[809,894,1868,1893]" captionStart="FIGURES 42 – 43" captionStartId="12.[151,263,1297,1320]" captionTargetBox="[169,1429,200,1290]" captionTargetId="figure@12.[165,1433,193,1291]" captionTargetPageId="12" captionText="FIGURES 42 – 43. Coecobrya draconis sp. nov.: 42, mucro; 43, thoracic chaetotaxy." httpUri="https://zenodo.org/record/229475/files/figure.png" pageId="12" pageNumber="272">
Fig. 
<number id="3DE3F107FFEEFFA5FC866A58FCD4F8BA" box="[862,894,1869,1893]" pageId="12" pageNumber="272" value="43.0">43</number>
</figureCitation>
). Abd.I with 
<number id="3DE3F107FFEEFFA5FBCC6A59FB88F8BA" box="[1044,1058,1868,1893]" pageId="12" pageNumber="272" value="6.0">6</number>
(a 
<number id="3DE3F107FFEEFFA5FB9A6A58FBFAF8BA" box="[1090,1104,1869,1893]" pageId="12" pageNumber="272" value="3.0">3</number>
, m 
<number id="3DE3F107FFEEFFA5FBAC6A58FB35F8BA" box="[1140,1183,1869,1893]" pageId="12" pageNumber="272" value="3.0" valueMax="4.0" valueMin="2.0">2–4</number>
, m 
<date id="FF95C5E0FFEEFFA5FB1B6A59FB73F8BA" box="[1219,1241,1868,1893]" pageId="12" pageNumber="272">
<number id="3DE3F107FFEEFFA5FB1B6A59FB7BF8BA" box="[1219,1233,1868,1893]" pageId="12" pageNumber="272" value="2.0">2</number>
i
</date>
, m 
<number id="3DE3F107FFEEFFA5FB246A58FAA0F8BA" box="[1276,1290,1869,1893]" pageId="12" pageNumber="272" value="4.0">4</number>
p) mac, 
