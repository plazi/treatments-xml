<paragraph id="8BC2A661FFEEB00CA2B0F9B9FC41F9D9" blockId="4.[151,1436,1487,1601]" box="[151,964,1578,1601]" pageId="4" pageNumber="283">
<taxonomicName id="4C7DDDE2FFEEB00CA2B0F9B9FDDDF9D9" authority="Liu &amp; Zhou. 2007" authorityName="Liu &amp; Zhou." authorityYear="2007" box="[151,600,1578,1601]" class="Insecta" family="Tettigoniidae" genus="Acosmetura" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="283" phylum="Arthropoda" rank="species" species="yachowensis">
