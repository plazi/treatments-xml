<taxonomicName id="ADF3A834E8E6EADE4B8E63F07493A6F8" authority="Pascoe, 1860" authorityName="Pascoe" authorityYear="1860" class="Insecta" family="Curculionidae" genus="Dinorhopala" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Dinorhopala" order="Coleoptera" pageId="12" pageNumber="135" phylum="Arthropoda" rank="genus">Dinorhopala</taxonomicName>
