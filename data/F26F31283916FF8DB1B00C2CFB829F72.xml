: see the references above; Bey-Bienko 
<number id="CC0E92193916FF8CB2B80D19FC049F47" box="[927,983,1818,1842]" pageId="30" pageNumber="31" value="1954.0">1954</number>
(as 
<taxonomicName id="BDC6FBBD3916FF8CB5290D18FB6D9F47" box="[1038,1214,1818,1842]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="longicaudata">
<emphasis id="48B25C2C3916FF8CB5290D18FB6D9F47" box="[1038,1214,1818,1842]" italics="true" pageId="30" pageNumber="31">I. longicaudata</emphasis>
</taxonomicName>
); Harz 
<number id="CC0E92193916FF8CB43B0D19FA879F47" box="[1308,1364,1818,1842]" pageId="30" pageNumber="31" value="1969.0">1969</number>
(as 
<taxonomicName id="BDC6FBBD3916FF8CB4AB0D18FE419F23" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="subSpecies" species="modesta" subSpecies="longicaudata">
<emphasis id="48B25C2C3916FF8CB4AB0D18FE419F23" italics="true" pageId="30" pageNumber="31">I. modesta longicaudata</emphasis>
</taxonomicName>
); Peshev 
<number id="CC0E92193916FF8CB32E0D3DFD929F23" box="[521,577,1854,1878]" pageId="30" pageNumber="31" value="1985.0">1985</number>
(as 
<taxonomicName id="BDC6FBBD3916FF8CB3500D43FC5D9F23" box="[631,910,1854,1879]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="subSpecies" species="modesta" subSpecies="longicaudata">
<emphasis id="48B25C2C3916FF8CB3500D43FC5D9F23" box="[631,910,1854,1879]" italics="true" pageId="30" pageNumber="31">I. modesta longicaudata</emphasis>
</taxonomicName>
