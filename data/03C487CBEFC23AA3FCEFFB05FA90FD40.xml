<taxonomicName id="4C6D4D5EEFC43AA0FEE3F8BAFCE2F84F" authority="Schellenberg, 1922" authorityName="Schellenberg" authorityYear="1922" box="[294,807,1908,1934]" class="Maxillopoda" family="Notodelphyidae" genus="Gunenotophorus" kingdom="Animalia" order="Cyclopoida" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="spinipes">
