<heading id="5E1481C5FF80FFB5FF19F9A89D07F920" bold="true" box="[151,342,1662,1689]" fontSize="11" level="2" pageId="59" pageNumber="60" reason="4">
<emphasis id="3797EABBFF80FFB5FF19F9A89D07F920" bold="true" box="[151,342,1662,1689]" pageId="59" pageNumber="60">
<taxonomicName id="C2E34D2AFF80FFB5FF19F9A89CB1F920" authority="Kim &amp; Boxshall, 2021" authorityName="Kim &amp; Boxshall" authorityYear="2021" box="[151,224,1662,1689]" class="Hexanauplia" family="Lichomolgidae" genus="Alupa" kingdom="Animalia" order="Cyclopoida" pageId="59" pageNumber="60" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="3797EABBFF80FFB5FF19F9A89CB1F920" bold="true" box="[151,224,1662,1689]" italics="true" pageId="59" pageNumber="60">Alupa</emphasis>
</taxonomicName>
<taxonomicNameLabel id="2CA457C0FF80FFB5FF66F9A99D07F920" box="[232,342,1663,1689]" pageId="59" pageNumber="60" rank="genus">gen. nov.</taxonomicNameLabel>
