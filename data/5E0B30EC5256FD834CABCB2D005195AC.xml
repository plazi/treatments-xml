<paragraph id="A3183D8D06D139E7662D7F3D77CDF065" pageId="1" pageNumber="72">
<taxonomicName id="51242AFD83D7EBE46DE36C2A13BC8A9C" class="Malacostraca" family="Melitidae" genus="Melita" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Melita megacheles" order="Amphipoda" pageId="1" pageNumber="72" phylum="Arthropoda" rank="species" species="megacheles">Melita megacheles</taxonomicName>
