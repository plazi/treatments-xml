<subScript id="2C25D15FFFE7A17FFF02F62CFF260670" attach="left" box="[197,208,290,306]" fontSize="7" pageId="8" pageNumber="842">2</subScript>
. 
<emphasis id="82D50F08FFE7A17FFF21F616FF0F0672" box="[230,249,280,304]" italics="true" pageId="8" pageNumber="842">v</emphasis>
<subScript id="2C25D15FFFE7A17FFF3EF62CFEF20670" attach="left" box="[249,260,290,306]" fontSize="7" pageId="8" pageNumber="842">3</subScript>
, 
<emphasis id="82D50F08FFE7A17FFED2F616FEDE0672" box="[277,296,280,304]" italics="true" pageId="8" pageNumber="842">v</emphasis>
<subScript id="2C25D15FFFE7A17FFEEFF62CFEC50670" attach="left" box="[296,307,290,306]" fontSize="7" pageId="8" pageNumber="842">4</subScript>
