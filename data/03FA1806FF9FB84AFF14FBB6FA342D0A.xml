<taxonomicName id="4C53D293FF9FB84BFDBAFBB6FD2C280E" box="[569,644,1124,1150]" class="Insecta" family="Cicadidae" genus="Semia" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="165" phylum="Arthropoda" rank="genus">
