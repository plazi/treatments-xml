<taxonomicName id="8E8B9A95FF89FFCDB5BDA7A5FDD4FB4C" authority="(Gravenhorst,1829)" box="[151,636,1241,1267]" class="Insecta" family="Ichneumonidae" genus="Diplazon" kingdom="Animalia" order="Hymenoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="multicolor">
<emphasis id="7BFF3D04FF89FFCDB5BDA7A5FE20FB4C" bold="true" box="[151,392,1241,1267]" italics="true" pageId="40" pageNumber="41">Diplazon multicolor</emphasis>
