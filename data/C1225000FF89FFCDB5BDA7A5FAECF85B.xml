<bibRefCitation id="2D1A9CE7FF89FFCDB4B2A7A5FD77FB4C" author="Gravenhorst, J. L. C." bookContentInfo="1097 pp." box="[408,735,1241,1267]" journalOrPublisher="Sumtibus auctoris, Vratislaviae" pageId="40" pageNumber="41" part="III" refId="ref116023" refString="Gravenhorst, J. L. C. (1829) Ichneumonologia Europaea. Pars III. Sumtibus auctoris, Vratislaviae, 1097 pp." title="Ichneumonologia Europaea" type="book" year="1829">
Gravenhorst 
<number id="FF43F331FF89FFCDB76AA7A5FDD4FB4C" box="[576,636,1241,1267]" pageId="40" pageNumber="41" value="1829.0">1829</number>
, 
<emphasis id="7BFF3D04FF89FFCDB7A0A7A5FD77FB4C" bold="true" box="[650,735,1241,1267]" italics="true" pageId="40" pageNumber="41">Bassus</emphasis>
