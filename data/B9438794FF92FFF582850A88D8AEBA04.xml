, 
<collectingCountry id="49FD7612FF90FFF281430AA5DAA9B9AF" box="[846,913,1945,1971]" name="Japan" pageId="3" pageNumber="96">Japan</collectingCountry>
, Korean Peninsula, 
<collectingCountry id="49FD7612FF90FFF2868D0AA4DDFEB9AE" box="[1152,1222,1944,1970]" name="Nepal" pageId="3" pageNumber="96">Nepal</collectingCountry>
