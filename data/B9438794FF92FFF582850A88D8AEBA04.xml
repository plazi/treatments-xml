, 
<collectingCountry id="49FD7612FF90FFF287400AA4DC94B9AE" box="[1357,1452,1944,1970]" name="Vietnam" pageId="3" pageNumber="96">Vietnam</collectingCountry>
(
<bibRefCitation id="557B4B73FF90FFF2829D0A80D8EFB9CB" author="Asthana, A. K. &amp; Srivastava, S. C." box="[144,471,1980,2007]" pageId="3" pageNumber="96" pagination="1 - 158" refId="ref4458" refString="Asthana, A. K. &amp; Srivastava, S. C. (1991) Indian hornworts (A taxonomic study). Bryophytorum Bibliotheca 42: 1 - 158." type="journal article" year="1991">Asthana &amp; Srivastava 1991</bibRefCitation>
, 
<bibRefCitation id="557B4B73FF90FFF283E50A81DB9AB9CB" author="Hasegawa, J." box="[488,674,1981,2007]" pageId="3" pageNumber="96" pagination="173 - 177" refId="ref6026" refString="Hasegawa, J. (1998) Taxonomic status of Anthoceros subbrevis Haseg. and taxonomy of its allied taxa. Bryological Research 7 (6): 173 - 177. https: // doi. org / 10.24474 / bryologicalresearch. 7.6 _ 173" type="journal article" year="1998">Hasegawa 1998</bibRefCitation>
, 
<bibRefCitation id="557B4B73FF90FFF280BE0A81DAB6B9CB" author="Peng, T. &amp; Zhu, R. - L." box="[691,910,1980,2007]" pageId="3" pageNumber="96" pagination="21 - 35" refId="ref6586" refString="Peng, T. &amp; Zhu, R. - L. (2013) A revision of the genus Anthoceros (Anthocerotaceae, Anthocerotophyta) in China. Phytotaxa 100 (1): 21 - 35. https: // doi. org / 10.11646 / phytotaxa. 100.1.3" type="journal article" year="2013">Peng &amp; Zhu 2013</bibRefCitation>
, 
<bibRefCitation id="557B4B73FF90FFF281930A80DDA0B9CB" author="Villarreal, J. C. &amp; Cargill, D. C. &amp; Soderstrom, L. &amp; Hagborg, A. &amp; von Konrat, M." box="[926,1176,1980,2007]" pageId="3" pageNumber="96" pagination="92 - 96" refId="ref8144" refString="Villarreal, J. C., Cargill, D. C., Soderstrom, L., Hagborg, A., von Konrat, M. (2015) Notes on early land plants today. 70. Nomenclatural notes in hornworts (Anthocerotophyta). Phytotaxa 208 (1): 92 - 96. https: // doi. org / 10.11646 / phytotaxa. 208.1.9" type="journal article" year="2015">
