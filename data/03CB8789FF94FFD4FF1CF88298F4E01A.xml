<taxonomicName id="4C624D1CFF94FFD7FA96F8689879E68A" authority="Meigen, 1803" authorityName="Meigen" authorityYear="1803" class="Insecta" family="Anthomyiidae" genus="Anthomyia" kingdom="Animalia" order="Diptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus">
