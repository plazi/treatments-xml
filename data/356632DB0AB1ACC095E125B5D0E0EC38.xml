<taxonomicName id="93956FC39E7C00E2F3C0AD2B14FF6FA3" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Coleoptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Coleoptera" order="Coleoptera" pageId="274" pageNumber="275" phylum="Tracheophyta" rank="order">Coleoptera</taxonomicName>
