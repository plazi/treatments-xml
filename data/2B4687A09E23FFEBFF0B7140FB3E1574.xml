<specimenCode id="F3499ECD9E23FFEBFD0E75DBFC81179B" box="[701,887,448,470]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="39" pageNumber="39">AMNH 134058</specimenCode>
