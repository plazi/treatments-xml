<emphasis id="B934694CFF8CFFE5FF34FD0444FAFCDC" bold="true" box="[151,737,754,781]" pageId="16" pageNumber="51">
<taxonomicName id="4C40CEDDFF8CFFE5FF34FD044467FCDC" authority="Li &amp; Qi &amp; Yang, 2019" authorityName="Li &amp; Qi &amp; Yang" authorityYear="2019" box="[151,636,754,781]" class="Insecta" family="Lauxaniidae" genus="Trypetisoma" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="51" phylum="Arthropoda" rank="species" species="trimaculata" status="sp. nov." subGenus="Trypaneoides">
<emphasis id="B934694CFF8CFFE5FF34FD044737FCDD" bold="true" box="[151,300,754,780]" italics="true" pageId="16" pageNumber="51">Trypetisoma</emphasis>
(
<emphasis id="B934694CFF8CFFE5FE9EFD0447C4FCDC" bold="true" box="[317,479,754,781]" italics="true" pageId="16" pageNumber="51">Trypaneoides</emphasis>
) 
<emphasis id="B934694CFF8CFFE5FE53FD044467FCDC" bold="true" box="[496,636,754,781]" italics="true" pageId="16" pageNumber="51">trimaculata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A207D437FF8CFFE5FD27FD0544FAFCDC" box="[644,737,755,781]" pageId="16" pageNumber="51" rank="species">sp. nov.</taxonomicNameLabel>
