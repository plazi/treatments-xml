). Length, 
<quantity id="4CEC9B1EFF9CFFE4FD869268FD15F99D" box="[519,646,1628,1653]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" metricValueMax="1.7" metricValueMin="1.3" pageId="1" pageNumber="3643" unit="mm" value="1.5" valueMax="1.7" valueMin="1.3">1.3–1.7mm</quantity>
