<collectingCountry id="F3164EFFFF8DFFADDC7CF5F9CD90FC33" box="[565,654,969,992]" name="Ethiopia" pageId="18" pageNumber="81">Ethiopia</collectingCountry>
and subsequently recorded from 
<collectingCountry id="F3164EFFFF8DFFADDFBAF5D9CD2DFBD3" box="[499,563,1001,1024]" name="Egypt" pageId="18" pageNumber="81">Egypt</collectingCountry>
(including 
<collectingRegion id="49C5C08DFF8DFFADDE8AF239CE6EFBF3" box="[195,368,1033,1056]" country="Egypt" name="Shamal Sina'" pageId="18" pageNumber="81">Sinai Peninsula</collectingRegion>
), 
<collectingCountry id="F3164EFFFF8DFFADDFCCF239CEDDFBF3" box="[389,451,1033,1056]" name="Israel" pageId="18" pageNumber="81">Israel</collectingCountry>
, 
<collectingCountry id="F3164EFFFF8DFFADDF99F239CD40FBF3" box="[464,606,1033,1056]" name="Saudi Arabia" pageId="18" pageNumber="81">Saudi Arabia</collectingCountry>
and southern 
<collectingCountry id="F3164EFFFF8DFFADDF48F219CE5AFB93" box="[257,324,1065,1088]" name="Oman" pageId="18" pageNumber="81">Oman</collectingCountry>
(
<bibRefCitation id="EF90739EFF8DFFADDF1FF219CD16FB93" author="WEWALKA G." box="[342,520,1065,1088]" pageId="18" pageNumber="81" pagination="105 - 113" refId="ref25969" refString="WEWALKA G. 1974: Systematische und faunistische Bemerkungen zu einigen palaarktischen Dytisciden (Coleoptera). Koleopterologische Rundschau 51: 105 - 113." type="journal article" year="1974">WEWALKA 1974</bibRefCitation>
