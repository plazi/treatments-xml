<taxonomicName id="1A394E28A9CD96E4F1C1AD3886FD06C5" LSID="http://zoobank.org/0437B68E-DA77-4DE4-BD6D-781FBFD38300" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus chorti" order="Hymenoptera" pageId="330" pageNumber="331" phylum="Arthropoda" rank="species" species="chorti">
<pageBreakToken id="67ABBA01920B760FCF3E7BF999A5BAB7" pageId="330" pageNumber="331" start="start">Heterospilus</pageBreakToken>
chorti Marsh
</taxonomicName>
<taxonomicNameLabel id="F26E24FA409767667A07DFA39C0E92DB" pageId="330" pageNumber="331">sp. n.</taxonomicNameLabel>
Figure 222
