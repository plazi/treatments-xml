<quantity id="8D6B9B35FF86FFEDFDB2FDB41E8AFDC3" box="[564,652,548,572]" metricMagnitude="1" metricUnit="m" metricValue="1.65" metricValueMax="3.0" metricValueMin="0.3" pageId="53" pageNumber="54" unit="m" value="16.5" valueMax="30.0" valueMin="3.0">
3– 
<specimenCount id="5C95FD59FF86FFEDFDD6FDB41E8AFDC3" box="[592,652,548,572]" pageId="53" pageNumber="54" type="generic">30 m</specimenCount>
