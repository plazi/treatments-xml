<collectingRegion id="49801AE0630DF971BB81F878FF1DF874" box="[151,243,2013,2036]" country="Panama" name="Panama" pageId="9" pageNumber="58">
<collectingCountry id="F3539492630DF971BB81F878FF1DF874" box="[151,243,2013,2036]" name="Panama" pageId="9" pageNumber="58">Panama</collectingCountry>
