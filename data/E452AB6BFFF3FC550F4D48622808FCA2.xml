<materialsCitation id="DC931020FFF2FC550DBA482F2A7AFEB3" ID-GBIF-Occurrence="2236964299" collectionCode="BMNH" country="Indonesia" location="Java" pageId="35" pageNumber="36" specimenCount="1" stateProvince="Jawa Timur" typeStatus="holotype">
