<taxonomicName id="ABFB61FEFFF2FC550FC1480B282FFE97" authorityName="Baly" authorityYear="1886" box="[710,795,303,328]" class="Insecta" family="Chrysomelidae" genus="Nacrea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="5E8FC66FFFF2FC550FC1480B282FFE97" box="[710,795,303,328]" italics="true" pageId="35" pageNumber="36">Nacrea</emphasis>
</taxonomicName>
/ 
<taxonomicName id="ABFB61FEFFF2FC550E33480B288FFE97" authorityName="Baly" authorityYear="1886" box="[820,955,303,328]" class="Insecta" family="Chrysomelidae" genus="Nacrea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="apicipennis">
<emphasis id="5E8FC66FFFF2FC550E33480B288FFE97" box="[820,955,303,328]" italics="true" pageId="35" pageNumber="36">apicipennis</emphasis>
