<taxonomicName id="4C46EABC4A069F50FF9EFD533286FD34" authority="Motschulsky, 1864" authorityName="Motschulsky" authorityYear="1864" box="[113,479,651,675]" class="Insecta" family="Carabidae" genus="Sinechostictus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="592" phylum="Arthropoda" rank="genus">
