<number id="782AE16BFF83245FFBA1F9721107F886" box="[1082,1131,1785,1811]" pageId="28" pageNumber="29" value="600.0">600</number>
&quot; / &quot; 
<typeStatus id="11594DEEFF83245FFB0EF97211BFF886" box="[1173,1235,1785,1811]" pageId="28" pageNumber="29">Type</typeStatus>
H.T.&quot; / &quot;Sandwich Is., 
<number id="782AE16BFF83245FFE21F8AA1499F8AE" box="[442,501,1825,1851]" pageId="28" pageNumber="29" value="1912.0">1912</number>
– 
<number id="782AE16BFF83245FFD9EF8AA175EF8AE" box="[517,562,1825,1851]" pageId="28" pageNumber="29" value="215.0">215</number>
&quot; (
<collectionCode id="A8F36B89FF83245FFDD1F8AA17C5F8AE" box="[586,681,1825,1851]" pageId="28" pageNumber="29">BMNH</collectionCode>
