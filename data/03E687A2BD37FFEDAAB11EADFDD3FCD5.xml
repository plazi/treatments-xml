<quantity id="4CB79B51BD37FFE8A9861FC4FBE7FE0E" box="[928,1024,436,460]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="1" pageNumber="191" unit="mm" value="3.1">3.10mm</quantity>
, HW: 
<quantity id="4CB79B51BD37FFE8AE761FC4FB48FE0E" box="[1104,1199,436,460]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="1" pageNumber="191" unit="mm" value="2.3">2.30mm</quantity>
, PW: 
<quantity id="4CB79B51BD37FFE8AEDD1FC4FABDFE0E" box="[1275,1370,436,460]" metricMagnitude="-3" metricUnit="m" metricValue="2.69" pageId="1" pageNumber="191" unit="mm" value="2.69">2.69mm</quantity>
