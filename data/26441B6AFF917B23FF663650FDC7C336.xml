<location id="AB32FCA7FF957B20FCEC3182FC68C283" LSID="urn:lsid:plazi:treatment:26441B6AFF917B23FF663650FDC7C336:AB32FCA7FF957B20FCEC3182FC68C283" box="[770,908,159,181]" country="China" county="Malipo County" latitude="23.135" longLatPrecision="21" longitude="104.85445" municipality="Xiajinchang" name="Shanggaotang" pageId="5" pageNumber="170" stateProvince="Yunnan">Shanggaotang</location>
