<taxonomicName id="62026CEBBC5A5074D28261CAA8DC2884" class="Gastropoda" family="Camaenidae" genus="Trichocathaica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trichocathaica lyonsae" order="Stylommatophora" pageId="12" pageNumber="31" phylum="Mollusca" rank="subSpecies" species="lyonsae" subSpecies="comosa">Trichocathaica lyonsae comosa</taxonomicName>
