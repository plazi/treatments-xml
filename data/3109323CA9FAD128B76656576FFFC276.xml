<taxonomicName id="8F4A7F8B7AB41DF316830F36DD05D422" authority="L." class="Liliopsida" family="Poaceae" genus="Poa" kingdom="Plantae" order="Poales" pageId="null" pageNumber="340" phylum="Tracheophyta" rank="species" species="pratensis">
