<number id="655C2484F969FFF2FB1FF9FBFB04F9E2" box="[1183,1213,1628,1652]" pageId="5" pageNumber="456" value="87.0">87</number>
, HWW 
<number id="655C2484F969FFF2FA9BF9FBFA80F9E2" box="[1307,1337,1628,1652]" pageId="5" pageNumber="456" value="17.0">17</number>
