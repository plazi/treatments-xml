, PW 
<number id="655C2484F969FFF2FD1AF9FBFD13F9E2" box="[666,682,1628,1652]" pageId="5" pageNumber="456" value="4.0">4</number>
, CL 
<number id="655C2484F969FFF2FD64F9FBFCBBF9E3" box="[740,770,1628,1653]" pageId="5" pageNumber="456" value="15.0">15</number>
, CW 
<number id="655C2484F969FFF2FCC4F9FAFCEDF9E2" box="[836,852,1629,1652]" pageId="5" pageNumber="456" value="7.0">7</number>
, FWL 
<number id="655C2484F969FFF2FC25F9FBFC68F9E3" box="[933,977,1628,1653]" pageId="5" pageNumber="456" value="135.0">135</number>
, FWW 
<number id="655C2484F969FFF2FBABF9FBFBF0F9E3" box="[1067,1097,1628,1653]" pageId="5" pageNumber="456" value="54.0">54</number>
