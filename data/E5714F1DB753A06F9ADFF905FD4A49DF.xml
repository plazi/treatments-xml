<subSubSection id="25C2AD80B753A0619ADFF8FDFDD04F1A" box="[151,585,1846,1871]" pageId="7" pageNumber="238" type="reference_group">
<paragraph id="6D67FE0BB753A0619ADFF8FDFDD04F1A" blockId="7.[151,585,1846,1871]" box="[151,585,1846,1871]" pageId="7" pageNumber="238">
<treatmentCitationGroup id="4DC8D925B753A0619ADFF8FDFDD04F1A" box="[151,585,1846,1871]" pageId="7" pageNumber="238">
<taxonomicName id="AAD88588B753A0619ADFF8FDFDDD4F1A" authority="Kudo, 1984: 494" authorityName="Kudo" authorityPageNumber="494" authorityYear="1984" box="[151,580,1846,1871]" class="Insecta" family="Thripidae" genus="Dendrothrips" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="238" phylum="Arthropoda" rank="species" species="magnoliae">
<emphasis id="5FAC2219B753A0619ADFF8FDFE094F1A" box="[151,400,1846,1870]" italics="true" pageId="7" pageNumber="238">Dendrothrips magnoliae</emphasis>
<treatmentCitation id="EC79D81AB753A0619BDEF8FDFDDD4F1A" author="Kudo, I." box="[406,580,1846,1871]" page="494" pageId="7" pageNumber="238" year="1984">
<bibRefCitation id="094983FAB753A0619BDEF8FDFD884F1A" author="Kudo, I." box="[406,529,1846,1871]" pageId="7" pageNumber="238" pagination="487 - 505" refId="ref10199" refString="Kudo, I. (1984) The Japanese Dendrothripini with descriptions of four new species (Thysanoptera, Thripidae). Kontyu, 52 (4), 487 - 505." type="journal article" year="1984">Kudô, 1984</bibRefCitation>
