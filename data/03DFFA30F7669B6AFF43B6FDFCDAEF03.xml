. 
<taxonomicName id="4C7630A5F7669B6AFEFBB6BCFC9DEE83" authority="Giorgi, 2001" authorityName="Giorgi" authorityYear="2001" box="[376,774,1757,1781]" class="Insecta" family="Cerambycidae" genus="Strioderes" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="13" phylum="Arthropoda" rank="species" species="peruanus">
