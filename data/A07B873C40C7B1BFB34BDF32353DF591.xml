<taxonomicName id="91D2E19E136F6EB49D45F3884B914753" class="Agaricomycetes" family="Polyporaceae" genus="Dendrodontia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Dendrodontia" order="Polyporales" pageId="10" pageNumber="35" phylum="Basidiomycota" rank="genus">Dendrodontia</taxonomicName>
