<taxonomicName id="2D1BD5840A55CDFEA3CE03A00CF393DA" authority="Bowerbank, 1845" authorityName="Bowerbank" authorityYear="1845" class="Demospongiae" family="Aplysinidae" genus="Verongia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Verongia cavernicola" order="Verongida" pageId="44" pageNumber="45" phylum="Porifera" rank="species" species="cavernicola">Verongia cavernicola</taxonomicName>
