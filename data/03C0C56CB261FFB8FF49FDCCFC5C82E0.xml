<taxonomicName id="4C690FF9B261FFBBFE8EF83AFDCA82F7" box="[336,531,1980,2001]" class="Insecta" family="Reduviidae" genus="Stenolemus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="598" phylum="Arthropoda" rank="species" species="alikakay">
