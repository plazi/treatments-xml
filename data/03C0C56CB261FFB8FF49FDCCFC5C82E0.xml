<taxonomicName id="4C690FF9B262FFB8FE84FB1AFDFA8197" box="[346,547,1180,1201]" class="Insecta" family="Reduviidae" genus="Stenolemus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="599" phylum="Arthropoda" rank="species" species="alikakay">
