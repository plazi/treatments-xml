57. 
<taxonomicName id="903CEFB84604CD1A6887709D748CF5C5" class="Arachnida" family="Ereynetidae" genus="Ereynetes" kingdom="Animalia" order="Prostigmata" pageId="19" pageNumber="158" phylum="Arthropoda" rank="species" species="insularis">Ereynetes insularis</taxonomicName>
