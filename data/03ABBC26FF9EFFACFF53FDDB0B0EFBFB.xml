, 
<collectingCountry id="F3154DA0FF9EFFACFC12FD0C0B20FD43" box="[982,1058,727,751]" name="Kenya" pageId="4" pageNumber="71">Kenya</collectingCountry>
, 
<collectingCountry id="F3154DA0FF9EFFACFBEAFD0D0B84FD43" box="[1070,1158,726,751]" name="Malawi" pageId="4" pageNumber="71">Malawi</collectingCountry>
