, 
<collectingCountry id="F3154DA0FF9EFFACFF53FD210FD5FCBF" box="[151,215,762,787]" name="South Africa" pageId="4" pageNumber="71">Natal</collectingCountry>
, 
<collectingCountry id="F3154DA0FF9EFFACFF25FD210E76FCBF" box="[225,372,762,787]" name="South Africa" pageId="4" pageNumber="71">South Africa</collectingCountry>
, 
