, 
<collectingCountry id="F3154DA0FF9EFFACFE2DFD210D53FCBF" box="[489,593,762,787]" name="Tanzania" pageId="4" pageNumber="71">Tanzania</collectingCountry>
, Transvaal, 
<collectingCountry id="F3154DA0FF9EFFACFD13FD210C33FCBF" box="[727,817,762,787]" name="Uganda" pageId="4" pageNumber="71">Uganda</collectingCountry>
