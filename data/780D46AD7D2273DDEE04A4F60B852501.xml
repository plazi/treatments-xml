<taxonomicName id="333957E4B7E671174821AC4C6AA79589" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia caelestis" order="Myrtales" pageId="56" pageNumber="57" phylum="Tracheophyta" rank="species" species="caelestis">Conostegia caelestis</taxonomicName>
