<taxonomicName id="1AD84D03FFCDFFE2DA1A652DFDB3FDD7" box="[283,513,596,618]" class="Rhabditophora" family="Calviriidae" genus="Paracalviria" kingdom="Animalia" order="Seriata" pageId="11" pageNumber="43" phylum="Platyhelminthes" rank="species" species="islandica">
