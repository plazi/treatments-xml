<quantity id="4CA79B5AFFE9FFB3FF37684569231709" box="[199,334,1035,1059]" metricMagnitude="-2" metricUnit="m" metricValue="2.159" metricValueMax="2.413" metricValueMin="1.905" pageId="5" pageNumber="23" unit="in" value="0.85" valueMax="0.95" valueMin="0.75">0.75–0.95 in</quantity>
