<subSubSection id="C3236575FFFAF60BFF58FBE9627A167A" box="[198,1405,1066,1092]" pageId="17" pageNumber="18" type="etymology">
<paragraph id="8B8636FEFFFAF60BFF58FBE9627A167A" blockId="17.[151,1436,266,1092]" box="[198,1405,1066,1092]" pageId="17" pageNumber="18">
<emphasis id="B94DEAECFFFAF60BFF58FBE96654167A" bold="true" box="[198,339,1066,1092]" pageId="17" pageNumber="18">Etymology.</emphasis>
