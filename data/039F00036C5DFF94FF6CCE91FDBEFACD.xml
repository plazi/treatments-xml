<taxonomicName id="4C36CA966C5AFF94FF6CCD15FE82FCE5" authorityName="Cameron" authorityYear="1904" box="[146,271,791,811]" class="Insecta" family="Figitidae" genus="Anacharoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="genus">
