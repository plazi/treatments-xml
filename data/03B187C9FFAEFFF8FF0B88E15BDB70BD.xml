<subSubSection id="C3026554FFACFFF8FF5B8E72583F71B1" pageId="3" pageNumber="273" type="etymology">
<paragraph id="8BA736DFFFACFFF8FF5B8E72583F71B1" blockId="3.[151,1437,152,935]" pageId="3" pageNumber="273">
<emphasis id="B96CEACDFFACFFF8FF5B8E72583A719D" bold="true" box="[199,338,621,647]" pageId="3" pageNumber="273">Etymology:</emphasis>
