<taxonomicName id="8ACCB1C7FFF8B37744D1FEF8CA5F0A6B" authority="Kingsley, 1878 b: 94" authorityName="Kingsley" authorityPageNumber="94" authorityYear="1878" box="[151,601,305,327]" class="Malacostraca" family="Pandalidae" genus="Pandalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="franciscorum">
