<collectingCountry id="26FFFDB1BF52FFECFDE1FE75FD8BFE17" box="[576,627,440,463]" name="Peru" pageId="25" pageNumber="26">Peru</collectingCountry>
(
<bibRefCitation id="3A79C0D0BF52FFECFD26FE7AFBA9FE08" author="Nutting" box="[647,1105,439,464]" pageId="25" pageNumber="26" refString="Nutting, C. C. (1904) American hydroids. Part II. The Sertularidae. Special Bulletin of the United States National Museum, 4 (2), 1 - 325." type="journal article" year="1904">
Nutting 
<number id="E820AF06BF52FFECFD4BFE7AFCDDFE17" box="[746,805,439,463]" pageId="25" pageNumber="26" value="1904.0">1904</number>
