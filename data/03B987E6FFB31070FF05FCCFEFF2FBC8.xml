<taxonomicName id="4C104D73FFB31070FF05FCCFEC10FCE8" box="[151,356,829,855]" class="Insecta" family="Tephritidae" genus="Tephritis" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="590" phylum="Arthropoda" rank="species" species="maccus">
<emphasis id="B964EAE2FFB31070FF05FCCFEC10FCE8" bold="true" box="[151,356,829,855]" italics="true" pageId="1" pageNumber="590">Tephritis maccus</emphasis>
