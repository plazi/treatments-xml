<taxonomicName id="BE914D02FFDC8410FC2254E0FAB18F94" baseAuthorityName="Scopoli" baseAuthorityYear="1763" box="[1021,1313,1411,1437]" class="Insecta" family="Laemobothriidae" genus="Laemobothrion" kingdom="Animalia" order="Psocodea" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="maximum">
