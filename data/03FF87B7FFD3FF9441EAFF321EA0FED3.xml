<taxonomicName id="4C564D22FFD3FF974475FBA51EBDFBF2" authorityName="Chamberlin" authorityYear="1903" box="[1288,1408,1039,1061]" class="Chilopoda" family="Henicopidae" genus="Zygethobius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lithobiomorpha" pageId="5" pageNumber="264" phylum="Arthropoda" rank="genus">Zygethobius</taxonomicName>
