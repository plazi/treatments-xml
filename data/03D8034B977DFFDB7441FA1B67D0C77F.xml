</bibRefCitation>
), 
<taxonomicName id="4C71C9DE977CFFDB75D0FF3B6711C5AF" authority="Dlussky, 1981" authorityName="Dlussky" authorityYear="1981" box="[203,559,180,204]" class="Insecta" family="Formicidae" genus="Casaleia" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="1046" phylum="Arthropoda" rank="species" species="inversa">
