), 
<taxonomicName id="4C71C9DE977CFFDB75C3FF7C6701C46C" authority="Zhang, 1989" authorityName="Zhang" authorityYear="1989" box="[216,575,241,265]" class="Insecta" family="Formicidae" genus="Myopopone" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="1046" phylum="Arthropoda" rank="species" species="sinensis">
<emphasis id="B9056E4F977CFFDB75C3FF7C648EC46D" box="[216,432,242,265]" italics="true" pageId="3" pageNumber="1046">Myopopone sinensis</emphasis>
<bibRefCitation id="EFE0CFAC977CFFDB74A3FF7F6701C46C" author="Zhang, J." bookContentInfo="459 p." box="[440,575,241,264]" journalOrPublisher="Jinan, Shandong Science and Technology Publishing House" pageId="3" pageNumber="1046" refString="Zhang, J., 1989, Fossil insects from Shangwang, Shandong, China, Jinan, Shandong Science and Technology Publishing House, 459 p." title="Fossil insects from Shangwang, Shandong, China" type="book" year="1989">
