<taxonomicName id="4C064D0819AFFF686AD2A7EF71A6F836" authority="Steenis &amp; Hippa &amp; Mutin, 2018" authorityName="Steenis &amp; Hippa &amp; Mutin" authorityYear="2018" box="[228,394,1927,1954]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="ornata" status="sp. nov." subGenus="Asiosphegina">
