<taxonomicName id="4C064D0819AFFF686952A70274CDF8EA" authority="Shiraki, 1930" authorityName="Shiraki" authorityYear="1930" box="[868,1249,1892,1919]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="nigerrima" subGenus="Asiosphegina">
