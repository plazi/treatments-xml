<taxonomicName id="89814D13FFAAFB6AFF39D93CFE8DFBB7" box="[226,370,1093,1118]" class="Arachnida" family="Deraiophoridae" genus="Dentibaiulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="111" phylum="Arthropoda" rank="genus">
