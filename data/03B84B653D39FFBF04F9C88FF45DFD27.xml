<taxonomicName id="4C1181F03D39FFBF04F9C88FF7A6FF50" authority="Jeannel, 1949" authorityName="Jeannel" authorityYear="1949" box="[151,445,221,247]" class="Insecta" family="Carabidae" genus="Afrotarus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="159" phylum="Arthropoda" rank="genus">
