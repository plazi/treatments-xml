<emphasis id="78E7EAC2FF93FFF8FEA3F8331E44F846" box="[293,578,1955,1977]" italics="true" pageId="32" pageNumber="33">
<taxonomicName id="8D934D53FF93FFF8FEA3F8331E38F846" box="[293,574,1955,1977]" class="Demospongiae" family="Placospongiidae" genus="Placospherastra" kingdom="Animalia" order="Hadromerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="antillensis">Placospherastra antillensis</taxonomicName>
