<taxonomicName id="7D0953DAFC0B21A55EDD5BA1CC65FDCA" LSID="http://zoobank.org/74A4E927-BEA1-4763-857E-20A3899F26B1" authority="Latreille, 1829" authorityName="Latreille" authorityYear="1829" class="Insecta" family="Tenebrionidae" genus="Neomida" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neomida diminuta" order="Coleoptera" pageId="2" pageNumber="135" phylum="Arthropoda" rank="species" species="diminuta">Neomida diminuta</taxonomicName>
<taxonomicNameLabel id="AB7D895806CBDE5D851EA2EC5E6E31E1" pageId="2" pageNumber="135">sp. n.</taxonomicNameLabel>
