<paragraph id="35D632009670933274FB6201A25D8863" pageId="0" pageNumber="71">
<taxonomicName id="75F826A26372B33DFE11B05474678157" authority="Scheffrahn, 2017" authorityName="Scheffrahn" authorityYear="2017" class="Insecta" family="Termitidae" genus="Disjunctitermes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Disjunctitermes insularis" order="Blattodea" pageId="0" pageNumber="71" phylum="Arthropoda" rank="species" species="insularis">Disjunctitermes insularis</taxonomicName>
