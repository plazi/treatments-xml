<number id="8924248CFF85FFCBD18A3B729A70E227" box="[1006,1068,828,854]" pageId="10" pageNumber="11" value="1984.0">1984</number>
&quot; (
<emphasis id="0D98EAB9FF85FFCBD6213B729A2DE224" box="[1093,1137,828,853]" italics="true" pageId="10" pageNumber="11">fide</emphasis>
