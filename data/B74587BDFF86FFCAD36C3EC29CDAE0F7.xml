<number id="8924248CFF85FFCBD1A53A029A5DE317" box="[961,1025,588,614]" pageId="10" pageNumber="11" value="1969.0">1969</number>
: 
<number id="8924248CFF85FFCBD66B3A029A61E317" box="[1039,1085,588,614]" pageId="10" pageNumber="11" value="334.0">334</number>
)
</bibRefCitation>
for 
<typeStatus id="E0578809FF85FFCBD6193A029A86E317" box="[1149,1242,588,614]" pageId="10" pageNumber="11">neotype</typeStatus>
in the 
<collectionCode id="59FDAE6EFF85FFCBD36C3A3A9F10E3FF" LSID="urn:lsid:biocol.org:col:33791" box="[264,332,628,654]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="10" pageNumber="11">MCZ</collectionCode>
. It is labelled &quot;PhilaNeck 5.10 Pa / Liebeck Collection/ 
<typeStatus id="E0578809FF85FFCBD1BC3A3A9A14E3FF" box="[984,1096,628,654]" pageId="10" pageNumber="11">Neotypus</typeStatus>
<taxonomicName id="F8EC4D28FF85FFCBD6343A3A9B77E3FF" authority="Say" authorityName="Say" box="[1104,1323,628,654]" class="Insecta" family="Carabidae" genus="Clivina" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="viridis">Clivina viridis Say</taxonomicName>
design. Lth. / 
<taxonomicName id="F8EC4D28FF85FFCBD3AF3AD29CB5E3C7" authority="Say" authorityName="Say" box="[459,745,668,694]" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="viridis">Ardistomis viridis Say</taxonomicName>
det. Lindroth 
<number id="8924248CFF85FFCBD1D63AD29D93E3C7" box="[946,975,668,694]" pageId="10" pageNumber="11" value="68.0">68</number>
/ M.C.Z. 
<typeStatus id="E0578809FF85FFCBD6013AD29A94E3C7" box="[1125,1224,668,694]" pageId="10" pageNumber="11">Neotype</typeStatus>
<number id="8924248CFF85FFCBD6BE3AD29B7FE3C7" box="[1242,1315,668,694]" pageId="10" pageNumber="11" value="33077.0">33077</number>
/ 
<taxonomicName id="F8EC4D28FF85FFCBD36C3A8A9C0BE3AF" authority="Say" authorityName="Say" box="[264,599,708,734]" class="Insecta" family="Carabidae" genus="Semiardistomis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="viridis">Semiardistomis viridis (Say)</taxonomicName>
