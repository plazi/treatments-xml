; 
<taxonomicName id="C1EB4D00FFB86E78FDABFDDBFCBEE0C8" authority="Huber" authorityName="Huber" box="[626,847,633,660]" class="Arachnida" family="Pholcidae" genus="Panjange" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="marilog" status="sp. nov.">
