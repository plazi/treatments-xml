<taxonomicName id="1D8896F2FFC3FFB0FF05F8D48375F8E3" authority="Kreis, 1929" authorityName="Kreis" authorityYear="1929" box="[179,532,1842,1867]" class="Chromadorea" family="Chromadoridae" genus="Deltanema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="50" pageNumber="51" phylum="Nematoda" rank="species" species="parvum">
