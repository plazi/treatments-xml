, 
<number id="573924FDE009FFCCFE9BFFE3FEB8FA00" box="[300,355,1424,1450]" pageId="3" pageNumber="67" value="6.3">6.30</number>
, 
<number id="573924FDE009FFCCFEC7FFE3FE7CFA00" box="[368,423,1424,1450]" pageId="3" pageNumber="67" value="3.13">3.13</number>
, 
<number id="573924FDE009FFCCFE03FFE3FE37FA00" box="[436,492,1424,1450]" pageId="3" pageNumber="67" value="2.43">2.43</number>
; length rostral segments I–IV = 
<number id="573924FDE009FFCCFCC7FFE3FC7CFA00" box="[880,935,1424,1450]" pageId="3" pageNumber="67" value="0.64">0.64</number>
