)— “ 
<collectingCountry id="F33C762E0C7D1E3BFF07FF39910D1457" box="[208,330,252,276]" name="Vietnam" pageId="5" pageNumber="257">VIETNAM</collectingCountry>
, Co Ma, Thuan Chau, 
<collectingRegion id="49EFF85C0C7D1E3BFDF8FF39923D1457" box="[559,634,252,276]" country="Vietnam" name="Son La" pageId="5" pageNumber="257">Son La</collectingRegion>
, 
<quantity id="4CD39B5B0C7D1E3BFD53FF3992951457" box="[644,722,252,277]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="257" unit="m" value="1600.0">1600 m</quantity>
