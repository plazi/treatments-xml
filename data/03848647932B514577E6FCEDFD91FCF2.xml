<collectionCode id="ED3CAF94932C514476BBF951FEC4F946" box="[219,328,1675,1699]" country="0" httpUri="http://grbio.org/cool/bmqw-2x87" name="Museo Nacional de Historia Natural del Paraguay" pageId="4" pageNumber="2581">MNHNP</collectionCode>
