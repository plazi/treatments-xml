<collectionCode id="8E82FAFEFFBAFFEEDBFFFD170D28C981" box="[237,307,741,762]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34784" lsid="urn:lsid:biocol.org:col:34784" name="Academy of Natural Sciences of Philadelphia" pageId="22" pageNumber="97">ANSP</collectionCode>
<accessionNumber id="F7C0FFD8FFBAFFEEDA28FD170D8FC981" box="[314,404,741,762]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/A19744" pageId="22" pageNumber="97">A19744</accessionNumber>
