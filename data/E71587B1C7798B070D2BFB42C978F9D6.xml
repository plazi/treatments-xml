<quantity id="A8449B42C77B8B060E39FEB7CBF4FE9C" box="[342,409,300,327]" metricMagnitude="-2" metricUnit="m" metricValue="8.89" pageId="18" pageNumber="77" unit="in" value="3.5">3.5 in</quantity>
<taxonomicName id="A8BC4D24C77B8B060ECFFEB6C830FE9C" authorityName="Timm" authorityYear="1961" box="[416,605,301,327]" class="Adenophorea" family="Choniolaimidae" genus="Halichoanolaimus" kingdom="Animalia" order="Desmodorida" pageId="18" pageNumber="77" phylum="Nematoda" rank="species" species="minutissimus">
<emphasis id="5DC8EAB5C77B8B060ECFFEB6CBD8FE9C" box="[416,437,301,326]" italics="true" pageId="18" pageNumber="77">H</emphasis>
. 
<emphasis id="5DC8EAB5C77B8B060EABFEB6C830FE9C" box="[452,605,301,326]" italics="true" pageId="18" pageNumber="77">minutissimus</emphasis>
