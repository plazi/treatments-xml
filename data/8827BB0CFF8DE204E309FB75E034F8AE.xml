<taxonomicName id="C78E7199FF8DE205E309FB75E051FB58" authority="Kimura &amp; Flynn &amp; Jacobs, 2017" authorityName="Kimura &amp; Flynn &amp; Jacobs" authorityYear="2017" box="[268,536,1232,1256]" class="Mammalia" family="Muridae" genus="Parapodemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="205" phylum="Chordata" rank="species" species="badgleyae" status="sp. nov.">
<emphasis id="32FAD608FF8DE205E309FB75E051FB58" bold="true" box="[268,536,1232,1256]" italics="true" pageId="8" pageNumber="205">Parapodemus badgleyae</emphasis>
