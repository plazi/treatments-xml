<collectionCode id="ED2A7EDEFFBD4261FE0AAC1B8164FAE7" box="[472,558,1306,1332]" country="Canada" httpUri="http://grbio.org/cool/yhs7-n61d" name="Ontario Insect Collection, University of Guelph" pageId="26" pageNumber="27">DEBU</collectionCode>
): 
<collectingCountry id="F32CA68BFFBD4261FD93AC1B8199FAE7" box="[577,723,1306,1332]" name="Ecuador" pageId="26" pageNumber="27">ECUADOR</collectingCountry>
: 
<collectingRegion id="49FF28F9FFBD4261FD0CAC1B806BFAE7" box="[734,801,1306,1332]" country="Ecuador" name="Napo" pageId="26" pageNumber="27">Napo</collectingRegion>
