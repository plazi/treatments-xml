<location id="8EE4B0C0FFBD4261FCFEAC1B803CFAE7" LSID="urn:lsid:plazi:treatment:0392570DFFBE4261FEDAAC7A8799FA57:8EE4B0C0FFBD4261FCFEAC1B803CFAE7" box="[812,886,1306,1332]" country="Ecuador" name="Baeza" pageId="26" pageNumber="27" stateProvince="Napo">Baeza</location>
