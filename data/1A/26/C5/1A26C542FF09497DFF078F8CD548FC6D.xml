(
<taxonomicName box="[486,578,874,895]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="6" pageNumber="744" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
