(
<taxonomicName box="[158,263,821,842]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="6" pageNumber="744" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
