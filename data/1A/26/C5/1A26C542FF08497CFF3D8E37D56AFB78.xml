<quantity box="[179,299,959,981]" metricMagnitude="-2" metricUnit="m" metricValue="1.125" metricValueMax="1.5" metricValueMin="0.75" pageId="7" pageNumber="745" unit="cm" value="1.125" valueMax="1.5" valueMin="0.75">0.75â€“1.5 cm</quantity>
