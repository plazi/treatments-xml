<paragraph blockId="1.[151,1108,1120,1142]" box="[151,1108,1120,1142]" pageId="1" pageNumber="132">
<typeStatus box="[151,201,1121,1142]" pageId="1" pageNumber="132">Type</typeStatus>
species: 
<taxonomicName authority="Kuznetzov, 1985" authorityName="Kuznetzov" authorityYear="1985" box="[294,706,1120,1142]" class="Insecta" family="Tortricidae" genus="Metacosma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="132" phylum="Arthropoda" rank="species" species="impolitana">
<emphasis box="[294,527,1120,1142]" italics="true" pageId="1" pageNumber="132">Metacosma impolitana</emphasis>
<bibRefCitation author="Kuznetzov, V. I." box="[533,706,1120,1142]" pageId="1" pageNumber="132" pagination="3 - 11" refId="ref4543" refString="Kuznetzov, V. I. (1985) New representatives of the tribe Eucosmini (Lepidoptera, Tortricidae) of the USSR Asiatic part. Festnik Zoologii, 1985 (1), 3 - 11. [in Russian]" type="journal article" year="1985">Kuznetzov, 1985</bibRefCitation>
</taxonomicName>
