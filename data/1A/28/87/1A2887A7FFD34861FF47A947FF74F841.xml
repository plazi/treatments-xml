<collectionCode LSID="urn:lsid:biocol.org:col:33791" box="[483,547,1898,1922]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="2" pageNumber="65">MCZ</collectionCode>
