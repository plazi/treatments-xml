Aug.-Dec. 
<number box="[701,757,1934,1958]" pageId="2" pageNumber="65" value="2004.0">2004</number>
</date>
/ 
<collectionCode LSID="urn:lsid:biocol.org:col:33791" box="[784,845,1934,1958]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="2" pageNumber="65">MCZ</collectionCode>
