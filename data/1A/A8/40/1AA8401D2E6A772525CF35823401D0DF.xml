<subSubSection pageId="0" pageNumber="1" type="distribution">
<paragraph pageId="0" pageNumber="1">Distribution.</paragraph>
<paragraph pageId="0" pageNumber="1">Indonesia (Sumatra), East &amp; West Malaysia, Thailand, Vietnam.</paragraph>
</subSubSection>
