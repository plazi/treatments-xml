<subSubSection pageId="33" pageNumber="350" type="discussion">
<paragraph blockId="33.[1243,2098,909,1365]" pageId="33" pageNumber="350">
<taxonomicName box="[1284,1439,957,992]" class="Reptilia" family="Dermatemydidae" genus="Basilemys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Testudines" pageId="33" pageNumber="350" phylum="Chordata" rank="genus">
