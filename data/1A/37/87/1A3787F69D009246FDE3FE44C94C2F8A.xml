<quantity box="[686,820,1854,1881]" metricMagnitude="2" metricUnit="m" metricValue="3.045" metricValueMax="3.59" metricValueMin="2.5" pageId="29" pageNumber="30" unit="m" value="304.5" valueMax="359.0" valueMin="250.0">250–359 m</quantity>
