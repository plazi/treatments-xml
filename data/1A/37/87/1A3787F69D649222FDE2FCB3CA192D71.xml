<taxonomicName authorityName="Telnov" authorityYear="2020" box="[718,809,1146,1172]" class="Insecta" family="Scarabaeidae" genus="Maechidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="120" pageNumber="121" phylum="Arthropoda" rank="species" species="trivialis">
