<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[314,436,1361,1387]" class="Magnoliopsida" family="Nepenthaceae" genus="Nepenthes" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="84" pageNumber="85" phylum="Tracheophyta" rank="genus">
