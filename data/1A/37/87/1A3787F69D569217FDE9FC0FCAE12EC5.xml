<typeStatus box="[449,555,1717,1743]" pageId="74" pageNumber="75">holotype</typeStatus>
, total body length 
<quantity box="[777,886,1716,1743]" metricMagnitude="-3" metricUnit="m" metricValue="6.35" pageId="74" pageNumber="75" unit="mm" value="6.35">6.35 mm</quantity>
. Head 
<quantity box="[969,1075,1716,1743]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="74" pageNumber="75" unit="mm" value="1.05">1.05 mm</quantity>
