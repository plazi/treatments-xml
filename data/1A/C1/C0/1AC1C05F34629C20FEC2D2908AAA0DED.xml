<taxonomicName class="Arachnida" family="Suctobelbidae" genus="Suctobelba" genus.bestMatchDistance="229" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="9" pageNumber="84" phylum="Arthropoda" rank="species" species="baloghi">S. baloghi</taxonomicName>
