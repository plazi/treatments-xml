<quantity box="[959,1079,1325,1352]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" metricValueMax="1.4" metricValueMin="1.3" pageId="3" pageNumber="250" unit="mm" value="13.5" valueMax="14.0" valueMin="13.0">13-14 mm</quantity>
.
</paragraph>
<paragraph blockId="3.[677,1210,1388,1700]" box="[677,916,1388,1414]" pageId="3" pageNumber="250">
<emphasis box="[677,916,1388,1414]" italics="true" pageId="3" pageNumber="250">
