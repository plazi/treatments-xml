<taxonomicName authority="Tsai &amp; Li" authorityName="Tsai &amp; Li" box="[264,702,1423,1449]" class="Insecta" family="Curculionidae" genus="Dendroctonus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="armandi">
<emphasis box="[264,548,1423,1448]" italics="true" pageId="13" pageNumber="14">Dendroctonus armandi</emphasis>
Tsai &amp; Li
</taxonomicName>
, 
<taxonomicName box="[732,768,1423,1448]" class="Insecta" family="Curculionidae" genus="Ips" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
<emphasis box="[732,768,1423,1448]" italics="true" pageId="13" pageNumber="14">Ips</emphasis>
