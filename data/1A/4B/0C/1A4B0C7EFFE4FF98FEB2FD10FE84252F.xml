<emphasis box="[735,974,1343,1368]" italics="true" pageId="13" pageNumber="14">Scolytus butovitschi</emphasis>
Stark
</taxonomicName>
and 
<taxonomicName authority="Blandford" authorityName="Blandford" class="Insecta" family="Curculionidae" genus="Scolytus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="esuriens">
<emphasis box="[1115,1323,1343,1368]" italics="true" pageId="13" pageNumber="14">Scolytus esuriens</emphasis>
Blandford
</taxonomicName>
of family the 
<taxonomicName box="[551,675,1383,1409]" class="Insecta" family="Curculionidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="family">Scolytidae</taxonomicName>
