<emphasis box="[716,991,484,509]" italics="true" pageId="13" pageNumber="14">Dendroctonus armandi</emphasis>
Tsai et Li
</taxonomicName>
, 
<taxonomicName authority="Gyllenhal" authorityName="Gyllenhal" class="Insecta" family="Curculionidae" genus="Ips" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="acuminatus">
<emphasis box="[1140,1323,484,509]" italics="true" pageId="13" pageNumber="14">Ips acuminatus</emphasis>
Gyllenhal
</taxonomicName>
, 
<taxonomicName authority="Hartig" authorityName="Hartig" box="[408,692,524,550]" class="Insecta" family="Curculionidae" genus="Tomicus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="minor">
<emphasis box="[408,596,524,549]" italics="true" pageId="13" pageNumber="14">Tomicus minor</emphasis>
Hartig
</taxonomicName>
and 
<taxonomicName box="[776,932,524,549]" class="Insecta" family="Curculionidae" genus="Blastophagus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="piniperda">
<emphasis box="[776,932,524,549]" italics="true" pageId="13" pageNumber="14">T. piniperda</emphasis>
</taxonomicName>
L. of the family 
<taxonomicName box="[1199,1323,524,550]" class="Insecta" family="Curculionidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="family">Scolytidae</taxonomicName>
(
<taxonomicName box="[272,408,564,590]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
).
