<taxonomicName authority="(Linnaeus)" authorityName="Linnaeus" baseAuthorityName="Linnaeus" baseAuthorityYear="1766" box="[531,909,1492,1517]" class="Aves" family="Hirundinidae" genus="Cecropis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="senegalensis">
