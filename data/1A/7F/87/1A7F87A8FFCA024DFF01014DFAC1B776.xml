<caption box="[151,914,1597,1619]" httpUri="https://zenodo.org/record/2599155/files/figure.png" pageId="29" pageNumber="30" startId="29.[151,250,1597,1619]" targetBox="[161,1425,250,1542]" targetPageId="29">
<paragraph blockId="29.[151,914,1597,1619]" box="[151,914,1597,1619]" pageId="29" pageNumber="30">
<emphasis bold="true" box="[151,280,1597,1619]" pageId="29" pageNumber="30">FIGURE 18</emphasis>
. 
<taxonomicName authorityName="Gaud and Atyeo" authorityYear="1987" box="[293,494,1598,1619]" class="Arachnida" family="Trouessartiidae" genus="Trouessartia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="quarta">
