<emphasis italics="true" pageId="24" pageNumber="25">Camposipterus nasutus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Rodrigues &amp; Kellner" authorityYear="2013" baseAuthorityName="Owen" baseAuthorityYear="1859" class="Reptilia" family="Not" genus="Camposipterus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="24" pageNumber="25" phylum="Chordata" rank="species" species="sedgwickii">
<emphasis italics="true" pageId="24" pageNumber="25">Camposipterus(?) sedgwickii</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Rodrigues &amp; Kellner" authorityYear="2013" baseAuthorityName="Seeley" baseAuthorityYear="1870" class="Reptilia" family="Not" genus="Camposipterus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="24" pageNumber="25" phylum="Chordata" rank="species" species="colorhinus">
