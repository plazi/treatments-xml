<paragraph blockId="9.[189,1398,450,547]" pageId="9" pageNumber="10">
<taxonomicName authority="Vitiello, 1972" authorityName="Vitiello" authorityYear="1972" box="[189,610,450,476]" class="Chromadorea" family="Diplopeltidae" genus="Diplopeltula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araeolaimida" pageId="9" pageNumber="10" phylum="Nematoda" rank="species" species="minuta">
<emphasis box="[189,430,450,476]" italics="true" pageId="9" pageNumber="10">Diplopeltula minuta</emphasis>
<bibRefCitation author="Vitiello P." box="[443,610,450,476]" pageId="9" pageNumber="10" pagination="535 - 544" refId="ref9787" refString="Vitiello P. 1972. Sur quelques especes de Diplopeltula (Nematoda, Araeolaimida). Vie et Milieu 21: 535 - 544." type="journal article" year="1972">Vitiello, 1972</bibRefCitation>
</taxonomicName>
is here transferred to the genus 
<taxonomicName authorityName="Leduc" authorityYear="2013" box="[1020,1165,450,476]" class="Chromadorea" family="Diplopeltidae" genus="Mudwigglus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araeolaimida" pageId="9" pageNumber="10" phylum="Nematoda" rank="genus">
<emphasis box="[1020,1165,450,476]" italics="true" pageId="9" pageNumber="10">Mudwigglus</emphasis>
</taxonomicName>
