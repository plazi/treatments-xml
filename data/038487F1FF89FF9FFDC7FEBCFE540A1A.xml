<subSubSection id="C337656CFF89FF9FFD97FB8DFEE308FB" pageId="44" pageNumber="45" type="etymology">
<paragraph id="8B9236E7FF89FF9FFD97FB8DFEE308FB" blockId="44.[151,1437,258,1795]" pageId="44" pageNumber="45">
<emphasis id="B959EAF5FF89FF9FFD97FB8DFDA208A6" box="[199,613,1446,1471]" italics="true" pageId="44" pageNumber="45">
<emphasis id="B959EAF5FF89FF9FFD97FB8DFE8008A7" bold="true" box="[199,327,1446,1471]" italics="true" pageId="44" pageNumber="45">Etymology.</emphasis>
