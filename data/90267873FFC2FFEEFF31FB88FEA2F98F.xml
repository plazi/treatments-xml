<typeStatus id="C73477C7FFC2FFEEFE73F9F6FDD9F9AA" box="[446,538,1538,1557]" pageId="58" pageNumber="58">holotype</typeStatus>
of 
<taxonomicName id="DF8FB2E6FFC2FFEEFDF0F9F7FF25F98F" authorityName="Thomas" authorityYear="1896" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="58" phylum="Chordata" rank="species" species="coelestis">
