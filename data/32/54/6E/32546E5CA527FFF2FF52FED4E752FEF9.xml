<quantity box="[842,927,1783,1808]" metricMagnitude="0" metricUnit="m" metricValue="8.0" metricValueMax="15.0" metricValueMin="1.0" pageId="22" pageNumber="103" unit="m" value="8.0" valueMax="15.0" valueMin="1.0">1–15 m</quantity>
