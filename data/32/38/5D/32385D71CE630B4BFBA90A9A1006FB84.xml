<td box="[1141,1205,264,287]" gridcol="1" gridrow="1" pageId="2" pageNumber="271">23.4</td>
<td box="[1141,1205,1628,1651]" gridcol="1" gridrow="32" pageId="2" pageNumber="271">43.3</td>
