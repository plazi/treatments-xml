<taxonomicName baseAuthorityName="Lin" baseAuthorityYear="1964" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia maai" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="maai">
<emphasis italics="true" pageId="7" pageNumber="8">Loboscelidia maai</emphasis>
</taxonomicName>
