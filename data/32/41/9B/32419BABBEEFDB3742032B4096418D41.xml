<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:7C17EB03-9625-4209-BC44-409FDDB50BAC" class="Actinopterygii" family="Chilodontidae" genus="Caenotropus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Caenotropus Günther 1864:297" order="Characiformes" pageNumber="59" phylum="Chordata" rank="genus">Caenotropus Guenther</taxonomicName>
