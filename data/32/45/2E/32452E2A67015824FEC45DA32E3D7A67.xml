<taxonomicName authority="Novak, 2007" authorityName="Novak" authorityYear="2007" box="[888,1126,475,500]" class="Insecta" family="Tenebrionidae" genus="Socotralia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="335" phylum="Arthropoda" rank="species" species="major">
