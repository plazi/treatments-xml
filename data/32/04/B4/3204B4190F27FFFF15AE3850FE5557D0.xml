<typeStatus box="[675,783,1376,1402]" pageId="3" pageNumber="48">syntypes</typeStatus>
(
<collectionCode box="[800,890,1376,1402]" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="3" pageNumber="48">SMNH</collectionCode>
