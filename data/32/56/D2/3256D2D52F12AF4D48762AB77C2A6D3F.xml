<taxonomicName authorityName="Delile" authorityYear="1843" class="Magnoliopsida" family="Rutaceae" genus="Teclea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Sapindales" pageId="9" pageNumber="50" phylum="Spermatophyta" rank="genus">
