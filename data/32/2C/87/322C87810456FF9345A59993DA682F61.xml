; 
<taxonomicName authority="(Thiele, 1899)" baseAuthorityName="Thiele" baseAuthorityYear="1899" box="[980,1431,1698,1724]" class="Demospongiae" family="Callyspongiidae" genus="Callyspongia" kingdom="Animalia" order="Haplosclerida" pageId="5" pageNumber="353" phylum="Porifera" rank="species" species="elegans">
<emphasis box="[980,1240,1699,1724]" italics="true" pageId="5" pageNumber="353">Callyspongia elegans</emphasis>
(
<bibRefCitation author="Thiele" box="[1262,1422,1698,1724]" firstAuthor="Thiele" pageId="5" pageNumber="353" pagination="1 - 33" refId="ref11554" refString="Thiele, J. (1899) Studien uber pazifische Spongien. II. Ueber einige Spongien von Celebes. Zoologica. Original- Abhandlungen aus dem Gesamtgebiete der Zoologie. Stuttgart, 24 (2), 1 - 33." type="journal article" year="1899">Thiele, 1899</bibRefCitation>
