(males) (n = 
<number box="[588,604,1657,1682]" pageId="12" value="5.0">5</number>
); 
<quantity box="[626,747,1657,1682]" metricMagnitude="-2" metricUnit="m" metricValue="5.75" metricValueMax="5.8" metricValueMin="5.7" pageId="12" unit="mm" value="57.5" valueMax="58.0" valueMin="57.0">57–58 mm</quantity>
(females) (n = 
<number box="[919,935,1658,1682]" pageId="12" value="3.0">3</number>
