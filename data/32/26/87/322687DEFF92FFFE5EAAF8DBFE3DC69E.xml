<accessionNumber box="[151,280,224,248]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KM370009" pageId="8" pageNumber="9">
KM 
<number box="[196,280,224,248]" pageId="8" pageNumber="9" value="370009.0">370009</number>
</accessionNumber>
and 
<accessionNumber box="[334,467,223,248]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KM369997" pageId="8" pageNumber="9">
KM 
<number box="[381,467,223,248]" pageId="8" pageNumber="9" value="369997.0">369997</number>
