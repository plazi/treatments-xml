<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29948" class="Insecta" family="Formicidae" genus="Leptogenys" kingdom="Animalia" lsidName-HNS="Leptogenys falcigera Roger" order="Hymenoptera" pageNumber="109" phylum="Arthropoda" rank="species" species="falcigera">LEPTOGENYS FALCIGERA</taxonomicName>
