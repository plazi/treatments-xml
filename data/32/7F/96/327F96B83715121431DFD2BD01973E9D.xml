<subSubSection pageId="null" pageNumber="356" type="nomenclature">
<paragraph pageId="null" pageNumber="356">
<taxonomicName authority="Pollich" class="Liliopsida" family="Poaceae" genus="Festuca" kingdom="Plantae" order="Poales" pageId="null" pageNumber="356" phylum="Tracheophyta" rank="species" species="duriuscula">
