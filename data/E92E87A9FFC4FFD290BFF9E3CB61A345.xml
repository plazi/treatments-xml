<taxonomicName id="A6874D3CFFC4FFD293F4F956CC0AA30D" authorityName="Oberthur" authorityYear="1912" box="[988,1146,1785,1811]" class="Insecta" family="Geometridae" genus="Zamarada" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="313" phylum="Arthropoda" rank="species" species="melpomene">
