<quantity id="4CB99B455C10EB11FD6FFD58FCF4D9A3" box="[736,840,692,717]" metricMagnitude="-3" metricUnit="m" metricValue="2.26" pageId="3" pageNumber="88" unit="mm" value="2.26">2.26 mm</quantity>
