<taxonomicName id="4C58CEDBFFA39D55FF61CB40FDC9F908" authority="Perret, Revue" authorityName="Perret, Revue" authorityYear="1976" box="[208,565,1644,1666]" class="Reptilia" family="Gekkonidae" genus="Garzoniella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="373" phylum="Chordata" rank="species" species="longipes">
