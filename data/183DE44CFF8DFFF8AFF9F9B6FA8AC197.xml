<emphasis id="A2E08948FF8EFFF8AFF9FA40FEFCC2C8" bold="true" box="[192,345,1461,1488]" pageId="14" pageNumber="13">
<typeStatus id="4F2FEBF8FF8EFFF8AFF9FA40FF5CC2D7" box="[192,249,1462,1488]" pageId="14" pageNumber="13">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="57942ED9FF8EFFF8AE66FA40FD87C2D7" authorityName="Zhang &amp; Cong &amp; Shen &amp; Grishin" authorityYear="2022" box="[351,546,1462,1488]" class="Insecta" family="Hesperiidae" genus="Ornilius" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="13" phylum="Arthropoda" rank="species" species="rotundus">
