<taxonomicName id="4C764D081864FFE5FEBEFE3FBB3FFE55" authority="Simon, 1900" authorityName="Simon" authorityYear="1900" box="[264,612,422,448]" class="Arachnida" family="Salticidae" genus="Marengo" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="27" phylum="Arthropoda" rank="species" species="nitida">
<emphasis id="B902EA991864FFE5FEBEFE3FBB3FFE55" bold="true" box="[264,612,422,448]" pageId="2" pageNumber="27">
<emphasis id="B902EA991864FFE5FEBEFE3FB89FFE55" bold="true" box="[264,452,422,448]" italics="true" pageId="2" pageNumber="27">Marengo nitida</emphasis>
Simon, 
<number id="3DBE24AC1864FFE5FD9CFE3FBB3FFE55" box="[554,612,422,448]" pageId="2" pageNumber="27" value="1900.0">1900</number>
</emphasis>
