<taxonomicName id="11E74D4FFD622E5FFC2D3910FB8F5429" baseAuthorityName="Lobl" baseAuthorityYear="1972" box="[903,1064,1033,1061]" class="Insecta" family="Staphylinidae" genus="Baeocera" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="268" phylum="Arthropoda" rank="species" species="boettcheri">
<emphasis id="E493EADEFD622E5FFC2D3910FC3E5429" box="[903,921,1033,1061]" italics="true" pageId="5" pageNumber="268">B</emphasis>
. 
<emphasis id="E493EADEFD622E5FFC003910FB8F5429" box="[938,1064,1033,1061]" italics="true" pageId="5" pageNumber="268">boettcheri</emphasis>
</taxonomicName>
