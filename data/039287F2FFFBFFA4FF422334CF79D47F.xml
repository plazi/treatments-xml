<subSubSection id="C321656FFFFBFFAAFF4223DEC809D099" pageId="39" pageNumber="40" type="diagnosis">
<paragraph id="8B8436E4FFFBFFAAFF4223DEC809D099" blockId="39.[151,1437,1470,2035]" pageId="39" pageNumber="40">
<emphasis id="B94FEAF6FFFBFFAAFF4223DECA79D0B4" bold="true" box="[151,266,1470,1495]" pageId="39" pageNumber="40">Diagnosis</emphasis>
