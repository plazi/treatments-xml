<materialsCitation id="03B6BA5FA52AFFDE2FDEFD87FEA5FD05" ID-GBIF-Occurrence="3034558341" collectingDate="1874-04" country="China" location="Information" pageId="17" pageNumber="73" specimenCount="1" specimenCount-female="1" stateProvince="Xinjiang Uygur" typeStatus="holotype">
