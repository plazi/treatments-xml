<taxonomicName id="0DF0CA456D832999FAC49BE999059106" authority="Vosmaer, 1883" authorityName="Vosmaer" authorityYear="1883" class="Demospongiae" family="Dictyodendrillidae" genus="Velinea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Velinea gracilis" order="Dendroceratida" pageId="10" pageNumber="11" phylum="Porifera" rank="species" species="gracilis">Velinea gracilis</taxonomicName>
