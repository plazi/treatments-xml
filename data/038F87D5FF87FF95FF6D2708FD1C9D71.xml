<subSubSection id="C33C6548FF87FF95FF6D2496FBC59CEA" box="[151,1097,1665,1692]" pageId="8" pageNumber="9" type="distribution">
<paragraph id="8B9936C3FF87FF95FF6D2496FBC59CEA" blockId="8.[151,1437,1665,1800]" box="[151,1097,1665,1692]" pageId="8" pageNumber="9">
<emphasis id="B952EAD1FF87FF95FF6D2496FEBD9CED" bold="true" box="[151,305,1665,1691]" pageId="8" pageNumber="9">Distribution.</emphasis>
<collectingCountry id="F3317653FF87FF95FECD2496FE0A9CED" box="[311,390,1665,1691]" name="Russia" pageId="8" pageNumber="9">Russia</collectingCountry>
: Siberia (Republic of 
<collectingRegion id="49E2F821FF87FF95FD802495FD389CEA" box="[634,692,1666,1692]" country="Russia" name="Tyva" pageId="8" pageNumber="9">Tyva</collectingRegion>
