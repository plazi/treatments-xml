<paragraph id="8BCF0A6E947BFFE7FDDA2A24BB46F960" blockId="46.[602,1383,1619,1680]" box="[602,1383,1637,1674]" pageId="46" pageNumber="467">
<heading id="D087BD02947BFFE7FDDA2A24BB46F960" box="[602,1383,1637,1674]" centered="true" fontSize="8" level="2" pageId="46" pageNumber="467" reason="2">
<taxonomicName id="4C7071ED947BFFE7FDDA2A24BA90F963" authority="Topsent, 1896" authorityName="Topsent" authorityYear="1896" box="[602,1201,1637,1673]" class="Demospongiae" family="Phloeodictyidae" genus="Oceanapia" kingdom="Animalia" order="Haplosclerida" pageId="46" pageNumber="467" phylum="Porifera" rank="species" species="amboinensis" status="n. sp.">
<emphasis id="B904D67C947BFFE7FDDA2A24BA90F963" box="[602,1201,1637,1673]" italics="true" pageId="46" pageNumber="467">Oceanapia amboinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2376B07947BFFE7FB542A2CBB46F960" box="[1236,1383,1645,1674]" pageId="46" pageNumber="467" rank="species">n. sp.</taxonomicNameLabel>
