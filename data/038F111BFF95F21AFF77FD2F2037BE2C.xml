<taxonomicName id="4C26DB8EFF95F21AFF77FBFE2018BE2C" box="[136,264,1111,1135]" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" kingdom="Plantae" order="Solanales" pageId="18" pageNumber="173" phylum="Tracheophyta" rank="genus">
