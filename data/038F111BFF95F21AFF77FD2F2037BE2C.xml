<taxonomicName id="4C26DB8EFF95F21AFF55FC7F21F7B9AD" box="[170,231,982,1006]" class="Insecta" family="Lampyridae" genus="Auge" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="187" phylum="Arthropoda" rank="genus">Auge</taxonomicName>
