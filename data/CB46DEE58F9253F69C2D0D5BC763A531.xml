<paragraph id="FE3FBCED59023D53FB37B43977EA966D" pageId="0" pageNumber="39">
<taxonomicName id="579CAC5440D6BCC0541B642775CFAA7D" genus="C." lsidName="C. improvisella" pageId="0" pageNumber="39" rank="species" species="improvisella">
<emphasis id="DCA643640FDB27C4C634F7B2063CB2D1" italics="true" pageId="0" pageNumber="39">C. improvisella</emphasis>
</taxonomicName>
