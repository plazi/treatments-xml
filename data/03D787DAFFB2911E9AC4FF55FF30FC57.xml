<treatmentCitation id="0ADF10DDFFB2911E9B67FF1CFE0CFF3D" author="Hood" box="[308,482,222,244]" page="431" pageId="7" pageNumber="332" year="1934">
<bibRefCitation id="EFEF4B3DFFB2911E9B67FF1CFE41FF3D" author="Hood" box="[308,431,222,244]" pageId="7" pageNumber="332" refString="Hood, J. D. (1934) New Thysanoptera from Panama. Journal of the New York Entomological Society, 41, 407 - 434." type="journal article" year="1934">
Hood, 
<number id="3DB624EBFFB2911E9B2AFF1CFE41FF3D" box="[377,431,222,244]" pageId="7" pageNumber="332" value="1934.0">1934</number>
</bibRefCitation>
