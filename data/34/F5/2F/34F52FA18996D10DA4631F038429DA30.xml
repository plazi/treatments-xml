<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181992" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mamore Mann" lsidName-HNS="Pheidole mamore Mann" order="Hymenoptera" pageNumber="659" phylum="Arthropoda" rank="species" species="mamore">mamore</taxonomicName>
