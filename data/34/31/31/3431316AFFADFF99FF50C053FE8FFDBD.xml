<caption httpUri="https://zenodo.org/record/187498/files/figure.png" pageId="29" pageNumber="30" targetBox="[264,1290,190,1493]" targetPageId="29">
<paragraph blockId="29.[151,1436,1494,1656]" pageId="29" pageNumber="30">
<emphasis bold="true" box="[151,355,1494,1518]" pageId="29" pageNumber="30">FIGURES 80–89.</emphasis>
<taxonomicName authority="Tonnoir." authorityName="Tonnoir." box="[363,661,1494,1518]" class="Insecta" family="Mycetophilidae" genus="Cawthronia" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="nigra">
<emphasis box="[363,561,1495,1518]" italics="true" pageId="29" pageNumber="30">Cawthronia nigra</emphasis>
