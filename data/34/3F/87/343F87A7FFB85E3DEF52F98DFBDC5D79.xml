<caption httpUri="https://zenodo.org/record/1211311/files/figure.png" pageId="6" pageNumber="7" startId="6.[189,232,1929,1955]" targetBox="[195,1394,885,1897]" targetPageId="6">
<paragraph blockId="6.[189,1399,1929,2026]" pageId="6" pageNumber="7">
