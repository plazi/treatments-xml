<subScript attach="left" box="[778,906,613,625]" fontSize="5" pageId="2" pageNumber="387">------------------------</subScript>
<taxonomicName authority="Bates" authorityName="Bates" authorityYear="1888" box="[926,1110,604,625]" class="Insecta" family="Rutelidae" genus="Platyrutela" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="387" phylum="Arthropoda" rank="genus">
