; 
<taxonomicName authority="Borojevic, 1966" authorityName="Borojevic" authorityYear="1966" box="[551,886,1665,1691]" class="Calcarea" family="Leucaltidae" genus="Ascandra" kingdom="Animalia" order="Clathrinida" pageId="10" pageNumber="11" phylum="Porifera" rank="species" species="minchini">
