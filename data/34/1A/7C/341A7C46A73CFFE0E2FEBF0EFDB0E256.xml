<typeStatus box="[303,407,1700,1724]" pageId="2" pageNumber="3">Holotype</typeStatus>
FEMALE. Fore wing 
<quantity box="[661,764,1700,1724]" metricMagnitude="-3" metricUnit="m" metricValue="9.35" pageId="2" pageNumber="3" unit="mm" value="9.35">9.35 mm</quantity>
