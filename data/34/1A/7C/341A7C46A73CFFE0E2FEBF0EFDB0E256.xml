<typeStatus box="[440,544,932,956]" pageId="4" pageNumber="5">Holotype</typeStatus>
Ƥ from 
<collectingCountry box="[641,780,932,956]" name="Ecuador" pageId="4" pageNumber="5">ECUADOR</collectingCountry>
, MorroMorro near 
<collectingCountry box="[997,1060,932,956]" name="Philippines" pageId="4" pageNumber="5">Piñas</collectingCountry>
, 
<quantity box="[1072,1159,932,956]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="4" pageNumber="5" unit="m" value="1500.0">1500 m</quantity>
, 
<date box="[1171,1318,932,956]" pageId="4" pageNumber="5" value="1941-08-22">
<number box="[1171,1201,932,956]" pageId="4" pageNumber="5" value="22.0">22</number>
.VIII. 
<number box="[1261,1318,932,956]" pageId="4" pageNumber="5" value="1941.0">1941</number>
