<emphasis bold="true" box="[198,339,231,257]" pageId="6" pageNumber="49">Etymology.</emphasis>
<taxonomicName authorityName="Gil-Santana &amp; Alves &amp; Barrett &amp; Costa" authorityYear="2007" box="[350,618,232,257]" class="Insecta" family="Reduviidae" genus="Phasmatocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="xavieri">
<emphasis box="[350,618,232,257]" italics="true" pageId="6" pageNumber="49">Phasmatocoris xavieri</emphasis>
