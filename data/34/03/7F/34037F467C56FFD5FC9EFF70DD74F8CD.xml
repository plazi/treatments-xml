<emphasis italics="true" pageId="9" pageNumber="148">C. eperiamorum</emphasis>
, 
<emphasis box="[315,439,1950,1973]" italics="true" pageId="9" pageNumber="148">C. kubaryi</emphasis>
stat. rev., and 
<taxonomicName box="[604,668,1950,1973]" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="148" phylum="Arthropoda" rank="species" species="tol" status="sp.n.">
