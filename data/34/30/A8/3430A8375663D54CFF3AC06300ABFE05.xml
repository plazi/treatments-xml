females); wing length 
<number box="[515,564,1580,1604]" pageId="17" pageNumber="18" value="3.12">3.12</number>
(
<typeStatus box="[579,683,1580,1604]" pageId="17" pageNumber="18">holotype</typeStatus>
), 
