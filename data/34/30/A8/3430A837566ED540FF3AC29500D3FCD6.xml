<number box="[969,1082,1919,1943]" pageId="28" pageNumber="29" value="3.1799999999999997" valueMax="3.42" valueMin="2.94">2.94–3.42</number>
(
<typeStatus box="[1101,1215,1919,1943]" pageId="28" pageNumber="29">paratypes</typeStatus>
); wing length 
<number box="[1387,1436,1919,1943]" pageId="28" pageNumber="29" value="3.38">3.38</number>
(
<typeStatus box="[159,263,1954,1978]" pageId="28" pageNumber="29">holotype</typeStatus>
); 
<number box="[282,394,1954,1978]" pageId="28" pageNumber="29" value="3.585" valueMax="3.77" valueMin="3.4">3.40–3.77</number>
(
<typeStatus box="[409,522,1954,1978]" pageId="28" pageNumber="29">paratypes</typeStatus>
), wing width 
<number box="[675,724,1954,1978]" pageId="28" pageNumber="29" value="1.1">1.10</number>
(
<typeStatus box="[739,843,1954,1978]" pageId="28" pageNumber="29">holotype</typeStatus>
), 
<number box="[861,987,1954,1978]" pageId="28" pageNumber="29" value="1.1625" valueMax="1.275" valueMin="1.05">1.05–1.275</number>
(
<typeStatus box="[1002,1116,1954,1978]" pageId="28" pageNumber="29">paratypes</typeStatus>
