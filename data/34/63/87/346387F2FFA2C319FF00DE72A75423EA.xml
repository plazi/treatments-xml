<taxonomicName authority="Foerster, 1862" authorityName="Foerster" authorityYear="1862" box="[321,402,1124,1147]" class="Insecta" family="Braconidae" genus="Agonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="516" phylum="Arthropoda" rank="genus">
