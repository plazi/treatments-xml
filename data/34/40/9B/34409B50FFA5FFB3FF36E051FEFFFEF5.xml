<collectingCountry box="[1004,1119,1880,1906]" name="Morocco" pageId="4" pageNumber="53">Morocco</collectingCountry>
, 
<collectingCountry box="[1133,1208,1880,1906]" name="Spain" pageId="4" pageNumber="53">Spain</collectingCountry>
