<number box="[1338,1369,223,247]" pageId="13" pageNumber="14" value="97.0">97</number>
º 
<number box="[1379,1407,223,247]" pageId="13" pageNumber="14" value="15.0">15</number>
’E
</geoCoordinate>
, 
<date box="[151,341,260,284]" pageId="13" pageNumber="14" value="2011-10-16">
<number box="[151,179,260,284]" pageId="13" pageNumber="14" value="16.0">16</number>
October 
<number box="[283,341,260,284]" pageId="13" pageNumber="14" value="2011.0">2011</number>
