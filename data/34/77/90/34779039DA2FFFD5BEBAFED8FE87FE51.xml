h 
<number box="[601,615,368,392]" pageId="12" pageNumber="13" value="3.0">3</number>
</emphasis>
and 
<emphasis box="[680,703,368,392]" italics="true" pageId="12" pageNumber="13">
f 
<number box="[693,703,368,392]" pageId="12" pageNumber="13" value="2.0">2</number>
</emphasis>
of about 
<number box="[826,854,367,391]" pageId="12" pageNumber="13" value="15.0">15</number>
