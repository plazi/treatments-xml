<taxonomicName class="Arachnida" family="Listrophoridae" genus="Schizocarpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="subornatus">
