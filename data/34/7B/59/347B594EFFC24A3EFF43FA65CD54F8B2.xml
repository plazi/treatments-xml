12.IV. 
<number box="[216,274,1591,1615]" pageId="22" pageNumber="473" value="2011.0">2011</number>
</date>
(CHI 
<number box="[348,376,1590,1615]" pageId="22" pageNumber="473" value="68.0">68</number>
), 22.073200ºN, 
<number box="[571,706,1591,1615]" pageId="22" pageNumber="473" value="100.18445">100.184450</number>
ºE, 
<quantity box="[750,831,1591,1615]" metricMagnitude="3" metricUnit="m" metricValue="1.329" pageId="22" pageNumber="473" unit="m" value="1329.0">1329m</quantity>
