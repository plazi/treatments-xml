<paragraph blockId="3.[151,1436,1736,2014]" pageId="3" pageNumber="526">
<emphasis bold="true" box="[199,333,1736,1761]" pageId="3" pageNumber="526">Variability.</emphasis>
Length 
<quantity box="[434,604,1736,1761]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" metricValueMax="1.65" metricValueMin="1.55" pageId="3" pageNumber="526" unit="mm" value="16.0" valueMax="16.5" valueMin="15.5">15.5–16.5 mm</quantity>
; wing 14. 
<quantity box="[721,856,1736,1761]" metricMagnitude="-3" metricUnit="m" metricValue="7.25" metricValueMax="14.5" metricValueMin="0.0" pageId="3" pageNumber="526" unit="mm" value="7.25" valueMax="14.5" valueMin="0.0">0–14.5 mm</quantity>
