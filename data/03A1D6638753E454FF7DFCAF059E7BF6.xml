<taxonomicName id="4C081CF6875CE455FE99F9DE06ED7E0F" box="[371,472,1607,1630]" class="Insecta" family="Chrysomelidae" genus="Torquispa" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">Torquispa</taxonomicName>
<taxonomicName id="4C081CF6875CE455FE34F9DF05677E0F" box="[478,594,1606,1630]" pageId="56" pageNumber="57" rank="species" species="covexifrons" status="sp. nov.">covexifrons</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B97CBB67875CE455FDB2F9DF059D7E0F" bold="true" box="[600,680,1606,1630]" pageId="56" pageNumber="57">
