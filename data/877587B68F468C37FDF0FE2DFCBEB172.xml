<treatmentCitationGroup id="2FCC118E8F468C37FEE7FE00FD4CB3B2" box="[268,737,443,465]" pageId="3" pageNumber="63">
<treatmentCitation id="8E7D10B18F468C37FEE7FE00FD25B3B2" author="Franz &amp; Nachtrag zur &amp; Monographie der Scydmaeniden" box="[268,648,443,465]" page="74" pageId="3" pageNumber="63" year="1988">
<taxonomicName id="C8DC4D238F468C37FEE7FE00FD25B3B2" authority="Franz 1988 a: 74" authorityName="Franz" authorityPageNumber="74" authorityYear="1988" box="[268,648,443,465]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="63" phylum="Arthropoda" rank="species" species="ranchoi">
<emphasis id="3DA8EAB28F468C37FEE7FE00FE60B3B3" box="[268,461,444,465]" italics="true" pageId="3" pageNumber="63">Euconnus ranchoi</emphasis>
<bibRefCitation id="6B4D4B518F468C37FE3CFE07FD25B3B2" author="Franz &amp; Nachtrag zur &amp; Monographie der Scydmaeniden" box="[471,648,443,464]" pageId="3" pageNumber="63" pagination="59 - 84" refId="ref4036" refString="Franz, H. 1988 a (1987). Nachtrag zur Monographie der Scydmaeniden venezuelas (Coleoptera, Scydmaenidae). Elytron 1: 59 - 84." type="journal article" year="1988">Franz 1988a: 74</bibRefCitation>
