<taxonomicName id="72874D40F808FF80FF5CF8C7FDABF8D9" authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[189,534,1849,1875]" class="Hydrozoa" family="Sertulariidae" genus="Gonaxia" kingdom="Animalia" order="Leptothecata" pageId="6" pageNumber="7" phylum="Cnidaria" rank="species" species="similis">
