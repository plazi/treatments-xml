<collectingCountry id="46E687A4FFFCFFFC5245FA28FB08FA1B" box="[1116,1215,1421,1445]" name="Hungary" pageId="34" pageNumber="109">Hungary</collectingCountry>
, 
<collectingCountry id="46E687A4FFFCFFFC52D5FA29FAA9FA1A" box="[1228,1310,1420,1444]" name="Greece" pageId="34" pageNumber="109">Greece</collectingCountry>
