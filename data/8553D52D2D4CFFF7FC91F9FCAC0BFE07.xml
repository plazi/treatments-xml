; thickness, 
<quantity id="CA02C9DE2D42FFF7FE59FEBDAC0FFE07" box="[488,576,374,401]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="25" pageNumber="72" unit="mm" value="4.8">4.8 mm</quantity>
