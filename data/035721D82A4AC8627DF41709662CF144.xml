<taxonomicName id="415D67E2F15CA8F7718CDCD7202C5F12" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26904" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus lubbocki Forel" order="Hymenoptera" phylum="Arthropoda" rank="species" species="lubbocki">C. Lubbocki</taxonomicName>
