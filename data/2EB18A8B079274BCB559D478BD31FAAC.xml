<taxonomicName id="B74F5A89D094D291D20CD44F51C245F7" class="Insecta" family="Tortricidae" genus="Ecdytolopha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ecdytolopha aurantiana" order="Lepidoptera" pageId="14" pageNumber="393" phylum="Arthropoda" rank="species" species="aurantiana">Ecdytolopha aurantiana</taxonomicName>
