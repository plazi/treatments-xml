<taxonomicName id="4C1B4D78FFA73749FAC7155884F3AD68" authorityName="Conway &amp; Kottelat" authorityYear="2010" box="[1296,1414,1571,1593]" class="Actinopterygii" family="Psilorhynchidae" genus="Psilorhynchus" kingdom="Animalia" order="Cypriniformes" pageId="89" pageNumber="139" phylum="Chordata" rank="species" species="melissa">
