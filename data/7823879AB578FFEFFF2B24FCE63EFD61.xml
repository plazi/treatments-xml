<emphasis id="C2FEEA9EB578FFEFFF2B24FCE3C6FF3C" bold="true" box="[151,471,151,178]" pageId="7" pageNumber="158">
<taxonomicName id="378A4D0FB578FFEFFF2B24FCE394FF3F" authorityName="Bauer" authorityYear="2003" box="[151,389,151,177]" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="158" phylum="Chordata" rank="species" species="gansi">
<emphasis id="C2FEEA9EB578FFEFFF2B24FCE394FF3F" bold="true" box="[151,389,151,177]" italics="true" pageId="7" pageNumber="158">Cyrtodactylus gansi</emphasis>
