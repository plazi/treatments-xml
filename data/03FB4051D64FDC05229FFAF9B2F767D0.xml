<collectingCountry id="F345B1D7D64FDC0421C9FB61B667620E" box="[961,1123,1866,1892]" name="Saudi Arabia" pageId="8" pageNumber="9">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="F345B1D7D64FDC04267BFB61B6BE620E" box="[1139,1210,1866,1892]" name="Oman" pageId="8" pageNumber="9">Oman</collectingCountry>
