<collectingCountry id="F345B1D7D64FDC0422C0FB61B36E620E" box="[200,362,1866,1892]" name="Saudi Arabia" pageId="8" pageNumber="9">Saudi Arabia</collectingCountry>
. The 
<taxonomicName id="4C528AC4D64FDC0423BBFB61B045620E" authorityName="Kaszab" authorityYear="1983" box="[435,577,1866,1892]" class="Insecta" family="Meloidae" genus="Diaphorocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="subSpecies" species="hemprichi" subSpecies="saudita">
ssp. 
<emphasis id="B9262D55D64FDC0423E3FB60B045620E" box="[491,577,1867,1892]" italics="true" pageId="8" pageNumber="9">saudita</emphasis>
</taxonomicName>
