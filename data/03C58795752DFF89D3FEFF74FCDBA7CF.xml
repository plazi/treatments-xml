<taxonomicName id="4C6C4D00752DFF89D1CAFD4FFB80A6A0" authorityName="von Hoffmannsegg &amp; Link" authorityYear="1809" box="[991,1095,729,753]" class="Magnoliopsida" family="Oleaceae" kingdom="Plantae" order="Lamiales" pageId="12" pageNumber="494" phylum="Tracheophyta" rank="family">Oleaceae</taxonomicName>
