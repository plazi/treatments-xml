, 
<collectingCountry id="F37B7613752DFF89D6AFFD6DFB3AA742" box="[1210,1277,763,787]" name="Libya" pageId="12" pageNumber="494">Libya</collectingCountry>
, 
<collectingCountry id="F37B7613752DFF89D71CFD6DFA91A742" box="[1289,1366,763,787]" name="Turkey" pageId="12" pageNumber="494">Turkey</collectingCountry>
