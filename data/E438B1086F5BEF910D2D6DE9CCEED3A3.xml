<taxonomicName id="0A85C1CA41E1B1E16B5E6B681737E921" class="Insecta" family="Agyrtidae" genus="Apteroloma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apteroloma jinfo" order="Coleoptera" pageId="5" pageNumber="24" phylum="Arthropoda" rank="species" species="jinfo">Apteroloma jinfo</taxonomicName>
