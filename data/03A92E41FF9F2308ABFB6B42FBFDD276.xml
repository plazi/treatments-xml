<collectingRegion id="49C451B5FF9F2308AFEF6E9CFC59D20A" country="Ecuador" name="Santo Domingo de los Tsachilas" pageId="4" pageNumber="669">Santo Domingo de los Tsáchilas</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308A8916F7EFBF6D209" box="[962,1046,262,286]" country="Ecuador" name="Guayas" pageId="4" pageNumber="669">Guayas</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308AF736F7DFB95D20A" box="[1056,1141,261,285]" country="Ecuador" name="Manabi" pageId="4" pageNumber="669">Manabí</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308AF2C6F7DFB1FD20A" box="[1151,1279,261,285]" country="Ecuador" name="Esmeraldas" pageId="4" pageNumber="669">Esmeraldas</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308AE5B6F7EFA8AD209" box="[1288,1386,262,286]" country="Ecuador" name="Los Rios" pageId="4" pageNumber="669">Los Ríos</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308AE276F7DFCB9D228" country="Ecuador" name="El Oro" pageId="4" pageNumber="669">El Oro</collectingRegion>
, 
<collectingRegion id="49C451B5FF9F2308A8376F5FFC47D228" box="[868,935,295,319]" country="Ecuador" name="Canar" pageId="4" pageNumber="669">Cañar</collectingRegion>
, and 
<collectingRegion id="49C451B5FF9F2308A88C6F5FFBF0D228" box="[991,1040,295,319]" country="Ecuador" name="Loja" pageId="4" pageNumber="669">Loja</collectingRegion>
