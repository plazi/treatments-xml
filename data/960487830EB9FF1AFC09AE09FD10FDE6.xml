<taxonomicName id="D9AD4D160EB9FF1BFCF6AE52FB4AFAB5" authority="Koenike, 1905" authorityName="Koenike" authorityYear="1905" box="[895,1207,1346,1368]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="130" pageNumber="271" phylum="Arthropoda" rank="species" species="vaginalis" status="syn. nov.">
