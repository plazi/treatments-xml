<emphasis id="3EE0EAFDFFF7EB3039F8FD313A47FD25" bold="true" box="[457,893,746,766]" pageId="27" pageNumber="594">
<taxonomicName id="CB944D6CFFF7EB3039F8FD313A26FD26" authority="Brandão, 2010" authorityName="Brandão" authorityYear="2010" box="[457,796,746,765]" class="Ostracoda" family="Macrocyprididae" genus="Macromckenziea" kingdom="Animalia" order="Podocopida" pageId="27" pageNumber="594" phylum="Arthropoda" rank="species" species="giambonini" status="sp. nov.">
