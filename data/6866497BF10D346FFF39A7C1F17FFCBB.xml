<date id="9471DEADF10C346FFDDEA261F772FE6E" box="[624,771,367,392]" pageId="27" pageNumber="28" value="1958-04-05" valueMax="1958-04-11" valueMin="1958-04-05">
<number id="5607EA4AF10C346FFDDEA261F6D9FE6E" box="[624,680,367,392]" pageId="27" pageNumber="28" value="8.0" valueMax="11.0" valueMin="5.0">5–11</number>
.iv. 
<number id="5607EA4AF10C346FFD69A261F772FE6E" box="[711,771,367,392]" pageId="27" pageNumber="28" value="1958.0">1958</number>
</date>
, T.C. Maa (
<specimenCount id="F6C933E4F10C346FFC27A27EF7DCFE6E" box="[905,941,368,392]" pageId="27" pageNumber="28" type="generic">
