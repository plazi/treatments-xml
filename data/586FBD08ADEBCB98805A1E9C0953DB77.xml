<taxonomicName id="EF1B5943F1F0BA576D760212444D6A96" class="Insecta" family="Stenotrachelidae" genus="Cephaloon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloon" order="Coleoptera" pageId="190" pageNumber="191" phylum="Arthropoda" rank="genus">Cephaloon</taxonomicName>
