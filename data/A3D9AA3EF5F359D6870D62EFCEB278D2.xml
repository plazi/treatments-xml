<taxonomicName id="94852FCED2188A75717AC8D881E4420D" authority="Tribull, Pankowski &amp; Colombo, 2021" authorityName="Tribull, Pankowski &amp; Colombo" authorityYear="2021" class="Insecta" family="Bethylidae" genus="Archeonesia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Archeonesia eocena" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="eocena">
