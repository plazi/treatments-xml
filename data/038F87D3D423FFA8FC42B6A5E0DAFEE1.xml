<heading id="D0D181A9D423FFB7FC42B6A5E510FF4E" box="[954,1312,198,219]" centered="true" fontSize="8" level="2" pageId="15" pageNumber="240" reason="8">
<taxonomicName id="4C264D46D423FFB7FC42B6A5E510FF4E" authority="WUNDERLICH, 2004" authorityName="Wunderlich" authorityYear="2004" box="[954,1312,198,219]" class="Arachnida" family="Araneidae" genus="Leviellus" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="240" phylum="Arthropoda" rank="genus">
