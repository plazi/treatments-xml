</taxonomicName>
(= 
<taxonomicName id="4C264D46D423FFB7FBB6B768E522FEB4" authorityName="Gregorič &amp; Agnarsson &amp; Blackledge &amp; Kuntner" authorityYear="2015" baseAuthorityName="Thorell" baseAuthorityYear="1870" box="[1102,1298,267,289]" class="Arachnida" family="Araneidae" genus="Leviellus" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="240" phylum="Arthropoda" rank="species" species="stroemi">
<emphasis id="B952EAD7D423FFB7FBB6B768E522FEB4" box="[1102,1298,267,289]" italics="true" pageId="15" pageNumber="240">Leviellus stroemi</emphasis>
