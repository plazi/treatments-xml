<taxonomicName id="B7E2C28DF2770735FC5CB91D8D79FDAE" baseAuthorityName="Poll" baseAuthorityYear="1967" box="[977,1103,565,587]" class="Actinopterygii" family="Mormyridae" genus="Paramormyrops" kingdom="Animalia" order="Osteoglossiformes" pageId="12" pageNumber="625" phylum="Chordata" rank="species" species="jacksoni">
