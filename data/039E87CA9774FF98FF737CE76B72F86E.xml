<taxonomicName id="4C374D5F9748FF99FEFF7AD56A5BF236" authority="Li" authorityName="Li" authorityYear="2017" box="[260,513,2033,2055]" class="Arachnida" family="Dictynidae" genus="Brommella" kingdom="Animalia" order="Araneae" pageId="32" pageNumber="157" phylum="Arthropoda" rank="species" species="linyuchengi" status="sp. nov.">
<emphasis id="B943EACE9748FF99FEFF7AD569BEF237" box="[260,484,2033,2054]" italics="true" pageId="32" pageNumber="157">Brommella linyuchengi</emphasis>
Li
</taxonomicName>
, 
<emphasis id="B943EACE9748FF99FDF77AD56A02F237" bold="true" box="[524,600,2033,2054]" pageId="32" pageNumber="157">
<taxonomicNameLabel id="A27057B59748FF99FDF77AD56A02F237" box="[524,600,2033,2054]" pageId="32" pageNumber="157" rank="species">sp. nov.</taxonomicNameLabel>
