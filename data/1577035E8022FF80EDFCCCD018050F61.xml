. 
<taxonomicName id="5ADEC9CB8022FF81EC75C9941BA809AF" authority="(Ruppell 1837)" authorityName="Ruppell" authorityYear="1837" baseAuthorityName="Ruppell" baseAuthorityYear="1837" box="[286,757,1981,2003]" class="Elasmobranchii" family="Carcharhinidae" genus="Carcharhinus" kingdom="Animalia" order="Carcharhiniformes" pageId="38" pageNumber="39" phylum="Chordata" rank="species" species="albimarginatus">
