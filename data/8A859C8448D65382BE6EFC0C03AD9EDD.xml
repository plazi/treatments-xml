<taxonomicName id="1249BAB6DAE117395F916ECC6160E129" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ilex perado" order="Aquifoliales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="perado">
