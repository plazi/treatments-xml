<taxonomicName id="F780EBFDC572FC61FC94233B8E93F8A8" baseAuthorityName="Lamarck" baseAuthorityYear="1801" box="[835,1005,1829,1851]" class="Anthozoa" family="Fungiidae" genus="Lobactis" kingdom="Animalia" order="Scleractinia" pageId="3" pageNumber="569" phylum="Cnidaria" rank="species" species="scutaria">
