<taxonomicName id="4C7E4D5326757439BBC2FB40FCD3FB0F" authority="Zamparo, Brooks &amp; Barriga, 1999" authorityName="Zamparo, Brooks &amp; Barriga" authorityYear="1999" box="[151,780,1193,1218]" class="Cestoda" family="Phyllobothriidae" genus="Pararhinebothroides" kingdom="Animalia" order="Tetraphyllidea" pageId="25" pageNumber="76" phylum="Platyhelminthes" rank="genus">
