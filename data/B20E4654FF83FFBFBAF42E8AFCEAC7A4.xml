<taxonomicName id="FDA78CC1FF83FFBDB85B2DEDFD14C0B4" authorityName="Portevin" authorityYear="1907" box="[568,667,1667,1691]" class="Insecta" family="Leiodidae" genus="Adelopsis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">
