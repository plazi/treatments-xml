<taxonomicName id="28C3A0E1B110386AF9E0FD99FD91FF5E" authorityName="Bennett &amp; Copley &amp; Copley" authorityYear="2021" box="[470,568,187,213]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="241" phylum="Arthropoda" rank="species" species="fraxineus">
<emphasis id="DDB70770B110386AF9E0FD99FD91FF5E" box="[470,568,187,213]" italics="true" pageId="17" pageNumber="241">fraxineus</emphasis>
</taxonomicName>
