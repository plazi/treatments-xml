<taxonomicName id="28C3A0E1B110386AF92BFD99FE0CFF5E" authorityName="Chamberlin &amp; Ivie" authorityYear="1932" box="[285,421,187,213]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="241" phylum="Arthropoda" rank="species" species="aspenicolens">
<emphasis id="DDB70770B110386AF92BFD99FE0CFF5E" box="[285,421,187,213]" italics="true" pageId="17" pageNumber="241">aspenicolens</emphasis>
</taxonomicName>
