<taxonomicName id="28C3A0E1B110386AF9B9FDFDFD92FF72" authorityName="Bennett. All" authorityYear="2009" box="[399,571,223,249]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="241" phylum="Arthropoda" rank="species" species="thermydrinos">
<emphasis id="DDB70770B110386AF9B9FDFDFD92FF72" box="[399,571,223,249]" italics="true" pageId="17" pageNumber="241">C. thermydrinos</emphasis>
