is grouped with 
<taxonomicName id="28C3A0E1B110386AFAD0FDB5FCCEFF3A" authority="Bennett &amp; Copley &amp; Copley, 2021" authorityName="Bennett &amp; Copley &amp; Copley" authorityYear="2021" box="[742,871,151,177]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="241" phylum="Arthropoda" rank="species" species="fraxineus" status="spec. nov.">
<emphasis id="DDB70770B110386AFAD0FDB5FCCEFF3A" box="[742,871,151,177]" italics="true" pageId="17" pageNumber="241">C. fraxineus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="C684BA0BB110386AFB5AFDB5FC7FFF3A" box="[876,982,151,177]" pageId="17" pageNumber="241" rank="species">spec. nov.</taxonomicNameLabel>
in the informal 
<taxonomicName id="28C3A0E1B110386AFC4AFDB5FB77FF3A" authorityName="Bennett &amp; Copley &amp; Copley" authorityYear="2021" box="[1148,1246,151,177]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="241" phylum="Arthropoda" rank="species" species="fraxineus">
