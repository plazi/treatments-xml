species: 
<taxonomicName id="4C524D33FFD7616B58251CF0BAB1F94F" authority="Yasunaga, 1999" authorityName="Yasunaga" authorityYear="1999" box="[294,726,1731,1754]" class="Insecta" family="Miridae" genus="Atractotomoidea" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="species" species="castanea">
