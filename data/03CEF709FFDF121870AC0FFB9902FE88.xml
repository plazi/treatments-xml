<emphasis id="B9139A0DFFDF121E74C408BF9D7AF9BD" box="[1279,1291,1523,1546]" italics="true" pageId="3" pageNumber="48">c</emphasis>
<emphasis id="B9139A0DFFDF121E75370B4D9D65F9B9" box="[1292,1300,1537,1550]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E75370B4D9D65F9B9" box="[1292,1300,1537,1550]" pageId="3" pageNumber="48" value="2.0">2</number>
</emphasis>
