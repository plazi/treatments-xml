<emphasis id="B9139A0DFFDF121E72EC0B0D9A94F9EF" box="[727,741,1601,1624]" italics="true" pageId="3" pageNumber="48">g</emphasis>
<emphasis id="B9139A0DFFDF121E72DF0B039A88F9EB" box="[740,761,1615,1628]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E72DF0B039A88F9EB" box="[740,761,1615,1628]" pageId="3" pageNumber="48" value="3.0" valueMax="5.0" valueMin="1.0">1-5</number>
</emphasis>
