<emphasis id="B9139A0DFFDF121E72D70BC39B79F911" box="[748,776,1679,1702]" italics="true" pageId="3" pageNumber="48">an</emphasis>
<emphasis id="B9139A0DFFDF121E73330BD09B61F91E" box="[776,784,1692,1705]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E73330BD09B61F91E" box="[776,784,1692,1705]" pageId="3" pageNumber="48" value="1.0">1</number>
</emphasis>
and 
<emphasis id="B9139A0DFFDF121E73730BC39B15F911" box="[840,868,1679,1702]" italics="true" pageId="3" pageNumber="48">an</emphasis>
<emphasis id="B9139A0DFFDF121E735F0BD09B1DF91E" box="[868,876,1692,1705]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E735F0BD09B1DF91E" box="[868,876,1692,1705]" pageId="3" pageNumber="48" value="2.0">2</number>
</emphasis>
