<emphasis id="B9139A0DFFDF121E70FD0B979893F944" box="[198,226,1755,1779]" italics="true" pageId="3" pageNumber="48">ad</emphasis>
<emphasis id="B9139A0DFFDF121E70D90BA6989BF940" box="[226,234,1770,1783]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E70D90BA6989BF940" box="[226,234,1770,1783]" pageId="3" pageNumber="48" value="3.0">3</number>
