</emphasis>
remote from anterior border, setae 
<emphasis id="B9139A0DFFDF121E738A08BF9BCCF9BD" box="[945,957,1523,1546]" italics="true" pageId="3" pageNumber="48">c</emphasis>
<emphasis id="B9139A0DFFDF121E73860B4D9BB4F9B9" box="[957,965,1537,1550]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E73860B4D9BB4F9B9" box="[957,965,1537,1550]" pageId="3" pageNumber="48" value="1.0">1</number>
</emphasis>
