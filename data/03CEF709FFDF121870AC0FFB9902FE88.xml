<emphasis id="B9139A0DFFDF121E72340B559A6CF986" box="[527,541,1561,1585]" italics="true" pageId="3" pageNumber="48">h</emphasis>
<emphasis id="B9139A0DFFDF121E72260B649A54F982" box="[541,549,1576,1589]" italics="true" pageId="3" pageNumber="48">
<number id="3DAF5438FFDF121E72260B649A54F982" box="[541,549,1576,1589]" pageId="3" pageNumber="48" value="1.0">1</number>
</emphasis>
