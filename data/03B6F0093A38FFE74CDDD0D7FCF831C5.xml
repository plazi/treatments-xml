<quantity id="4CE7ECFA3A38FFE64865D0F1FEB8361C" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.9" metricValueMin="1.35" pageId="16" pageNumber="911" unit="m" value="1625.0" valueMax="1900.0" valueMin="1350.0">1350- 1900 m</quantity>
