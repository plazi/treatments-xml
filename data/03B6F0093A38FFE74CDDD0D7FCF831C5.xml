<collectingCountry id="F308018F3A39FFE74E4DD4AFFDC431C6" box="[546,607,432,454]" name="China" pageId="17" pageNumber="912">China</collectingCountry>
and 
<collectingCountry id="F308018F3A39FFE74EFFD4AFFD5731C5" box="[656,716,432,453]" name="Japan" pageId="17" pageNumber="912">Japan</collectingCountry>
, new for 
