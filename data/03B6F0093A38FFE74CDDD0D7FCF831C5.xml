<geoCoordinate id="EE2B27D83A38FFE64F60D0F1FCE43604" box="[783,895,1518,1540]" degrees="20" direction="north" minutes="13" orientation="latitude" pageId="16" pageNumber="911" precision="15" seconds="30" value="20.225">20°13´30´´N</geoCoordinate>
