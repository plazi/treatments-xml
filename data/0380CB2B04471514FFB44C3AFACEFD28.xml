<taxonomicName id="4C2901BE04471514FB444CE8FAF5FD2F" authorityName="Tryon" authorityYear="1872" box="[1127,1310,705,731]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="78" pageNumber="79" phylum="Mollusca" rank="species" species="peasei">
