......... 
<taxonomicName id="8D968CDC856DDA1C2B4AFEA279B8FED5" authorityName=": De Lang" authorityYear="2013" box="[1303,1436,269,290]" class="Reptilia" family="Colubridae" genus="Stegonotus" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="509" phylum="Chordata" rank="species" species="batjanensis">
