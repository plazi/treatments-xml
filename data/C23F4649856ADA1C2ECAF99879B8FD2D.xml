<collectingCountry id="3281B7CF856ADA1B2A27F82178DCF853" box="[1146,1272,1934,1956]" name="Flores Island" pageId="19" pageNumber="508">Flores Island</collectingCountry>
, 
<collectingRegion id="885239BD856ADA1B2B5DF8207D4DF835" country="Indonesia" name="Nusa Tenggara Timur" pageId="19" pageNumber="508">East Nusa Tenggara Province</collectingRegion>
, 
<collectingCountry id="3281B7CF856ADA1B2F2FF8037DEBF835" box="[370,463,1964,1986]" name="Indonesia" pageId="19" pageNumber="508">Indonesia</collectingCountry>
....................................................................... 
