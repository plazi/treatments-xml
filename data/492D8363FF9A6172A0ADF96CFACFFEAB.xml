<taxonomicName id="068449F6FF9C6173A219FA64FD54FA55" authorityName="Smith" authorityYear="1933" box="[547,691,1481,1505]" class="Reptilia" family="Gekkonidae" genus="Dravidogecko" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="13" phylum="Chordata" rank="genus">
