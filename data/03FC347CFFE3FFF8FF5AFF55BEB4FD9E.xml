<collectingCountry id="F342C5FAFFE3FFF8FC1CFE0BB95DFE7A" box="[977,1086,457,482]" name="Indonesia" pageId="45" pageNumber="46">Indonesia</collectingCountry>
east to 
<collectingCountry id="F342C5FAFFE3FFF8FB6AFE0BB805FE7A" box="[1191,1382,457,482]" name="Marshall Islands" pageId="45" pageNumber="46">Marshall Islands</collectingCountry>
