<paragraph id="21DE78CE03E4DB4B3868732C265EC332" pageId="18" pageNumber="33" type="caption">
<pageBreakToken id="9C2875EE1DE21AB49CA458F5B2DE6D38" pageId="18" pageNumber="33" start="start">FIGURES</pageBreakToken>
34-42. 
<taxonomicName id="C10A431D7DD0B055057757B1BABC978B" class="Arachnida" family="Nemesiidae" genus="Aname" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="33" phylum="Arthropoda" rank="species" species="ellenae">Aname ellenae</taxonomicName>
<taxonomicNameLabel id="6EA2C5F11A556DB1D588738EB9771925" pageId="18" pageNumber="33" rank="species">n. sp.</taxonomicNameLabel>
