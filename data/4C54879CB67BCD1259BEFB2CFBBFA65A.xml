<collectingCountry id="BCEA761AB67BCD135A32FA23FC85A19C" box="[795,880,1475,1501]" name="Bolivia" pageId="38" pageNumber="39">Bolivia</collectingCountry>
, 
<collectingCountry id="BCEA761AB67BCD135A56FA23FC33A19C" box="[895,966,1475,1501]" name="Brazil" pageId="38" pageNumber="39">Brazil</collectingCountry>
, 
<collectingCountry id="BCEA761AB67BCD135AFFFA23FBB2A19C" box="[982,1095,1475,1501]" name="Colombia" pageId="38" pageNumber="39">Colombia</collectingCountry>
, 
<collectingCountry id="BCEA761AB67BCD135D7FFA23FB40A19C" box="[1110,1205,1475,1501]" name="Ecuador" pageId="38" pageNumber="39">Ecuador</collectingCountry>
, and 
<collectingCountry id="BCEA761AB67BCD135DDEFA23FADFA19C" box="[1271,1322,1475,1501]" name="Peru" pageId="38" pageNumber="39">Peru</collectingCountry>
(
<bibRefCitation id="A06C4B7BB67BCD135C14FA23FF17A243" author="Sanborn, A. F." pageId="38" pageNumber="39" pagination="1 - 104" refId="ref108562" refString="Sanborn, A. F. (2019 b) The cicadas (Hemiptera: Cicadidae) of Bolivia including the descriptions of fifteen new species, the resurrection of one genus and two species, seven new combinations, six new synonymies, and twenty-eight new records. Zootaxa, 4655 (1), 1 - 104. https: // doi. org / 10.11646 / zootaxa. 4655.1.1" type="journal article" year="2019">Sanborn 2019b</bibRefCitation>
