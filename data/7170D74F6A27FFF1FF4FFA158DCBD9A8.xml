<location id="FC0630826A24FFF1FD0CFE738D00DDED" LSID="urn:lsid:plazi:treatment:7170D74F6A27FFF1FF4FFA158DCBD9A8:FC0630826A24FFF1FD0CFE738D00DDED" box="[724,805,475,501]" country="United States of America" county="Margins" municipality="Dorsal" name="Ventral" pageId="38" pageNumber="39" stateProvince="New York">Ventral</location>
