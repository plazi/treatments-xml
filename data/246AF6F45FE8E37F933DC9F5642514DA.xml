<emphasis id="62ADDDB8BE97D918845AB92FA3D15EC6" italics="true" pageNumber="496">Genus</emphasis>
<taxonomicName id="FD1F32F4B567CF066792ED7B3B3F4899" class="Magnoliopsida" family="Berberidaceae" genus="Epimedium" kingdom="Plantae" order="Ranunculales" phylum="Tracheophyta" rank="genus">Epimedium</taxonomicName>
