<taxonomicName id="4C3F4D44FFBB65379FA4FB41461CC0FF" authority="(Kieffer, 1906)" baseAuthorityName="Kieffer" baseAuthorityYear="1906" box="[151,601,1197,1223]" class="Insecta" family="Megaspilidae" genus="Conostigmus" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="nigripes">
