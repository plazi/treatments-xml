<taxonomicName id="0EA54D376E520C139A36F8BDFCCACA76" authority="R. Rice" authorityName="R. Rice" box="[567,824,1853,1875]" class="Liliopsida" family="Orchidaceae" genus="Coelogyne" kingdom="Plantae" order="Asparagales" pageId="14" pageNumber="108" phylum="Tracheophyta" rank="species" species="josephii">
