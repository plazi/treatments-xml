<taxonomicName id="64302F2467D11CBFF3E87C781C70014B" authority="Tod. &amp; Nuovi Gen.Sp.Orto Palermo" authorityName="Tod. &amp; Nuovi Gen.Sp.Orto Palermo" class="Magnoliopsida" family="Leguminosae" genus="Biancaea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Biancaea scandens" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="scandens">
