<taxonomicName id="A26CF51FC775F84035E05692D8FD85C5" authority="Tod. &amp; Nuovi Gen.Sp.Orto Palermo" authorityName="Tod. &amp; Nuovi Gen.Sp.Orto Palermo" class="Magnoliopsida" family="Leguminosae" genus="Biancaea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Biancaea" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="genus">Biancaea</taxonomicName>
