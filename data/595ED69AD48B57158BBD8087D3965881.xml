<subSubSection id="5465736798B0361AAD9D90786609112F" pageId="44" pageNumber="45" type="reference_group">
<paragraph id="179A59A36EA9A8820ED3E0E43EB8ED8C" pageId="44" pageNumber="45">
<taxonomicName id="3C438572A31987A544CF96A6F74CD79A" authority="Adanson, 1763" authorityName="Adanson" authorityYear="1763" class="Magnoliopsida" family="Leguminosae" genus="Campecia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Campecia" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="genus">
