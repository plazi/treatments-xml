[[ 
<taxonomicName id="B38AAB8C023CA820602A4EAE6363E7D2" LSID-ZBK="urn:lsid:zoobank.org:act:3B55DB48-DA82-4437-9CD5-367CB961753C" class="Actinopterygii" family="Gobiidae" genus="Caragobius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Caragobius Smith &amp; Seale 1906:81" order="Perciformes" pageNumber="1" phylum="Chordata" rank="genus">Caragobius Smith and Seale</taxonomicName>
