<number id="3D8724D4FFF1FFE8FF14F86FE7B5C0E0" box="[159,189,1982,2005]" pageId="14" pageNumber="39" value="36.0">36</number>
), spermathecae (37, 38, 
<number id="3D8724D4FFF1FFE8FE4AF86FE6D7C0E0" box="[449,479,1982,2005]" pageId="14" pageNumber="39" value="41.0">41</number>
