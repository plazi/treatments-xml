<taxonomicName id="5CBECCE51351EE5FFE57FA09FDC5FA6A" authorityName="MOORE &amp; BRUSCA" authorityYear="2003" box="[416,611,1442,1464]" class="Malacostraca" family="Cirolanidae" genus="Colopisthus" kingdom="Animalia" order="Isopoda" pageId="39" pageNumber="1367" phylum="Arthropoda" rank="species" species="ronrico">
