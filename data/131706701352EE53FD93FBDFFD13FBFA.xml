<taxonomicName id="5CBECCE5135FEE51FE57FA03FDC4FA6C" authorityName="MOORE &amp; BRUSCA" authorityYear="2003" box="[416,610,1448,1470]" class="Malacostraca" family="Cirolanidae" genus="Colopisthus" kingdom="Animalia" order="Isopoda" pageId="41" pageNumber="1369" phylum="Arthropoda" rank="species" species="ronrico">
