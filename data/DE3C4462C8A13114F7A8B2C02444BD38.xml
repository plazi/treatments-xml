<taxonomicName id="AAF3B63AEE48E4DDDA211FF0C888287D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27175" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Camponotus sexguttatus (Fabricius)" lsidName-HNS="Camponotus sexguttatus (Fabricius)" order="Hymenoptera" pageNumber="38" phylum="Arthropoda" rank="species" species="sexguttatus">C. sexguttatus Fab</taxonomicName>
