<heading id="D0C181CCAC3EFFDFFC8BA507FDC0F8B7" bold="true" box="[151,559,1869,1895]" fontSize="11" level="1" pageId="12" pageNumber="155" reason="1">
<taxonomicName id="4C364D23AC3EFFDFFC8BA507FDC0F8B7" authority="Melika &amp; Pujade-Villar" authorityName="Melika &amp; Pujade-Villar" box="[151,559,1869,1895]" class="Insecta" family="Cynipidae" genus="Ufo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="155" phylum="Arthropoda" rank="species" species="abei">
