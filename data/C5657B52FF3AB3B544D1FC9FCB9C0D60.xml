<collectingCounty id="A412B2C8FF3AB3B54565FAB6C9B50EBB" box="[291,435,1407,1431]" pageId="166" pageNumber="167">Okhotsk Sea</collectingCounty>
off 
<collectingCountry id="35DB8AD4FF3AB3B545A3FA49CA2E0EBB" box="[485,552,1408,1431]" name="Japan" pageId="166" pageNumber="167">Japan</collectingCountry>
; 
<collectingMunicipality id="AD17503EFF3AB3B54673FAB6CAB40EBB" box="[565,690,1407,1431]" pageId="166" pageNumber="167">Bering Sea</collectingMunicipality>
to south of Tortugas 
<location id="48139C9FFF3AB3B547ECFA49CBDE0EB4" LSID="urn:lsid:plazi:treatment:C5657B52FF3AB3B544D1FC9FCB9C0D60:48139C9FFF3AB3B547ECFA49CBDE0EB4" box="[938,984,1408,1432]" country="Japan" county="Okhotsk Sea" municipality="Bering Sea" name="Bay" pageId="166" pageNumber="167" stateProvince="Baja California">Bay</location>
, 
<collectingRegion id="8F0804A6FF3AB3B547A3FAB6CC930EBB" box="[997,1173,1407,1431]" country="Mexico" name="Baja California" pageId="166" pageNumber="167">Baja California</collectingRegion>
, 
<collectingCountry id="35DB8AD4FF3AB3B540E4FAB6CCFD0EBB" box="[1186,1275,1407,1431]" name="Mexico" pageId="166" pageNumber="167">Mexico</collectingCountry>
