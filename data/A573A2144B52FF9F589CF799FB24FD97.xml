- 
<collectingRegion id="EF1EDDE04B51FF9F5A38FE12FD94FE03" box="[556,612,410,436]" country="China" name="Xizang" pageId="3" pageNumber="235">Tibet</collectingRegion>
<collectorName id="802F76D44B51FF9F5A78FE12FD15FE03" box="[620,741,410,436]" pageId="3" pageNumber="235">Expedition</collectorName>
1700
