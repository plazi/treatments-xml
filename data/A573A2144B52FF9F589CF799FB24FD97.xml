— 
<collectingCountry id="55CD53924B51FF9F5ACEFEFEFCC5FE27" box="[730,821,374,400]" name="China" pageId="3" pageNumber="235">CHINA</collectingCountry>
. 
<collectingRegion id="EF1EDDE04B51FF9F5B2BFEFEFC63FE27" box="[831,915,374,400]" country="China" name="Xizang" pageId="3" pageNumber="235">Xizang</collectingRegion>
, 
<collectingCounty id="C4046B8E4B51FF9F5B8AFEFEFBBCFE27" box="[926,1100,374,400]" pageId="3" pageNumber="235">Medog County</collectingCounty>
: 
<location id="280545D94B51FF9F5C42FEFEFAF1FE27" LSID="urn:lsid:plazi:treatment:A573A2144B52FF9F589CF799FB24FD97:280545D94B51FF9F5C42FEFEFAF1FE27" box="[1110,1281,374,400]" country="China" county="Medog County" name="Beibeng Xiang" pageId="3" pageNumber="235" stateProvince="Xizang">Beibeng Xiang</location>
, in forest, elev. ca. 
<quantity id="EA22BEE74B51FF9F58A4FE12FF09FE02" box="[176,249,410,437]" metricMagnitude="2" metricUnit="m" metricValue="8.3" pageId="3" pageNumber="235" unit="m" value="830.0">
<elevation id="A6F7F4314B51FF9F58A4FE12FF09FE02" box="[176,249,410,437]" metricMagnitude="2" metricUnit="m" metricValue="8.3" pageId="3" pageNumber="235" unit="m" value="830.0">830 m</elevation>
</quantity>
, 
<date id="596435C24B51FF9F5912FE12FE4BFE03" box="[262,443,410,436]" pageId="3" pageNumber="235" value="1974-08-10">
<collectingDate id="4920CC2A4B51FF9F5912FE12FE4BFE03" box="[262,443,410,436]" pageId="3" pageNumber="235" value="1974-08-10">10 August 1974</collectingDate>
</date>
</materialsCitation>
, 
<materialsCitation id="9DB2195F4B51FF9F59D3FE12FE0FFE6F" ID-GBIF-Occurrence="3408913305" collectingDate="1980-07-08" collectionCode="KUN" collectorName="Expedition" country="China" elevation="900" location="Jiangxin Cun" municipality="Beibeng Xiang" pageId="3" pageNumber="235" specimenCode="KUN 0424111" specimenCount="1" stateProvince="Qinghai">
