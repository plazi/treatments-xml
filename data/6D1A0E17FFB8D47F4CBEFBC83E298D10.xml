; LAN = 
<quantity id="224B12E4FFB9D47F4940FC8F3B818AE0" metricMagnitude="-3" metricUnit="m" metricValue="1.64" pageId="9" pageNumber="10" unit="mm" value="1.64">1.64 mm</quantity>
; 
<collectionCode id="83A227C4FFB9D47F4CFFFCA73A6D8AE0" box="[214,292,872,898]" pageId="9" pageNumber="10">LAED</collectionCode>
= 
<quantity id="224B12E4FFB9D47F4D6EFCA73AFC8AE0" box="[327,437,872,898]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="9" pageNumber="10" unit="mm" value="0.85">0.85 mm</quantity>
; LB = 
<quantity id="224B12E4FFB9D47F4E22FCA739318AE0" box="[523,632,872,898]" metricMagnitude="-3" metricUnit="m" metricValue="1.99" pageId="9" pageNumber="10" unit="mm" value="1.99">1.99 mm</quantity>
; LE/LP = 
<number id="537BAD26FFB9D47F4ED1FCA738668AE0" box="[760,815,872,898]" pageId="9" pageNumber="10" value="3.6">3.60</number>
; WE/WP = 
<number id="537BAD26FFB9D47F4FE8FCA738B18AE0" box="[961,1016,872,898]" pageId="9" pageNumber="10" value="1.5">1.50</number>
; WP/LP = 
<number id="537BAD26FFB9D47F4856FCA73FFF8AE0" box="[1151,1206,872,898]" pageId="9" pageNumber="10" value="1.49">1.49</number>
; LE/ 
<collectionCode id="83A227C4FFB9D47F48D9FCA73E778AE0" box="[1264,1342,872,898]" pageId="9" pageNumber="10">LAED</collectionCode>
