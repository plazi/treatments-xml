<taxonomicName id="5CC24D36FFA9FF8BFF71FE6D6AE45C90" authority="Hirst, 1931" authorityName="Hirst" authorityYear="1931" box="[199,649,396,420]" class="Arachnida" family="Microtrombidiidae" genus="Microtrombidium" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="23" phylum="Arthropoda" rank="species" species="willungae">
