<collectingCountry box="[952,1064,355,377]" name="Indonesia" pageId="51" pageNumber="598">Indonesia</collectingCountry>
(
<collectingRegion box="[1081,1185,355,377]" country="Indonesia" name="Sulawesi Utara" pageId="51" pageNumber="598">Sulawesi</collectingRegion>
) (
<figureCitation box="[1210,1292,355,377]" captionStart="Figure 49" captionStartId="15.[164,243,1277,1296]" captionTargetBox="[167,1442,198,1246]" captionTargetId="figure-317@15.[164,1444,195,1248]" captionTargetPageId="15" captionText="Figure 49. Geographical distribution of the species of the C. baduvi group and C. moatensis." figureDoi="http://doi.org/10.5281/zenodo.5358200" httpUri="https://zenodo.org/record/5358200/files/figure.png" pageId="51" pageNumber="598">Fig. 49</figureCitation>
