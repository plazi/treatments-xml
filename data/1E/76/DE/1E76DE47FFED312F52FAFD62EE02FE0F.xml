<subSubSection box="[192,1037,906,930]" pageId="7" pageNumber="6" type="etymology">
<paragraph blockId="7.[192,1037,906,930]" box="[192,1037,906,930]" pageId="7" pageNumber="6">
<emphasis bold="true" box="[192,347,906,930]" pageId="7" pageNumber="6">Etymology.</emphasis>
