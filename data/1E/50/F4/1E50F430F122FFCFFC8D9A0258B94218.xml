) and 
<taxonomicName authority="Assing, 2018" authorityName="Assing" authorityYear="2018" box="[672,816,2009,2031]" class="Insecta" family="Staphylinidae" genus="Medon" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="73" phylum="Arthropoda" rank="species" species="acutapicalis" status="spec. nov.">
<emphasis box="[672,816,2009,2031]" italics="true" pageId="4" pageNumber="73">M. acutapicalis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[821,914,2010,2031]" pageId="4" pageNumber="73" rank="species">spec. nov.</taxonomicNameLabel>
(
<emphasis bold="true" box="[927,975,2009,2030]" pageId="4" pageNumber="73">7–12</emphasis>
): forebody (
<emphasis bold="true" box="[1094,1129,2009,2030]" pageId="4" pageNumber="73">1, 7</emphasis>
); male sternite VII (
<emphasis bold="true" box="[1324,1359,2009,2030]" pageId="4" pageNumber="73">2, 8</emphasis>
