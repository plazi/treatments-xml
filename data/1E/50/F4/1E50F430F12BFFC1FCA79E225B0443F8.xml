<subSubSection pageId="10" pageNumber="79" type="etymology">
<paragraph blockId="10.[854,1459,928,1016]" pageId="10" pageNumber="79">
<emphasis bold="true" box="[854,973,928,952]" pageId="10" pageNumber="79">Etymology</emphasis>
