<emphasis bold="true" box="[634,651,1283,1302]" italics="true" pageId="11" pageNumber="256">v</emphasis>
, 
<emphasis bold="true" box="[666,679,1282,1301]" italics="true" pageId="11" pageNumber="256">Q</emphasis>
, 
