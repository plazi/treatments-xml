</materialsCitation>
f, 
<accessionNumber box="[412,539,1404,1430]" httpUri="http://www.ncbi.nlm.nih.gov/protein/LGE19014" pageId="27" pageNumber="102">
LGE 
<quantity box="[469,539,1404,1430]" metricMagnitude="4" metricUnit="m" metricValue="1.9014" pageId="27" pageNumber="102" unit="m" value="19014.0">19014</quantity>
