<paragraph pageId="0" pageNumber="1">
<taxonomicName family="Microthyriaceae" genus="Triscelophorus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Triscelophorus monosporus" order="Microthyriales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="monosporus">
