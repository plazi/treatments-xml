<geoCoordinate id="F3F01D68EAAF1B7AD8993866FAED4519" degrees="33.46" direction="south" orientation="latitude" precision="555" value="-33.46">33.46S</geoCoordinate>
, 
<geoCoordinate id="C71AB15FC9B5F03B1D99F5A38C54C340" degrees="18.27" direction="east" orientation="longitude" precision="555" value="18.27">18.27E</geoCoordinate>
