<taxonomicName id="C8264CD8819033E79FF6C2C380DC7547" authorityName="Kunth" authorityYear="1843" class="Liliopsida" family="Asphodelaceae" genus="Trachyandra" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asparagales" pageId="3" pageNumber="98" phylum="Tracheophyta" rank="genus">
<emphasis id="3DB129F9F63827C16D0D561506846587" italics="true" pageId="3" pageNumber="98">Trachyandra</emphasis>
