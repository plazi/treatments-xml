<taxonomicName id="987C062B4E14335B905211F9E1C13482" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145695" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Cryptoceridae" genus="Cryptocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptocerus jucundus Smith" lsidName-HNS="Cryptocerus jucundus Smith" order="Hymenoptera" pageNumber="606" phylum="Arthropoda" rank="species" species="jucundus">Cryptocerus jucundus</taxonomicName>
