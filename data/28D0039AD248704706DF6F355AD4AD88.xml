<paragraph id="C288BED86D44209129E9C6E10699C36C" pageId="8" pageNumber="9">
<taxonomicName id="85F57E0F925F61424368D2B2C9CA19AA" LSID="urn:lsid:zoobank.org:act:BA3EF765-DA34-4DEB-B5B7-DF59061031A6" class="Insecta" family="Carabidae" genus="Singilis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Singilis kabakovi" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="kabakovi">
