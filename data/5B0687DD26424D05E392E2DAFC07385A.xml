<number id="656724EC26424D08E3DAE127FED3384E" box="[223,295,1554,1578]" pageId="7" pageNumber="8" value="28.7" valueMax="38.4" valueMin="19.0">19–38</number>
</figureCitation>
, 
<figureCitation id="4B942A4E26424D08E237E127FEA5384E" box="[306,337,1554,1578]" captionStart="FIGURES 39, 41" captionStartId="11.[151,264,1969,1992]" captionTargetBox="[234,1335,192,1950]" captionTargetId="figure@11.[234,1336,181,1952]" captionTargetPageId="11" captionText="FIGURES 39, 41 Gergithus nonomaculatus sp. nov.; 40, 42 G. tristriatus sp. nov. 39 – 40. female genitalia, ventral view; 41 – 42. general structure of the female reproductive organs, lateral view." httpUri="https://zenodo.org/record/211933/files/figure.png" pageId="7" pageNumber="8">40</figureCitation>
