<taxonomicName id="A1DF4D2FFF5CFF5DBF61BB9D11708828" authority="Kikuchi &amp; Sasa (From Kikuchi &amp; Sasa, 1990)" authorityName="Kikuchi &amp; Sasa (From Kikuchi &amp; Sasa" authorityYear="1990" box="[604,1288,1790,1812]" class="Insecta" family="Chironomidae" genus="Pseudosmittia" kingdom="Animalia" order="Diptera" pageId="171" pageNumber="172" phylum="Arthropoda" rank="species" species="tobaduovicesima">
