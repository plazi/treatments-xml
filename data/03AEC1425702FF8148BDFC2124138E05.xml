<paragraph id="8BB870545704FF804810F9FB25C78A71" blockId="60.[113,646,1610,1700]" box="[113,155,1610,1636]" pageId="60" pageNumber="321">
<emphasis id="B973AC465704FF804810F9FB25C78A71" box="[113,155,1610,1636]" italics="true" pageId="60" pageNumber="321">Size</emphasis>
</paragraph>
<paragraph id="8BB870545704FF804810F9D824B58AB1" blockId="60.[113,646,1610,1700]" pageId="60" pageNumber="321">
Males CL 
<quantity id="4CFFDDB15704FF804884F9D824378A91" box="[229,363,1641,1668]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" metricValueMax="3.9" metricValueMin="3.7" pageId="60" pageNumber="321" unit="mm" value="3.8" valueMax="3.9" valueMin="3.7">3.7-3.9 mm</quantity>
; females CL 
<quantity id="4CFFDDB15704FF80499DF9D827DE8A91" box="[508,642,1641,1668]" metricMagnitude="-3" metricUnit="m" metricValue="4.15" metricValueMax="4.6" metricValueMin="3.7" pageId="60" pageNumber="321" unit="mm" value="4.15" valueMax="4.6" valueMin="3.7">3.7-4.6 mm</quantity>
, ovigerous females CL 
<quantity id="4CFFDDB15704FF80493EF93824B98AB1" box="[351,485,1673,1700]" metricMagnitude="-3" metricUnit="m" metricValue="4.15" metricValueMax="4.6" metricValueMin="3.7" pageId="60" pageNumber="321" unit="mm" value="4.15" valueMax="4.6" valueMin="3.7">3.7-4.6 mm</quantity>
