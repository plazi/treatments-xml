<taxonomicName id="91E94D79592FFFEBB197FEC94E30FEF5" authority="Marathe &amp; Sanborn &amp; Kunte, 2018" authorityName="Marathe &amp; Sanborn &amp; Kunte" authorityYear="2018" box="[1257,1376,298,319]" class="Insecta" family="Cicadidae" genus="Rustia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="442" phylum="Arthropoda" rank="species" species="kodagura" status="n. sp.">
