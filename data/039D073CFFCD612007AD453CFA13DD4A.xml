<typeStatus id="548F0888FFCD6120068E42DDFD94DCF2" box="[436,539,1237,1260]" pageId="2" pageNumber="599">Holotype</typeStatus>
: male, 
<collectingCountry id="F323F6BAFFCD6120054A42DDFD3DDCF2" box="[624,690,1237,1260]" name="China" pageId="2" pageNumber="599">China</collectingCountry>
, Guizhou Province, Fanjingshan Mountain, 
<date id="FF8A90EAFFCD6120039A42DDFA99DCF2" box="[1184,1302,1237,1260]" pageId="2" pageNumber="599" value="2012-08-05">
<number id="3DFCA40DFFCD6120039A42DDFB23DCF2" box="[1184,1196,1237,1260]" pageId="2" pageNumber="599" value="5.0">5</number>
.viii. 
<number id="3DFCA40DFFCD612003E042DDFA99DCF2" box="[1242,1302,1237,1260]" pageId="2" pageNumber="599" value="2012.0">2012</number>
