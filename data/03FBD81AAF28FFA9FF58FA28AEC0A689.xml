<typeStatus id="54E9D7AEAF25FFA8FF58F99DAFD1A156" box="[151,201,1629,1650]" pageId="10" pageNumber="311">Type</typeStatus>
species: 
<taxonomicName id="4C52128FAF25FFA8FEE9F99CAEE8A156" box="[294,496,1628,1650]" class="Insecta" family="Mesoraphidiidae" genus="Burmoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="10" pageNumber="311" phylum="Arthropoda" rank="species" species="reni" status="sp. nov.">
