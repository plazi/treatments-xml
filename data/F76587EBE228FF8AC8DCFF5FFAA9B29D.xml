<taxonomicName id="B8CC4D7EE22EFF8AC9A5FF75FD86B29C" box="[494,615,189,214]" class="Insecta" family="Delphacidae" genus="Longtania" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="66" phylum="Arthropoda" rank="species" species="arcuata">
