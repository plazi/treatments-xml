<taxonomicName id="B8CC4D7EE22EFF8ACBADFF75FBA4B29C" box="[998,1093,189,214]" class="Insecta" family="Delphacidae" genus="Longtania" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="66" phylum="Arthropoda" rank="species" species="picea">
