<taxonomicName id="B8CC4D7EE22EFF8ACB5FFF50FC93B2FB" box="[788,882,152,177]" class="Insecta" family="Delphacidae" genus="Longtania" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="66" phylum="Arthropoda" rank="species" species="picea">
