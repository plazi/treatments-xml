<collectingCountry id="62A8761DB33B032EFE3BB6515D705E3F" box="[410,521,1294,1315]" name="Democratic Republic of the Congo" pageId="21" pageNumber="1656">DR Congo</collectingCountry>
, new for 
<collectingCountry id="62A8761DB33B032EFDC9B6525DC35E3F" box="[616,698,1293,1315]" name="Uganda" pageId="21" pageNumber="1656">Uganda</collectingCountry>
