Genus 
<taxonomicName id="33CE36FA2D78FFE3FF51F84BEEBB7663" authority="Fibiger" authorityName="Fibiger" authorityYear="2010" box="[238,483,311,337]" class="Insecta" family="Erebidae" genus="Asytegumen" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="C6BA916B2D78FFE3FF51F84BEEDA7663" bold="true" box="[238,386,311,337]" italics="true" pageId="75" pageNumber="76">Asytegumen</emphasis>
Fibiger
</taxonomicName>
, 
<taxonomicNameLabel id="DD892C102D78FFE3FE4FF84BED297663" box="[496,625,311,337]" pageId="75" pageNumber="76" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
