Off Cojima, 
<collectingCountry id="5CFB91B16000625FFEA2FE238C296987" box="[287,354,461,485]" name="Cuba" pageId="16" pageNumber="243">Cuba</collectingCountry>
, 
<quantity id="E3147CC46000625FFED2FE238CFC6987" box="[367,439,461,485]" metricMagnitude="2" metricUnit="m" metricValue="8.1" pageId="16" pageNumber="243" unit="m" value="810.0">810 m</quantity>
