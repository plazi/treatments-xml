<location id="8E8C6016FFA3460E2CD1AE7DD1402415" LSID="urn:lsid:plazi:treatment:03FA87DBFFA3460F2D8DACB2D1232453:8E8C6016FFA3460E2CD1AE7DD1402415" box="[469,579,1572,1595]" country="Turkey" county="Caucasus" municipality="The" name="Caucasian" pageId="32" pageNumber="81" stateProvince="Artvin">Caucasian</location>
