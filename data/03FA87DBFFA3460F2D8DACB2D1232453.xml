<location id="8E8C6016FFA3460E2F5FAE1DD18A2475" LSID="urn:lsid:plazi:treatment:03FA87DBFFA3460F2D8DACB2D1232453:8E8C6016FFA3460E2F5FAE1DD18A2475" box="[603,649,1604,1627]" country="Turkey" county="Caucasus" municipality="The" name="Figs" pageId="32" pageNumber="81" stateProvince="Artvin">Figs</location>
