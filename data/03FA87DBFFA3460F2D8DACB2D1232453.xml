<location id="8E8C6016FFA3460E2FD5AE7DD1F92415" LSID="urn:lsid:plazi:treatment:03FA87DBFFA3460F2D8DACB2D1232453:8E8C6016FFA3460E2FD5AE7DD1F92415" box="[721,762,1572,1595]" country="Turkey" county="Caucasus" municipality="The" name="The" pageId="32" pageNumber="81" stateProvince="Artvin">The</location>
