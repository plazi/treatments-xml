C, 
<number id="3D9885738F3CB16631ACFE2754F63B48" box="[384,411,511,535]" pageId="32" pageNumber="233" value="11.0">11</number>
O, 
<specimenCount id="9D565CDD8F3CB16631E8FE27576D3B49" box="[452,512,511,535]" pageId="32" pageNumber="233" type="generic">
