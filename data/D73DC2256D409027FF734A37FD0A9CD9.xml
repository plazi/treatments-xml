<taxonomicName id="989408B06D469026FD384BEDFCCF99E9" baseAuthorityName="Walker" baseAuthorityYear="1849" box="[732,833,1921,1944]" class="Insecta" family="Anthomyiidae" genus="Egle" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="ciliata">
