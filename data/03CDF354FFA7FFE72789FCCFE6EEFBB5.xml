<taxonomicName id="4C6439C1FFA7FFE72789FCCFE405FCDD" authority="Meigen, 1822" authorityName="Meigen" authorityYear="1822" box="[151,564,835,861]" class="Insecta" family="Syrphidae" genus="Eumerus" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="ruficornis">
