<taxonomicName id="21BFCCB6FFD1B9610E16F878962CF817" authorityName="Zhou" authorityYear="2017" box="[303,379,1926,1947]" class="Insecta" family="Staphylinidae" genus="Ocypus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="297" phylum="Arthropoda" rank="genus">
