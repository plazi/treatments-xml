<taxonomicName id="4C624D1CFFFFFFBCFF46FF1298E9E1A1" authority="Wachtl, 1894" authorityName="Wachtl" authorityYear="1894" box="[205,480,226,252]" class="Insecta" family="Tachinidae" genus="Nowickia" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="genus">
