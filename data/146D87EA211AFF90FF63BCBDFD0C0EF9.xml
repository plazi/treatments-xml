<caption id="C8BB6674211BFF93FF63BB50FC550B1B" ID-DOI="http://doi.org/10.5281/zenodo.5425782" ID-Zenodo-Dep="5425782" httpUri="https://zenodo.org/record/5425782/files/figure.png" pageId="5" pageNumber="54" startId="5.[136,229,1817,1839]" targetBox="[147,1439,187,1776]" targetPageId="5">
<paragraph id="9C7B36FC211BFF93FF63BB50FC550B1B" blockId="5.[136,1452,1817,1875]" pageId="5" pageNumber="54">
