<taxonomicName id="868A6DA7C2753428FF6B4B58FE1CF990" authority="Hope, 1841: 54" authorityName="Hope" authorityPageNumber="54" authorityYear="1841" box="[151,441,1624,1646]" class="Insecta" family="Coreidae" genus="Meropachys" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="74" phylum="Arthropoda" rank="genus">
