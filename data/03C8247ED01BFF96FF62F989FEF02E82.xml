<emphasis id="B915497AD018FF96FF62FCD4FEB0289F" bold="true" box="[151,288,889,912]" pageId="26" pageNumber="27">FIGURE 25.</emphasis>
<taxonomicName id="4C61EEEBD018FF96FED0FCD7FE77289F" authorityName="Hincks" authorityYear="1868" box="[293,487,890,912]" class="Hydrozoa" family="Campanulinidae" genus="Cuspidella" kingdom="Animalia" order="Leptothecata" pageId="26" pageNumber="27" phylum="Cnidaria" rank="species" species="grandis">
