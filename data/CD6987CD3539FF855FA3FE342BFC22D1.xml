<collectingCountry id="3DD7764B3538FF855E53FE592EC020ED" box="[359,430,475,501]" name="China" pageId="3" pageNumber="268">China</collectingCountry>
: 
<collectingRegion id="8704F8393538FF855E8DFE592D7720ED" box="[441,537,475,501]" country="China" name="Guangxi" pageId="3" pageNumber="268">Guangxi</collectingRegion>
