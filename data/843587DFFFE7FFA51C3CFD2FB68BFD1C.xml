<caption id="58E36641FFE1FFA71DB2FBD1B4FBFB96" ID-DOI="http://doi.org/10.5281/zenodo.5346131" ID-Zenodo-Dep="5346131" box="[169,976,1073,1092]" httpUri="https://zenodo.org/record/5346131/files/figure.png" pageId="4" startId="4.[169,200,1073,1092]" targetBox="[148,978,194,1052]" targetPageId="4">
