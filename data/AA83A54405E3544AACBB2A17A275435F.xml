<taxonomicName id="6ED0E2362D46DA713AF3F462764A3C86" class="Sordariomycetes" family="Clavicipitaceae" genus="Cordyceps" kingdom="Fungi" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="Ascomycota" rank="order">Coleoptera</taxonomicName>
