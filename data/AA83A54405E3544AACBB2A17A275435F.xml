<taxonomicName id="4D0DFBCBB936BFC0A952D34E62270B97" authorityName="Leach" authorityYear="1815" class="Insecta" family="Elateridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="? arthropoda" rank="family">Elateridae</taxonomicName>
