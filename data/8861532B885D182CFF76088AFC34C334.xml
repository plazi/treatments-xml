<taxonomicName id="C7C899BE885D182CFB790D4AFAFDC37C" authorityName="Zunino" authorityYear="1979" box="[1176,1374,1053,1079]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="subGenus" subGenus="Furconthophagus">
