<taxonomicName id="4C2901BE04511502FB134CCCFAC0FD0B" authorityName="Deshayes" authorityYear="1866" box="[1072,1323,741,767]" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="88" pageNumber="89" phylum="Mollusca" rank="species" species="inaequipartita">
