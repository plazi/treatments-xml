156. 
<taxonomicName id="E05EE4C91CA1D223FAE2BAD870483589" LSID-HNS="urn:lsid:biosci.ohio-state.edu:134561" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica agra Smith" order="Hymenoptera" pageId="47" pageNumber="47" phylum="Arthropoda" rank="species" species="agra">Formica Agra</taxonomicName>
