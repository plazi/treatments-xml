<paragraph id="A803EA1FEF08BF4FFF17F89BFC19DD4E" blockId="23.[151,1436,1804,1972]" box="[151,897,1804,1829]" pageId="23" pageNumber="227">
<typeStatus id="770754BDEF08BF4FFF17F89BFF57DD4F" box="[151,207,1805,1829]" pageId="23" pageNumber="227">Type</typeStatus>
