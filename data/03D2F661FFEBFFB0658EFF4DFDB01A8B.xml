<materialsCitation id="3B134D2AFFEBFFB065DEFE34FDB01A8B" ID-GBIF-Occurrence="1437751869" collectingDate="1955" collectingDateMax="1957" collectingDateMin="1955" country="China" county="Pinbian County" elevation="1300" location="Mt. Tapa-Shan" municipality="Tapa-Shan" pageId="32" pageNumber="297" specimenCount="2" stateProvince="Sichuan" typeStatus="holotype">
<emphasis id="B90F9B65FFEBFFB065DEFE34FEF01A53" bold="true" box="[199,315,494,519]" pageId="32" pageNumber="297">Remarks.</emphasis>
