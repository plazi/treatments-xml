<typeStatus id="54C0F9D5FFEBFFB06105FE35FB871A53" box="[1052,1100,495,519]" pageId="32" pageNumber="297">type</typeStatus>
specimen was collected from 
<location id="8EA411ACFFEBFFB0658EFDC9FE8F1A7F" LSID="urn:lsid:plazi:treatment:03D2F661FFEBFFB0658EFF4DFDB01A8B:8EA411ACFFEBFFB0658EFDC9FE8F1A7F" box="[151,324,530,555]" country="China" county="Pinbian County" municipality="Tapa-Shan" name="Mt. Tapa-Shan" pageId="32" pageNumber="297" stateProvince="Sichuan">Mt. Tapa-Shan</location>
[Daba-Shan], near Pin-P’ien [
<collectingCounty id="62A53FFBFFEBFFB067ACFDC8FCBF1A7F" box="[693,884,530,555]" pageId="32" pageNumber="297">Pinbian County</collectingCounty>
], 
<elevation id="0056A044FFEBFFB06694FDC9FC211A7E" box="[909,1002,531,555]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="32" pageNumber="297" unit="m" value="1300.0">
<quantity id="4C83EA92FFEBFFB06694FDC9FC211A7E" box="[909,1002,531,555]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="32" pageNumber="297" unit="m" value="1300.0">1300 m</quantity>
</elevation>
by 
<location id="8EA411ACFFEBFFB0613AFDC9FBAB1A7E" LSID="urn:lsid:plazi:treatment:03D2F661FFEBFFB0658EFF4DFDB01A8B:8EA411ACFFEBFFB0613AFDC9FBAB1A7E" box="[1059,1120,531,554]" country="China" county="Pinbian County" municipality="Tapa-Shan" name="Huan" pageId="32" pageNumber="297" stateProvince="Sichuan">Huan</location>
K’e-jen [
<location id="8EA411ACFFEBFFB061C1FDC9FA591A7F" LSID="urn:lsid:plazi:treatment:03D2F661FFEBFFB0658EFF4DFDB01A8B:8EA411ACFFEBFFB061C1FDC9FA591A7F" box="[1240,1426,531,555]" country="China" county="Pinbian County" municipality="Tapa-Shan" name="Ke-Ren Huang" pageId="32" pageNumber="297" stateProvince="Sichuan">Ke-Ren Huang</location>
]. 
<location id="8EA411ACFFEBFFB0658EFDECFF321A1B" LSID="urn:lsid:plazi:treatment:03D2F661FFEBFFB0658EFF4DFDB01A8B:8EA411ACFFEBFFB0658EFDECFF321A1B" box="[151,249,566,591]" country="China" county="Pinbian County" municipality="Tapa-Shan" name="Actually" pageId="32" pageNumber="297" stateProvince="Sichuan">Actually</location>
, no mountain named 
<location id="8EA411ACFFEBFFB064ECFDECFDA71A1B" LSID="urn:lsid:plazi:treatment:03D2F661FFEBFFB0658EFF4DFDB01A8B:8EA411ACFFEBFFB064ECFDECFDA71A1B" box="[501,620,566,591]" country="China" county="Pinbian County" municipality="Tapa-Shan" name="Tapa-Shan" pageId="32" pageNumber="297" stateProvince="Sichuan">Tapa-Shan</location>
