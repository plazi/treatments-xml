<taxonomicName id="4C454CFFFFAE546221E1FBA0FE33FB6A" authorityName="Prins" authorityYear="1983" box="[224,422,1149,1175]" class="Insecta" family="Formicidae" genus="Agraulomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis id="B931EB6EFFAE546221E1FBA0FE33FB6A" box="[224,422,1149,1175]" italics="true" pageId="2" pageNumber="3">Agraulomyrmex</emphasis>
