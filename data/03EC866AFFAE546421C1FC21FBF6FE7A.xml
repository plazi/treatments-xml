<paragraph id="8BFA377CFFAE546221E1FB15FDA9FAF5" blockId="2.[192,1124,1075,1736]" pageId="2" pageNumber="3">
<taxonomicName id="4C454CFFFFAE546221E1FB15FE33FB1F" authorityName="Prins" authorityYear="1983" box="[224,422,1224,1250]" class="Insecta" family="Formicidae" genus="Agraulomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
