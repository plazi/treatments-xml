<paragraph id="AD5F39712BBB219FAAB6831B59A47A18" pageId="14" pageNumber="15">
<taxonomicName id="49593C3669166740D9DD860930BB0ACA" class="Insecta" family="Trogossitidae" genus="Acalanthis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="quadrisignata">
