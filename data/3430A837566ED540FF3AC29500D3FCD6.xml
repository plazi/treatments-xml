<taxonomicName id="7B9962A2566CD541FDBAC2A506F3F967" box="[535,657,1687,1709]" class="Insecta" family="Teratomyzidae" genus="Poecilovitila" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="variegata" status="sp. nov.">
