<taxonomicName id="C1CBB14DE0DD20C9A54E2918723AEC2D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181886" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hizemops Wilson" lsidName-HNS="Pheidole hizemops Wilson" order="Hymenoptera" pageNumber="437" phylum="Arthropoda" rank="species" species="hizemops">Pheidole hizemops</taxonomicName>
