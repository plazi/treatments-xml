<subSubSection id="C3006520315C8E354DC3DBD68E82F942" pageId="2" pageNumber="283" type="materials_examined">
<paragraph id="8BA536AB315C8E354DC3DBD68E82F942" blockId="2.[151,1436,1525,2019]" pageId="2" pageNumber="283">
<emphasis id="B96EEAB9315C8E354DC3DBD68B56F9AA" bold="true" box="[199,376,1525,1550]" pageId="2" pageNumber="283">
<typeStatus id="54A18809315C8E354DC3DBD68B2CF9AA" box="[199,258,1525,1550]" pageId="2" pageNumber="283">Type</typeStatus>
material.
