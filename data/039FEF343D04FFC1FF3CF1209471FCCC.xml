<taxonomicName id="4C3625A13D04FFC4FE5CF04B95A2F84C" authorityName="Huber" authorityYear="2019" box="[503,648,1918,1943]" class="Arachnida" family="Pholcidae" genus="Belisana" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="185" phylum="Arthropoda" rank="species" species="minneriya">
