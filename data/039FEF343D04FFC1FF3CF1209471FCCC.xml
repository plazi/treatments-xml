<taxonomicName id="4C3625A13D04FFC4FC28F0979339F860" authorityName="Huber" authorityYear="2019" box="[899,1043,1954,1979]" class="Arachnida" family="Pholcidae" genus="Belisana" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="185" phylum="Arthropoda" rank="species" species="minneriya">
