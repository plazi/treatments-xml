<geoCoordinate id="EE235007FFAFFF8B9940FC89FBF33390" box="[317,434,891,917]" degrees="121" direction="west" minutes="12" orientation="longitude" pageId="62" pageNumber="63" precision="925" value="-121.2">121°12’W</geoCoordinate>
) (Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, California); 
<geoCoordinate id="EE235007FFAFFF8B992DFC6DFBE433BF" box="[336,421,927,954]" degrees="0" direction="north" minutes="54" orientation="latitude" pageId="62" pageNumber="63" precision="925" value="0.9">0°54’N</geoCoordinate>
