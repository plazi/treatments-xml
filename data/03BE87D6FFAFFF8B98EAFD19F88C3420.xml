<geoCoordinate id="EE235007FFAFFF8B9D63FCC1FFD5334B" box="[1310,1428,819,846]" degrees="146" direction="west" minutes="06" orientation="longitude" pageId="62" pageNumber="63" precision="925" value="-146.1">146°06’W</geoCoordinate>
