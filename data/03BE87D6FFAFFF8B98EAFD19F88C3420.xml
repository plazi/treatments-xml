<geoCoordinate id="EE235007FFAFFF8B9A92FCA5F96D3377" box="[751,812,855,882]" degrees="55" direction="north" orientation="latitude" pageId="62" pageNumber="63" precision="55555" value="55.0">55°N</geoCoordinate>
