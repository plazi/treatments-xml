<taxonomicName id="4C0E4D3BFFE1AE13536305D1C5BEF931" authorityName="Gurjanova" authorityYear="1951" box="[274,484,1675,1697]" class="Malacostraca" family="Dexaminidae" genus="Lepechinella" kingdom="Animalia" order="Amphipoda" pageId="22" pageNumber="1117" phylum="Arthropoda" rank="species" species="arctica">
