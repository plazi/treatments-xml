<taxonomicName id="A0CA4D1B9A1FFFC0FF6F4E2EFB9FF9B4" authority="Zaitzev, Lelej, Storozhenko &amp; Kurzenko, 2006" authorityName="Zaitzev, Lelej, Storozhenko &amp; Kurzenko" authorityYear="2006" box="[151,1079,1598,1624]" class="Insecta" family="Aphididae" genus="Uroleucon" kingdom="Animalia" order="Aphidomorpha" pageId="4" pageNumber="567" phylum="Arthropoda" rank="species" species="lehri" subGenus="Uromelan">
