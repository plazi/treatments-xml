<taxonomicName id="EEC9F9A9FFB1FF84F76DA8FAA2A8A0A5" authorityName="W. F. Erichson" authorityYear="1842" class="Insecta" family="Curculionidae" genus="Melanterius" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="porcatus">
