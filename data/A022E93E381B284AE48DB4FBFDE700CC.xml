<subSubSection id="60910BA3381B284CE4DFB536FB1D0162" box="[197,1232,346,372]" pageId="11" pageNumber="42" type="etymology">
<paragraph id="28345828381B284CE4DFB536FB1D0162" blockId="11.[151,1437,266,1981]" box="[197,1232,346,372]" pageId="11" pageNumber="42">
<emphasis id="1AFF843A381B284CE4DFB536FE870162" bold="true" box="[197,330,346,372]" pageId="11" pageNumber="42">Etymology</emphasis>
. From the 
<typeStatus id="F730E68A381B284CE5D6B536FE330162" box="[460,510,346,372]" pageId="11" pageNumber="42">type</typeStatus>
