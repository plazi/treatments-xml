<taxonomicName id="53E382BE3E3E4F0EFF0F3FDFFD8EFC4B" authority="Enderlein, 1912: 42" authorityName="Enderlein" authorityPageNumber="42" authorityYear="1912" box="[143,570,900,922]" class="Insecta" family="Phoridae" genus="Udamochiras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
<emphasis id="A697252F3E3E4F0EFF0F3FDFFE92FC4B" box="[143,294,900,921]" italics="true" pageId="10" pageNumber="11">Udamochiras</emphasis>
<bibRefCitation id="F07284CC3E3E4F0EFEBD3FDFFD8EFC4B" author="Enderlein G" box="[317,570,900,922]" pageId="10" pageNumber="11" pagination="16 - 45" refId="ref31084" refString="Enderlein G. 1912. Die Phoridenfauna Sud-Brasiliens. Stettiner Entomologische Zeitung 73: 16 - 45." type="journal article" year="1912">Enderlein, 1912: 42</bibRefCitation>
