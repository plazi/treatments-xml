<specimenCode id="DBB69ED55523FFAF21A2FC31B717FC4E" box="[648,768,988,1012]" pageId="7" pageNumber="54">W0045839</specimenCode>
, 
<specimenCode id="DBB69ED55523FFAF23AEFC1AB530FBB5" box="[132,295,1015,1039]" pageId="7" pageNumber="54">W18960003994</specimenCode>
