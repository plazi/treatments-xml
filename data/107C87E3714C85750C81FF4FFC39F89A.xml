<emphasis id="AAA1EAE7714C85750C81FF85FE95FD22" bold="true" box="[151,306,685,710]" pageId="1" pageNumber="432">
<typeStatus id="476E8857714C85750C81FF85FF75FD22" box="[151,210,685,710]" pageId="1" pageNumber="432">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="5FD54D76714C85750D2FFF85FDF4FD22" authority="Cui &amp; Xue &amp; Jiang, 2018" authorityName="Cui &amp; Xue &amp; Jiang" authorityYear="2018" box="[313,595,685,710]" class="Insecta" family="Geometridae" genus="Aquilargilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="432" phylum="Arthropoda" rank="species" species="ceratophora" status="sp. nov.">
<emphasis id="AAA1EAE7714C85750D2FFF85FDF4FD22" box="[313,595,685,710]" italics="true" pageId="1" pageNumber="432">Aquilargilla ceratophora</emphasis>
</taxonomicName>
<taxonomicNameLabel id="B192579C714C85750E4FFF85FD15FD21" box="[601,690,685,710]" pageId="1" pageNumber="432" rank="species">
