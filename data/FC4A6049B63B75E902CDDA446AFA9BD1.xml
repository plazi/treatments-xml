<paragraph id="E971A78E9472E538688690B38539BAE3" pageId="12" pageNumber="103">
<taxonomicName id="135A6E66D0AAA483138885024A20EDAC" LSID="http://zoobank.org/D56A3761-3318-4CC9-BBC3-FE4C77B2FFA1" authority="Chen &amp; Ren" class="Insecta" family="Coccinellidae" genus="Scymnus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scymnus (Orthoscymnus) duomaculatus" order="Coleoptera" pageId="12" pageNumber="103" phylum="Arthropoda" rank="species" species="duomaculatus" subGenus="Orthoscymnus">Scymnus (Orthoscymnus) duomaculatus Chen &amp; Ren</taxonomicName>
<taxonomicNameLabel id="B66504B5B9B21897F94C0DA99D6D09EA" pageId="12" pageNumber="103">sp. n.</taxonomicNameLabel>
